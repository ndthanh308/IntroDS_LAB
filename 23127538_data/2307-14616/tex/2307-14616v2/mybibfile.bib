@article{VIEIRA2020168197,
title = {Scalar fields in a five-dimensional Lovelock black hole spacetime},
journal = {Annals of Physics},
volume = {418},
pages = {168197},
year = {2020},
issn = {0003-4916},
doi = {https://doi.org/10.1016/j.aop.2020.168197},
