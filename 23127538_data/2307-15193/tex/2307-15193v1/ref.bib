@article{CollusionInAuctions1989,
 ISSN = {0769489X, 22726497},
 URL = {http://www.jstor.org/stable/20075758},
 abstract = {Despite substantial legal evidence of collusion in auctions, there has been very little theoretical or empirical work on this subject by economists. This survey paper discusses mechanisms that are likely to facilitate collusion in auctions, as well as methods of detecting the presence of these schemes. The principal message of this paper is that the presence and the characteristics of collusive mechanisms depend critically on the nature of the object being auctioned, and on the particular auction rules. Accordingly, empirical work should be tailored to specific cases. /// Malgré de nombreux exemples juridiques de collusion lors d'enchères, les économistes se sont peu intéressés à ce sujet, en théorie ou en pratique. La présente revue de littérature discute les mécanismes susceptibles de favoriser la collusion dans les enchères, ainsi que des méthodes permettant de détecter celleci. Le principal message de l'article est que la présence et les caractéristiques des mécanismes avec collusion est très sensible à la nature de l'objet mis aux enchères, et aux modalités de l'enchère. Les exemples des appels d'offres d'une part, et des concessions minières d'autre part, servent à illustrer la discussion.},
 author = {Kenneth Hendricks and Robert H. Porter},
 journal = {Annales d'Économie et de Statistique},
