@article{harris_volcanic_2013,
  title = {Volcanic Plume and Bomb Field Masses from Thermal Infrared Camera Imagery},
  author = {Harris, A.J.L. and Delle Donne, D. and Dehn, J. and Ripepe, M. and Worden, A.K.},
  year = {2013},
  month = mar,
  journal = {Earth and Planetary Science Letters},
  volume = {365},
