@article{jeon_ambient_2018,
  title = {Ambient Density Effects on Initial Flow Breakup and Droplet Size Distribution of Hollow-Cone Sprays from Outwardly-Opening {{GDI}} Injector},
  author = {Jeon, Jeeyeon and Moon, Seoksu},
  year = {2018},
