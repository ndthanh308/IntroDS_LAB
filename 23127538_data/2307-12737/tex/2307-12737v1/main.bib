  author = {Mukundan, Anirudh Asuri and Tretola, Giovanni and M{\'e}nard, Thibaut and Herrmann, Marcus and {Navarro-Martinez}, Salvador and Vogiatzaki, Konstantina and {de Motta}, Jorge C{\'e}sar Br{\"a}ndle and Berlemont, Alain},
