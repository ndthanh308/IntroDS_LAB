@article{rimbert_crossover_2011,
  title = {Crossover between {{Rayleigh-Taylor}} Instability and Turbulent Cascading Atomization Mechanism in the Bag-Breakup Regime},
  author = {Rimbert, Nicolas and Castanet, Guillaume},
  year = {2011},
