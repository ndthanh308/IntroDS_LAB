@article{gaudin_3-d_2016,
  title = {3-{{D}} High-Speed Imaging of Volcanic Bomb Trajectory in Basaltic Explosive Eruptions: 3-{{D IMAGING OF VOLCANIC BOMB TRAJECTORY}}},
  shorttitle = {3-{{D}} High-Speed Imaging of Volcanic Bomb Trajectory in Basaltic Explosive Eruptions},
  author = {Gaudin, D. and Taddeucci, J. and Houghton, B. F. and Orr, T. R. and Andronico, D. and Del Bello, E. and Kueppers, U. and Ricci, T. and Scarlato, P.},
