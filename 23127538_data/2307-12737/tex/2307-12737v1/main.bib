@article{gauding_combined_2021,
  title = {On the Combined Effect of Internal and External Intermittency in Turbulent Non-Premixed Jet Flames},
  author = {Gauding, M. and Bode, M. and Denker, D. and Brahami, Y. and Danaila, L. and Varea, E.},
  year = {2021},
