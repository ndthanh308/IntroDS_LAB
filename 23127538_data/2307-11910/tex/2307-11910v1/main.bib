@article{Norton1989,
    author = {Norton, A. J. and Watson, M. G.},
    title = "{Spin modulated X-ray emission from intermediate polars}",
    journal = {Monthly Notices of the Royal Astronomical Society},
    volume = {237},
    number = {4},
    pages = {853-874},
    year = {1989},
    month = {04},
    abstract = "{This paper describes a comparative study of all the intermediate polars observed by EXOSAT. We present the X-ray pulse profiles of each source and show that, in all cases, the modulation can be described in terms of an increasing depth with decreasing energy. By considering the pulse phase-resolved spectra, we show that the modulation cannot be due to simple phase dependent absorption alone, but that models which combine photoelectric absorption with an effect due to self occultation to produce the modulation can explain the data. In addition, models using partial covering of the X-ray emission region produce an improved fit to the overall dataset. To account for the behaviour seen we present a picture of the accretion process using a structured accretion flow and emission region. In this model the absorption and occultation effects act in phase and both arise at the same pole of the white dwarf. The typical sizes of the modulation due to occultation and absorption both imply that the emission region must occupy, on average, at least one quarter of the white dwarf's surfce.}",
    issn = {0035-8711},
    doi = {10.1093/mnras/237.4.853},
    url = {https://doi.org/10.1093/mnras/237.4.853},
    eprint = {https://academic.oup.com/mnras/article-pdf/237/4/853/3922432/mnras237-0853.pdf},
}



