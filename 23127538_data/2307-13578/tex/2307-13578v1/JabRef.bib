@Article{SokZhi08,
  Title                    = {how well a chaotic quantum system can retain memory of its initial state?},
  Author                   = {V. V. Sokolov and O. V. Zhirov},
  Journal                  = EPL,
  Year                     = {2008},
  Note                     = {Preprint only},
  Number                   = {3},
  Pages                    = {30001 (5pp)},
  Volume                   = {84},

  Abstract                 = {In classical mechanics the local exponential instability effaces the memory of initial conditions and leads to practical irreversibility. In striking contrast, quantum mechanics appears to exhibit strong memory of the initial state. We relate the latter fact to the low (at most linear) rate with which the system's Wigner function gets during the evolution a more and more complicated structure and we establish the existence of a critical strength of external influence below which such a memory still survives.},
