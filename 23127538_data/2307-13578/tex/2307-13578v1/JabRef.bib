@Article{Sok09,
  Title                    = {quantum dynamics against a noisy background},
  Author                   = {V. V. Sokolov and O. V. Zhirov and Y. A. Kharkov},
  Journal                  = EPL,
  Year                     = {2009},
  Number                   = {6},
  Pages                    = {60002 (5pp)},
  Volume                   = {88},

  Abstract                 = {By the example of a kicked quartic oscillator we investigate the dynamics of classically chaotic quantum systems with few degrees of freedom affected by persistent external noise. Stability and reversibility of the motion are analyzed in detail in dependence on the noise level s. The critical level sc(t), below which the response of the system to the noise remains weak, is studied vs. the evolution time. In the regime with the Ehrenfest time interval tE so short that the classical Lyapunov exponential decay of the Peres fidelity does not show up the time dependence of this critical value is proved to be power-like. We estimate also the decoherence time after which the motion turns into a Markovian process.},
  Annote                   = {Project:Gustavo/Ruido},
  File                     = {:../Paper/EPL/Sokolov09EPL.pdf:PDF;:C\:\\cygwin64\\home\\TG\\Documentos/Paper/EPL/Sokolov09EPL.pdf:PDF},
  Keywords                 = {echo dynamics, decoherence, noise, quantum chaos},
