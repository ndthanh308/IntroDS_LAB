%% various environments

\newenvironment{comments}
{\begin{list}{}{
\setlength{\labelsep}{1em}\setlength{\leftmargin}{0pt}%
\setlength{\labelwidth}{0pt}%
\setlength{\listparindent}{0pt}}}
{\end{list}}
\def\com#1{\item[$\mathbf{#1}$]}

\def\dash{\list{---}{
  \settowidth{\labelwidth}{---} \itemindent=\labelwidth
  \leftmargin=0pt
  \itemsep=1ex plus 1pt minus 1pt
  \def\makelabel##1{##1\hss}}}
\let\enddash\endlist

\newenvironment{enumdashi}
{\begin{list}{{\bf \arabic{enumi}}}
        {\usecounter{enumi}
         \setlength{\leftmargin}{0pt}
         \setlength{\labelwidth}{0pt}
         \setlength{\listparindent}{\parindent} 
        }}%
{\end{list}}

\newenvironment{enumdashii}
{\begin{list}{{\bf \arabic{enumi}.\alph{enumii}}}
        {\usecounter{enumii}
         \setlength{\leftmargin}{0pt}
         \setlength{\labelwidth}{0pt}
         \setlength{\listparindent}{\parindent} 
        }}%
{\end{list}}

\newenvironment{enumdashiii}
{\begin{list}{{\bf \arabic{enumi}.\alph{enumii}.\roman{enumiii}}}
        {\usecounter{enumiii}
         \setlength{\leftmargin}{0pt}
         \setlength{\labelwidth}{0pt}
         \setlength{\listparindent}{\parindent} 
        }}%
{\end{list}}

\newenvironment{enumeraterm}
{\begin{enumerate}\renewcommand{\labelenumi}{(\roman{enumi})}}
{\end{enumerate}}

