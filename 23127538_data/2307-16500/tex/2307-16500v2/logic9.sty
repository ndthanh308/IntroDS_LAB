%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Time %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcount\hour \hour=\time
\newcount\minute \minute=\hour
\divide\hour by 60
\newcount\temp \temp=\hour
\multiply\temp by 60
\advance\minute by -\temp
\newcount\temp \temp=\minute
\divide\temp by 10
\def\optzero{\ifcase\temp 0\fi}
\def\czas{\number\hour :$\,$\optzero\number\minute}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Date %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\def\miesiaca{\ifcase\month\or stycznia\or lutego\or marca\or kwietnia\or maja
                           \or czerwca\or lipca\or sierpnia\or wrze\'snia
                           \or pa\'zdziernika\or listopada\or grudnia\fi}
\def\dzisiaj{\ \number\day\ \miesiaca\ \number\year}
