	journal = {Theoretical and Applied Fracture Mechanics},
	author = {Morel, F. and Huyen, N.},
	month = feb,
	year = {2008},
	keywords = {High cycle fatigue, Microcrack, Multiaxial loading, Probability},
	pages = {98--127}
},

