@article{persson_theory_1993,
	title = {Theory and simulation of sliding friction},
	volume = {71},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.71.1212},
	doi = {10.1103/PhysRevLett.71.1212},
	abstract = {When an external force acts on an adsorbate structure, the structure may slide or flow relative to the substrate. The mechanism behind this sliding motion is of fundamental importance for the understanding of friction and lubrication between two flat macroscopic surfaces. Here I present a study of the nonlinear (in the external driving force) sliding friction which is found to exhibit hysteresis giving rise to the well-known phenomenon of ‘‘stick-and-slip’’ motion. The theory predicts that for a large class of sliding systems, the ratio fk/fs between the kinetic and the static friction coefficients equals 1/2, in good agreement with experimental data.},
