@article{sen_atomistic_2010,
	title = {Atomistic Study of {Crack-Tip} Cleavage to Dislocation Emission Transition in Silicon Single Crystals},
	volume = {104},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.104.235502},
	doi = {10.1103/PhysRevLett.104.235502},
	abstract = {At low temperatures silicon is a brittle material that shatters catastrophically, whereas at elevated temperatures, the behavior of silicon changes drastically over a narrow temperature range and suddenly becomes ductile. This brittle-to-ductile transition has been observed in experimental studies, yet its fundamental mechanisms remain unknown. Here we report an atomistic-level study of a fundamental event in this transition, the change from brittle cleavage fracture to dislocation emission at crack tips, using the first principles based reactive force field. By solely raising the temperature, we observe an abrupt change from brittle cracking to dislocation emission from a crack within a ≈10 K temperature interval.},
	number = {23},
