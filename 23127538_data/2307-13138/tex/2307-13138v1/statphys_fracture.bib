@article{lippiello_influence_2008,
	title = {Influence of Time and Space Correlations on Earthquake Magnitude},
	volume = {100},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.100.038501},
	doi = {10.1103/PhysRevLett.100.038501},
	abstract = {A crucial point in the debate on the feasibility of earthquake predictions is the dependence of an earthquake magnitude from past seismicity. Indeed, while clustering in time and space is widely accepted, much more questionable is the existence of magnitude correlations. The standard approach generally assumes that magnitudes are independent and therefore in principle unpredictable. Here we show the existence of clustering in magnitude: earthquakes occur with higher probability close in time, space, and magnitude to previous events. More precisely, the next earthquake tends to have a magnitude similar but smaller than the previous one. A dynamical scaling relation between magnitude, time, and space distances reproduces the complex pattern of magnitude, spatial, and temporal correlations observed in experimental seismic catalogs.},
	number = {3},
	journal = {Physical Review Letters},
	author = {Lippiello, E. and de Arcangelis, L. and Godano, C.},
	month = jan,
	year = {2008},
	pages = {038501}
},

 	
