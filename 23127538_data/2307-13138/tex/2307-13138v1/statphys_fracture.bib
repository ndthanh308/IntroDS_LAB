@article{holtzman_crossover_2010,
	title = {Crossover from fingering to fracturing in deformable disordered media},
	volume = {82},
	url = {http://link.aps.org/doi/10.1103/PhysRevE.82.046305},
	doi = {10.1103/PhysRevE.82.046305},
	abstract = {We investigate the displacement of one fluid by another in a deformable medium with pore-scale disorder. We develop a model that captures the dynamic pressure redistribution at the invasion front and the feedback between fluid invasion and microstructure rearrangement. Our results suggest how to collapse the transition between invasion percolation and viscous fingering in the presence of quenched disorder. We predict the emergence of a fracturing pattern for sufficiently deformable media, in agreement with observations of drainage in granular material. We identify a dimensionless number that appears to govern the crossover from fingering to fracturing.},
