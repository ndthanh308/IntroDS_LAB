@article{crane_power_2010,
	title = {Power law signature of media exposure in human response waiting time distributions},
	volume = {81},
	url = {http://link.aps.org/doi/10.1103/PhysRevE.81.056101},
	doi = {10.1103/PhysRevE.81.056101},
	abstract = {We study the humanitarian response to the destruction brought by the tsunami generated by the Sumatra earthquake of December 26, 2004, as measured by donations, and find that it decays in time as a power law ∼1/tα with α=2.5±0.1. This behavior is suggested to be the rare outcome of a priority queuing process in which individuals execute tasks at a rate slightly faster than the rate at which new tasks arise. We believe this to be an empirical evidence documenting the recently predicted {[G.} Grinstein and R. Linsker, Phys. Rev. E 77, 012101 (2008)] regime, and provide additional independent evidence that suggests that this “highly attentive regime” arises as a result of the intense focus placed on this donation “task” by the media.},
	number = {5},
