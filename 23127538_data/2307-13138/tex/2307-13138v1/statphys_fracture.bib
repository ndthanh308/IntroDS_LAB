@article{miyazima_aggregation_1987,
	title = {Aggregation of oriented anisotropic particles},
	volume = {36},
	url = {http://link.aps.org/doi/10.1103/PhysRevA.36.1421},
	doi = {10.1103/PhysRevA.36.1421},
	abstract = {The diffusion-limited aggregation of oriented anisotropic particles is explored with use of computer simulations and scaling theory. The time-dependent cluster-size distribution (in both two- and three-dimensional systems) can be well represented by the dynamic scaling form for the cluster-size distribution {Ns(t)=s-2f(s/S(t))} where Ns(t) is the number of clusters of size s at time t. The mean cluster size S(t) diverges as S(t)∼tz. The value of the exponent z is found to agree with the mean-field Smoluchowski result z=1/(1-γ) in d≥dc=2 dimensions, where dc is the upper critical dimension, and γ is the exponent describing the dependence of the diffusion coefficient on the cluster mass. Below dc, we find that our results agree with the expression z=d/(2-dγ). At high particle densities, a crossover from two or three dimensions to one-dimensional behavior is observed.},
	number = {3},
	journal = {Physical Review A},
	author = {Miyazima, Sasuke and Meakin, Paul and Family, Fereydoon},
