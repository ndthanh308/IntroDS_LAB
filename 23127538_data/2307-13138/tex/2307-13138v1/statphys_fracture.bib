	keywords = {Brittle, Concrete, Damage, Discrete element method, Fracturing, Grain, Heterogeneous, numerical simulation},
	pages = {31--38}
},

@article{chen_silkworm_2010-1,
	title = {Silkworm cocoons inspire models for random fiber and particulate composites},
	volume = {82},
	url = {http://link.aps.org/doi/10.1103/PhysRevE.82.041911},
	doi = {10.1103/PhysRevE.82.041911},
	abstract = {The bioengineering design principles evolved in silkworm cocoons make them ideal natural prototypes and models for structural composites. Cocoons depend for their stiffness and strength on the connectivity of bonding between their constituent materials of silk fibers and sericin binder. Strain-activated mechanisms for loss of bonding connectivity in cocoons can be translated directly into a surprisingly simple yet universal set of physically realistic as well as predictive quantitative structure-property relations for a wide range of technologically important fiber and particulate composite materials.},
	number = {4},
	journal = {Physical Review E},
	author = {Chen, Fujia and Porter, David and Vollrath, Fritz},
