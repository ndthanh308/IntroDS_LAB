@misc {Ha2023,
	Title = {Hybrid QAOA and Genetic Algorithm for Solving Max-Cut Problem},
	Author = {Ha, Huy Phuc Nguyen and Ta, Anh Son},
	DOI = {10.21203/rs.3.rs-2800852/v1},
	Abstract = {Quantum Approximate Optimization Algorithm (QAOA) is one of the variational quantum optimizations that is used for solving combinatorial optimization. The QAOA calculates the average of all solutions provided by the quantum circuit. To improve the result, we investigate a new method to combine QAOA and genetic algorithms. The result of QAOA is considered as an initial population method for a genetic algorithm. This approach is applied to solve the max-cut problem, which is very important for quantum computing research. This method is applied to benchmark datasets and the results have been improved significantly.},
	Publisher = {Research Square},
	Year = {2023},
	URL = {https://doi.org/10.21203/rs.3.rs-2800852/v1},
}


