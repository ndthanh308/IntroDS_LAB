@article {Ely429,
	author = {Ely, John W and Osheroff, Jerome A and Gorman, Paul N and Ebell, Mark H and Chambliss, M Lee and Pifer, Eric A and Stavri, P Zoe},
	title = {A taxonomy of generic clinical questions: classification study},
	volume = {321},
	number = {7258},
	pages = {429--432},
	year = {2000},
	doi = {10.1136/bmj.321.7258.429},
	publisher = {BMJ Publishing Group Ltd},
	abstract = {Objective: To develop a taxonomy of doctors{\textquoteright} questions about patient care that could be used to help answer such questions. Design: Use of 295 questions asked by Oregon primary care doctors to modify previously developed taxonomy of 1101 clinical questions asked by Iowa family doctors. Setting: Primary care practices in Iowa and Oregon. Participants: Random samples of 103 Iowa family doctors and 49 Oregon primary care doctors. Main outcome measures: Consensus among seven investigators on a meaningful taxonomy of generic questions; interrater reliability among 11 individuals who used the taxonomy to classify a random sample of 100 questions: 50 from Iowa and 50 from Oregon. Results: The revised taxonomy, which comprised 64 generic question types, was used to classify 1396 clinical questions. The three commonest generic types were {\textquotedblleft}What is the drug of choice for condition x?{\textquotedblright} (150 questions, 11\%); {\textquotedblleft}What is the cause of symptom x?{\textquotedblright} (115 questions, 8\%); and {\textquotedblleft}What test is indicated in situation x?{\textquotedblright} (112 questions, 8\%). The mean interrater reliability among 11 coders was moderate (κ=0.53, agreement 55\%). Conclusions: Clinical questions in primary care can be categorised into a limited number of generic types. A moderate degree of interrater reliability was achieved with the taxonomy developed in this study. The taxonomy may enhance our understanding of doctors{\textquoteright} information needs and improve our ability to meet those needs.},
	issn = {0959-8138},
	URL = {http://www.bmj.com/content/321/7258/429.1},
	eprint = {http://www.bmj.com/content/321/7258/429.1.full.pdf},
	journal = {BMJ}
