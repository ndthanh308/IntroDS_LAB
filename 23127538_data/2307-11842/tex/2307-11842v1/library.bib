journal = {SPE/AAPG Western Regional Meeting 2000},
keywords = {SPE 62550,laser,laser drilling},
mendeley-tags = {laser,laser drilling},
pages = {1--10},
title = {{Electromagnetic Heating Methods for Heavy Oil Reservoirs}},
year = {2000}
}
