@inproceedings{Graves2002,
abstract = {AbstractThe temperature effects of high power lasers on rock strength and mechanical properties are presented. It was found that the high temperature induced by lasers drilling through rock enhances porosity and permeability and reduces strength. High temperature causes microcracks, vaporizes cementation, and dehydrates clays.Microcracks were monitored using thin sections and SEM analyses. Representative samples were cut for thin section analyses before and after lasing. For the SEM test, a small fragment of the sample was removed and coated with gold for better electron conductivity, thus allowing fracture imaging in three dimensions.Phase changes also affect rock properties such as permeability, porosity and strength. The effect depends on the temperature distribution and gradient as well as the mineralogy. At each temperature, different reactions take place. For example, at 550{\^{A}}°C smectite clay will collapse and dehydrate. This mechanism results in increasing void space in the rock sample. At the same temperature, quartz grains will expand by 1.75{\%}1 and when the grains cool, microcracks are created, which also increases the void space. This increase in void space may or may not change permeability, but it does decrease the acoustic velocities that in turn change the calculated porosity and elastic moduli.Results show in all cases, the Young's modulus, shear modulus, bulk modulus, and combined modulus near the lased portion, were reduced when compared to the unlased portion.In sandstones, the decrease was 50{\%} or more. A bulk modulus decreased of 50{\%} causes the compressibility near the wellbore to increase by 50{\%}. Depending on the lithology, Poisson's ratio decreased or increased slightly. For limestones and shales, the differences between the moduli of lased and unlased samples were less significant than in sandstone. IntroductionReservoir rock strength and mechanical (elastic) properties have applications in many areas relating to petroleum engineering. Many of these applications, such as; compaction, borehole stability, friable sand stability, hydraulic fracture design, algorithms used in logging, compressive strength for calculating the failure envelope, and anelastic behavior models used to estimate in-situ stress, are near wellbore phenomena. Temperature induced changes to reservoir rocks are also near wellbore phenomena.Traditionally, temperature effects on rocks are considered minimal when compared to the effects of pressure and introduced fluids, such as completion fluids and drilling mud. Application of lasers for rock destruction in drilling and perforating generates a tremendous temperature which changes the rock structure and therefore, the rock strength and elastic properties.The thermal properties of rocks are very complex, as are most rock properties. They are a function of mineralogy, density, melting temperature, porosity, permeability, etc. Whether the changes in rock properties are considered positive or negative depends on the applications mentioned above. For example, high thermal conductivity results in a wide range of temperature distribution resulting in improved porosity and permeability and decreased rock strength. Weakening the rock is good for hydraulic fracturing applications. Low thermal conductivity does not allow the rock to weaken as far into the formation which is good for sand control.},
address = {Irvine, Texas},
author = {Graves, R M and Batarseh, S and Parker, Richard A and Gahan, Brian C},
booktitle = {SPE/ISRM Rock Mechanics Conference},
doi = {10.2118/78154-MS},
file = {:Users/sanromd/Dropbox/research/library/SPEISRM Rock Mechanics Conference/2002{\_}Temperatures Induced by High Power Lasers Effects on Reservoir Rock Strength and Mechanical Properties{\_}Graves et al.pdf:pdf},
keywords = {laser,laser drilling},
mendeley-tags = {laser,laser drilling},
pages = {SPE/ISRM--78154: 1--7},
publisher = {Society of Petroleum Engineers},
title = {{Temperatures Induced by High Power Lasers: Effects on Reservoir Rock Strength and Mechanical Properties}},
url = {http://dx.doi.org/10.2118/78154-MS},
year = {2002}
}
