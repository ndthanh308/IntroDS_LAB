keywords = {Crude petroleum,Drilling,Error analysis,Extended-reach drilling,High energy lasers,Mathematical models,Numerical analysis,Oil wells,Petroleum wells,Ultrafast drilling,Vaporization,laser drilling,laser-rock interaction,numerical modeling,rocks},
