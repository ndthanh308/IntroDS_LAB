journal = {Optics and Laser Technology},
keywords = {Laser drilling,Melt ejection,Repeatability,laser,laser drilling},
mendeley-tags = {laser,laser drilling},
pages = {393--402},
title = {{The effect of laser peak power and pulse width on the hole geometry repeatability in laser percussion drilling}},
volume = {33},
year = {2001}
}
