title = {{Chalcogenide glass photonic crystals}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1569441007000661 http://dx.doi.org/10.1016/j.photonics.2007.11.001},
volume = {6},
year = {2008}
}
