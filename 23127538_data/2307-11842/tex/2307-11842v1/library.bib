journal = {Laser Physics Letters},
language = {en},
month = {jul},
number = {7},
pages = {532--536},
publisher = {IOP Publishing},
title = {{Suppressing the stimulated Brillouin scattering in high power fiber amplifiers by dual-single-frequency amplification}},
url = {http://iopscience.iop.org/1612-202X/9/7/011},
volume = {9},
year = {2012}
