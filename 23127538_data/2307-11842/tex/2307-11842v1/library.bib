month = {aug},
number = {17},
pages = {20326--46},
pmid = {25321242},
publisher = {OSA},
shorttitle = {Opt. Express},
title = {{SBS management in Yb-fiber-amplifiers using multimode seeds and pulse-shaping.}},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-22-17-20326},
volume = {22},
