journal = {Technology},
number = {x},
pages = {1--14},
title = {{Gaussian Beam Optics}},
url = {http://www.cvimellesgriot.com/Products/Documents/TechnicalGuide/Gaussian-Beam-Optics.pdf},
year = {2006}
