journal = {Journal of Solid State Chemistry},
keywords = {Al4SiC4,Aluminum silicon oxycarbide,Crystal structure,New material,Powder diffraction,Rietveld method},
number = {3},
pages = {636--642},
publisher = {Elsevier},
title = {{Synthesis and structural characterization of Al4SiC4-homeotypic aluminum silicon oxycarbide, [Al4.4Si0.6][O1.0C2.0]C}},
url = {http://dx.doi.org/10.1016/j.jssc.2010.01.012},
volume = {183},
year = {2010}
}
