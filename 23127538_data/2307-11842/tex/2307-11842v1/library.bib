journal = {Journal of Petroleum Science and Engineering},
keywords = {effective thermal conductivity,high pressure and temperature,laser drilling,laser-rock interaction,numerical modeling,optimal neural networks,rocks,sandstone,thermodynamics,thermophysics},
mendeley-tags = {laser drilling,laser-rock interaction,numerical modeling,rocks,thermodynamics,thermophysics},
month = {jul},
pages = {69--78},
title = {{Modeling and analysis of effective thermal conductivity of sandstone at high pressure and temperature using optimal artificial neural networks}},
url = {http://www.sciencedirect.com/science/article/pii/S0920410514001016},
volume = {119},
year = {2014}
}
