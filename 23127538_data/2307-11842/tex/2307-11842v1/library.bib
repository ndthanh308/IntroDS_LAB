journal = {Journal of Petroleum Science and Engineering},
keywords = {Heavy oil reservoir,Improved oil recovery,Multi-thermal fluid,SAGD,Thermal recovery,aquathermolysis,chemistry,fluid,heavy oil,review},
mendeley-tags = {aquathermolysis,chemistry,fluid,heavy oil,review},
month = {sep},
pages = {1--11},
title = {{Multi-thermal fluid assisted gravity drainage process: A new improved-oil-recovery technique for thick heavy oil reservoir}},
