@article{eibensteiner2021people,
  title = {People's {{Willingness}} to {{Vaccinate Against COVID-19 Despite Their Safety Concerns}}: {{Twitter Poll Analysis}}},
  shorttitle = {People's {{Willingness}} to {{Vaccinate Against COVID-19 Despite Their Safety Concerns}}},
  author = {Eibensteiner, Fabian and Ritschl, Valentin and Nawaz, Faisal A. and Fazel, Sajjad S. and Tsagkaris, Christos and Kulnik, Stefan Tino and Crutzen, Rik and Klager, Elisabeth and {V{\"o}lkl-Kernstock}, Sabine and Schaden, Eva and {Kletecka-Pulker}, Maria and Willschke, Harald and Atanasov, Atanas G.},
