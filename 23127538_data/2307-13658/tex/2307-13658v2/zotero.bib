@article{ross2022women,
  title = {Women Are Credited Less in Science than Men},
  author = {Ross, Matthew B. and Glennon, Britta M. and {Murciano-Goroff}, Raviv and Berkes, Enrico G. and Weinberg, Bruce A. and Lane, Julia I.},
  year = {2022},
  month = aug,
  journal = {Nature},
  volume = {608},
  number = {7921},
  pages = {135--145},
  issn = {0028-0836, 1476-4687},
  doi = {10.1038/s41586-022-04966-w},
  urldate = {2023-05-18},
  abstract = {Abstract                            There is a well-documented gap between the observed number of works produced by women and by men in science, with clear consequences for the retention and promotion of women               1               . The gap might be a result of productivity differences               2\textendash 5               , or it might be owing to women's contributions not being acknowledged               6,7               . Here we find that at least part of this gap is the result of unacknowledged contributions: women in research teams are significantly less likely than men to be credited with authorship. The findings are consistent across three very different sources of data. Analysis of the first source\textemdash large-scale administrative data on research teams, team scientific output and attribution of credit\textemdash show that women are significantly less likely to be named on a given article or patent produced by their team relative to their~male peers. The gender gap in attribution is present across most scientific fields and almost all~career stages. The second source\textemdash an extensive survey of authors\textemdash similarly shows that women's scientific contributions are systematically less likely to be recognized. The third source\textemdash qualitative responses\textemdash suggests that the reason that women are less likely to be credited is because their work is often not known, is not appreciated or is ignored. At least some of the observed gender gap in scientific output may be owing not to differences in scientific contribution, but rather to differences in attribution.},
  langid = {english},
  file = {/Users/grabowicz/Zotero/storage/XNLEKDZN/Ross et al. - 2022 - Women are credited less in science than men.pdf}
