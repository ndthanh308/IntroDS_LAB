@article{rosenkrantz2017use,
  title = {Use of {{Twitter Polls}} to {{Determine Public Opinion Regarding Content Presented}} at a {{Major National Specialty Society Meeting}}},
  author = {Rosenkrantz, Andrew B. and Hawkins, C. Matthew},
