@article{gargouri2010selfselected,
  title = {Self-Selected or Mandated, Open Access Increases Citation Impact for Higher Quality Research},
  author = {Gargouri, Yassine and Hajjem, Chawki and Larivi{\'e}re, Vincent and Gingras, Yves and Carr, Les and Brody, Tim and Harnad, Stevan},
  year = {2010},
  journal = {PLoS ONE},
  volume = {5},
