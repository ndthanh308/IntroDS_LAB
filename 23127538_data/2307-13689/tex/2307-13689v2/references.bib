@article{Terauchi2011a,
abstract = {We observed the parameters of the discharge-produced plasma in cylindrical capillary. Plasma parameters of the waveguide were investigated by use of both a Normarski laser interferometer and a hydrogen plasma line spectrum. A space-averaged maximum temperature of 3.3 eV with electron densities of the order of 10 17 cm-3 was observed at a discharge time of 150 ns and a maximum discharge current of 200 A. One-dimensional dissipative magnetohydrodynamic (MHD) code was used to analyze the discharge dynamics in the gas-filled capillary discharge waveguide for high-intensity laser pulses. Simulations were performed for the conditions of the experiment. We compared the temporal behavior of the electron temperature and the radial electron density profiles, measured in the experiment with the results of the numerical simulations. They occurred to be in a good agreement. An ultrashort, intense laser pulse was guided by use of this plasma channel. {\textcopyright} 2011 American Institute of Physics.},
author = {Terauchi, Hiromitsu and Bobrova, Nadezhda and Sasorov, Pavel and Kikuchi, Takashi and Sasaki, Toru and Higashiguchi, Takeshi and Yugami, Noboru and Kodama, Ryosuke},
doi = {10.1063/1.3560529},
file = {:Users/alexpicksley/Documents/Mendeley Desktop/Journal of Applied Physics/Terauchi et al. - 2011 - Observation and numerical analysis of plasma parameters in a capillary discharge-produced plasma channel wav(2).pdf:pdf},
issn = {00218979},
journal = {Journal of Applied Physics},
mendeley-groups = {Plasma Channels},
number = {5},
title = {{Observation and numerical analysis of plasma parameters in a capillary discharge-produced plasma channel waveguide}},
volume = {109},
year = {2011}
}
@article{Hansen2006,
abstract = {High Mach number blast waves were created by focusing a laser pulse on a solid pin, surrounded by nitrogen or xenon gas. In xenon, the initial shock is strongly radiative, sending out a supersonic radiative heat wave far ahead of itself. The shock propagates into the heated gas, diminishing in strength as it goes. The radiative heat wave also slows, and when its Mach number drops to two with respect to the downstream plasma, the heat wave drives a second shock ahead of itself to satisfy mass and momentum conservation in the heat wave reference frame; the heat wave becomes subsonic behind the second shock. For some time both shocks are observed simultaneously. Eventually the initial shock diminishes in strength so much that it can longer be observed, but the second shock continues to propagate long after this time. This sequence of events is a new phenomenon that has not previously been discussed in the literature. Numerical simulation clarifies the origin of the second shock, and its position is consistent with an analytical estimate. {\textcopyright} 2006 American Institute of Physics.},
author = {Hansen, J. F. and Edwards, M. J. and Froula, D. H. and Gregori, G. and Edens, A. D. and Ditmire, T.},
doi = {10.1063/1.2168157},
file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physics of Plasmas/Hansen et al. - 2006 - Laboratory observation of secondary shock formation ahead of a strongly radiative blast wave.pdf:pdf},
issn = {1070664X},
journal = {Physics of Plasmas},
mendeley-groups = {Shock Papers - Random},
number = {2},
title = {{Laboratory observation of secondary shock formation ahead of a strongly radiative blast wave}},
volume = {13},
year = {2006}
}
