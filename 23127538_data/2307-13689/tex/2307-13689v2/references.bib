file = {:Users/alexpicksley/Documents/Mendeley Desktop/Propellants, Explosives, Pyrotechnics/Knock, Davies, Reeves - 2015 - Predicting blast waves from the axial direction of a cylindrical charge.pdf:pdf},
issn = {15214087},
journal = {Propellants, Explosives, Pyrotechnics},
keywords = {Blast,Cylindrical charges,Secondary shock waves},
mendeley-groups = {Blast Waves},
number = {2},
