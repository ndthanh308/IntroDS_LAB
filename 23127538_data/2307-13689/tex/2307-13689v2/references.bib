title = {{Effect of laser-focusing conditions on propagation and monoenergetic electron production in laser-wakefield accelerators}},
volume = {98},
year = {2007}
}

@article{Lu2006,
abstract = {We present a theory for nonlinear, multidimensional plasma waves with phase velocities near the speed of light. It is appropriate for describing plasma waves excited when all electrons are expelled out from a finite region by either the space charge of a short electron beam or the radiation pressure of a short intense laser. It works very well for the first bucket before phase mixing occurs. We separate the plasma response into a cavity or blowout region void of all electrons and a sheath of electrons just beyond the cavity. This simple model permits the derivation of a single equation for the boundary of the cavity. It works particularly well for narrow electron bunches and for short lasers with spot sizes matched to the radius of the cavity. It is also used to describe the structure of both the accelerating and focusing fields in the wake. {\textcopyright} 2006 The American Physical Society.},
author = {Lu, W. and Huang, C. and Zhou, M. and Mori, W. B. and Katsouleas, T.},
doi = {10.1103/PhysRevLett.96.165002},
file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physical Review Letters/Lu et al. - 2006 - Nonlinear theory for relativistic plasma wakefields in the blowout regime.pdf:pdf},
issn = {00319007},
journal = {Physical Review Letters},
mendeley-groups = {Important LWFA Papers},
number = {16},
