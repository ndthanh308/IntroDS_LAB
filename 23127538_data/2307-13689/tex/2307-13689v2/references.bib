journal = {Applied Physics B: Lasers and Optics},
mendeley-groups = {LWFA Theory Papers},
number = {4-5},
pages = {355--361},
title = {{Laser wake field acceleration: The highly non-linear broken-wave regime}},
volume = {74},
year = {2002}
}
