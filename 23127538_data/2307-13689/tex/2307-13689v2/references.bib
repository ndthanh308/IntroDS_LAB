doi = {10.1103/PhysRevSTAB.14.041301},
file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physical Review Special Topics - Accelerators and Beams/Kimura et al. - 2011 - Hollow plasma channel for positron plasma wakefield acceleration.pdf:pdf},
issn = {10984402},
journal = {Physical Review Special Topics - Accelerators and Beams},
mendeley-groups = {Plasma Channels},
number = {4},
pages = {1--11},
title = {{Hollow plasma channel for positron plasma wakefield acceleration}},
volume = {14},
year = {2011}
