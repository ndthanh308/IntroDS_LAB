file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physical Review X/G{\"{o}}tzfried et al. - 2020 - Physics of High-Charge Electron Beams in Laser-Plasma Wakefields.pdf:pdf},
issn = {2160-3308},
