file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physics of Plasmas/Schroeder et al. - 2013 - Control of focusing forces and emittances in plasma-based accelerators using near-hollow plasma channels(2).pdf:pdf},
issn = {1070-664X},
journal = {Physics of Plasmas},
mendeley-groups = {Plasma Channels/Low Density Channels,Plasma Channels/Pulse Propagation in Channels},
number = {8},
pages = {080701},
title = {{Control of focusing forces and emittances in plasma-based accelerators using near-hollow plasma channels}},
volume = {20},
year = {2013}
}
