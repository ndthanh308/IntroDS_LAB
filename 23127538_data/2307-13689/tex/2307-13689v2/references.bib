journal = {Studia Geophysica et Geodaetica},
keywords = {Bessel beams,Bessel-Gauss beams,Gaussian beams,wave propagation},
mendeley-groups = {Laser Theory},
number = {2},
pages = {355--372},
title = {{A tale of two beams: An elementary overview of Gaussian beams and Bessel beams}},
volume = {56},
year = {2012}
}
