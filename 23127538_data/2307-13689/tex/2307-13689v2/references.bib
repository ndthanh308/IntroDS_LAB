doi = {10.1063/1.1482156},
file = {:Users/alexpicksley/Documents/Mendeley Desktop/Review of Scientific Instruments/Dribinski et al. - 2002 - Reconstruction of Abel-transformable images The Gaussian basis-set expansion Abel transform method.pdf:pdf},
