journal = {Plasma Physics and Controlled Fusion},
mendeley-groups = {Injection},
number = {8},
title = {{Role of direct laser acceleration in energy gained by electrons in a laser wakefield accelerator with ionization injection}},
volume = {56},
year = {2014}
}
