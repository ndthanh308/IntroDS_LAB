doi = {10.1103/PhysRevSTAB.15.011302},
file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physical Review Special Topics - Accelerators and Beams/Mangles et al. - 2012 - Self-injection threshold in self-guided laser wakefield accelerators.pdf:pdf},
