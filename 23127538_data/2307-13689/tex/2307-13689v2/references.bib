file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physical Review Letters/Jalas et al. - 2021 - Bayesian Optimization of a Laser-Plasma Accelerator.pdf:pdf},
issn = {10797114},
journal = {Physical Review Letters},
keywords = {doi:10.1103/PhysRevLett.126.104801 url:https://doi},
mendeley-groups = {Important LWFA Papers},
