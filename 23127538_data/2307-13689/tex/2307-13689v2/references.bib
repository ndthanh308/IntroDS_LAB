doi = {10.1364/oe.410343},
file = {:Users/alexpicksley/Documents/Mendeley Desktop/Optics Express/Borot, Qu{\'{e}}r{\'{e}} - 2018 - Spatio-spectral metrology at focus of ultrashort lasers a phase-retrieval approach erratum.pdf:pdf},
