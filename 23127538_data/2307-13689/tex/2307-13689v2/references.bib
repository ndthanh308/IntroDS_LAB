file = {:Users/alexpicksley/Documents/Mendeley Desktop/Physical Review X/Cole et al. - 2018 - Experimental Evidence of Radiation Reaction in the Collision of a High-Intensity Laser Pulse with a Laser-Wakefield.pdf:pdf},
issn = {21603308},
journal = {Physical Review X},
mendeley-groups = {LPA Applications},
number = {1},
title = {{Experimental Evidence of Radiation Reaction in the Collision of a High-Intensity Laser Pulse with a Laser-Wakefield Accelerated Electron Beam}},
volume = {8},
year = {2018}
}
