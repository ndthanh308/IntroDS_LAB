issn = {13616587},
journal = {Plasma Physics and Controlled Fusion},
keywords = {blowout regime,comb-like electron beams,inverse Thomson scattering,laser plasma acceleration,negatively chirped pulse,plasma channel},
mendeley-groups = {LPA Applications,LPA Electron Acceleration Studies},
number = {3},
publisher = {IOP Publishing},
title = {{Controlled generation of comb-like electron beams in plasma channels for polychromatic inverse Thomson $\gamma$-ray sources}},
volume = {58},
year = {2016}
}
