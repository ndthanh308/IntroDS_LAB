@article{HuHu88,
title = "Space-time finite element methods for elastodynamics: Formulations and error estimates",
OPTjournal = "Computer Methods in Applied Mechanics and Engineering",
JOURNAL = {Comput. Methods Appl. Mech. Engrg.},
volume = "66",
number = "3",
