@article{JaLo08,
author = {Jansson, Johan and Logg, Anders},
title = {Algorithms and Data Structures for Multi-Adaptive Time-Stepping},
year = {2008},
issue_date = {October 2008},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {35},
number = {3},
