journal = {Opt. Lett.},
keywords = {Laser trapping; Materials; Defect-center materials ; Fluorescent and luminescent materials; Optical tweezers or optical manipulation; High power lasers; Laser light; Nitrogen vacancy centers; Photoluminescence; Power spectral density; Pulsed operation},
number = {16},
