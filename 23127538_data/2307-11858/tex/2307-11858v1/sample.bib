@article{Magrini2018,
author = {Lorenzo Magrini and Richard A. Norte and Ralf Riedinger and Igor Marinkovi\'{c} and David Grass and Uro\v{s} Deli\'{c} and Simon Gr\"{o}blacher and Sungkun Hong and Markus Aspelmeyer},
journal = {Optica},
keywords = {Optical tweezers; Optomechanics; Photonic crystal cavities; Photons; Power spectral density; Single mode fibers},
