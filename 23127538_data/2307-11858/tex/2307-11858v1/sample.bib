@article{Kumar2017,
author = {Pardeep Kumar and M. Bhattacharya},
journal = {Opt. Express},
keywords = {Laser trapping; Quantum optics; Optical tweezers or optical manipulation; Optomechanics; Magnetic fields; Magnetic resonance imaging; Nitrogen vacancy centers; Power spectral density; Quantum information processing; Whispering gallery modes},
number = {16},
pages = {19568--19582},
publisher = {Optica Publishing Group},
title = {Magnetometry via spin-mechanical coupling in levitated optomechanics},
volume = {25},
month = {Aug},
year = {2017},
url = {http://opg.optica.org/oe/abstract.cfm?URI=oe-25-16-19568},
doi = {10.1364/OE.25.019568},
abstract = {We analyze magnetometry using an optically levitated nanodiamond. We consider a configuration where a magnetic field gradient couples the mechanical oscillation of the diamond with its spin degree of freedom provided by a nitrogen vacancy center. First, we investigate the measurement of the position spectrum of the mechanical oscillator. We find that conditions of ultrahigh vacuum and feedback cooling allow a magnetic field gradient sensitivity of 1\&\#x003BC;Tm\&\#x02212;1\&\#x0002F;Hz. At high pressure and room temperature, this sensitivity degrades and can attain a value of the order of 100mTm\&\#x02212;1\&\#x0002F;Hz. Subsequently, we characterize the magnetic field gradient sensitivity obtainable by maneuvering the spin degrees of freedom using Ramsey interferometry. We find that this technique can offer photon-shot noise and spin-projection noise limited magnetic field gradient sensitivity of 100\&\#x003BC;Tm\&\#x02212;1\&\#x0002F;Hz. We conclude that this hybrid levitated nanomechanical magnetometer provides a favorable and versatile platform for sensing applications.},
}




