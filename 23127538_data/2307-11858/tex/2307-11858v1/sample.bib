@article{opticaltrapmag2,
author = {A. Garc\'{i}a-Etxarri and R. G\'{o}mez-Medina and L. S. Froufe-P\'{e}rez and C. L\'{o}pez and L. Chantada and F. Scheffold and J. Aizpurua and M. Nieto-Vesperinas and J. J. S\'{a}enz},
journal = {Opt. Express},
keywords = {Anisotropic optical materials; Magneto-optical materials; Scattering, particles; Effective refractive index; Electromagnetic scattering; Light scattering; Optical force; Optical media; Radiation pressure},
number = {6},
pages = {4815--4826},
publisher = {OSA},
title = {Strong magnetic response of submicron Silicon particles in the infrared},
volume = {19},
month = {Mar},
year = {2011},
url = {http://opg.optica.org/oe/abstract.cfm?URI=oe-19-6-4815},
doi = {10.1364/OE.19.004815},
abstract = {High-permittivity dielectric particles with resonant magnetic properties are being explored as constitutive elements of new metamaterials and devices. Magnetic properties of low-loss dielectric nanoparticles in the visible or infrared are not expected due to intrinsic low refractive index of optical media in these regimes. Here we analyze the dipolar electric and magnetic response of lossless dielectric spheres made of moderate permittivity materials. For low material refractive index (â‰² 3) there are no sharp resonances due to strong overlapping between different multipole contributions. However, we find that Silicon particles with index of refraction $\sim$ 3.5 and radius $\sim$ 200nm present strong electric and magnetic dipolar resonances in telecom and near-infrared frequencies, (i.e. at wavelengths $\approx$ 1.2 -- 2$\mu$m) without spectral overlap with quadrupolar and higher order resonances. The light scattered by these Si particles can then be perfectly described by dipolar electric and magnetic fields.},
}



@article{opticaltrapmag1,
author = {M. Nieto-Vesperinas and J. J. S\'{a}enz and R. G\'{o}mez-Medina and L. Chantada},
journal = {Opt. Express},
keywords = {Optical confinement and manipulation; Electromagnetic optics ; Mie theory; Optical tweezers or optical manipulation; Scattering theory ; Magnetic fields; Optical fields; Optical force; Photons; Radiation pressure; Refractive index},
number = {11},
pages = {11428--11443},
publisher = {OSA},
title = {Optical forces on small magnetodielectric particles},
volume = {18},
month = {May},
year = {2010},
url = {http://opg.optica.org/oe/abstract.cfm?URI=oe-18-11-11428},
doi = {10.1364/OE.18.011428},
abstract = {We present a study of the optical force on a small particle with both electric and magnetic response, immersed in an arbitrary non-dissipative medium, due to a generic incident electromagnetic field. This permits us to establish conclusions for any sign of this medium refractive index. Expressions for the gradient force, radiation pressure and curl components are obtained for the force due to both the electric and magnetic dipoles excited in the particle. In particular, for the magnetic force we tentatively introduce the concept of curl of the spin angular momentum density of the magnetic field, also expressed in terms of 3D generalizations of the Stokes parameters. From the formal analogy between the conservation of momentum and the optical theorem, we discuss the origin and significance of the electric-magnetic dipolar interaction force; this is done in connection with that of the angular distribution of scattered light and of the extinction cross section.},
}



