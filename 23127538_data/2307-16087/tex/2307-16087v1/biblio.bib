@Article{Houdayer2001,
author={Houdayer, J.},
title={A cluster Monte Carlo algorithm for 2-dimensional spin glasses},
journal={The European Physical Journal B - Condensed Matter and Complex Systems},
year={2001},
month={Aug},
day={01},
volume={22},
number={4},
pages={479-484},
abstract={A new Monte Carlo algorithm for 2-dimensional spin glasses is presented. The use of clusters makes possible global updates and leads to a gain in speed of several orders of magnitude. As an example, we study the 2-dimensional {\textpm}J Edwards-Anderson model. The new algorithm allows us to equilibrate systems of size 1002 down to temperature T = 0.1. Our main result is that the correlation length diverges as an exponential ( $\xi$∼e2$\beta$J) and not as a power law as T↦Tc = 0.},
issn={1434-6036},
doi={10.1007/PL00011151},
url={https://doi.org/10.1007/PL00011151}
}

