\usepackage{graphicx}
\usepackage{calc}
\newlength{\depthofsumsign}
\setlength{\depthofsumsign}{\depthof{$\sum$}}
\newlength{\totalheightofsumsign}
\newlength{\heightanddepthofargument}

\newcommand{\nsum}[1][1.4]{% only for \displaystyle
	\mathop{%
		\raisebox
		{-#1\depthofsumsign+1\depthofsumsign}
		{\scalebox
			{#1}
			{$\displaystyle\vertsum$}%
		}
	}
}
\newcommand{\nsumsubscript}[1][1.4]{% only for \displaystyle
	\mathop{%
		\raisebox
		{-#1\depthofsumsign+1\depthofsumsign}
		{\scalebox
			{#1}
			{$\displaystyle\vertsumsubscript$}%
		}
	}
}

\newcommand{\vertsum}{\raisebox{0\depth}{$\sum$}}
\newcommand{\vertsumsubscript}{\raisebox{-0.4\depth}{$\sum$}}


