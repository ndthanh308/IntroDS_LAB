@ARTICLE{CaiLubensky1994,
  title = {Covariant {H}ydrodynamics of {F}luid {M}embranes},
  author = {Cai, W. C. and Lubensky, T. C.},
  journal = {Phys. Rev. Lett.},
  year = {1994},
  volume = {73},
  number = {8},
  pages = {1186--1189},
  month = aug,
  abstract = {We derive covariant, reparametrization invariant, hydrodynamical
       equations for a fluctuating fluid membrane, which describe both
       tangent-plane motion and shape-changing normal motion. We
       calculate the renormalization of parameters of a Rouse model in
       which there is a friction force proportional to the local
       membrane velocity. Our calculations include both the Fadeev-
       Popov determinant and a Liouville-like factor needed to ensure
       rotational invariance.},
  keywords = {lyotropic lamellar phases, thermal fluctuations, elasticity, tension,
       gravity},
}

