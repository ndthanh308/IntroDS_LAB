@article{hwang_effect_2015,
	title = {Effect of surface energy on size-dependent deformation twinning of defect-free {Au} nanowires},
	volume = {7},
	issn = {2040-3364, 2040-3372},
	url = {http://xlink.rsc.org/?DOI=C5NR03902A},
	doi = {10.1039/C5NR03902A},
	abstract = {In this study, we report the size-dependent transition of deformation twinning studied using
              in situ
              SEM/TEM tensile testing of defect-free [110] Au nanowires/ribbons with controlled geometry.
            
          , 
            
              In this study, we report the size-dependent transition of deformation twinning studied using
