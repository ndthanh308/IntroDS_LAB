@article{kim_lattice_2014,
	title = {Lattice thermal conductivity of {UO2} using \textit{ab-initio} and classical molecular dynamics},
	volume = {115},
	issn = {0021-8979, 1089-7550},
	url = {https://pubs.aip.org/jap/article/115/12/123510/346963/Lattice-thermal-conductivity-of-UO2-using-ab},
	doi = {10.1063/1.4869669},
	abstract = {We applied the non-equilibrium ab-initio molecular dynamics and predict the lattice thermal conductivity of the pristine uranium dioxide for up to 2000 K. We also use the equilibrium classical molecular dynamics and heat-current autocorrelation decay theory to decompose the lattice thermal conductivity into acoustic and optical components. The predicted optical phonon transport is temperature independent and small, while the acoustic component follows the Slack relation and is in good agreement with the limited single-crystal experimental results. Considering the phonon grain-boundary and pore scatterings, the effective lattice thermal conductivity is reduced, and we show it is in general agreement with the sintered-powder experimental results. The charge and photon thermal conductivities are also addressed, and we find small roles for electron, surface polaron, and photon in the defect-free structures and for temperatures below 1500 K.},
	language = {en},
	number = {12},
	urldate = {2023-07-17},
	journal = {Journal of Applied Physics},
	author = {Kim, Hyoungchul and Kim, Moo Hwan and Kaviany, Massoud},
	month = mar,
	year = {2014},
	pages = {123510},
}

