	file = {Pizzolato et al_2018_Guided ultrasonic wave beam skew in silicon wafers.pdf:/Users/dkiefer/Zotero/storage/KW2CENC4/Pizzolato et al_2018_Guided ultrasonic wave beam skew in silicon wafers.pdf:application/pdf},
}

@article{masserey_defect_2019,
