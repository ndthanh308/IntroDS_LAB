@article{planelles-herrero_mechanistic_2017,
	title = {Mechanistic and structural basis for activation of cardiac myosin force production by omecamtiv mecarbil},
	volume = {8},
	copyright = {2017 The Author(s)},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/s41467-017-00176-5},
	doi = {10.1038/s41467-017-00176-5},
	abstract = {Omecamtiv mecarbil is a selective, small-molecule activator of cardiac myosin that is being developed as a potential treatment for heart failure with reduced ejection fraction. Here we determine the crystal structure of cardiac myosin in the pre-powerstroke state, the most relevant state suggested by kinetic studies, both with (2.45 Å) and without (3.10 Å) omecamtiv mecarbil bound. Omecamtiv mecarbil does not change the motor mechanism nor does it influence myosin structure. Instead, omecamtiv mecarbil binds to an allosteric site that stabilizes the lever arm in a primed position resulting in accumulation of cardiac myosin in the primed state prior to onset of cardiac contraction, thus increasing the number of heads that can bind to the actin filament and undergo a powerstroke once the cardiac cycle starts. The mechanism of action of omecamtiv mecarbil also provides insights into uncovering how force is generated by molecular motors.},
	language = {en},
	number = {1},
	urldate = {2022-11-28},
	journal = {Nature Communications},
	author = {Planelles-Herrero, Vicente J. and Hartman, James J. and Robert-Paganin, Julien and Malik, Fady I. and Houdusse, Anne},
	month = aug,
