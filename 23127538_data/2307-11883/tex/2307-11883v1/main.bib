pages = {125006},
author = {Monica Capone and Matteo Luca Ruggiero},
title = {Jumping from metric $f(R)$ to scalar–tensor theories and the relations between post-Newtonian parameters},
journal = {Classical and Quantum Gravity},
abstract = {We review the dynamical equivalence between $f(R)$ gravity in the metric formalism and scalar–tensor gravity, and use this equivalence to deduce the post-Newtonian parameters γ and β for an $f(R)$ theory, obtaining a result that is different with respect to that known in the literature. Then, we obtain explicit expressions of these parameters in terms of the mass of the scalar field (or, in other words, the mass of the additional scalar degree of freedom associated with an $f(R)$ theory) which can be used to constrain $f(R)$ gravity by means of current observations.}
}
