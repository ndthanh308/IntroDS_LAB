@article{ZHANG_exp2005,
title = "Experimental constraints on the phase diagram of elemental zirconium",
journal = "Journal of Physics and Chemistry of Solids",
volume = "66",
number = "7",
pages = "1213 - 1219",
year = "2005",
issn = "0022-3697",
doi = "https://doi.org/10.1016/j.jpcs.2005.03.004",
url = "http://www.sciencedirect.com/science/article/pii/S0022369705000636",
author = "Jianzhong Zhang and Yusheng Zhao and Cristian Pantea and Jiang Qian and Luke L. Daemen and Paulo A. Rigg and Robert S. Hixson and Carl W. Greeff and George T. Gray and Yunpeng Yang and Liping Wang and Yanbin Wang and Takeyuki Uchida",
keywords = "A. Metals, A. Amorphous materials, C. X-ray diffraction, D. Phase transitions, C. High pressure",
abstract = "The phase diagram of zirconium metal has been studied using synchrotron X-ray diffraction and time-of-flight neutron scattering at temperatures and pressures up to 1273K and 17GPa. The equilibrium phase boundary of the α–�?transition has a dT/dP slope of 473K/GPa, and the extrapolated transition pressure at ambient temperature is located at 3.4GPa. For the ω–�?transition, the phase boundary has a negative dT/dP slope of 15.5K/GPa between 6.4 and 15.3GPa, which is substantially smaller than a previously reported value of �?9±5K/GPa in the pressure range of 32�?5GPa. This difference indicates a significant curvature of the phase boundary between 15.3 and 35GPa. The α–ω–�?triple point was estimated to be at 4.9GPa and 953K, which is comparable to previous results obtained from a differential thermal analysis. Except for the three known crystalline forms, the β phase of zirconium metal was found to possess an extraordinary glass forming ability at pressures between 6.4 and 8.6GPa. This transformation leads to a limited stability field for the β phase in the pressure range of 6�?6GPa and to complications of high-temperature portion of phase diagram for zirconium metal."
}


