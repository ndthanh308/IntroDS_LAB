@article{https://doi.org/10.1111/j.1151-2916.2001.tb00885.x,
author = {Schelling, Patrick K. and Phillpot, Simon R. and Wolf, Dieter},
title = {Mechanism of the Cubic-to-Tetragonal Phase Transition in Zirconia and Yttria-Stabilized Zirconia by Molecular-Dynamics Simulation},
