@article{Behler_NN_review2017,
author = {Behler, J\"{o}rg},
title = {First Principles Neural Network Potentials for Reactive Simulations of Large Molecular and Condensed Systems},
journal = {Angew. Chem. Int. Ed.},
volume = {56},
