@article{HU_DFT2011,
title = "Density functional study of the phase diagram and thermodynamic properties of Zr",
journal = "Computational Materials Science",
volume = "50",
number = "3",
pages = "835 - 840",
year = "2011",
issn = "0927-0256",
doi = "https://doi.org/10.1016/j.commatsci.2010.10.017",
url = "http://www.sciencedirect.com/science/article/pii/S0927025610005768",
author = "Cui-E. Hu and Zhao-Yi Zeng and Lin Zhang and Xiang-Rong Chen and Ling-Cang Cai",
keywords = "Density functional theory, Lattice dynamics, Thermal equation of state",
abstract = "We have investigated the structures, phonon dispersions, phase transitions and thermodynamics of Zr from first-principles calculations. At zero temperature, α-Zr transforms to ω-Zr at 0.98GPa and then to β-Zr at 31.6GPa. The axial ratio c/a increases with increasing pressure for α-Zr, but it is nearly invariant under compression for ω-Zr. Within the quasi-harmonic Debye model, the full phase diagram of Zr was obtained. The zero pressure phase transition from α-Zr to β-Zr occurs at 1130K. The transition pressure from α-Zr to ω-Zr at 300K is 2.10GPa and the predicted triple point is at 6.35GPa, 910K, which is close to the experimental data. The thermal properties including the entropy, isotherm, isobar and thermal pressure in a wide range of pressure and temperature were predicted successfully."
}


