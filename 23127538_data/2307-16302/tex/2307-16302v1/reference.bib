@article{BENTZ1999143,
title = {NJL model on the light cone and pion structure function},
journal = {Nuclear Physics A},
volume = {651},
number = {2},
pages = {143-173},
year = {1999},
issn = {0375-9474},
doi = {https://doi.org/10.1016/S0375-9474(99)00130-X},
url = {https://www.sciencedirect.com/science/article/pii/S037594749900130X},
author = {W. Bentz and T. Hama and T. Matsuki and K. Yazaki},
keywords = {Effective quark theories, Light-cone theories, Structure functions},
abstract = {The NJL model is formulated on the light cone. Using the 1N expansion to solve the fermionic constraint, an effective 4-Fermi Lagrangian is derived. Pionic properties are investigated using this Lagrangian, and the formal equivalence to the equal-time formulation is demonstrated. Two regularization schemes in terms of light-cone variables are discussed: An extension of the ‘invariant mass cut-off scheme’, and a transverse momentum cut-off scheme. It is shown that the first one is equivalent to the covariant 3-momentum cut-off (dispersion cut-off) scheme in the equal-time formulation. As an application the structure function of the pion is studied in both regularization schemes.}
}

