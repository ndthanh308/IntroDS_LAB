@incollection{SCG2002:CMPPBOOK,
  author    = {Stewart, A. and Clint, M. and Gabarr\'o, J.},
  title     = {{Algebraic Rules for Reasoning about BSP Programs}},
  booktitle = {Constructive Methods for Parallel Programming},
  publisher = {Nova Science Publishers},
  month     = {august},
  year      = 2002,
  editor    = {Gorlatch, S. and Lengauer, C.},
  series    = {Advances in Computation: Theory and Practice},
  abstract  = { A collection of algebraic laws describing the
               behaviour of BSP programming structures is
               developed. Asynchronous communications are treated
               as assignments to a message relation and, as such,
               obey assignment laws. Supersteps are identified as
               compositions of independent processes followed by a
               barrier operation (global synchronization) which
               ensures that ongoing asynchronous communications are
               resolved; barrier is treated as a generalized form
               of multiple assignment. The model has similarities
               to that of He et al. [6] - for example, the
               construction of communication bundles. However, He's
               system is based on a trace model [9] while the
               approach taken here is grounded on multiple
               assignment and independent parallel composition. The
               derivation, from its sequential representation, of a
               parallel BSP-style version of Floyd's shortest path
               algorithm is used to illustrate the use of the
               proposed algebraic laws.  }
}

