@article{STE2011:FAC,
  author   = {Stewart, Alan},
  title    = {A programming model for {BSP} with partitioned
              synchronisation},
  journal  = {Formal Aspects of Computing},
  year     = 2011,
  volume   = 23,
  number   = 4,
  pages    = {421--432},
  abstract = {A BSP superstep is a distributed computation
              comprising a number of simultaneously executing
              processes which may generate asynchronous
              messages. A superstep terminates with a barrier
              which enforces a global synchronisation and delivers
              all ongoing communications. Multilevel supersteps
              can utilise barriers in which subsets of processes,
              interacting through shared memories, are locally
              synchronised (partitioned synchronisation). In this
              paper a state-based semantics, closely related to
              the classical sequential programming model, is
              derived for distributed BSP with partitioned
              synchronisation.},
  doi      = {10.1007/s00165-010-0163-2}
}

