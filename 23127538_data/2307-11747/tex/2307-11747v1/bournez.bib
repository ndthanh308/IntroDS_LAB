	pages = {162--195},
	title = {Continuity and Computability of Reachable Sets},
	url = {http://dx.doi.org/10.1016/j.tcs.2005.05.001},
	volume = {341},
	year = {2005},
	bdsk-url-1 = {http://dx.doi.org/10.1016/j.tcs.2005.05.001}}

