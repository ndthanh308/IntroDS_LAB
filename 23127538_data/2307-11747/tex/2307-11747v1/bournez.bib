	local-url = {file://localhost/Users/bournez/BASE-DE-DONNEES/base-devon-pro.dtBase/Files/MycCos05.pdf},
	pages = {179--189},
	title = {What Lies Beyond the Mountains?},
	volume = 85,
	year = {2005}}

@inproceedings{ColSch04a,
	abstract = { In this paper we discuss the observability of 
		hybrid systems and Turing machines. We give an elementary 
		example to show that observability is undecidable for Turing 
