@article{AaronsonW09,
	abstract = {While closed timelike curves (CTCs) are not known to exist, studying their consequences has led to non-trivial insights into general relativity, quantum information and other areas. In this paper, we show that, if CTCs existed, quantum computers would be no more powerful than classical computers: both would have the (extremely large) power of the complexity class polynomial space (PSPACE), consisting of all problems solvable by a conventional computer using a polynomial amount of memory. This solves an open problem proposed by one of us in 2005, and gives an essentially complete understanding of computational complexity in the presence of CTCs. Following the work of Deutsch, we treat a CTC as simply a region of spacetime where a 'causal consistency' condition is imposed, meaning that nature has to produce a (probabilistic or quantum) fixed point of some evolution operator. Our conclusion is then a consequence of the following theorem: given any quantum circuit (not necessarily unitary), a fixed point of the circuit can be (implicitly) computed in PSPACE. This theorem might have independent applications in quantum information.},
	author = {Aaronson, Scott and Watrous, John},
	date-added = {2018-05-14 09:15:41 +0000},
	date-modified = {2018-05-14 09:15:41 +0000},
	issn = {13645021},
	journal = {Proceedings: Mathematical, Physical and Engineering Sciences},
	number = {2102},
	pages = {631-647},
	publisher = {The Royal Society},
	title = {Closed Timelike Curves Make Quantum and Classical Computing Equivalent},
	volume = {465},
	year = {2009}}

