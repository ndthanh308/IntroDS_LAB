	url = {http://arxiv.org/abs/math/0302183v1},
	year = {2003},
	bdsk-url-1 = {http://arxiv.org/abs/math/0302183v1}}

@article{OrdKie03,
	abstract = {We show how to determine the $k$-th bit of Chaitin's algorithmically random real number $\Omega$ by solving $k$ instances of the halting problem. From this we then reduce the problem of determining the $k$-th bit of $\Omega$ to determining whether a certain Diophantine equation with two parameters, $k$ and $N$, has solutions for an odd or an even number of values of $N$. We also demonstrate two further examples of $\Omega$ in number theory: an exponential Diophantine equation with a parameter $k$ which has an odd number of solutions iff the $k$-th bit of $\Omega$ is 1, and a polynomial of positive integer variables and a parameter $k$ that takes on an odd number of positive values iff the $k$-th bit of $\Omega$ is 1.},
	author = {Ord, Toby and Kieu, Tien D.},
	date-added = {2008-01-24 09:24:30 +0100},
	date-modified = {2008-01-24 09:24:30 +0100},
	journal = {Fundamenta Informaticae},
	keywords = {Number Theory (math.NT); Computational Complexity (cs.CC); Quantum Physics (quant-ph)},
	local-url = {file://localhost/Users/bournez/BASE-DE-DONNEES/base-devon-pro.dtBase/Files/OrdKie03.pdf},
	pages = {273--284},
	title = {On the existence of a new family of Diophantine equations for $\Omega$},
	url = {http://arxiv.org/abs/math/0301274v3},
	volume = {56},
	year = {2003},
	bdsk-url-1 = {http://arxiv.org/abs/math/0301274v3}}

@techreport{Ord02,
	abstract = {Due to common misconceptions about the Church-Turing thesis, it has been widely assumed that the Turing machine provides an upper bound on what is computable. This is not so. The new field of hypercomputation studies models of computation that can compute more than the Turing machine and addresses their implications. In this report, I survey much of the work that has been done on hypercomputation, explaining how such non-classical models fit into the classical theory of computation and comparing their relative powers. I also examine the physical requirements for such machines to be constructible and the kinds of hypercomputation that may be possible within the universe. Finally, I show how the possibility of hypercomputation weakens the impact of Godel's Incompleteness Theorem and Chaitin's discovery of 'randomness' within arithmetic.},
	author = {Ord, Toby},
	date-added = {2008-01-24 09:24:30 +0100},
	date-modified = {2008-01-24 09:24:30 +0100},
	local-url = {file://localhost/Users/bournez/BASE-DE-DONNEES/base-devon-pro.dtBase/Files/Ord02.pdf},
	title = {Hypercomputation: computing more than the Turing machine},
	url = {http://arxiv.org/abs/math/0209332},
	year = {2002},
	bdsk-url-1 = {http://arxiv.org/abs/math/0209332}}

