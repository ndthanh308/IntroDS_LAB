	volume = {4162},
	year = {2006}}

@article{Koi04,
	abstract = {Let \tau(n) be the minimum number of arithmetic operations
		required to build the integer n \in N from the constants 1 and 2. A
		sequence xn is said to be "easy to compute" if there exists a
		polynomial p such that \tau(xn ) \leq p(log n) for all n \geq 1. It
		is natural to conjecture that sequences such as 2^n ln 2 or n! are
		not easy to compute. In this paper we show that a proof of this
