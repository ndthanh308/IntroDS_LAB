arxivId = {2302.01345},
author = {Revalski, Mitchell and Rafelski, Marc and Fumagalli, Michele and Fossati, Matteo and Pirzkal, Norbert and Sunnquist, Ben and Prichard, Laura J. and Henry, Alaina and Bagley, Micaela and Dutta, Rajeshwari and Papini, Giulia and Battaia, Fabrizio Arrigoni and D'Odorico, Valentina and Dayal, Pratika and Estrada-Carpenter, Vicente and Lofthouse, Emma K. and Lusso, Elisabeta and Morris, Simon L. and Nedkova, Kalina V. and Papovich, Casey and Peroux, Celine},
