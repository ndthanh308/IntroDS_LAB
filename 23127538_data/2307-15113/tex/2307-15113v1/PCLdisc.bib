month = {mar},
number = {1},
pages = {170--182},
primaryClass = {astro-ph},
title = {{Ly$\alpha$ Imaging of a Proto–Cluster Region at {\textless}z{\textgreater}=3.09}},
url = {http://arxiv.org/abs/astro-ph/9910144{\%}0Ahttp://dx.doi.org/10.1086/308568 https://iopscience.iop.org/article/10.1086/308568},
