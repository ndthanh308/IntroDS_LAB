author = {Wilkins, Stephen M and Bouwens, Rychard J and Oesch, Pascal A and Labbe, Ivo and Sargent, Mark and Caruana, Joseph and Wardlow, Julie and Clay, Scott},
doi = {10.1093/mnras/stv2263},
