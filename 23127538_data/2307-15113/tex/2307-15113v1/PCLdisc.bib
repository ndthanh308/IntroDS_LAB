@article{Spergel2013,
abstract = {The tension between the best fit parameters derived by the Planck team and a number of other astronomical measurements suggests either systematics in the astronomical measurements, systematics in the Planck data, the need for new physics, or a combination thereof. We re-analyze the Planck data and find that the {\$}217\backslash,\backslashtext{\{}GHz{\}}\backslashtimes 217\backslash,\backslashtext{\{}GHz{\}}{\$} detector set spectrum used in the Planck analysis is responsible for some of this tension. We use a map-based foreground cleaning procedure, relying on a combination of 353 GHz and 545 GHz maps to reduce residual foregrounds in the intermediate frequency maps used for cosmological inference. For our baseline data analysis, which uses 47{\%} of the sky and makes use of both 353 and 545 GHz data for foreground cleaning, we find the {\$}\backslashLambda{\$}CDM cosmological parameters {\$}\backslashOmega{\_}c h{\^{}}2 = 0.1169 \backslashpm 0.0025{\$}, {\$}n{\_}s = 0.9671 \backslashpm 0.0069{\$}, {\$}H{\_}0 = 68.0 \backslashpm 1.1\backslash,\backslashmathrm{\{}km{\}} \backslashmathrm{\{}s{\}}{\^{}}{\{}-1{\}}\backslashmathrm{\{}Mpc{\}}{\^{}}{\{}-1{\}}{\$}, {\$}\backslashOmega{\_}b h{\^{}}2 = 0.02197 \backslashpm 0.00027{\$}, {\$}\backslashln 10{\^{}}{\{}10{\}}A{\_}s = 3.080 \backslashpm 0.025{\$}, and {\$}\backslashtau = 0.089 \backslashpm 0.013 {\$}. While in broad agreement with the results reported by the Planck team, these revised parameters imply a universe with a lower matter density of {\$}\backslashOmega{\_}m=0.302\backslashpm0.015{\$}, and parameter values generally more consistent with pre-Planck CMB analyses and astronomical observations. We compare our cleaning procedure with the foreground modeling used by the Planck team and find good agreement. The difference in parameters between our analysis and that of the Planck team is mostly due to our use of cross-spectra from the publicly available survey maps instead of their use of the detector set cross-spectra which include pixels only observed in one of the surveys. We show evidence suggesting residual systematics in the detector set spectra used in the Planck likelihood code, which is substantially reduced for our spectra.},
archivePrefix = {arXiv},
arxivId = {1312.3313},
author = {Spergel, David and Flauger, Raphael and Hlozek, Renee},
eprint = {1312.3313},
file = {:Users/ben/Documents/Mendeley Desktop/Spergel, Flauger, Hlozek - 2013 - Planck Data Reconsidered.pdf:pdf},
month = {dec},
number = {Xxi},
pages = {15},
title = {{Planck Data Reconsidered}},
