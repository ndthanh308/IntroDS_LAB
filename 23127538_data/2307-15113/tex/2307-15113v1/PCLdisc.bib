title = {{MASS AND ENVIRONMENT AS DRIVERS OF GALAXY EVOLUTION IN SDSS AND zCOSMOS AND THE ORIGIN OF THE SCHECHTER FUNCTION}},
url = {http://adsabs.harvard.edu/abs/2010ApJ...721..193P{\%}5Cnhttp://stacks.iop.org/0004-637X/721/i=1/a=193?key=crossref.884ca20e0f924a9981b09cfb134a5de0},
