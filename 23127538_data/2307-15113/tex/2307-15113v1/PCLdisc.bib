doi = {10.1088/0004-637X/700/1/221},
eprint = {0905.1692},
file = {:Users/ben/Documents/Mendeley Desktop/Kriek et al. - 2009 - An ultra-deep near-infrared spectrum of a compact quiescent galaxy at z=2.2.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {color figures,evolution,formation,galaxies,high-redshift,online-only material},
month = {jul},
number = {1},
pages = {221--231},
title = {{AN ULTRA-DEEP NEAR-INFRARED SPECTRUM OF A COMPACT QUIESCENT GALAXY AT z = 2.2}},
