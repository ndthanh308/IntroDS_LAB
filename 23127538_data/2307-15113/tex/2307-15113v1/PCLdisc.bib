@article{Puglisi2015,
abstract = {We combined the spectroscopic information from the 3D-HST survey with the PEP/Herschel data to characterize the H$\backslash$alpha dust attenuation properties of a sample of 79 normal star-forming galaxies at {\$}0.7\backslashleq z\backslashleq1.5{\$} in the GOODS-S field. The sample was selected in the far-IR, at $\backslash$lambda=100 and/or 160 $\backslash$mu m, and only includes galaxies with a secure H$\backslash$alpha detection (S/N{\textgreater}3). From the low resolution 3D-HST spectra we measured z and F(H$\backslash$alpha) for the whole sample, rescaling the observed flux by a constant factor of 1.2 to remove the contamination by [NII]. The stellar masses, infrared and UV luminosities were derived from the SEDs by fitting multi-band data from GALEX near-UV to SPIRE500 $\backslash$mu m. We derived the continuum extinction Estar(B-V) from both the IRX ratio and the UV-slope, and found an excellent agreement among them. Galaxies in the sample have 2.6x10{\^{}}9{\$}\backslashleq{\$}M*{\$}\backslashleq{\$}3.5x10{\^{}}11 Msun, intense infrared luminosity (L{\_}IR{\textgreater}1.2x10{\^{}}10 Lsun), high level of dust obscuration (0.1{\$}\backslashleq{\$}Estar(B-V){\$}\backslashleq{\$}1.1) and strong H$\backslash$alpha emission (typical observed fluxes Fobs(H$\backslash$alpha){\$}\backslashgeq{\$}4.1x10{\^{}}-17 erg/s/cm2). The nebular extinction was estimated by comparing the observed SFR{\_}H$\backslash$alpha and the SFR{\_}UV. We obtained f=Estar(B-V)/Eneb(B-V)=0.93{\$}\backslashpm{\$}0.06, i.e. higher than the value measured in the local Universe. This result could be partially due to the adopted selection criteria, picking up the most obscured but also the H$\backslash$alpha brightest sources. The derived dust correction produces a good agreement between H$\backslash$alpha and IR+UV SFRs for objects with SFR{\$}\backslashgtrsim{\$}20 Msun/yr and M*{\$}\backslashgtrsim{\$}5x10{\^{}}10 Msun, while sources with lower SFR and M* seem to require a smaller f-factor (i.e. higher H$\backslash$alpha extinction correction). Our results then imply that for our sample the nebular and optical-UV extinctions are comparable and suggest that the f-factor is a function of both M* and SFR, according with previous studies.},
archivePrefix = {arXiv},
arxivId = {1507.00005},
author = {Puglisi, a and Rodighiero, G and Franceschini, A and Talia, M and Cimatti, A and Baronchelli, I and Daddi, E and Renzini, A. and Schawinski, K and Mancini, C and Silverman, J and Gruppioni, C and Lutz, D and Berta, S and Oliver, S J},
