@article{Sanders2022,
abstract = {We present observations of CO(3-2) in 13 main-sequence {\$}z=2.0-2.5{\$} star-forming galaxies at {\$}\backslashlog(M{\_}*/M{\_}{\{}\backslashodot{\}})=10.2-10.6{\$} that span a wide range in metallicity (O/H) based on rest-optical spectroscopy. We find that CO(3-2)/SFR decreases with decreasing metallicity, implying that the CO luminosity per unit gas mass is lower in low-metallicity galaxies at {\$}z\backslashsim2{\$}. We constrain the CO-to-H{\$}{\_}2{\$} conversion factor ({\$}\backslashalpha{\_}{\{}\backslashtext{\{}CO{\}}{\}}{\$}) and find that {\$}\backslashalpha{\_}{\{}\backslashtext{\{}CO{\}}{\}}{\$} inversely correlates with metallicity at {\$}z\backslashsim2{\$}. We derive molecular gas masses ({\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$}) and characterize the relations among {\$}M{\_}*{\$}, SFR, {\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$}, and metallicity. At {\$}z\backslashsim2{\$}, {\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$} increases and molecular gas fraction ({\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$}/{\$}M{\_}*{\$}) decrease with increasing {\$}M{\_}*{\$}, with a significant secondary dependence on SFR. Galaxies at {\$}z\backslashsim2{\$} lie on a near-linear molecular KS law that is well-described by a constant depletion time of 700 Myr. We find that the scatter about the mean SFR-{\$}M{\_}*{\$}, O/H-{\$}M{\_}*{\$}, and {\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$}-{\$}M{\_}*{\$} relations is correlated such that, at fixed {\$}M{\_}*{\$}, {\$}z\backslashsim2{\$} galaxies with larger {\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$} have higher SFR and lower O/H. We thus confirm the existence of a fundamental metallicity relation at {\$}z\backslashsim2{\$} where O/H is inversely correlated with both SFR and {\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$} at fixed {\$}M{\_}*{\$}. These results suggest that the scatter of the {\$}z\backslashsim2{\$} star-forming main sequence, mass-metallicity relation, and {\$}M{\_}{\{}\backslashtext{\{}mol{\}}{\}}{\$}-{\$}M{\_}*{\$} relation are primarily driven by stochastic variations in gas inflow rates. We place constraints on the mass loading of galactic outflows and perform a metal budget analysis, finding that massive {\$}z\backslashsim2{\$} star-forming galaxies retain only 30{\%} of metals produced, implying that a large mass of metals resides in the circumgalactic medium.},
