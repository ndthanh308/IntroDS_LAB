@article{Vanderhoof2022,
abstract = {We present {\$}10{\$} main-sequence ALPINE galaxies (log({\$}M/M{\_}{\{}\backslashodot{\}}{\$}) = 9.2-11.1 and {\$}{\{}\backslashrm SFR{\}}=23-190\backslash,{\{}\backslashrm M{\_}{\{}\backslashodot{\}}\backslash,yr{\^{}}{\{}-1{\}}{\}}{\$}) at {\$}z\backslashsim4.5{\$} with optical [OII] measurements from Keck/MOSFIRE spectroscopy and Subaru/MOIRCS narrow-band imaging. This is the largest such multi-wavelength sample at these redshifts, combining various measurements in the ultra-violet, optical, and far-infrared including [CII]{\$}{\_}{\{}158{\{}\backslashrm \backslashmu m{\}}{\}}{\$} line emission and dust continuum from ALMA and H{\$}\backslashalpha{\$} emission from Spitzer photometry. For the first time, this unique sample allows us to analyze the relation between [OII] and total star-formation rate (SFR) and the interstellar medium (ISM) properties via [OII]/[CII] and [OII]/$\backslash$halpha luminosity ratios at {\$}z\backslashsim4.5{\$}. The [OII]{\$}-{\$}SFR relation at {\$}z\backslashsim4.5{\$} cannot be described using standard local descriptions, but is consistent with a metal-dependent relation assuming metallicities around {\$}50\backslash{\%}{\$} solar. To explain the measured dust-corrected luminosity ratios of {\$}L[OII]/L[CII] \backslashsim 0.98{\^{}}{\{}+0.21{\}}{\_}{\{}-0.22{\}}{\$} and {\$}L[OII]/LHa \backslashsim -0.22{\^{}}{\{}+0.13{\}}{\_}{\{}-0.15{\}}{\$} for our sample, ionization parameters {\$}\backslashlog(U){\textless} -2{\$} and electron densities {\$}\backslashlog(\backslashrm n{\_}e / {\{}\backslashrm [cm{\^{}}{\{}-3{\}}]{\}}) \backslashsim 2.5-3{\$} are required. The former is consistent with galaxies at {\$}z\backslashsim2-3{\$}, however lower than at {\$}z{\textgreater}6{\$}. The latter may be slightly higher than expected given the galaxies' specific SFR. The analysis of this pilot sample suggests that typical log({\$} M/M{\_}{\{}\backslashodot{\}}){\$} {\textgreater} 9 galaxies at {\$}z\backslashsim4.5{\$} to have broadly similar ISM properties as their descendants at {\$}z\backslashsim2{\$} and suggest a strong evolution of ISM properties since the Epoch of Reionization at {\$}z{\textgreater}6{\$}.},
