}
@article{Mehta2017,
abstract = {We present the rest-1500$\backslash$AA$\backslash$ UV luminosity functions (LF) for star-forming galaxies during the cosmic $\backslash$textit{\{}high noon{\}} -- the peak of cosmic star formation rate at {\$}1.5{\textless}z{\textless}3{\$}. We use deep NUV imaging data obtained as part of the $\backslash$textit{\{}Hubble{\}} Ultra-Violet Ultra Deep Field (UVUDF) program, along with existing deep optical and NIR coverage on the HUDF. We select F225W, F275W and F336W dropout samples using the Lyman break technique, along with samples in the corresponding redshift ranges selected using photometric redshifts and measure the rest-frame UV LF at {\$}z\backslashsim1.7,2.2,3.0{\$} respectively, using the modified maximum likelihood estimator. We perform simulations to quantify the survey and sample incompleteness for the UVUDF samples to correct the effective volume calculations for the LF. We select galaxies down to {\$}M{\_}{\{}UV{\}}=-15.9,-16.3,-16.8{\$} and fit a faint-end slope of {\$}\backslashalpha=-1.20{\^{}}{\{}+0.10{\}}{\_}{\{}-0.13{\}}, -1.32{\^{}}{\{}+0.10{\}}{\_}{\{}-0.14{\}}, -1.39{\^{}}{\{}+0.08{\}}{\_}{\{}-0.12{\}}{\$} at {\$}1.4{\textless}z{\textless}1.9{\$}, {\$}1.8{\textless}z{\textless}2.6{\$}, and {\$}2.4{\textless}z{\textless}3.6{\$}, respectively. We compare the star formation properties of {\$}z\backslashsim2{\$} galaxies from these UV observations with results from H$\backslash$alpha$\backslash$ and UV{\$}+{\$}IR observations. We find a lack of high SFR sources in the UV LF compared to the H$\backslash$alpha$\backslash$ and UV{\$}+{\$}IR, likely due to dusty SFGs not being properly accounted for by the generic {\$}IRX-\backslashbeta{\$} relation used to correct for dust. We compute a volume-averaged UV-to-H$\backslash$alpha$\backslash$ ratio by $\backslash$textit{\{}abundance matching{\}} the rest-frame UV LF and H$\backslash$alpha$\backslash$ LF. We find an increasing UV-to-H$\backslash$alpha$\backslash$ ratio towards low mass galaxies ({\$}M{\_}\backslashstar \backslashlesssim 5\backslashtimes10{\^{}}9{\$} M{\$}{\_}\backslashodot{\$}). We conclude that this could be due to a larger contribution from starbursting galaxies compared to the high-mass end.},
archivePrefix = {arXiv},
arxivId = {1702.06953},
author = {Mehta, Vihang and Scarlata, Claudia and Rafelski, Marc and Gburek, Timothy and Teplitz, Harry I. and Alavi, Anahita and Boylan-Kolchin, Michael and Finkelstein, Steven and Gardner, Jonathan P. and Grogin, Norman and Koekemoer, Anton and Kurczynski, Peter and Siana, Brian and Codoreanu, Alex and de Mello, Duilia F. and Lee, Kyoung-Soo and Soto, Emmaris},
