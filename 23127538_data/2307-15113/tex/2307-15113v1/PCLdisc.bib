@article{Suess2022a,
abstract = {Galaxy sizes and their evolution over cosmic time have been studied for decades and serve as key tests of galaxy formation models. However, at {\$}z\backslashgtrsim1{\$} these studies have been limited by a lack of deep, high-resolution rest-frame infrared imaging that accurately traces galaxy stellar mass distributions. Here, we leverage the new capabilities of the James Webb Space Telescope to measure the 4.4{\$}\backslashmu{\$}m sizes of {\$}{\{}\backslashsim{\}}1000{\$} galaxies with {\$}\backslashlog{\{}\backslashrm{\{}M{\}}{\_}*/\backslashrm{\{}M{\}}{\_}\backslashodot{\}}\backslashge9{\$} and {\$}1.0\backslashle z \backslashle 2.5{\$} from public CEERS imaging in the EGS deep field. We compare the sizes of galaxies measured from NIRCam imaging at 4.4{\$}\backslashmu{\$}m ({\$}\backslashlambda{\_}{\{}\backslashmathrm{\{}rest{\}}{\}}\backslashsim1.6\backslashmu {\$}m) with sizes measured at {\$}1.5\backslashmu{\$}m ({\$}\backslashlambda{\_}{\{}\backslashmathrm{\{}rest{\}}{\}}\backslashsim5500{\$}A). We find that, on average, galaxy half-light radii are {\$}\backslashsim8{\$}$\backslash${\%} smaller at 4.4{\$}\backslashmu{\$}m than 1.5{\$}\backslashmu{\$}m in this sample. This size difference is markedly stronger at higher stellar masses and redder rest-frame {\$}V-J{\$} colors: galaxies with {\$}{\{}\backslashrm M{\}}{\_}* \backslashsim 10{\^{}}{\{}11{\}}\backslash,{\{}\backslashrm M{\}}{\_}\backslashodot{\$} have 4.4{\$}\backslashmu{\$}m sizes that are {\$}\backslashsim 25{\$}$\backslash$,$\backslash${\%} smaller than their 1.5{\$}\backslashmu{\$}m sizes. Our results indicate that galaxy mass profiles are significantly more compact than their rest-frame optical light profiles at cosmic noon, and demonstrate that spatial variations in age and attenuation are important, particularly for massive galaxies. The trend that we find here impacts our understanding of the size growth and evolution of galaxies, and suggests that previous studies based on rest-frame optical light may not have captured the mass-weighted structural evolution of galaxies. This paper represents a first step towards a new understanding of the morphologies of early massive galaxies enabled by JWST's infrared window into the distant universe.},
archivePrefix = {arXiv},
arxivId = {2207.10655},
author = {Suess, Katherine A. and Bezanson, Rachel and Nelson, Erica J. and Setton, David J. and Price, Sedona H. and van Dokkum, Pieter and Brammer, Gabriel and Labbe, Ivo and Leja, Joel and Miller, Tim B. and Robertson, Brant and Weaver, John R. and Whitaker, Katherine E.},
eprint = {2207.10655},
file = {:Users/ben/Documents/Mendeley Desktop/2207.10655.pdf:pdf},
keywords = {evolution,formation,galaxies,high-redshift,structure},
title = {{Rest-frame near-infrared sizes of galaxies at cosmic noon: objects in JWST's mirror are smaller than they appeared}},
url = {http://arxiv.org/abs/2207.10655},
year = {2022}
}
