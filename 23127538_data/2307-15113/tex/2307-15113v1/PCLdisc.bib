@article{Estrada-Carpenter2020,
abstract = {The origin of the correlations between mass, morphology, quenched fraction, and formation history in galaxies is difficult to define, primarily due to the uncertainties in galaxy star-formation histories. Star-formation histories are better constrained for higher redshift galaxies, observed closer to their formation and quenching epochs. Here we use "non-parametric" star-formation histories and a nested sampling method to derive constraints on the formation and quenching timescales of quiescent galaxies at {\$}0.7{\textless}z{\textless}2.5{\$}. We model deep HST grism spectroscopy and photometry from the CLEAR (CANDELS Lyman{\$}-\backslashalpha{\$} Emission at Reionization) survey. The galaxy formation redshifts, {\$}z{\_}{\{}50{\}}{\$} (defined as the point where they had formed 50$\backslash${\%} of their stellar mass) range from {\$}z{\_}{\{}50{\}}\backslashsim 2{\$} (shortly prior to the observed epoch) up to {\$}z{\_}{\{}50{\}} \backslashsimeq 5-8{\$}. $\backslash$editone{\{}We find that early formation redshifts are correlated with high stellar-mass surface densities, {\$}\backslashlog \backslashSigma{\_}1 / (M{\_}\backslashodot\backslash \backslashmathrm{\{}kpc{\}}{\^{}}{\{}-2{\}}) {\textgreater}{\$}10.25, where {\$}\backslashSigma{\_}1{\$} is the stellar mass within 1{\~{}}pkpc (proper kpc). Quiescent galaxies with the highest stellar-mass surface density, {\$}\backslashlog\backslashSigma{\_}1 / (M{\_}\backslashodot\backslash \backslashmathrm{\{}kpc{\}}{\^{}}{\{}-2{\}}) {\textgreater} 10.25{\$}, {\}} show a $\backslash$textit{\{}minimum{\}} formation redshift: all such objects in our sample have {\$}z{\_}{\{}50{\}} {\textgreater} 2.9{\$}. Quiescent galaxies with lower surface density, {\$}\backslashlog \backslashSigma{\_}1 / (M{\_}\backslashodot\backslash \backslashmathrm{\{}kpc{\}}{\^{}}{\{}-2{\}}) = 9.5 - 10.25{\$}, show a range of formation epochs ({\$}z{\_}{\{}50{\}} \backslashsimeq 1.5 - 8{\$}), implying these galaxies experienced a range of formation and assembly histories. We argue that the surface density threshold {\$}\backslashlog\backslashSigma{\_}1/(M{\_}\backslashodot\backslash \backslashmathrm{\{}kpc{\}}{\^{}}{\{}-2{\}}){\textgreater}10.25{\$} uniquely identifies galaxies that formed in the first few Gyr after the Big Bang, and we discuss the implications this has for galaxy formation models.},
archivePrefix = {arXiv},
arxivId = {2005.12289},
author = {Estrada-Carpenter, Vicente and Papovich, Casey and Momcheva, Ivelina and Brammer, Gabriel and Simons, Raymond and Bridge, Joanna and Cleri, Nikko J. and Ferguson, Henry and Finkelstein, Steven L. and Giavalisco, Mauro and Jung, Intae and Matharu, Jasleen and Trump, Jonathan R. and Weiner, Benjamin},
eprint = {2005.12289},
file = {:Users/ben/Documents/Mendeley Desktop/2005.12289.pdf:pdf},
month = {may},
title = {{CLEAR II: Evidence for Early Formation of the Most Compact Quiescent Galaxies at High Redshift}},
url = {http://arxiv.org/abs/2005.12289},
year = {2020}
}
@article{Tully2014,
abstract = {Galaxies congregate in clusters and along filaments, and are missing from large regions referred to as voids. These structures are seen in maps derived from spectroscopic surveys that reveal networks of structure that are interconnected with no clear boundaries. Extended regions with a high concentration of galaxies are called 'superclusters', although this term is not precise. There is, however, another way to analyse the structure. If the distance to each galaxy from Earth is directly measured, then the peculiar velocity can be derived from the subtraction of the mean cosmic expansion, the product of distance times the Hubble constant, from observed velocity. The peculiar velocity is the line-of-sight departure from the cosmic expansion and arises from gravitational perturbations; a map of peculiar velocities can be translated into a map of the distribution of matter. Here we report a map of structure made using a catalogue of peculiar velocities. We find locations where peculiar velocity flows diverge, as water does at watershed divides, and we trace the surface of divergent points that surrounds us. Within the volume enclosed by this surface, the motions of galaxies are inward after removal of the mean cosmic expansion and long range flows. We define a supercluster to be the volume within such a surface, and so we are defining the extent of our home supercluster, which we call Laniakea. A key component of this paper is an accompanying movie that can be viewed (also in 3D) and downloaded at http://irfu.cea.fr/laniakea or http://vimeo.com/pomarede/laniakea},
archivePrefix = {arXiv},
arxivId = {1409.0880},
author = {Tully, R. Brent and Courtois, H{\'{e}}l{\`{e}}ne and Hoffman, Yehuda and Pomar{\`{e}}de, Daniel},
doi = {10.1038/nature13674},
eprint = {1409.0880},
file = {:Users/ben/Documents/Mendeley Desktop/nature13674.pdf:pdf},
issn = {0028-0836},
journal = {Nature},
number = {7516},
pages = {71--73},
title = {{The Laniakea supercluster of galaxies}},
volume = {513},
year = {2014}
}
