@article{VanDokkum2011,
abstract = {We present first results from the 3D-HST program, a near-IR spectroscopic survey performed with the Wide Field Camera 3 (WFC3) on the HST. We have used 3D-HST spectra to measure redshifts and H$\alpha$ equivalent widths (EWH$\alpha$) for a complete, stellar mass-limited sample of 34 galaxies at 1 {\textless} z {\textless} 1.5 with M star {\textgreater} 1011 M ⊙ in the COSMOS, GOODS, and AEGIS fields. We find that a substantial fraction of massive galaxies at this epoch are forming stars at a high rate: the fraction of galaxies with EWH$\alpha$ {\textgreater}10 {\AA} is 59{\%}, compared to 10{\%} among Sloan Digital Sky Survey galaxies of similar masses at z = 0.1. Galaxies with weak H$\alpha$ emission show absorption lines typical of 2-4 Gyr old stellar populations. The structural parameters of the galaxies, derived from the associated WFC3 F140W imaging data, correlate with the presence of H$\alpha$ quiescent galaxies are compact with high S{\'{e}}rsic index and high inferred velocity dispersion, whereas star-forming galaxies are typically large two-armed spiral galaxies, with low S{\'{e}}rsic index. Some of these star-forming galaxies might be progenitors of the most massive S0 and Sa galaxies. Our results challenge the idea that galaxies at fixed mass form a homogeneous population with small scatter in their properties. Instead, we find that massive galaxies form a highly diverse population at z {\textgreater} 1, in marked contrast to the local universe.},
archivePrefix = {arXiv},
arxivId = {astro-ph.CO/1108.6060},
author = {van Dokkum, Pieter G. and Brammer, Gabriel and Fumagalli, Mattia and Nelson, Erica and Franx, Marijn and Rix, Hans-Walter and Kriek, Mariska and Skelton, Rosalind E. and Patel, Shannon and Schmidt, Kasper B. and Bezanson, Rachel and Bian, Fuyan and da Cunha, Elisabete and Erb, Dawn K. and Fan, Xiaohui and {F{\"{o}}rster Schreiber}, Natascha and Illingworth, Garth D. and Labb{\'{e}}, Ivo and Lundgren, Britt and Magee, Dan and Marchesini, Danilo and McCarthy, Patrick and Muzzin, Adam and Quadri, Ryan and Steidel, Charles C. and Tal, Tomer and Wake, David and Whitaker, Katherine E. and Williams, Anna},
