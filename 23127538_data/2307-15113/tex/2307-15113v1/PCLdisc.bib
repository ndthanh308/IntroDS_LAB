arxivId = {1802.07297},
author = {Jaff{\'{e}}, Yara L. and Poggianti, Bianca M. and Moretti, Alessia and Gullieuszik, Marco and Smith, Rory and Vulcani, Benedetta and Fasano, Giovanni and Fritz, Jacopo and Tonnesen, Stephanie and Bettoni, Daniela and Hau, George and Biviano, Andrea and Bellhouse, Callum and McGee, Sean},
doi = {10.1093/mnras/sty500},
