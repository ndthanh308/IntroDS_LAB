issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {1 i n t,bimodality in,evolution,formation,fundamental parameters,galaxies,galaxies in the local,high-redshift,ro d u c,structure,t i o n,universe display a striking},
month = {dec},
number = {2},
