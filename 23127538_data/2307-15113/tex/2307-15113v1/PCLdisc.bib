@article{Faisst2016,
abstract = {The intrinsic escape fraction of ionizing Lyman continuum photons ({\$}f{\_}{\{}esc{\}}{\$}) is crucial to understand whether galaxies are capable of reionizing the neutral hydrogen in the early universe at z{\textgreater}6. Unfortunately, it is not possible to access {\$}f{\_}{\{}esc{\}}{\$} at z{\textgreater}4 with direct observations and the handful of measurements from low redshift galaxies consistently find {\$}f{\_}{\{}esc{\}}{\$} {\textless} 10{\%}, while at least {\$}f{\_}{\{}esc{\}}{\$} {\~{}} 10{\%} is necessary for galaxies dominate reionization. Here, we present the first empirical prediction of {\$}f{\_}{\{}esc{\}}{\$} at z{\textgreater}6 by combining the (sparsely populated) relation between [OIII]/[OII] and {\$}f{\_}{\{}esc{\}}{\$} with the redshift evolution of [OIII]/[OII] as predicted from local high-z analogs selected by their H{\$}\backslashalpha{\$} equivalent-width. We find {\$}f{\_}{\{}esc{\}}{\$} = {\$}5.7{\_}{\{}-3.3{\}}{\^{}}{\{}+8.3{\}}{\$}{\%} at z=6 and {\$}f{\_}{\{}esc{\}}{\$} = {\$}10.4{\_}{\{}-6.3{\}}{\^{}}{\{}+15.5{\}}{\$}{\%} at z=9 for galaxies with log(M/M{\$}{\_}{\{}sun{\}}{\$}) {\~{}} 9.0 (errors given as 1{\$}\backslashsigma{\$}). However, there is a negative correlation with stellar mass and we find up to 50{\%} larger {\$}f{\_}{\{}esc{\}}{\$} per 0.5 dex decrease in stellar mass. The population averaged escape fraction increases according to {\$}f{\_}{\{}esc{\}}{\$} = {\$}f{\_}{\{}esc,0{\}} ((1+z)/3){\^{}}a{\$}, with {\$}f{\_}{\{}esc,0{\}} = 2.3 \backslashpm 0.05{\$}{\%} and {\$}a=1.17 \backslashpm 0.02{\$} at z {\textgreater} 2 for log(M/M{\$}{\_}{\{}sun{\}}{\$}) {\~{}} 9.0. With our empirical prediction of {\$}f{\_}{\{}esc{\}}{\$} (thus fixing an important previously unknown variable) and further reasonable assumption on clumping factor and the production efficiency of Lyman continuum photons, we conclude that the average population of galaxies is just capable to reionize the universe by z {\~{}} 6.},
archivePrefix = {arXiv},
arxivId = {1605.06507},
author = {Faisst, Andreas L.},
doi = {10.3847/0004-637X/829/2/99},
eprint = {1605.06507},
file = {:Users/ben/Documents/Mendeley Desktop//Faisst - 2016 - REVISITING THE LYMAN CONTINUUM ESCAPE CRISIS PREDICTIONS FOR z   6 FROM LOCAL GALAXIES.pdf:pdf},
