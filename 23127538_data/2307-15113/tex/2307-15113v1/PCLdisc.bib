file = {:Users/ben/Documents/Mendeley Desktop/2012.09308.pdf:pdf},
keywords = {drift,flexure,keck,mosfire,near-infrared,spectroscopy},
pages = {114},
title = {{Flexure updates to MOSFIRE on the Keck I telescope}},
year = {2020}
}
@article{Whitaker2012,
abstract = {We study the star formation rate (SFR)-stellar mass (M sstarf) relation in a self-consistent manner from 0 {\textless} z {\textless} 2.5 with 22,816 star-forming galaxies selected from the NEWFIRM Medium-Band Survey. We find a significant nonlinear slope of the relation, SFRvpropM 0.6 sstarf, and a constant observed scatter of 0.34 dex, independent of redshift and M sstarf. However, if we select only blue galaxies we find a linear relation SFRvpropM sstarf, similar to previous results at z = 0 by Peng et al. This selection excludes red, dusty, star-forming galaxies with higher masses, which brings down the slope. By selecting on L IR/L UV (a proxy for dust obscuration) and the rest-frame U - V colors, we show that star-forming galaxies fall in three distinct regions of the log(SFR)-log(M sstarf) plane: (1) actively star-forming galaxies with "normal" dust obscuration and associated colors (54{\%} for log (M sstarf) {\textgreater} 10 at 1 {\textless} z {\textless} 1.5), (2) red star-forming galaxies with low levels of dust obscuration and low-specific SFRs (11{\%}), and (3) dusty, blue star-forming galaxies with high-specific SFRs (7{\%}). The remaining 28{\%} comprises quiescent galaxies. Galaxies on the "normal" star formation sequence show strong trends of increasing dust attenuation with stellar mass and a decreasing specific SFR, with an observed scatter of 0.25 dex. The dusty, blue galaxies reside in the upper envelope of the star formation sequence with remarkably similar spectral shapes at all masses, suggesting that the same physical process is dominating the stellar light. The red, low-dust star-forming galaxies may be in the process of shutting off and migrating to the quiescent population.},
archivePrefix = {arXiv},
arxivId = {1205.0547},
author = {Whitaker, Katherine E and van Dokkum, Pieter G. and Brammer, Gabriel and Franx, Marijn},
doi = {10.1088/2041-8205/754/2/L29},
eprint = {1205.0547},
file = {:Users/ben/Documents/Mendeley Desktop/Whitaker et al. - 2012 - THE STAR FORMATION MASS SEQUENCE OUT TO z = 2.5.pdf:pdf},
issn = {2041-8205},
journal = {The Astrophysical Journal},
keywords = {galaxies: evolution,galaxies: formation,galaxies: high-redshift},
number = {2},
pages = {L29},
title = {{THE STAR FORMATION MASS SEQUENCE OUT TO z = 2.5}},
url = {http://adsabs.harvard.edu/abs/2012ApJ...754L..29W},
volume = {754},
year = {2012}
}
@article{Leethochawalit2019,
abstract = {We present the stellar mass-[Fe/H] and mass-[Mg/H] relation of quiescent galaxies in two galaxy clusters at {\$}z\backslashsim0.39{\$} and {\$}z\backslashsim0.54{\$}. We derive the age, [Fe/H], and [Mg/Fe] for each individual galaxy using a full-spectrum fitting technique. By comparing with the relations for {\$}z\backslashsim0{\$} SDSS galaxies, we confirm our previous finding that the mass-[Fe/H] relation evolves with redshift. The mass-[Fe/H] relation at higher redshift has lower normalization and possibly steeper slope. However, based on our sample, the mass-[Mg/H] relation does not evolve over the observed redshift range. We use a simple analytic chemical evolution model to constrain average outflow that these galaxies experience over their lifetime, via the calculation of mass-loading factor. We find that the average mass-loading factor {\$}\backslasheta{\$} is a power-law function of galaxy stellar mass, {\$}\backslasheta \backslashpropto M{\_}*{\^{}}{\{}-0.21\backslashpm0.09{\}}{\$}. The measured mass-loading factors are consistent with the results of other observational methods for outflow measurements and with the predictions where outflow is caused by star formation feedback in turbulent disks.},
