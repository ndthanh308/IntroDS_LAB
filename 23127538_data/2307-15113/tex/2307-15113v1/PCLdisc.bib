@article{Fiore2022,
abstract = {The JWST discovery of a number of super-early (redshift {\$}z{\textgreater}10{\$}), blue galaxies requires these systems to be essentially dust-free in spite of their large stellar masses. A possible explanation is that dust is evacuated by radiatively-driven outflows. We test this hypothesis by deriving the Eddington ratio, {\$}\backslashlambda{\_}E{\$}, for 134 galaxies at {\$}6.5{\textless} z {\textless}16{\$}. We find a strong anti-correlation between {\$}\backslashlambda{\_}E{\$} and dust UV optical depth, {\$}\backslashtau{\_}{\{}1500{\}} \backslashpropto \backslashlambda{\_}E{\^{}}{\{}-0.63{\}}{\$}; also, {\$}\backslashlambda{\_}E{\$} increases with redshift. We confirm that galaxies exceeding a specific star formation rate {\$}{\{}\backslashrm sSFR{\}} {\textgreater} 13\backslash, \backslashrm Gyr{\^{}}{\{}-1{\}}{\$} develop powerful outflows clearing the galaxy from its dust. This result is supported by ALMA dust continuum non-detections in three super-early systems},
