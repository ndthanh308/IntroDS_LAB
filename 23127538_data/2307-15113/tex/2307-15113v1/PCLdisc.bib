@article{Chartab2021,
abstract = {Using the near-IR spectroscopy of the MOSFIRE Deep Evolution Field (MOSDEF) survey, we investigate the role of local environment in the gas-phase metallicity of galaxies. The local environment measurements are derived from accurate and uniformly calculated photometric redshifts with well-calibrated probability distributions. Based on rest-frame optical emission lines, [NII]{\$}\backslashlambda6584{\$} and H{\$}\backslashalpha{\$}, we measure gas-phase oxygen abundance of 167 galaxies at {\$}1.37\backslashleq z\backslashleq1.7{\$} and 303 galaxies at {\$}2.09\backslashleq z\backslashleq2.61{\$}, located in diverse environments. We find that at {\$}z\backslashsim1.5{\$}, the average metallicity of galaxies in overdensities with {\$}M{\_}*\backslashsim10{\^{}}{\{}9.8{\}}M{\_}\backslashodot, 10{\^{}}{\{}10.2{\}}M{\_}\backslashodot{\$} and {\$}10{\^{}}{\{}10.8{\}}M{\_}\backslashodot{\$} is higher relative to their field counterparts by {\$}0.094\backslashpm0.051{\$}, {\$}0.068\backslashpm0.028{\$} and {\$}0.052\backslashpm0.043{\$} dex, respectively. However, this metallicity enhancement does not exist at higher redshift, {\$}z\backslashsim2.3{\$}, where, compared to the field galaxies, we find {\$}0.056\backslashpm0.043{\$}, {\$}0.056\backslashpm0.028{\$} and {\$}0.096\backslashpm 0.034{\$} dex lower metallicity for galaxies in overdense environments with {\$}M{\_}*\backslashsim10{\^{}}{\{}9.8{\}}M{\_}\backslashodot, 10{\^{}}{\{}10.2{\}}M{\_}\backslashodot{\$} and {\$}10{\^{}}{\{}10.7{\}}M{\_}\backslashodot{\$}, respectively. Our results suggest that, at {\$}1.37\backslashleq z\backslashleq2.61{\$}, the variation of mass-metallicity relation with local environment is small ({\$}{\textless}0.1{\$}dex), and reverses at {\$}z\backslashsim2{\$}. Our results support the hypothesis that, at the early stages of cluster formation, owing to efficient gas cooling, galaxies residing in overdensities host a higher fraction of pristine gas with prominent primordial gas accretion, which lowers their gas-phase metallicity compared to their coeval field galaxies. However, as the Universe evolves to lower redshifts ({\$}z\backslashlesssim2{\$}), shock-heated gas in overdensities cannot cool down efficiently, and galaxies become metal-rich rapidly due to the suppression of pristine gas inflow and re-accretion of metal-enriched outflows in overdensities.},
archivePrefix = {arXiv},
arxivId = {2101.01706},
author = {Chartab, Nima and Mobasher, Bahram and Shapley, Alice E. and Shivaei, Irene and Sanders, Ryan L. and Coil, Alison L. and Kriek, Mariska and Reddy, Naveen A. and Siana, Brian and Freeman, William R. and Azadi, Mojegan and Barro, Guillermo and Fetherolf, Tara and Leung, Gene and Price, Sedona H. and Zick, Tom},
eprint = {2101.01706},
file = {:Users/ben/Documents/Mendeley Desktop/2101.01706.pdf:pdf},
keywords = {1031,2029,594,734,902,galaxies,galaxy environments,galaxy evolution,high-redshift,large-scale structure of the,metallicity,universe},
title = {{The MOSDEF Survey: Environmental dependence of the gas-phase metallicity of galaxies at {\$}1.4 \backslashleq z \backslashleq 2.6{\$}}},
