file = {:Users/ben/Documents/Mendeley Desktop/Shull, Smith, Danforth - 2012 - THE BARYON CENSUS IN A MULTIPHASE INTERGALACTIC MEDIUM 30{\%} OF THE BARYONS MAY STILL BE MISSING.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
