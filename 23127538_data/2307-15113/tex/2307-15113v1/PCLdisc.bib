keywords = {abundances,content,evolution,galaxies,h ii regions,ism,starburst,stars,stellar,wolf-rayet},
month = {apr},
number = {1},
pages = {1--22},
pmid = {9010224},
title = {{The evolution of emission lines in H II galaxies}},
url = {http://arxiv.org/abs/0901.0974 http://dx.doi.org/10.1051/0004-6361:200811415 http://www.aanda.org/10.1051/0004-6361:20010232},
