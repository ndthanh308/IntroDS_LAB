file = {:Users/ben/Documents/Mendeley Desktop/Conley et al. - 2006 - Measurement of Omega{\_}m, Omega{\_}{\{}Lambda{\}} from a blind analysis of Type Ia supernovae with CMAGIC Using color inform.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
month = {feb},
number = {1},
pages = {53},
primaryClass = {astro-ph},
title = {{Measurement of $\backslash$Omega{\_}m, $\backslash$Omega{\_}{\{}$\backslash$Lambda{\}} from a blind analysis of Type Ia supernovae with CMAGIC: Using color information to verify the acceleration of the Universe}},
