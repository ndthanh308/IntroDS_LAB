@article{Shen2014,
abstract = {Quasars are rapidly accreting supermassive black holes at the centres of massive galaxies. They display a broad range of properties across all wavelengths, reflecting the diversity in the physical conditions of the regions close to the central engine. These properties, however, are not random, but form well-defined trends. The dominant trend is known as a 'Eigenvector 1', in which many properties correlate with the strength of optical iron and [O III] emission. The main physical driver of Eigenvector 1 has long been suspected to be the quasar luminosity normalized by the mass of the hole (the 'Eddington ratio'), which is an important parameter of the black hole accretion process. But a definitive proof has been missing. Here we report an analysis of archival data that reveals that the Eddington ratio indeed drives Eigenvector 1. We also find that orientation plays a significant role in determining the observed kinematics of the gas in the broad-line region, implying a flattened, disk-like geometry for the fast-moving clouds close to the black hole. Our results show that most of the diversity of quasar phenomenology can be unified using two simple quantities: Eddington ratio and orientation.},
archivePrefix = {arXiv},
arxivId = {1409.2887},
author = {Shen, Yue and Ho, Luis C.},
doi = {10.1038/nature13712},
eprint = {1409.2887},
file = {:Users/ben/Documents/Mendeley Desktop/nature13712.pdf:pdf},
issn = {14764687},
journal = {Nature},
number = {7517},
pages = {210--213},
publisher = {Nature Publishing Group},
title = {{The diversity of quasars unified by accretion and orientation}},
url = {http://dx.doi.org/10.1038/nature13712},
volume = {513},
year = {2014}
}
@article{Connor2019,
abstract = {In clusters of galaxies, the red sequence is believed to be a consequence of a correlation between stellar mass and chemical abundances, with more massive galaxies being more metal-rich and, as a consequence, redder. However, there is a color scatter around the red sequence that holds even with precision photometry, implying that the galaxy population is more complicated than as described by a mass-metallicity relation. We use precision photometry from the Cluster Lensing and Supernova survey with Hubble (CLASH) to investigate what drives this scatter. In four CLASH clusters at {\$}z=0.355 \backslashpm 0.007{\$}, we find that the optical-IR galaxy colors confirm the previously known trend of metallicity along the red sequence but also show a strong connection between stellar age and red sequence offset, with ages ranging from 3 to 8 Gyr. Starting with fixed-age color-magnitude relations motivated by the mass-metallicity correlations of CLASH cluster galaxies, and by adjusting galaxy colors through stellar population models to put them all at the age of our red sequence, we are able to reduce the, e.g., F625W {\$}-{\$} F814W scatter from 0.051 mag to 0.026 mag with median photometric errors of 0.029 mag. While we will extend this analysis to the full CLASH sample, in four clusters our technique already provides a color precision in near-total-light apertures to resolve the spread in stellar population formation ages that drives the scatter in the red sequence.},
archivePrefix = {arXiv},
arxivId = {1903.03618},
author = {Connor, Thomas and Kelson, Daniel D. and Donahue, Megan and Moustakas, John},
eprint = {1903.03618},
file = {:Users/ben/Documents/Mendeley Desktop/Connor et al. - 2019 - On the Origin of the Scatter in the Red Sequence An Analysis of Four CLASH Clusters.pdf:pdf},
keywords = {clusters,evolution,formation,galaxies,general},
number = {2007},
title = {{On the Origin of the Scatter in the Red Sequence: An Analysis of Four CLASH Clusters}},
url = {http://arxiv.org/abs/1903.03618},
year = {2019}
}
@article{Mehta2022,
abstract = {The UltraViolet imaging of the Cosmic Assembly Near-infrared Deep Extragalactic Legacy Survey Fields (UVCANDELS) program provides HST/UVIS F275W imaging for four CANDELS fields. We combine this UV imaging with existing HST/near-IR grism spectroscopy from 3D-HST+AGHAST to directly compare the resolved rest-frame UV and H{\$}\backslashalpha{\$} emission for a sample of 979 galaxies at {\$}0.7{\textless}z{\textless}1.5{\$} spanning a range in stellar mass of {\$}10{\^{}}{\{}8-11.5{\}}{\~{}}M{\_}\backslashodot{\$}. Since both rest-UV and H{\$}\backslashalpha{\$} are sensitive to on-going star-formation but over different timescales, their resolved comparison allows us to infer the burstiness in star-formation as a function of galaxy structural parameters. We generate homogenized maps of rest-UV and H{\$}\backslashalpha{\$} emission for all galaxies in our sample and stack them to compute the average UV-to-H{\$}\backslashalpha{\$} luminosity ratio as a function of galactocentric radius. We find that galaxies below stellar mass of {\$}\backslashsim10{\^{}}{\{}9.5{\}}{\~{}}M{\_}\backslashodot{\$}, at all radii, have a UV-to-H{\$}\backslashalpha{\$} ratio higher than the equilibrium value expected from constant star-formation, indicating a significant contribution from bursty star-formation. Even for galaxies with stellar mass {\$}\backslashgtrsim10{\^{}}{\{}9.5{\}} M{\_}\backslashodot{\$}, the UV-to-H{\$}\backslashalpha{\$} ratio is elevated towards in their outskirts ({\$}R/R{\_}{\{}eff{\}}{\textgreater}1.5{\$}), suggesting that bursty star-formation is likely prevalent in the outskirts of even the most massive galaxies but is likely over-shadowed by their brighter cores. Furthermore, we present the UV-to-H{\$}\backslashalpha{\$} ratio as a function of galaxy surface brightness, a proxy for stellar mass surface density, and find that regions below {\$}\backslashsim10{\^{}}8{\~{}}M{\_}\backslashodot{\~{}}kpc{\^{}}{\{}-2{\}}{\$} are consistent with bursty star-formation, regardless of their galaxy stellar mass, potentially suggesting that local star-formation is independent of global galaxy properties at the smallest scales.},
archivePrefix = {arXiv},
arxivId = {2211.02056},
author = {Mehta, Vihang and Teplitz, Harry I. and Scarlata, Claudia and Wang, Xin and Alavi, Anahita and Colbert, James and Rafelski, Marc and Grogin, Norman and Koekemoer, Anton and Prichard, Laura and Windhorst, Rogier and Barber, Justin M. and Conselice, Christopher J. and Dai, Y. Sophia and Gardner, Jonathan P. and Gawiser, Eric and Guo, Yicheng and Hathi, Nimish and Haro, Pablo Arrabal and Hayes, Matthew and Iyer, Kartheik G. and Jansen, Rolf A. and Ji, Zhiyuan and Kurczynski, Peter and Kuschel, Maxwell and Lucas, Ray A. and Mantha, Kameswara and O'Connell, Robert W. and Ravindranath, Swara and Robertson, Brant E. and Rutkowski, Michael and Siana, Brian and Yung, L. Y. Aaron},
eprint = {2211.02056},
