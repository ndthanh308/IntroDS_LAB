file = {:Users/ben/Documents/Mendeley Desktop/Mowla et al. - 2019 - A Mass-dependent Slope of the Galaxy Size–Mass Relation out to z ∼ 3 Further Evidence for a Direct Relation be.pdf:pdf},
issn = {2041-8213},
journal = {The Astrophysical Journal},
keywords = {evolution,galaxies,halos,high-redshift,structure},
