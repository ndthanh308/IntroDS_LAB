title = {{The EDGE-CALIFA survey: The local and global relations between {\$}\backslashSigma{\_}\backslashast{\$} , {\$}\backslashSigma{\_}{\{}SFR{\}}{\$} and {\$}\backslashSigma{\_}{\{}mol{\}}{\$} that regulate star-formation}},
