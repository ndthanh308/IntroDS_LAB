issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {evolution,fundamental parameters,galaxies,star},
month = {sep},
number = {2},
pages = {6027--6028},
title = {{Modelling the mass-metallicity relation of star-forming galaxies from z ∼ 3.5 to z ∼ 0}},
url = {http://arxiv.org/abs/1809.04079 http://dx.doi.org/10.1093/mnras/sty2506 https://academic.oup.com/mnras/advance-article/doi/10.1093/mnras/sty2506/5097892},
