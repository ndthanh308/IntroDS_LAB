issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {color figures,distance scale,dust,extinction,general,online-only material,supernovae},
month = {nov},
number = {1},
pages = {9},
title = {{Measuring Ejecta Velocity Improves Type Ia Supernova Distances}},
