@article{Kocevski2023,
abstract = {We report on the discovery of two low-luminosity, broad-line AGN at {\$}z{\textgreater}5{\$} identified using JWST NIRSpec spectroscopy from the CEERS Survey. We detect broad H{\$}\backslashalpha{\$} emission from both sources, with FWHM of {\$}2038\backslashpm286{\$} and {\$}1807\backslashpm207{\$} km s{\$}{\^{}}{\{}-1{\}}{\$}, resulting in black hole (BH) masses that are 1-2 dex below that of existing samples of luminous quasars at {\$}z{\textgreater}5{\$}. The first source, CEERS 1670 at {\$}z=5.242{\$}, is 2-3 dex fainter than known quasars at similar redshifts and was previously identified as a candidate low-luminosity AGN based on its rest-frame optical SED. We measure a BH mass of {\$}M{\_}{\{}\backslashrm BH{\}}=1.3\backslashpm0.4\backslashtimes 10{\^{}}{\{}7{\}}{\~{}}M{\_}{\{}\backslashodot{\}}{\$}, confirming that this AGN is powered by the least-massive BH known in the universe at the end of cosmic reionization. The second source, CEERS 3210 at {\$}z=5.624{\$}, is inferred to be a heavily obscured, broad-line AGN caught in a transition phase between a dust-obscured starburst and an unobscured quasar. We estimate its BH mass to be {\$}M{\_}{\{}\backslashrm BH{\}}\backslashsimeq 0.9-4.7 \backslashtimes 10{\^{}}{\{}7{\}}{\~{}}M{\_}{\{}\backslashodot{\}}{\$}, depending on the level of dust obscuration assumed. We derive host stellar masses, {\$}M{\_}\backslashstar{\$}, allowing us to place constraints on the BH-galaxy mass relationship in the lowest mass range yet probed in the early universe. The {\$}M{\_}{\{}\backslashrm BH{\}}/M{\_}\backslashstar{\$} ratio for CEERS 1670, in particular, is consistent with or higher than the empirical relationship seen in massive galaxies at {\$}z=0{\$}. We examine the emission-line ratios of both sources and find that their location on the BPT and OHNO diagrams is consistent with model predictions for low-metallicity AGN with {\$}Z/Z{\_}\backslashodot \backslashsimeq 0.2-0.4{\$}. The spectroscopic identification of low-luminosity, broad-line AGN at {\$}z{\textgreater}5{\$} with {\$}M{\_}{\{}\backslashrm BH{\}}\backslashsimeq 10{\^{}}{\{}7{\}}{\~{}}M{\_}{\{}\backslashodot{\}}{\$} demonstrates the capability of JWST to push BH masses closer to the range predicted for the BH seed population and provides a unique opportunity to study the early stages of BH-galaxy assembly.},
