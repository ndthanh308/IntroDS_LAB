arxivId = {1107.2653},
author = {Reddy, N. and Dickinson, M. and Elbaz, D. and Morrison, G. and Giavalisco, M. and Ivison, R. and Papovich, C. and Scott, D. and Buat, V. and Burgarella, D. and Charmandaris, V. and Daddi, E. and Magdis, G. and Murphy, E. and Altieri, B. and Aussel, H. and Dannerbauer, H. and Dasyra, K. and Hwang, H. S. and Kartaltepe, J. and Leiton, R. and Magnelli, B. and Popesso, P.},
doi = {10.1088/0004-637X/744/2/154},
eprint = {1107.2653},
file = {:Users/ben/Documents/Mendeley Desktop/Reddy et al. - 2011 - GOODS-Herschel Measurements of the Dust Attenuation of Typical Star-Forming Galaxies at High Redshift Observations.pdf:pdf},
issn = {0004-637X},
