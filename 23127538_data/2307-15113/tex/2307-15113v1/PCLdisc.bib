issn = {0004-637X},
journal = {The Astrophysical Journal},
month = {jul},
number = {1},
pages = {L13--L16},
title = {{Is There Evidence for a Hubble Bubble? The Nature of Type Ia Supernova Colors and Dust in External Galaxies}},
url = {http://arxiv.org/abs/0705.0367 http://stacks.iop.org/1538-4357/664/i=1/a=L13},
volume = {664},
