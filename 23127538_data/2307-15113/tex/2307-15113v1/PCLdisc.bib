@article{Darvish2018,
abstract = {We study the effects of the local environment on the molecular gas content of a large sample of log({\$}M{\_}{\{}*{\}}{\$}/{\$}M{\_}{\{}\backslashodot{\}}{\$}) {\$}\backslashgtrsim{\$} 10 star-forming and starburst galaxies with specific star-formation rates (sSFRs) on and above the main-sequence (MS) to {\$}z{\$} {\$}\backslashsim{\$} 3.5. ALMA observations of the dust continuum in the COSMOS field are used to estimate molecular gas masses at {\$}z{\$} {\$}\backslashapprox{\$} 0.5-3.5. We also use a local universe sample from the ALFALFA HI survey after converting it to molecular masses. The molecular mass ({\$}M{\_}{\{}ISM{\}}{\$}) scaling relation shows a dependence on {\$}z{\$}, {\$}M{\_}{\{}*{\}}{\$}, and sSFR relative to the MS, but no dependence on environmental overdensity {\$}\backslashDelta{\$} ({\$}M{\_}{\{}ISM{\}}{\$} {\$}\backslashpropto{\$} {\$}\backslashDelta{\^{}}{\{}0.03{\}}{\$}). Similarly, gas mass fraction (f{\$}{\_}{\{}gas{\}}{\$}) and depletion timescale ({\$}\backslashtau{\$}) show no environmental dependence to {\$}z{\$} {\$}\backslashsim{\$} 3.5. At {\$}\backslashlangle z\backslashrangle{\$} {\$}\backslashsim{\$} 1.8, the average {\$}\backslashlangle M{\_}{\{}ISM{\}}\backslashrangle{\$},{\$}\backslashlangle{\$}f{\$}{\_}{\{}gas{\}}\backslashrangle{\$}, and {\$}\backslashlangle \backslashtau \backslashrangle{\$} in densest regions is (1.6{\$}\backslashpm{\$}0.2){\$}\backslashtimes{\$}10{\$}{\^{}}{\{}11{\}}{\$} {\$}M{\_}{\{}\backslashodot{\}}{\$}, 55{\$}\backslashpm{\$}2{\%}, and 0.8{\$}\backslashpm{\$}0.1 Gyr, respectively, similar to those in the lowest density bin. Independent of the environment, f{\$}{\_}{\{}gas{\}}{\$} decreases and {\$}\backslashtau{\$} increases with increasing cosmic time. Cosmic molecular mass density ({\$}\backslashrho{\$}) in the lowest density bins peaks at {\$}z{\$} {\$}\backslashsim{\$} 1-2 and this peak happens at {\$}z{\$} {\$}{\textless}{\$} 1 in densest bins. This differential evolution of {\$}\backslashrho{\$} across environments is likely due to the growth of the large-scale structure with cosmic time. Our results suggest that the molecular gas content and the subsequent star-formation activity of log({\$}M{\_}{\{}*{\}}{\$}/{\$}M{\_}{\{}\backslashodot{\}}{\$}) {\$}\backslashgtrsim{\$} 10 star-forming and starburst galaxies is primarily driven by internal processes, not their local environment since {\$}z{\$} {\$}\backslashsim{\$} 3.5},
