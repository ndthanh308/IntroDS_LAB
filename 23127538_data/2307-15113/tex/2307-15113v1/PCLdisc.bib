arxivId = {1709.03505},
author = {Schreiber, Corentin and Labb{\'{e}}, Ivo and Glazebrook, Karl and Bekiaris, Georgios and Papovich, Casey and Costa, Tiago and Elbaz, David and Kacprzak, Glenn G. and Nanayakkara, Themiya and Oesch, Pascal and Pannella, Maurilio and Spitler, Lee and Straatman, Caroline and Tran, K.-V. and Wang, Tao},
doi = {10.1051/0004-6361/201731917},
