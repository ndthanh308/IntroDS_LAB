arxivId = {1605.03182},
author = {Darvish, Behnam and Mobasher, Bahram and Sobral, David and Rettura, Alessandro and Scoville, Nick and Faisst, Andreas and Capak, Peter},
doi = {10.3847/0004-637X/825/2/113},
