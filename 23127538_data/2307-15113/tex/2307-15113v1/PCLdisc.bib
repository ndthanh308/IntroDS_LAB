eprint = {2102.07791},
file = {:Users/ben/Documents/Mendeley Desktop/2102.07791.pdf:pdf},
keywords = {evolution,galaxies,high redshift,infrared,star formation,starburst,submillimetre},
number = {February},
pages = {1--17},
title = {{Resolving a dusty, star-forming SHiZELS galaxy at z=2.2 with HST, ALMA and SINFONI on kiloparsec scales}},
