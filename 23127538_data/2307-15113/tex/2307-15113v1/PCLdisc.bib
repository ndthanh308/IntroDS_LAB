title = {{Beyond UVJ : More Efficient Selection of Quiescent Galaxies with Ultraviolet/Mid-infrared Fluxes}},
url = {http://arxiv.org/abs/1907.02970 http://dx.doi.org/10.3847/2041-8213/ab2f8c https://iopscience.iop.org/article/10.3847/2041-8213/ab2f8c},
