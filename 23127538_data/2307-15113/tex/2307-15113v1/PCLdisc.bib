@article{Alvarez-Marquez2019,
abstract = {This work explores from a statistical point of view the rest-frame far-ultraviolet (FUV) to far-infrared (FIR) emission of a population of Lyman break galaxies (LBGs) at {\$}z\backslashsim3{\$} that cannot be individually detected from current FIR observations. We perform a stacking analysis over a sample of {\$}\backslashsim{\$}17000 LBGs at redshift {\$}2.5{\textless}z{\textless}3.5{\$} in the COSMOS field. The sample is binned as a function of UV luminosity ({\$}L{\_}{\{}\backslashmathrm{\{}FUV{\}}{\}}{\$}), UV continuum slope ({\$}\backslashbeta{\_}{\{}\backslashmathrm{\{}UV{\}}{\}}{\$}), and stellar mass (M{\$}{\_}{\{}*{\}}{\$}), and then, stacked at optical ({\$}BVriz{\$} bands), near-infrared ({\$}YJHKs{\$} bands), IRAC (3.6, 4.5, 5.6 and 8.0 {\$}\backslashmu{\$}m), MIPS (24{\$}\backslashmu{\$}m), PACS (100 and 160{\~{}}{\$}\backslashmu{\$}m), SPIRE (250, 350, and 500{\~{}}{\$}\backslashmu{\$}m), and AzTEC (1.1mm) observations. We obtain thirty rest-frame FUV-to-FIR spectral energy distribution (SEDs) of LBGs at {\$}z\backslashsim3{\$}, and analyse them with CIGALE SED-fitting analysis code. We are able to derive fully consistent physical parameters (M{\$}{\_}{\{}*{\}}{\$}, {\$}\backslashbeta{\_}{\{}\backslashmathrm{\{}UV{\}}{\}}{\$}, {\$}L{\_}{\{}\backslashmathrm{\{}FUV{\}}{\}}{\$}, {\$}L{\_}{\{}\backslashmathrm{\{}IR{\}}{\}}{\$}, A{\$}{\_}{\{}FUV{\}}{\$}, SFR, and slope of the dust attenuation law), and build a semi-empirical library of thirty rest-frame FUV-to-FIR stacked LBG SEDs as a function of {\$}L{\_}{\{}\backslashmathrm{\{}FUV{\}}{\}}{\$}, {\$}\backslashbeta{\_}{\{}\backslashmathrm{\{}UV{\}}{\}}{\$}, and M{\$}{\_}{\{}*{\}}{\$}. We used the so-called IR-excess ({\$}IRX \backslashequiv L{\_}{\{}\backslashmathrm{\{}IR{\}}{\}} / L{\_}{\{}\backslashmathrm{\{}FUV{\}}{\}}{\$}) to investigate the dust attenuation as a function of {\$}\backslashbeta{\_}{\{}\backslashmathrm{\{}UV{\}}{\}}{\$} and M{\$}{\_}{\{}*{\}}{\$}. Additionally, the SED-fitting analysis results provide a diversity of dust attenuation curve along the LBG sample, and their slope are well correlated with M{\$}{\_}{\{}*{\}}{\$}. Stepper dust attenuations curves than Calzetti's are favoured in low stellar mass LBGs ({\$}\backslashlog(M{\_}{\{}*{\}} [M{\_}{\{}\backslashodot{\}}]) {\textless} 10.25{\$}), while grayer ones are favoured in high stellar mass LBGs ({\$}\backslashlog(M{\_}{\{}*{\}} [M{\_}{\{}\backslashodot{\}}]) {\textgreater} 10.25{\$}).},
