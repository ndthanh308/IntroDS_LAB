issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {dark matter,galaxies: abundances,galaxies: evolution,methods: numerical},
number = {1},
pages = {57},
title = {{THE AVERAGE STAR FORMATION HISTORIES OF GALAXIES IN DARK MATTER HALOS FROM z = 0-8}},
