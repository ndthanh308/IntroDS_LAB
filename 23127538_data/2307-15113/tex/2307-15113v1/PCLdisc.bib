arxivId = {1604.05723},
author = {Spilker, J. S. and Marrone, D. P. and Aravena, Manuel and B{\'{e}}thermin, M. and Bothwell, M. S. and Carlstrom, J. E. and Chapman, S. C. and Crawford, T. M. and de Breuck, C. and Fassnacht, C. D. and Gonzalez, A. H. and Greve, T. R. and Hezaveh, Yashar and Litke, Katrina and Ma, Jingzhe and Malkan, Matt and Rotermund, K. M. and Strandet, Maria and Vieira, J. D. and Weiss, Axel and Welikala, Niraj},
