author = {Nelson, Erica June and van Dokkum, Pieter G. and {F{\"{o}}rster Schreiber}, Natascha M. and Franx, Marijn and Brammer, Gabriel B. and Momcheva, Ivelina G. and Wuyts, Stijn and Whitaker, Katherine E. and Skelton, Rosalind E. and Fumagalli, Mattia and Hayward, Christopher C. and Kriek, Mariska and Labb{\'{e}}, Ivo and Leja, Joel and Rix, Hans-Walter and Tacconi, Linda J. and van der Wel, Arjen and van den Bosch, Frank C. and Oesch, Pascal A. and Dickey, Claire and Lange, Johannes Ulf},
