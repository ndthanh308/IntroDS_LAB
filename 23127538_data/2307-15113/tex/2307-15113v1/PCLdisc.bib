month = {jul},
number = {809},
pages = {763--795},
primaryClass = {astro-ph},
title = {{Galactic Stellar and Substellar Initial Mass Function}},
url = {http://arxiv.org/abs/astro-ph/0304382 http://dx.doi.org/10.1086/376392 http://www.jstor.org/stable/10.1086/376392},
