title = {{Interstellar dust from the Milky Way to the Magellanic Clouds}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=1992ApJ...395..130P{\&}link{\_}type=ABSTRACT{\%}5Cnpapers2://publication/doi/10.1086/171637},
volume = {395},
year = {1992}
