issn = {13652966},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Galaxies: abundances,Galaxies: evolution,Galaxies: formation,Galaxies: luminosity function,Mass function},
number = {2},
pages = {1671--1687},
title = {{MUFASA: The assembly of the red sequence}},
volume = {471},
year = {2017}
}
