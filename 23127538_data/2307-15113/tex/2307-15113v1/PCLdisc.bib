file = {:Users/ben/Documents/Mendeley Desktop/Tollerud et al. - 2011 - SMALL-SCALE STRUCTURE IN THE SLOAN DIGITAL SKY SURVEY AND $\Lambda$CDM ISOLATED ∼ L GALAXIES WITH BRIGHT SATELLITES.pdf:pdf},
issn = {0004-637X},
