journal = {The Astrophysical Journal},
month = {dec},
number = {2},
pages = {144},
title = {{Discovery of Four Apparently Cold Dusty Galaxies at z = 3.62–5.85 in the COSMOS Field: Direct Evidence of Cosmic Microwave Background Impact on High-redshift Galaxy Observables}},
