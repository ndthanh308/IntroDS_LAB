@article{Shapley2003,
abstract = {We present the results of a systematic study of the rest-frame UV spectroscopic properties of Lyman Break Galaxies (LBGs). The database of almost 1000 LBG spectra proves useful for constructing high S/N composite spectra. The composite spectrum of the entire sample reveals a wealth of features attributable to hot stars, HII regions, dust, and outflowing neutral and ionized gas. By grouping the database according to galaxy parameters such as Lyman-alpha equivalent width, UV spectral slope, and interstellar kinematics, we isolate some of the major trends in LBG spectra which are least compromised by selection effects. We find that LBGs with stronger Lyman-alpha emission have bluer UV continua, weaker low-ionization interstellar absorption lines, smaller kinematic offsets between Lyman-alpha and the interstellar absorption lines, and lower star-formation rates. There is a decoupling between the dependence of low- and high-ionization outflow features on other spectral properties. Most of the above trends can be explained in terms of the properties of the large-scale outflows seen in LBGs. According to this scenario, the appearance of LBG spectra is determined by a combination of the covering fraction of outflowing neutral gas which contains dust, and the range of velocities over which this gas is absorbing. Higher sensitivity and spectral resolution observations are still required for a full understanding of the covering fraction and velocity dispersion of the outflowing neutral gas in LBGs, and its relationship to the escape fraction of Lyman continuum radiation in galaxies at z{\~{}}3.},
archivePrefix = {arXiv},
arxivId = {astro-ph/0301230},
author = {Shapley, a. E. and Steidel, C. C. and Pettini, M. and Adelberger, K. L.},
doi = {10.1086/373922},
eprint = {0301230},
file = {:Users/ben/Documents/Mendeley Desktop/Shapley et al. - 2003 - Rest-Frame Ultraviolet Spectra of z{\$}sim{\$}3 Lyman Break Galaxies.pdf:pdf},
issn = {1538-4357},
journal = {The Astrophysical Journal},
pages = {65--89},
primaryClass = {astro-ph},
title = {{Rest-Frame Ultraviolet Spectra of z{\$}\backslashsim{\$}3 Lyman Break Galaxies}},
url = {http://arxiv.org/abs/astro-ph/0301230},
volume = {588},
year = {2003}
}
@article{Gabor2012a,
abstract = {We examine the cosmic growth of the red sequence in a cosmological hydrodynamic simulation that includes a heuristic prescription for quenching star formation that yields a realistic passive galaxy population today. In this prescription, halos dominated by hot gas are continually heated to prevent their coronae from fueling new star formation. Hot coronae primarily form in halos above $\backslash$sim10{\^{}}12 M$\backslash$odot, so that galaxies with stellar masses $\backslash$sim10{\^{}}10.5 M$\backslash$odot are the first to be quenched and move onto the red sequence at z {\textgreater} 2. The red sequence is concurrently populated at low masses by satellite galaxies in large halos that are starved of new fuel, resulting in a dip in passive galaxy number densities around $\backslash$sim10{\^{}}10 M$\backslash$odot. Stellar mass growth continues for galaxies even after joining the red sequence, primarily through minor mergers with a typical mass ratio $\backslash$sim1:5. For the most massive systems, the size growth implied by the distribution of merger mass ratios is typically $\backslash$sim2$\backslash$times the corresponding mass growth, consistent with observations. This model reproduces mass-density and colour-density trends in the local universe, with essentially no evolution to z = 1, with the hint that such relations may be washed out by z $\backslash$sim 2. Simulated galaxies are increasingly likely to be red at high masses or high local overdensities. In our model, the presence of surrounding hot gas drives the trends with both mass and environment.},
archivePrefix = {arXiv},
arxivId = {1202.5315},
author = {Gabor, J M and Dav{\'{e}}, R.},
doi = {10.1111/j.1365-2966.2012.21640.x},
eprint = {1202.5315},
file = {:Users/ben/Documents/Mendeley Desktop//Gabor, Dav{\'{e}} - 2012 - The growth of red sequence galaxies in a cosmological hydrodynamic simulation(2).pdf:pdf},
issn = {00358711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Galaxies: evolution,Galaxies: formation},
number = {3},
pages = {1816--1829},
title = {{The growth of red sequence galaxies in a cosmological hydrodynamic simulation}},
volume = {427},
year = {2012}
}
@article{Wiklind2019,
abstract = {We present an ALMA survey of dust continuum emission in a sample of 70 galaxies in the redshift range z=2-5 selected from the CANDELS GOODS-S field. Multi-Epoch Abundance Matching (MEAM) is used to define potential progenitors of a z = 0 galaxy of stellar mass 1.5 10{\^{}}11 M{\_}sun. Gas masses are derived from the 850um luminosity. Ancillary data from the CANDELS GOODS-S survey are used to derive the gas mass fractions. The results at z{\textless}=3 are mostly in accord with expectations: The detection rates are 75{\%} for the z=2 redshift bin, 50{\%} for the z=3 bin and 0{\%} for z{\textgreater}=4. The average gas mass fraction for the detected z=2 galaxies is f{\_}gas = 0.55+/-0.12 and f{\_}gas = 0.62+/-0.15 for the z=3 sample. This agrees with expectations for galaxies on the star-forming main sequence, and shows that gas fractions have decreased at a roughly constant rate from z=3 to z=0. Stacked images of the galaxies not detected with ALMA give upper limits to f{\_}gas of {\textless}0.08 and {\textless}0.15, for the z=2 and z=3 redshift bins. None of our galaxies in the z=4 and z=5 sample are detected and the upper limit from stacked images, corrected for low metallicity, is f{\_}gas{\textless}0.66. We do not think that lower gas-phase metallicities can entirely explain the lower dust luminosities. We briefly consider the possibility of accretion of very low-metallicity gas to explain the absence of detectable dust emission in our galaxies at z{\textgreater}4.},
author = {Wiklind, Tommy and Ferguson, Henry C. and Guo, Yicheng and Koo, David C. and Kocevski, Dale and Mobasher, Bahram and Brammer, Gabriel B. and Kassin, Susan and Koekemoer, Anton M. and Giavalisco, Mauro and Papovich, Casey and Ravindranath, Swara and Faber, Sandra M. and Freundlich, Jonathan and de Mello, Duilia F.},
doi = {10.3847/1538-4357/ab1089},
file = {:Users/ben/Documents/Mendeley Desktop/Wiklind et al. - 2019 - Evolution of the Gas Mass Fraction of Progenitors to Today's Massive Galaxies ALMA Observations in the CANDELS.pdf:pdf},
issn = {1538-4357},
journal = {The Astrophysical Journal},
keywords = {cosmology,cosmology: observations,formation,galaxies,galaxies: formation,galaxies: high redshift,high redshift,observations},
number = {2},
pages = {83},
publisher = {IOP Publishing},
title = {{Evolution of the Gas Mass Fraction of Progenitors to Today's Massive Galaxies: ALMA Observations in the CANDELS GOODS-S Field}},
url = {http://dx.doi.org/10.3847/1538-4357/ab1089},
volume = {878},
year = {2019}
}
@article{Topping2020,
abstract = {We present a joint analysis of rest-UV and rest-optical spectra obtained using Keck/LRIS and Keck/MOSFIRE for a sample of 62 star-forming galaxies at z ∼ 2.3. We divide our sample into two bins based on their location in the [OIII]5007/H$\beta$ vs. [NII]6584/H$\alpha$ BPT diagram, and perform the first differential study of the rest-UV properties of massive ionizing stars as a function of rest-optical emission-line ratios. Fitting BPASS stellar population synthesis models, including nebular continuum emission, to our rest-UV composite spectra, we find that high-redshift galaxies offset towards higher [OIII]$\lambda$5007/H$\beta$ and [NII]$\lambda$6584/H$\alpha$ have younger ages ({\$}\backslashlog (\backslashtextrm {\{}{\~{}}Age/yr{\}})=7.20{\^{}}{\{}+0.57{\}}{\_}{\{}-0.20{\}}{\$}) and lower stellar metallicities ({\$}Z{\_}*=0.0010{\^{}}{\{}+0.0011{\}}{\_}{\{}-0.0003{\}}{\$}) resulting in a harder ionizing spectrum, compared to the galaxies in our sample that lie on the local BPT star-forming sequence ({\$}\backslashlog (\backslashtextrm {\{}Age/yr{\}})=8.57{\^{}}{\{}+0.88{\}}{\_}{\{}-0.84{\}}{\$}, {\$}Z{\_}*=0.0019{\^{}}{\{}+0.0006{\}}{\_}{\{}-0.0006{\}}{\$}). Additionally, we find that the offset galaxies have an ionization parameter of {\$}\backslashlog (U)=-3.04{\^{}}{\{}+0.06{\}}{\_}{\{}-0.11{\}}{\$} and nebular metallicity of ({\$}12+\backslashlog (\backslashtextrm {\{}{\~{}}O/H{\}})=8.40{\^{}}{\{}+0.06{\}}{\_}{\{}-0.07{\}}{\$}), and the non-offset galaxies have an ionization parameter of {\$}\backslashlog (U)=-3.11{\^{}}{\{}+0.08{\}}{\_}{\{}-0.08{\}}{\$} and nebular metallicity of {\$}12+\backslashlog (\backslashtextrm {\{}{\~{}}O/H{\}})=8.30{\^{}}{\{}+0.05{\}}{\_}{\{}-0.06{\}}{\$}. The stellar and nebular metallicities derived for our sample imply that the galaxies offset from the local BPT relation are more $\alpha$-enhanced ({\$}7.28{\^{}}{\{}+2.52{\}}{\_}{\{}-2.82{\}}\backslashtextrm {\{}{\~{}}O/Fe{\}}{\_}{\{}\backslashodot {\}}{\$}) compared to those consistent with the local sequence ({\$}3.04{\^{}}{\{}+0.95{\}}{\_}{\{}-0.54{\}}\backslashtextrm {\{}{\~{}}O/Fe{\}}{\_}{\{}\backslashodot {\}}{\$}). However, even galaxies that are entirely consistent with the local nebular excitation sequence appear to be $\alpha$-enhanced – in contrast with typical local systems. Such differences must be considered when estimating gas-phase oxygen abundances at high redshift based on strong emission-line ratios. Specifically, a similarity in the location of high-redshift and local galaxies in the BPT diagram may not be indicative of a similarity in their physical properties.},
