@article{Phillips1993,
abstract = {Absolute magnitudes in the B, V, and I bands are derived for nine well-observed Type Ia supernovae, using host galaxy distances estimated via the surface brightness fluctuations or Tully-Fisher methods. These data indicate that there is a significant intrinsic dispersion in the absolute magnitudes at maximum light of Type Ia supernovae, amounting to +/- 0.8 mag in B, +/- 0.6 mag in V, and +/- 0.5 mag in I. Moreover, the absolute magnitudes appear to be tightly correlated with the initial rate of decline of the B light curve, with the slope of the correlation being steepest in B and becoming progressively flatter in the V and I bands. This implies that the intrinsic B - V colors of Type Ia supernovae at maximum light are not identical, with the fastest declining light curves corresponding to the intrinsically reddest events. Certain spectroscopic properties may also be correlated with the initial decline rate. These results are most simply interpreted as evidence for a range of progenitor masses, although variations in the explosion mechanism are also possible. Considerable care must be exercised in employing Type Ia supernovae as cosmological standard candles, particularly at large redshifts where Malmquist bias could be an important effect.},
annote = {Delta M15 paper (Section 2)


Faster decline -{\textgreater} intrinsically redder
(B band declines faster than V, faster than I)},
author = {Phillips, M. M.},
doi = {10.1086/186970},
file = {:Users/ben/Documents/Mendeley Desktop/Phillips - 1993 - The absolute magnitudes of Type IA supernovae.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
month = {aug},
pages = {L105},
title = {{The absolute magnitudes of Type IA supernovae}},
url = {http://adsabs.harvard.edu/doi/10.1086/186970},
volume = {413},
year = {1993}
}
@article{Decarli2019,
abstract = {We present kpc-scale ALMA and HST imaging of the quasar PJ308-21 at {\$}z{\$}={\$}6.2342{\$}, tracing dust, gas (via the [CII] 158 {\$}\backslashmu{\$}m line) and young stars. At a resolution of {\$}\backslashsim0.3''{\$} ({\$}\backslashapprox1.7{\$} kpc), the system is resolved over {\$}{\textgreater}4''{\$} ({\$}{\textgreater}{\$}20 kpc). In particular, it features a main component, identified to be the quasar host galaxy, centered on the accreting supermassive black hole; and two other extended components on the West and East side, one redshifted and the other blueshifted relative to the quasar. The [CII] emission of the entire system stretches over {\$}{\textgreater}{\$}1500 km/s along the line of sight. All the components of the system are observed in dust, [CII], and rest-frame UV emission. The inferred [CII] luminosities [(0.9-4.6){\$}\backslashtimes 10{\^{}}9{\$} L{\$}{\_}\backslashodot{\$}], dust luminosities [(0.15-2.6){\$}\backslashtimes10{\^{}}{\{}12{\}}{\$} L{\$}{\_}\backslashodot{\$}], and rest-frame UV luminosities [(6.6-15){\$}\backslashtimes10{\^{}}{\{}10{\}}{\$} L{\$}{\_}\backslashodot{\$}], their ratios, and the implied gas/dust masses and star formation rates [11-290 M{\$}{\_}\backslashodot{\$} yr{\$}{\^{}}{\{}-1{\}}{\$}] are typical of high-redshift star-forming galaxies. A toy model of a single satellite galaxy that is tidally stripped by the interaction with the quasar host galaxy can account for the observed velocity and spatial extent of the two extended components. An outflow interpretation of the unique features in PJ308-21 is not supported by the data. PJ308-21 is thus one of the earliest galaxy mergers imaged at cosmic dawn.},
archivePrefix = {arXiv},
arxivId = {1906.05308},
author = {Decarli, Roberto and Dotti, Massimo and Ba{\~{n}}ados, Eduardo and Farina, Emanuele Paolo and Walter, Fabian and Carilli, Chris and Fan, Xiaohui and Mazzucchelli, Chiara and Neeleman, Marcel and Novak, Mladen and Riechers, Dominik and Strauss, Michael A. and Venemans, Bram P. and Yang, Yujin and Wang, Ran},
eprint = {1906.05308},
file = {:Users/ben/Documents/Mendeley Desktop/1906.05308.pdf:pdf},
keywords = {galaxies,general,high-redshift,ism,quasars,star formation},
pages = {1--9},
title = {{ALMA and HST kiloparsec-scale imaging of a quasar-galaxy merger at {\$}z\backslashapprox 6.2{\$}}},
