arxivId = {1011.6098},
author = {Overzier, Roderik A. and Heckman, Timothy M. and Wang, Jing and Armus, Lee and Buat, Veronique and Howell, Justin and Meurer, Gerhardt and Seibert, Mark and Siana, Brian and Basu-Zych, Antara and Charlot, St{\'{e}}phane and Gon{\c{c}}alves, Thiago S. and Martin, D. Christopher and Neill, James D. and Rich, R. Michael and Salim, Samir and Schiminovich, David},
