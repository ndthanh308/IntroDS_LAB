arxivId = {2002.10511},
author = {Ando, Makoto and Shimasaku, Kazuhiro and Momose, Rieko},
eprint = {2002.10511},
file = {:Users/ben/Documents/Mendeley Desktop/2002.10511.pdf:pdf},
keywords = {clusters,formation,galaxies,general,high-redshift},
number = {February},
