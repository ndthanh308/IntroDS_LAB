arxivId = {2212.00841},
author = {Estrada, Nicolas and Mercurio, Amata and Vulcani, Benedetta and Rodighiero, Giulia and Nonino, Mario and Annunziatella, Marianna and Rosati, Piero and Grillo, Claudio and Caminha, Gabriel Bartosch and Angora, Giuseppe and Biviano, Andrea and Brescia, Massimo and {De Lucia}, Gabriella and Demarco, Ricardo and Girardi, Marisa and Gobat, Raphael and Lemaux, Brian C.},
eprint = {2212.00841},
file = {:Users/ben/Documents/Mendeley Desktop/2212.00841.pdf:pdf},
keywords = {a,a proofs,aanda,article number,catalogues,clusters,data analysis,galaxies,general,manuscript no,methods,observational,page 1 of 19,photometry,virtual observatory tools},
title = {{VST-GAME: Galaxy Assembly as a function of Mass and Environment with VST. Photometric assessment and density field of MACSJ0416}},
