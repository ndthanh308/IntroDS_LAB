issn = {03650138},
journal = {Astronomy and Astrophysics Supplement Series},
keywords = {Stars: Hertzsprung-Russell (HR) diagram,Stars: evolution,Stars: interiors,Stars: low-mass},
number = {3},
pages = {371--383},
primaryClass = {astro-ph},
title = {{Evolutionary tracks and isochrones for low- and intermediate-mass stars: From 0.15 to 7 M⊙, and from Z = 0.0004 to 0.03}},
