@article{Stefanon2021,
abstract = {Deep rest-optical observations are required to accurately constrain the stellar populations of {\$}z\backslashsim8{\$} galaxies. Due to significant limitations in the availability of such data for statistically complete samples, observational results have been limited to modest numbers of bright or lensed sources. To revolutionize the present characterization of {\$}z\backslashsim8{\$} galaxies, we exploit the ultradeep ({\$}\backslashsim27{\$} mag, {\$}3\backslashsigma{\$}) Spitzer/IRAC {\$}3.6\backslashmu{\$}m and {\$}4.5\backslashmu{\$}m data, probing the rest-frame optical at {\$}z\backslashsim8{\$}, over {\$}\backslashsim200{\$} arcmin{\$}{\^{}}2{\$} of the GOODS fields from the recently completed GOODS Re-ionization Era wide-Area Treasury from Spitzer (GREATS) program, combined with observations in the CANDELS UDS and COSMOS fields. We stacked {\$}\backslashgtrsim100{\$} {\$}z\backslashsim8{\$} Lyman-Break galaxies in four bins of UV luminosity ({\$}M{\_}\backslashmathrm{\{}UV{\}}\backslashsim -20.7{\$} to {\$}-18.4{\$}) and study their {\$}H{\_}\backslashmathrm{\{}160{\}}-[3.6]{\$} and {\$}[3.6]-[4.5]{\$} colors. We find young ages ({\$}\backslashlesssim100{\$} Myr) for the three faintest stacks, inferred from their blue {\$}H{\_}\backslashmathrm{\{}160{\}}-[3.6]\backslashsim 0{\$} mag colors, consistent with a negative Balmer break. Meanwhile, the redder {\$}H{\_}\backslashmathrm{\{}160{\}}-[3.6]{\$} color seen in the brightest stack is suggestive of slightly older ages. We explored the existence of a correlation between the UV luminosity and age, and find either no trend or fainter galaxies being younger. The stacked SEDs also exhibit very red {\$}[3.6]-[4.5]\backslashsim0.5{\$} mag colors, indicative of intense [OIII]+H{\$}\backslashbeta{\$} nebular emission and SFR. The correspondingly high specific star-formation rates, sSFR{\$}\backslashgtrsim10{\$}Gyr{\$}{\^{}}{\{}-1{\}}{\$}, are consistent with recent determinations at similar redshifts and higher luminosities, and support the co-evolution between the sSFR and the specific halo mass accretion rate.},
archivePrefix = {arXiv},
arxivId = {2103.06279},
author = {Stefanon, Mauro and Bouwens, Rychard J. and Labb{\'{e}}, Ivo and Illingworth, Garth D. and Oesch, Pascal A. and van Dokkum, Pieter and Gonzalez, Valentino},
eprint = {2103.06279},
file = {:Users/ben/Documents/Mendeley Desktop/2103.06279.pdf:pdf},
keywords = {ages,galaxy,galaxy colors,galaxy formation,galaxy luminosities,galaxy properties,high-redshift galaxies,lyman-break galaxies},
title = {{Blue Rest-Frame UV-Optical Colors in z{\~{}}8 Galaxies from GREATS: Very Young Stellar Populations at {\~{}}650 Myr of Cosmic Time}},
url = {http://arxiv.org/abs/2103.06279},
year = {2021}
}
@article{Taylor2022,
abstract = {We present the first direct spectroscopic measurement of the stellar velocity dispersion function (VDF) for massive quiescent and star-forming galaxies at {\$}0.6 {\textless} z \backslashleq 1.0{\$}. For this analysis we use individual measurements of stellar velocity dispersion from high-S/N spectra from the public Large Early Galaxy Astrophysics Census (LEGA-C) survey. We report a remarkable stability of the VDF for both quiescent and star-forming galaxies within this redshift range, though we note the presence of weak evolution in the number densities of star-forming galaxies. We compare both VDFs with previous direct and inferred measurements at local and intermediate redshifts, with the caveat that previous measurements of the VDF for star-forming galaxies are poorly constrained at all epochs. We emphasize that this work is the first to directly push to low-stellar velocity dispersion ({\$}\backslashsigma{\_}\backslashstar {\textgreater} 100{\$} km s{\$}{\^{}}{\{}-1{\}}{\$}) and extend to star-forming galaxies. We are largely consistent with the high-sigma tail measured from BOSS, and we find that the VDF remains constant from the median redshift of LEGA-C, {\$}z\backslashsim0.8{\$}, to the present day.},
archivePrefix = {arXiv},
arxivId = {2210.00144},
author = {Taylor, Lance and Bezanson, Rachel and van der Wel, Arjen and Pearl, Alan and Bell, Eric F. and D'Eugenio, Francesco and Franx, Marijn and Maseda, Michael V. and Muzzin, Adam and Sobral, David and Straatman, Caroline and Whitaker, Katherine E. and Wu, Po-Feng},
eprint = {2210.00144},
file = {:Users/ben/Documents/Mendeley Desktop/2210.00144.pdf:pdf},
pages = {1--11},
title = {{The Velocity Dispersion Function for Massive Quiescent and Star-Forming Galaxies at 0.6 {\$}{\textless}{\$} z {\$}\backslashleq{\$} 1.0}},
url = {http://arxiv.org/abs/2210.00144},
year = {2022}
}
@article{Man2014,
abstract = {We measure the merger fraction of massive galaxies using the UltraVISTA/COSMOS {\$}Ks{\$}-band selected catalog, complemented with the deeper, higher resolution 3DHST+CANDELS catalog selected in the HST/WFC3 {\$}H{\$}-band, presenting the largest mass-complete photometric merger sample up to {\$}z\backslashsim3{\$}. We find that selecting mergers using the {\$}H{\_}{\{}160{\}}{\$}-band flux ratio leads to an increasing merger fraction with redshift, while selecting mergers using the stellar mass ratio causes a diminishing redshift dependence. Defining major and minor mergers as having stellar mass ratios of 1:1 - 4:1 and 4:1 - 10:1 respectively, the results imply {\$}\backslashsim{\$}1 major and {\$}\backslashlesssim{\$}1 minor merger for an average massive (log{\$}(M{\_}{\{}\backslashstar{\}}/M{\_}{\{}\backslashodot{\}}) \backslashgeqslant 10.8{\$}) galaxy during {\$}z=0.1-2.5{\$}. There may be an additional {\$}\backslashsim 0.5(0.3){\$} major (minor) merger if we use the {\$}H{\$}-band flux ratio selection. The observed amount of major merging alone is sufficient to explain the observed number density evolution for the very massive (log{\$}(M{\_}{\{}\backslashstar{\}}/M{\_}{\{}\backslashodot{\}}) \backslashgeqslant 11.1{\$}) galaxies. We argue that these very massive galaxies can put on a maximum of {\$}6\backslash{\%}{\$} of stellar mass in addition to major and minor merging, so that their number density evolution remains consistent with observations. The observed number of major and minor mergers can increase the size of a massive quiescent galaxy by a factor of two at most. This amount of merging is enough to bring the compact quiescent galaxies formed at {\$}z{\textgreater}2{\$} to lie at {\$}1\backslashsigma{\$} below the mean of the stellar mass-size relation as measured in some works (e.g. Newman et al. 2012), but additional mechanisms are needed to fully explain the evolution, and to be consistent with works suggesting stronger evolution (e.g. van der Wel et al. 2014).},
archivePrefix = {arXiv},
arxivId = {1410.3479},
