@article{Cullen2021,
abstract = {We present results from the NIRVANDELS survey investigating the gas-phase metallicity ({\$}\backslashmathrm{\{}Z{\}}{\_}{\{}\backslashmathrm{\{}gas{\}}{\}}{\$}, tracing O/H) and stellar metallicity ({\$}Z{\_}{\{}\backslashstar{\}}{\$}, tracing Fe/H) of 33 star-forming galaxies at redshifts {\$}2.95 {\textless} z {\textless} 3.80{\$}. Based on a combined analysis of deep optical and near-IR spectra, tracing the rest-frame far ultraviolet and rest-frame optical respectively, we present the first simultaneous determination of the stellar and gas-phase mass-metallicity relationships (MZRs) at {\$}z\backslashsimeq3.4{\$}. In both cases, we find that metallicity increases with increasing stellar mass ({\$}M{\_}{\{}\backslashstar{\}}{\$}), and that the power-law slope at {\$}M{\_}{\{}\backslashstar{\}} \backslashlesssim 10{\^{}}{\{}10{\}} \backslashmathrm{\{}M{\}}{\_}{\{}\backslashodot{\}}{\$} of both MZRs scales as {\$}Z \backslashpropto M{\_}{\{}\backslashstar{\}}{\^{}}{\{}0.3{\}}{\$}. Comparing the stellar and gas-phase MZRs, we present the first direct evidence for super-solar O/Fe ratios (i.e., {\$}\backslashalpha{\$}-enhancement) at {\$}z{\textgreater}3{\$}, finding {\$}\backslashmathrm{\{}(O/Fe){\}}\backslashsimeq (2.54 \backslashpm 0.38) \backslashtimes \backslashmathrm{\{}(O/Fe){\}}{\_}{\{}\backslashodot{\}}{\$}, with no clear dependence on {\$}M{\_}{\{}\backslashstar{\}}{\$}.},
