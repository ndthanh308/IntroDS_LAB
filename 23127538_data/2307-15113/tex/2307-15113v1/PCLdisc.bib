@article{Hashimoto2017a,
abstract = {We present rest-frame Ly{\$}\backslashalpha{\$} equivalent widths (EW) of 417 Ly{\$}\backslashalpha{\$} emitters (LAEs) detected with Multi Unit Spectroscopic Explorer (MUSE) on the Very Large Telescope (VLT) at {\$}2.9 {\textless} z {\textless} 6.6{\$} in the Hubble Ultra Deep Field. Based on the deep MUSE spectroscopy and ancillary Hubble Space Telescope (HST) photometry data, we carefully measured EW values taking into account extended Ly{\$}\backslashalpha{\$} emission and UV continuum slopes ({\$}\backslashbeta{\$}). Our LAEs reach unprecedented depths, both in Ly{\$}\backslashalpha{\$} luminosities and UV absolute magnitudes, from log({\$}L{\_}{\{}\backslashrm Ly\backslashalpha{\}}{\$}/erg s{\$}{\^{}}{\{}-1{\}}{\$}) {\$}\backslashsim{\$}41.0 to 43.0 and from Muv {\$}\backslashsim{\$} -16 to -21 (0.01-1.0 {\$}L{\^{}}{\{}*{\}}{\_}{\{}\backslashrm z=3{\}}{\$}). The EW values span the range of {\$}\backslashsim{\$} 5 to 240 $\backslash$AA$\backslash$ or larger, and their distribution can be well fitted by an exponential law {\$}N = N{\_}{\{}\backslashrm 0{\}}{\$} exp({\$}-{\$}EW/{\$}w{\_}{\{}\backslashrm 0{\}}{\$}). Owing to the high dynamic range in Muv, we find that the scale factor, {\$}w{\_}{\{}\backslashrm 0{\}}{\$}, depends on Muv in the sense that including fainter Muv objects increases {\$}w{\_}{\{}\backslashrm 0{\}}{\$}, i.e., the Ando effect. The results indicate that selection functions affect the EW scale factor. Taking these effects into account, we find that our {\$}w{\_}{\{}\backslashrm 0{\}}{\$} values are consistent with those in the literature within {\$}1\backslashsigma{\$} uncertainties at {\$}2.9 {\textless} z {\textless} 6.6{\$} at a given threshold of Muv and {\$}L{\_}{\{}\backslashrm Ly\backslashalpha{\}}{\$}. Interestingly, we find 12 objects with EW {\$}{\textgreater}200{\$} $\backslash$AA$\backslash$ above {\$}1\backslashsigma{\$} uncertainties. Two of these 12 LAEs show signatures of merger or AGN activity: the weak CIV {\$}\backslashlambda 1549{\$} emission line. For the remaining 10 very large EW LAEs, we find that the EW values can be reproduced by young stellar ages ({\$}{\textless} 100{\$} Myr) and low metallicities ({\$}\backslashlesssim 0.02{\$} {\$}Z{\_}{\{}\backslashrm \backslashodot{\}}{\$}). Otherwise, at least part of the Ly{\$}\backslashalpha{\$} emission in these LAEs needs to arise from anisotropic radiative transfer effects, fluorescence by hidden AGN or quasi-stellar object activity, or gravitational cooling.},
archivePrefix = {arXiv},
arxivId = {1711.01747},
author = {Hashimoto, Takuya and Garel, Thibault and Guiderdoni, Bruno and Drake, Alyssa. B and Bacon, Roland and Blaizot, Jeremy and Richard, Johan and Leclercq, Floriane and Inami, Hanae and Verhamme, Anne and Bouwens, Rychard and Brinchmann, Jarle and Cantalupo, Sebastiano and Carollo, Marcella and Caruana, Joseph and Herenz, Edmund C and Kerutt, Josephine and Marino, Raffaella A. and Mitchell, Peter and Schaye, Joop},
