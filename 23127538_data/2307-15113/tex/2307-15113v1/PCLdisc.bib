title = {{Hot Dust in Panchromatic SED Fitting: Identification of Active Galactic Nuclei and Improved Galaxy Properties}},
url = {http://arxiv.org/abs/1709.04469 http://dx.doi.org/10.3847/1538-4357/aaa8db https://iopscience.iop.org/article/10.3847/1538-4357/aaa8db},
