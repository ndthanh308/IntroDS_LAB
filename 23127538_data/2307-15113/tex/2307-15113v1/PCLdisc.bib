file = {:Users/ben/Documents/Mendeley Desktop/Bullock et al. - 2010 - STEALTH GALAXIES IN THE HALO OF THE MILKY WAY.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {color figures,dark matter,dwarf,formation,galaxies,galaxy,halo,local group,luminosity function,mass function,online-only material},
month = {jul},
number = {2},
pages = {1043--1053},
title = {{STEALTH GALAXIES IN THE HALO OF THE MILKY WAY}},
url = {http://stacks.iop.org/0004-637X/717/i=2/a=1043?key=crossref.411ba4541215f141394f8a36ea712ac1 http://arxiv.org/abs/0912.1873},
volume = {717},
year = {2010}
}
@article{Alatalo2016,
abstract = {We investigate the optical and Wide-field Survey Explorer (WISE) colors of "E+A" identified poststarburst galaxies, including a deep analysis on 190 poststarbursts detected in the 2MASS Extended Source Catalog. The poststarburst galaxies appear in both the optical green valley and the WISE Infrared Transition Zone (IRTZ). Furthermore, we find that poststarbursts occupy a distinct region [3.4]-[4.6] vs. [4.6]-[12] WISE colors, enabling the identification of this class of transitioning galaxies through the use of broad band photometric criteria alone. We have investigated possible causes for the WISE colors of poststarbursts by constructing a composite spectral energy distribution (SED), finding that mid-infrared (4--12$\backslash$micron) properties of poststarbursts are consistent with either 11.3um polycyclic aromatic hydrocarbon emission, or Thermally Pulsating Asymptotic Giant Branch (TP-AGB) and post-AGB stars. The composite SED of extended poststarburst galaxies with 22um emission detected with signal to noise {\textgreater}3 requires a hot dust component to produce their observed 22um emission, consistent with the presence of an AGN. The composite SED of WISE 22um non-detections (S/N{\textless}3) was created by stacking 22um images and is also consistent with an AGN contribution. Given that the E+A selection selects against AGNs, the non-negligible number of AGNs found in this sample should cause us to reconsider the relative importance of the presence of an AGN in transitioning galaxies.},
archivePrefix = {arXiv},
arxivId = {1608.00256},
author = {Alatalo, K. and Lacy, M. and Bitsakis, T. and Brown, M. J. I. and Ciesla, L. and Appleton, P. N. and Beaton, R. L. and Cales, S. L. and Crossett, J. and Falcon-Barroso, J. and French, K. D. and Kewley, L. J. and Kelson, D. D. and Kriek, M. and Lanz, L. and Medling, A. M. and Mulchaey, J. S. and Nyland, K. and Rich, J. A. and Urry, C. M.},
eprint = {1608.00256},
file = {:Users/ben/Documents/Mendeley Desktop/Alatalo et al. - 2016 - Welcome to the Twilight Zone The Mid-Infrared Properties of Poststarburst Galaxies.pdf:pdf},
title = {{Welcome to the Twilight Zone: The Mid-Infrared Properties of Poststarburst Galaxies}},
url = {http://arxiv.org/abs/1608.00256},
year = {2016}
}
@article{Qiu2018,
abstract = {We investigate the dependence of galaxy clustering at {\$}z \backslashsim 4 - 7{\$} on UV-luminosity and stellar mass. Our sample consists of {\$}\backslashsim{\$} 10,000 Lyman-break galaxies (LBGs) in the XDF and CANDELS fields. As part of our analysis, the {\$}M{\_}\backslashstar - M{\_}{\{}\backslashrm UV{\}}{\$} relation is estimated for the sample, which is found to have a nearly linear slope of {\$}d\backslashlog{\_}{\{}10{\}} M{\_}\backslashstar / d M{\_}{\{}\backslashrm UV{\}} \backslashsim 0.44{\$}. We subsequently measure the angular correlation function and bias in different stellar mass and luminosity bins. We focus on comparing the clustering dependence on these two properties. While UV-luminosity is only related to recent starbursts of a galaxy, stellar mass reflects the integrated build-up of the whole star formation history, which should make it more tightly correlated with halo mass. Hence, the clustering segregation with stellar mass is expected to be larger than with luminosity. However, our measurements suggest that the segregation with luminosity is larger with {\$}\backslashsimeq 90\backslash{\%}{\$} confidence (neglecting contributions from systematic errors). We compare this unexpected result with predictions from the $\backslash$textsc{\{}Meraxes{\}} semi-analytic galaxy formation model. Interestingly, the model reproduces the observed angular correlation functions, and also suggests stronger clustering segregation with luminosity. The comparison between our observations and the model provides evidence of multiple halo occupation in the small scale clustering.},
