@article{Jiang2016,
abstract = {We present the discovery of nine quasars at {\$}z\backslashsim6{\$} identified in the Sloan Digital Sky Survey (SDSS) imaging data. This completes our survey of {\$}z\backslashsim6{\$} quasars in the SDSS footprint. Our final sample consists of 52 quasars at {\$}5.7{\textless}z\backslashle6.4{\$}, including 29 quasars with {\$}z{\_}{\{}\backslashrm AB{\}}\backslashle20{\$} mag selected from 11,240 deg{\$}{\^{}}2{\$} of the SDSS single-epoch imaging survey (the main survey), 10 quasars with {\$}20\backslashle z{\_}{\{}\backslashrm AB{\}}\backslashle20.5{\$} selected from 4223 deg{\$}{\^{}}2{\$} of the SDSS overlap regions (regions with two or more imaging scans), and 13 quasars down to {\$}z{\_}{\{}\backslashrm AB{\}}\backslashapprox22{\$} mag from the 277 deg{\$}{\^{}}2{\$} in Stripe 82. They span a wide luminosity range of {\$}-29.0\backslashle M{\_}{\{}1450{\}}\backslashle-24.5{\$}. This well-defined sample is used to derive the quasar luminosity function (QLF) at {\$}z\backslashsim6{\$}. After combining our SDSS sample with two faint ({\$}M{\_}{\{}1450{\}}\backslashge-23{\$} mag) quasars from the literature, we obtain the parameters for a double power-law fit to the QLF. The bright-end slope {\$}\backslashbeta{\$} of the QLF is well constrained to be {\$}\backslashbeta=-2.8\backslashpm0.2{\$}. Due to the small number of low-luminosity quasars, the faint-end slope {\$}\backslashalpha{\$} and the characteristic magnitude {\$}M{\_}{\{}1450{\}}{\^{}}{\{}\backslashast{\}}{\$} are less well constrained, with {\$}\backslashalpha=-1.90{\_}{\{}-0.44{\}}{\^{}}{\{}+0.58{\}}{\$} and {\$}M{\^{}}{\{}\backslashast{\}}=-25.2{\_}{\{}-3.8{\}}{\^{}}{\{}+1.2{\}}{\$} mag. The spatial density of luminous quasars, parametrized as {\$}\backslashrho(M{\_}{\{}1450{\}}{\textless}-26,z)=\backslashrho(z=6)\backslash,10{\^{}}{\{}k(z-6){\}}{\$}, drops rapidly from {\$}z\backslashsim5{\$} to 6, with {\$}k=-0.72\backslashpm0.11{\$}. Based on our fitted QLF and assuming an IGM clumping factor of {\$}C=3{\$}, we find that the observed quasar population cannot provide enough photons to ionize the {\$}z\backslashsim6{\$} IGM at {\$}\backslashsim90{\$}$\backslash${\%} confidence. Quasars may still provide a significant fraction of the required photons, although much larger samples of faint quasars are needed for more stringent constraints on the quasar contribution to reionization.},
