@article{Yang2017,
abstract = {We studied Lyman-{\$}\backslashalpha{\$} (Ly{\$}\backslashalpha{\$}) escape in a statistical sample of 43 Green Peas with HST/COS Ly{\$}\backslashalpha{\$} spectra. Green Peas are nearby star-forming galaxies with strong [OIII]{\$}\backslashlambda{\$}5007 emission lines. Our sample is four times larger than the previous sample and covers a much more complete range of Green Pea properties. We found that about 2/3 of Green Peas are strong Ly{\$}\backslashalpha{\$} line emitters with rest-frame Ly{\$}\backslashalpha{\$} equivalent width {\$}{\textgreater}20{\$} $\backslash$AA. The Ly{\$}\backslashalpha{\$} profiles of Green Peas are diverse. The Ly{\$}\backslashalpha{\$} escape fraction, defined as the ratio of observed Ly{\$}\backslashalpha{\$} flux to intrinsic Ly{\$}\backslashalpha{\$} flux, shows anti-correlations with a few Ly{\$}\backslashalpha{\$} kinematic features -- both the blue peak and red peak velocities, the peak separations, and FWHM of the red portion of the Ly{\$}\backslashalpha{\$} profile. Using properties measured from SDSS optical spectra, we found many correlations -- Ly{\$}\backslashalpha{\$} escape fraction generally increases at lower dust reddening, lower metallicity, lower stellar mass, and higher [OIII]/[OII] ratio. We fit their Ly{\$}\backslashalpha{\$} profiles with the HI shell radiative transfer model and found Ly{\$}\backslashalpha{\$} escape fraction anti-correlates with the best-fit {\$}N{\_}{\{}HI{\}}{\$}. Finally, we fit an empirical linear relation to predict Ly{\$}\backslashalpha{\$} escape fraction from the dust extinction and Ly{\$}\backslashalpha{\$} red peak velocity. The standard deviation of this relation is about 0.3 dex. This relation can be used to isolate the effect of IGM scatterings from Ly{\$}\backslashalpha{\$} escape and to probe the IGM optical depth along the line of sight of each {\$}z{\textgreater}7{\$} Ly{\$}\backslashalpha{\$} emission line galaxy in the JWST era.},
archivePrefix = {arXiv},
arxivId = {1701.01857},
author = {Yang, Huan and Malhotra, Sangeeta and Gronke, Max and Rhoads, James E. and Leitherer, Claus and Wofford, Aida and Jiang, Tianxing and Dijkstra, Mark and Tilvi, V. and Wang, Junxian},
eprint = {1701.01857},
