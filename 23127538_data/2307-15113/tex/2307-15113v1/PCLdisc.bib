author = {Gordon, Karl D. and Clayton, Geoffrey C. and Misselt, K. A. and Landolt, Arlo U. and Wolff, Michael J.},
doi = {10.1086/376774},
eprint = {0305257},
file = {:Users/ben/Documents/Mendeley Desktop/Gordon et al. - 2003 - A Quantitative Comparison of SMC, LMC, and Milky Way UV to NIR Extinction Curves.pdf:pdf},
isbn = {4010802030},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {asteroids,general,minor planets,solar system},
pages = {279--293},
primaryClass = {astro-ph},
title = {{A Quantitative Comparison of SMC, LMC, and Milky Way UV to NIR Extinction Curves}},
url = {http://arxiv.org/abs/astro-ph/0305257},
volume = {594},
