file = {:Users/ben/Documents/Mendeley Desktop/Ito et al. - 2023 - COSMOS2020 Discovery of a Protocluster of Massive Quiescent Galaxies at z = 2.77.pdf:pdf},
issn = {2041-8205},
journal = {The Astrophysical Journal Letters},
month = {mar},
