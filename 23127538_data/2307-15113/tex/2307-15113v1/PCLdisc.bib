issn = {20418213},
journal = {Astrophysical Journal Letters},
keywords = {Dark matter,Galaxies: elliptical and lenticular, cD,Galaxies: halos,Galaxies: structure},
number = {2 PART 2},
pages = {163--167},
title = {{Dark matter contraction and the stellar content of massive early-type galaxies: Disfavoring "light" initial mass functions}},
volume = {721},
year = {2010}
}
