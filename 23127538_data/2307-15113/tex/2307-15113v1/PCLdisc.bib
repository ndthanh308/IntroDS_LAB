file = {:Users/ben/Documents/Mendeley Desktop/Speagle et al. - 2014 - A HIGHLY CONSISTENT FRAMEWORK FOR THE EVOLUTION OF THE STAR-FORMING “MAIN SEQUENCE” FROM z ∼ 0-6.pdf:pdf},
issn = {00670049},
journal = {The Astrophysical Journal Supplement Series},
keywords = {galaxies: evolution,galaxies: star formation,radio continuum: galaxies,surveys},
