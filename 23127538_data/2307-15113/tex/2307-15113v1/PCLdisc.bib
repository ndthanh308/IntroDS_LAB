file = {:Users/ben/Documents/Mendeley Desktop/Jaff{\'{e}} et al. - 2018 - GASP. IX. Jellyfish galaxies in phase-space An orbital study of intense ram-pressure stripping in clusters.pdf:pdf},
issn = {13652966},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Galaxies: ISM,Galaxies: clusters: intracluster medium,Galaxies: peculiar,Galaxies:evolution},
number = {4},
