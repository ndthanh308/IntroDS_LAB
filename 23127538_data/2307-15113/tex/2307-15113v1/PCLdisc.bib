arxivId = {1902.02792},
author = {Finkelstein, Steven L. and D'Aloisio, Anson and Paardekooper, Jan-Pieter and {Russell Ryan Jr.} and Behroozi, Peter and Finlator, Kristian and Livermore, Rachael and Sanderbeck, Phoebe R. Upton and Vecchia, Claudio Dalla and Khochfar, Sadegh},
doi = {10.3847/1538-4357/ab1ea8},
