journal = {The Astrophysical Journal},
month = {apr},
number = {1},
pages = {122--148},
primaryClass = {astro-ph},
title = {{Improved Distances to Type Ia Supernovae with Multicolor Light‐Curve Shapes: MLCS2k2}},
