doi = {10.1093/mnras/stx1903},
eprint = {1707.07989},
file = {:Users/ben/Documents/Mendeley Desktop/Rowlands et al. - 2018 - Galaxy And Mass Assembly (GAMA) The mechanisms for quiescent galaxy formation at z 1.pdf:pdf},
