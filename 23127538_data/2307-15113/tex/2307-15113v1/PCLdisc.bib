file = {:Users/ben/Documents/Mendeley Desktop/Meurer, Heckman, Calzetti - 1999 - Dust Absorption and the Ultraviolet Luminosity Density at z ≈ 3 as Calibrated by Local Starburst Ga.pdf:pdf},
issn = {0004-637X},
