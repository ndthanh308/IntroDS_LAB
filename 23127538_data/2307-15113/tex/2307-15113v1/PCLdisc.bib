author = {Erben, T and Hildebrandt, H and Lerchster, M and Hudelot, P. and Benjamin, J. and van Waerbeke, L. and Schrabback, T. and Brimioulle, F. and Cordes, O. and Dietrich, J. P. and Holhjem, K. and Schirmer, M. and Schneider, P.},
doi = {10.1051/0004-6361:200810426},
eprint = {0811.2239},
file = {:Users/ben/Documents/Mendeley Desktop/Erben et al. - 2009 - CARS the CFHTLS-Archive-Research Survey.pdf:pdf},
issn = {0004-6361},
journal = {Astronomy and Astrophysics},
keywords = {galaxies,photometry,surveys},
month = {jan},
