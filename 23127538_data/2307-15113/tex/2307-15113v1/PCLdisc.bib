eprint = {9906470},
file = {:Users/ben/Documents/Mendeley Desktop/Balogh et al. - 1999 - Differential Galaxy Evolution in Cluster and Field Galaxies at {\$}zapprox 0.3{\$}.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {GALAXIES: CLUSTERS: GENERAL,GALAXIES: EVOLUTION,GALAXIES: PHOTOMETRY,GALAXIES: STELLAR CONTENT,Galaxies: Clusters: General,Galaxies: Evolution,Galaxies: Photometry,Galaxies: Stellar Content},
month = {dec},
