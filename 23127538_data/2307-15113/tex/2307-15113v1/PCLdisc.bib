issn = {2041-8205},
journal = {The Astrophysical Journal},
keywords = {galaxies: formation,galaxies: fundamental parameters,galaxies: kinematics and dynamics,galaxies: statistics},
month = {jun},
number = {2},
pages = {L44},
title = {{REVEALING VELOCITY DISPERSION AS THE BEST INDICATOR OF A GALAXY's COLOR, COMPARED TO STELLAR MASS, SURFACE MASS DENSITY, OR MORPHOLOGY}},
