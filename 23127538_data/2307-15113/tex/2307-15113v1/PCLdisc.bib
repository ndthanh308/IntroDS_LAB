@article{Sorba2018,
abstract = {We perform spatially-resolved, pixel-by-pixel SED fitting on galaxies up to {\$}z\backslashsim2.5{\$} in the Hubble Extreme Deep Field (XDF). Comparing stellar mass estimates from spatially resolved and spatially unresolved photometry we find that unresolved masses can be systematically underestimated by factors of up to 5. The ratio of the unresolved to resolved mass measurement depends on the galaxy's specific star formation rate (sSFR): at low sSFRs the bias is small, but above sSFR{\$}\backslash \backslashsim 10{\^{}}{\{}-9.5{\}}{\$} yr{\$}{\^{}}{\{}-1{\}}{\$} the discrepancy increases rapidly such that galaxies with sSFRs{\$}\backslash \backslashsim 10{\^{}}{\{}-8{\}}{\$} yr{\$}{\^{}}{\{}-1{\}}{\$} have unresolved mass estimates of only one half to one fifth of the resolved value. This result indicates that stellar masses estimated from spatially-unresolved datasets need to be systematically corrected, in some cases by large amounts, and we provide an analytic prescription for applying this correction. We show that correcting stellar mass measurements for this bias changes the normalization and slope of the star-forming main sequence and reduces its intrinsic width; most dramatically, correcting for the mass bias increases the stellar mass density of the Universe at high redshift and can resolve the long-standing discrepancy between the directly-measured cosmic star formation rate density at {\$}z\backslashgtrsim1{\$} and that inferred from stellar mass densities ("the missing mass problem").},
archivePrefix = {arXiv},
arxivId = {1801.07368},
author = {Sorba, Robert and Sawicki, Marcin},
eprint = {1801.07368},
file = {:Users/ben/Documents/Mendeley Desktop/Sorba, Sawicki - 2018 - Spatially-unresolved SED fitting can underestimate galaxy masses a solution to the missing mass problem.pdf:pdf},
keywords = {galaxies,high-redshift,mass function,statistics,stellar content},
number = {January},
pages = {1--17},
title = {{Spatially-unresolved SED fitting can underestimate galaxy masses: a solution to the missing mass problem}},
url = {http://arxiv.org/abs/1801.07368},
volume = {17},
year = {2018}
}
@article{Price2019,
abstract = {We present ionized gas kinematics for 708 galaxies at {\$}z\backslashsim 1.4-3.8{\$} from the MOSFIRE Deep Evolution Field survey, measured using models which account for random galaxy-slit misalignments together with structural parameters derived from CANDELS Hubble Space Telescope (HST) imaging. Kinematics and sizes are used to derive dynamical masses. Baryonic masses are estimated from stellar masses and inferred gas masses from dust-corrected star formation rates (SFRs) and the Kennicutt-Schmidt relation. We measure resolved rotation for 108 galaxies. For the remaining 600 galaxies we use models based on HST imaging structural parameters together with integrated velocity dispersions and baryonic masses to statistically constrain the median ratio of intrinsic ordered-to-disordered motion, {\$}V/\backslashsigma{\_}{\{}V,0{\}}{\$}. We find {\$}V/\backslashsigma{\_}{\{}V,0{\}}{\$} increases with increasing stellar mass and decreasing specific SFR (SSFR). These trends may reflect marginal disk stability, where systems with higher gas fractions have thicker disks. For galaxies with detected rotation we assess trends between their kinematics and mass, SSFR, and baryon surface density ({\$}\backslashSigma{\_}{\{}\backslashmathrm{\{}bar{\}},e{\}}{\$}). Intrinsic dispersion correlates most with {\$}\backslashSigma{\_}{\{}\backslashmathrm{\{}bar{\}},e{\}}{\$} and velocity correlates most with mass. By comparing dynamical and baryonic masses, we find that galaxies at {\$}z\backslashsim 1.4-3.8{\$} are baryon dominated within their effective radii ({\$}R{\_}E{\$}), with Mdyn/Mbaryon increasing over time. The inferred baryon fractions within {\$}R{\_}E{\$}, {\$}f{\_}{\{}\backslashmathrm{\{}bar{\}}{\}}{\$}, decrease over time, even at fixed mass, size, or surface density. At fixed redshift, {\$}f{\_}{\{}\backslashmathrm{\{}bar{\}}{\}}{\$} does not appear to vary with stellar mass, but increases with decreasing {\$}R{\_}E{\$} and increasing {\$}\backslashSigma{\_}{\{}\backslashmathrm{\{}bar{\}},e{\}}{\$}. For galaxies at {\$}z\backslashgeq2{\$}, the median inferred baryon fractions generally exceed 100{\%}. We discuss possible explanations and future avenues to resolve this tension.},
