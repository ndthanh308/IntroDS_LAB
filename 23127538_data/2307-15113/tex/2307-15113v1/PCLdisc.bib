url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2002AJ....124.3135K{\&}link{\_}type=ABSTRACT{\%}5Cnpapers2://publication/doi/10.1086/344487{\%}5Cnhttp://stacks.iop.org/1538-3881/124/i=6/a=3135{\%}5Cnhttp://stacks.iop.org/1538-3881/124/i=6/a=3135},
