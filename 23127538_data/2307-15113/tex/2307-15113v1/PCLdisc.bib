eprint = {1507.07932},
file = {:Users/ben/Documents/Mendeley Desktop/Kewley et al. - 2015 - A rise in the ionizing photons in star-forming galaxies over the past 5 billion years.pdf:pdf},
month = {jul},
number = {2014},
title = {{A rise in the ionizing photons in star-forming galaxies over the past 5 billion years}},
url = {http://arxiv.org/abs/1507.07932 http://dx.doi.org/10.1088/2041-8205/812/2/L20},
