doi = {10.1038/s41550-017-0297-8},
file = {:Users/ben/Documents/Mendeley Desktop/s41550-017-0297-8.pdf:pdf},
issn = {23973366},
journal = {Nature Astronomy},
number = {1},
pages = {56--62},
publisher = {Springer US},
title = {{A dusty star-forming galaxy at z = 6 revealed by strong gravitational lensing}},
url = {http://dx.doi.org/10.1038/s41550-017-0297-8},
volume = {2},
year = {2018}
}
@article{Tacchella2021,
abstract = {We investigate the stellar populations for a sample of 161 massive, mainly quiescent galaxies at {\$}\backslashlangle z{\_}{\{}\backslashrm obs{\}} \backslashrangle=0.8{\$} with deep Keck/DEIMOS rest-frame optical spectroscopy (HALO7D survey). With the fully Bayesian framework Prospector, we simultaneously fit the spectroscopic and photometric data with an advanced physical model (including non-parametric star-formation histories, emission lines, variable dust attenuation law, and dust and AGN emission) together with an uncertainty and outlier model. We show that both spectroscopy and photometry are needed to break the dust-age-metallicity degeneracy. We find a large diversity of star-formation histories: although the most massive ({\$}M{\_}{\{}\backslashstar{\}}{\textgreater}2\backslashtimes10{\^{}}{\{}11{\}}{\~{}}M{\_}{\{}\backslashodot{\}}{\$}) galaxies formed the earliest (formation redshift of {\$}z{\_}{\{}\backslashrm f{\}}\backslashapprox5-10{\$} with a short star-formation timescale of {\$}\backslashtau{\_}{\{}\backslashrm SF{\}}\backslashlesssim1{\~{}}\backslashmathrm{\{}Gyr{\}}{\$}), lower-mass galaxies have a wide range of formation redshifts, leading to only a weak trend of {\$}z{\_}{\{}\backslashrm f{\}}{\$} with {\$}M{\_}{\{}\backslashstar{\}}{\$}. Interestingly, several low-mass galaxies with have formation redshifts of {\$}z{\_}{\{}\backslashrm f{\}}\backslashapprox5-8{\$}. Star-forming galaxies evolve about the star-forming main sequence, crossing the ridgeline several times in their past. Quiescent galaxies show a wide range and continuous distribution of quenching timescales ({\$}\backslashtau{\_}{\{}\backslashrm quench{\}}\backslashapprox0-5{\~{}}\backslashmathrm{\{}Gyr{\}}{\$}) with a median of {\$}\backslashlangle\backslashtau{\_}{\{}\backslashrm quench{\}}\backslashrangle=1.0{\_}{\{}-0.9{\}}{\^{}}{\{}+0.8{\}}{\~{}}\backslashmathrm{\{}Gyr{\}}{\$} and of quenching epochs of {\$}z{\_}{\{}\backslashrm quench{\}}\backslashapprox0.8-5.0{\$} ({\$}\backslashlangle z{\_}{\{}\backslashrm quench{\}}\backslashrangle=1.3{\_}{\{}-0.4{\}}{\^{}}{\{}+0.7{\}}{\$}). This large diversity of quenching timescales and epochs points toward a combination of internal and external quenching mechanisms. In our sample, rejuvenation and "late bloomers" are uncommon. In summary, our analysis supports the "grow {\&} quench" framework and is consistent with a wide and continuously-populated diversity of quenching timescales.},
archivePrefix = {arXiv},
arxivId = {2102.12494},
author = {Tacchella, Sandro and Conroy, Charlie and Faber, S. M. and Johnson, Benjamin D. and Leja, Joel and Barro, Guillermo and Cunningham, Emily C. and Deason, Alis J. and Guhathakurta, Puragra and Guo, Yicheng and Hernquist, Lars and Koo, David C. and McKinnon, Kevin and Rockosi, Constance M. and Speagle, Joshua S. and van Dokkum, Pieter and Yesuf, Hassen M.},
eprint = {2102.12494},
file = {:Users/ben/Documents/Mendeley Desktop/2102.12494.pdf:pdf},
keywords = {evolution,formation,galaxies,star formation,stellar content},
title = {{Fast, Slow, Early, Late: Quenching Massive Galaxies at {\$}z\backslashsim0.8{\$}}},
url = {http://arxiv.org/abs/2102.12494},
year = {2021}
}
