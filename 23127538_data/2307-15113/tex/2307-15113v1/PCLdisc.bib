volume = {706},
year = {2009}
}
@article{Carnall2018,
abstract = {We present Bayesian Analysis of Galaxies for Physical Inference and Parameter EStimation, or BAGPIPES, a new Python tool which can be used to rapidly generate complex model galaxy spectra and to fit these to arbitrary combinations of spectroscopic and photometric data using the MultiNest algorithm. We extensively test our ability to recover realistic star-formation histories (SFHs) with BAGPIPES by fitting mock observations of quiescent galaxies from the MUFASA simulation. We show that a double-power-law model produces better agreement with realistic SFHs than an exponentially-declining model. We then perform a detailed analysis of the SFHs of a sample of 9312 quiescent galaxies from UltraVISTA with stellar masses, {\$}M{\_}* {\textgreater} 10{\^{}}{\{}10{\}}\backslash \backslashmathrm{\{}M{\_}\backslashodot{\}}{\$} and redshifts {\$}0.25 {\textless} z{\_}\backslashmathrm{\{}obs{\}} {\textless} 3.75{\$}. The majority of our quiescent sample exhibit SFHs which rise gradually then quench relatively rapidly, over {\$}\backslashsim 1{\{}-{\}}2{\$} Gyr. This behaviour is consistent with recent cosmological hydrodynamic simulations, where AGN-driven feedback in the low-accretion (jet) mode is the dominant quenching mechanism. In addition, we identify two further subsets of objects with distinct SFH shapes. At {\$}z{\_}\backslashmathrm{\{}obs{\}} \backslashgtrsim 1{\$} we find a class of objects with SFHs which rise and fall very rapidly, with quenching timescales of {\$}\backslashlesssim 1{\$} Gyr. These objects are consistent with (potentially merger-triggered) quasar-mode AGN feedback. Also, at {\$}z{\_}\backslashmathrm{\{}obs{\}} \backslashlesssim 1{\$} we find a population with SFHs which quench more slowly than they rise, over {\$}\backslashgtrsim3{\$} Gyr, which we speculate to be the result of a 'natural' quenching process, where galaxy SFHs die down gradually due to the diminishing overall cosmic gas supply. Purely passive evolution of the quiescent population at {\$}z{\_}\backslashmathrm{\{}obs{\}} \backslashgtrsim 0.5{\$} is disfavoured by our results.},
archivePrefix = {arXiv},
arxivId = {1712.04452},
author = {Carnall, A. C. and McLure, R. J. and Dunlop, J. S. and Dav{\'{e}}, R.},
doi = {10.1093/mnras/sty2169},
eprint = {1712.04452},
file = {:Users/ben/Documents/Mendeley Desktop/Carnall et al. - 2018 - Inferring the star formation histories of massive quiescent galaxies with bagpipes evidence for multiple quenchi.pdf:pdf},
issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {evolution,galaxies,methods,star formation,statistical},
month = {nov},
number = {4},
pages = {4379--4401},
title = {{Inferring the star formation histories of massive quiescent galaxies with bagpipes: evidence for multiple quenching mechanisms}},
url = {http://arxiv.org/abs/1712.04452 https://academic.oup.com/mnras/article/480/4/4379/5068189},
volume = {480},
year = {2018}
}
@article{Saracco2019,
abstract = {We present the analysis, based on spectra collected at the Large Binocular Telescope, of the stellar populations in seven spheroidal galaxies in the cluster XLSSJ0223 at {\$}z{\$}{\$}\backslashsim{\$}1.22. The aim is to constrain the epoch of their formation and their star formation history. Using absorption line strenghts and full spectral fitting, we derive for the stellar populations of the seven spheroids a median age {\textless}Age{\textgreater}=2.4{\$}\backslashpm{\$}0.6 Gyr, corresponding to a median formation redshift {\$}{\textless}{\$}{\$}z{\_}f{\$}{\$}{\textgreater}{\$}{\$}\backslashsim2.6{\_}{\{}-0.5{\}}{\^{}}{\{}+0.7{\}}{\$} (lookback time = 11{\$}{\_}{\{}-1.0{\}}{\^{}}{\{}+0.6{\}}{\$} Gyr). We find a significant scatter in age, showing that massive spheroids, at least in our targeted cluster, are not coeval. The median metallicity is [Z/H]=0.09{\$}\backslashpm{\$}0.16, as for early-types in clusters at 0{\$}{\textless}{\$}{\$}z{\$}{\textless}0.9. This lack of evolution of [Z/H] over the range 0{\$}{\textless}{\$}{\$}z{\$}{\$}{\textless}{\$}1.3, corresponding to the last 9 billions years, suggests that no significant additional star formation and chemical enrichment are required for cluster spheroids to reach the present-day population. We do not detect significant correlation between age and velocity dispersion {\$}\backslashsigma{\_}e{\$}, or dynamical mass M{\$}{\_}{\{}dyn{\}}{\$}, or effective stellar mass density {\$}\backslashSigma{\_}e{\$}. On the contrary, the metallicity [Z/H] of the seven spheroids is correlated to their dynamical mass M{\$}{\_}{\{}dyn{\}}{\$}, according to a relation similar to the one for local spheroids. [Z/H] is also anticorrelated to stellar mass density {\$}\backslashSigma{\_}e{\$} because of the anticorrelation between M{\$}{\_}{\{}dyn{\}}{\$} and {\$}\backslashSigma{\_}e{\$}. Therefore, the basic trends observed in the local universe were already established at {\$}z\backslashsim1.3{\$}, i.e. more massive spheroids are more metal rich, have lower stellar mass density and tend to be older than lower-mass galaxies.},
archivePrefix = {arXiv},
arxivId = {1901.01595},
author = {Saracco, P. and {La Barbera}, F and Gargiulo, A. and Mannucci, F. and Marchesini, D. and Nonino, M. and Ciliegi, P.},
doi = {10.1093/mnras/sty3509},
eprint = {1901.01595},
file = {:Users/ben/Documents/Mendeley Desktop/1901.01595.pdf:pdf},
