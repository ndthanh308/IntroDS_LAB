issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {abundances,disc galaxies form first,evolution,formation,galaxies,in it,lu-,mass function,minosity function,of morphol-,ogy and ongoing star,owing,scenario for the co-evolution,there exists a canonical},
month = {oct},
number = {2},
