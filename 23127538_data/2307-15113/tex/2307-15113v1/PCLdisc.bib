keywords = {cosmology,evolution,formation,galaxies,methods,nu-,theory},
month = {dec},
number = {December},
pages = {1--24},
title = {{The stellar mass function and star formation rate-stellar mass relation of galaxies at z {\~{}} 4 - 7}},
