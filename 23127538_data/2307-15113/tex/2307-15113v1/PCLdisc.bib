author = {Wilson, Gillian and Muzzin, Adam and Yee, H. K.C. and Lacy, Mark and Surace, Jason and Gilbank, David and Blindert, Kris and Hoekstra, Henk and Majumdar, Subhabrata and Demarco, Ricardo and Gardner, Jonathan P. and Gladders, Michael D. and Lonsdale, Carol},
doi = {10.1088/0004-637X/698/2/1943},
