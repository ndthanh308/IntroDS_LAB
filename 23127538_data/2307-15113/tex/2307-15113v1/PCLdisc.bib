@article{Ito2023a,
abstract = {We present the results of a systematic study of the rest-frame optical morphology of quiescent galaxies at {\$}z \backslashgeq 3{\$} using the Near-Infrared Camera (NIRCam) onboard {\$}JWST{\$}. Based on a sample selected by {\$}UVJ{\$} color or {\$}NUVUVJ{\$} color, we focus on 26 quiescent galaxies with {\$}9.8{\textless}\backslashlog{\{}(M{\_}\backslashstar/M{\_}\backslashodot){\}}{\textless}11.4{\$} at {\$}2.8{\textless}z{\_}{\{}\backslashrm phot{\}}{\textless}4.6{\$} with publicly available {\$}JWST{\$} data. Their sizes are constrained by fitting the S$\backslash$'ersic profile to all available NIRCam images. We see a negative correlation between the observed wavelength and the size in our sample and derive their size at the rest-frame {\$}0.5\backslash, {\{}\backslashrm \backslashmu m{\}}{\$} taking into account this trend. Our quiescent galaxies show a significant correlation between the rest-frame {\$}0.5\backslash, {\{}\backslashrm \backslashmu m{\}}{\$} size and the stellar mass at {\$}z\backslashgeq3{\$}. The analytical fit for them at {\$}\backslashlog{\{}(M{\_}\backslashstar/M{\_}\backslashodot){\}}{\textgreater}10.3{\$} implies that our size - stellar mass relations are below those at lower redshifts, with the amplitude of {\$}\backslashsim0.6\backslash, {\{}\backslashrm kpc{\}}{\$} at {\$}M{\_}\backslashstar = 5\backslashtimes 10{\^{}}{\{}10{\}}\backslash, M{\_}\backslashodot{\$}. This value agrees with the extrapolation from the size evolution of quiescent galaxies at {\$}z{\textless}3{\$} in the literature, implying that the size of quiescent galaxies increases monotonically from {\$}z\backslashsim3-5{\$}. Our sample is mainly composed of galaxies with bulge-like structures according to their median S$\backslash$'ersic index and axis ratio of {\$}n\backslashsim3-4{\$} and {\$}q\backslashsim0.6-0.8{\$}, respectively. On the other hand, there is a trend of increasing fraction of galaxies with low S$\backslash$'ersic index, suggesting {\$}3{\textless}z{\textless}5{\$} might be the epoch of onset of morphological transformation with a fraction of very notable disky quenched galaxies.},
