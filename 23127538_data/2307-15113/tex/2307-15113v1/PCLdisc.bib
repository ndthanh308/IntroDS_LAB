author = {Donnari, Martina and Pillepich, Annalisa and Joshi, Gandhali D. and Nelson, Dylan and Genel, Shy and Marinacci, Federico and Rodriguez-Gomez, Vicente and Pakmor, R{\"{u}}diger and Torrey, Paul and Vogelsberger, Mark and Hernquist, Lars},
doi = {10.1093/mnras/staa3006},
