@article{vandeSande2013,
abstract = {Using stellar kinematics measurements, we investigate the growth of massive, quiescent galaxies from z ∼ 2 to today. We present X-Shooter spectra from the UV to NIR and dynamical mass measurements of five quiescent massive ({\textgreater}1011 M⊙) galaxies at z ∼ 2. This triples the sample of z {\textgreater} 1.5 galaxies with well-constrained ($\delta$$\sigma$ {\textless} 100 km s-1) velocity dispersion measurements. From spectral population synthesis modeling we find that these galaxies have stellar ages that range from 0.5 to 2 Gyr, with no signs of ongoing star formation. We measure velocity dispersions (290-450 km s-1) from stellar absorption lines and find that they are 1.6-2.1 times higher than those of galaxies in the Sloan Digital Sky Survey at the same mass. Sizes are measured using GALFIT from Hubble Space Telescope Wide Field Camera 3 H160 and UDS K-band images. The dynamical masses correspond well to the spectral energy distribution based stellar masses, with dynamical masses that are ∼15{\%} higher. We find that M*/Mdyn may decrease slightly with time, which could reflect the increase of the dark matter fraction within an increasing effective radius. We combine different stellar kinematic studies from the literature and examine the structural evolution from z ∼ 2 to z ∼ 0: we confirm that at fixed dynamical mass, the effective radius increases by a factor of ∼2.8, and the velocity dispersion decreases by a factor of ∼1.7. The mass density within one effective radius decreases by a factor of ∼20, while within a fixed physical radius (1 kpc) it decreases only mildly (factor of ∼2). When we allow for an evolving mass limit by selecting a population of galaxies at fixed number density, a stronger size growth with time is found (factor of ∼4), velocity dispersion decreases by a factor of ∼1.4, and interestingly, the mass density within 1 kpc is consistent with no evolution. This finding suggests that massive quiescent galaxies at z ∼ 2 grow inside out, consistent with the expectations from minor mergers. {\textcopyright} 2013. The American Astronomical Society. All rights reserved.},
author = {van de Sande, Jesse and Kriek, Mariska and Franx, Marijn and van Dokkum, Pieter G. and Bezanson, Rachel and Bouwens, Rychard J. and Quadri, Ryan F. and Rix, Hans-Walter and Skelton, Rosalind E.},
doi = {10.1088/0004-637X/771/2/85},
file = {:Users/ben/Documents/Mendeley Desktop/van{\_}de{\_}Sande{\_}2013{\_}ApJ{\_}771{\_}85.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {cosmology: observations,galaxies: evolution,galaxies: formation},
month = {jun},
number = {2},
pages = {85},
title = {{STELLAR KINEMATICS OF z ∼ 2 GALAXIES AND THE INSIDE-OUT GROWTH OF QUIESCENT GALAXIES ,}},
url = {https://iopscience.iop.org/article/10.1088/0004-637X/771/2/85},
volume = {771},
year = {2013}
}
@article{Khostovan2015,
abstract = {We investigate the evolution of the H{\$}\backslashbeta{\$}+[OIII] and [OII] luminosity functions from {\$}z \backslashsim 0.8{\$} to {\$}\backslashsim5{\$} in four redshift slices per emission line using data from the High-{\{}$\backslash$it z{\}} Emission Line Survey (HiZELS). This is the first time that the H{\$}\backslashbeta{\$}+[OIII] and [OII] luminosity functions have been studied at these redshifts in a self-consistent analysis. This is also the largest sample of [OII] and H{\$}\backslashbeta{\$}+[OIII] emitters (3475 and 3298 emitters, respectively) in this redshift range, with large co-moving volumes {\$}\backslashsim 1 \backslashtimes 10{\^{}}6{\$} Mpc{\$}{\^{}}{\{}-3{\}}{\$} in two independent volumes (COSMOS and UDS), greatly reducing the effects of cosmic variance. The emitters were selected by a combination of photometric redshift and color-color selections, as well as spectroscopic follow-up, including recent spectroscopic observations using DEIMOS and MOSFIRE on the Keck Telescopes and FMOS on Subaru. We find a strong increase in {\$}L{\_}\backslashstar{\$} and a decrease in {\$}\backslashphi{\_}\backslashstar{\$} for both H{\$}\backslashbeta{\$}+[OIII] and [OII] emitters. We derive the [OII] star-formation history of the Universe since {\$}z\backslashsim5{\$} and find that the cosmic SFRD rises from {\$}z \backslashsim 5{\$} to {\$}\backslashsim 3{\$} and then drops towards {\$}z \backslashsim 0{\$}. We also find that our star-formation history is able to reproduce the evolution of the stellar mass density up to {\$}z\backslashsim 5{\$} based only on a single tracer of star-formation. When comparing the H{\$}\backslashbeta{\$}+[OIII] SFRDs to the [OII] and H{\$}\backslashalpha{\$} SFRD measurements in the literature, we find that there is a remarkable agreement, suggesting that the H{\$}\backslashbeta{\$}+[OIII] sample is dominated by star-forming galaxies at high-{\$}z{\$} rather than AGNs.},
