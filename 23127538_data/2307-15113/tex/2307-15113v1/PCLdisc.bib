@article{Beverage2023,
abstract = {We present elemental abundance patterns (C, N, Mg, Si, Ca, Ti, V, Cr, Fe, Co, and Ni) for a population of 135 massive quiescent galaxies at {\$}z\backslashsim0.7{\$} with ultra-deep rest-frame optical spectroscopy drawn from the LEGA-C survey. We derive average ages and elemental abundances in four bins of stellar velocity dispersion ({\$}\backslashsigma{\_}v{\$}) ranging from 150{\$}{\~{}}{\$}km{\$}\backslash,{\$}s{\$}{\^{}}{\{}-1{\}}{\$} to 250{\$}{\~{}}{\$}km{\$}\backslash,{\$}s{\$}{\^{}}{\{}-1{\}}{\$} using a full-spectrum hierarchical Bayesian model. The resulting elemental abundance measurements are precise to 0.05{\$}\backslash,{\$}dex. The majority of elements, as well as the total metallicity and stellar age, show a positive correlation with {\$}\backslashsigma{\_}v{\$}. Thus, the highest dispersion galaxies formed the earliest and are the most metal-rich. We find only mild or non-significant trends between [X/Fe] and {\$}\backslashsigma{\_}v{\$}, suggesting that the average star-formation timescale does not strongly depend on velocity dispersion. To first order, the abundance patterns of the {\$}z\backslashsim0.7{\$} quiescent galaxies are strikingly similar to those at {\$}z\backslashsim0{\$}. However, at the lowest velocity dispersions the {\$}z\backslashsim0.7{\$} galaxies have slightly enhanced N, Mg, Ti, and Ni abundance ratios and earlier formation redshifts than their {\$}z\backslashsim0{\$} counterparts. Thus, while the higher-mass quiescent galaxy population shows little evolution, the low-mass quiescent galaxies population has grown significantly over the past six billion years. Finally, the abundance patterns of both {\$}z\backslashsim0{\$} and {\$}z\backslashsim0.7{\$} quiescent galaxies differ considerably from theoretical prediction based on a chemical evolution model, indicating that our understanding of the enrichment histories of these galaxies is still very limited.},
archivePrefix = {arXiv},
arxivId = {2303.03412},
author = {Beverage, Aliza G. and Kriek, Mariska and Conroy, Charlie and Sandford, Nathan R. and Bezanson, Rachel and Franx, Marijn and van der Wel, Arjen and Weisz, Daniel R.},
eprint = {2303.03412},
file = {:Users/ben/Documents/Mendeley Desktop/2303.03412.pdf:pdf},
keywords = {abundances,early-type,evolution,galaxies,stellar content},
title = {{From Carbon to Cobalt: Chemical compositions and ages of {\$}z\backslashsim0.7{\$} quiescent galaxies}},
url = {http://arxiv.org/abs/2303.03412},
year = {2023}
}
