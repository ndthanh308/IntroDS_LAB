@article{Katz2018,
abstract = {The presence of spectroscopically confirmed Balmer breaks in galaxy spectral energy distributions (SEDs) at {\$}z{\textgreater}9{\$} provides one of the best probes of the assembly history of the first generations of stars in our Universe. Recent observations of the gravitationally lensed source, MACS 1149{\_}JD1 (JD1), indicate that significant amounts of star formation likely occurred at redshifts as high as {\$}z\backslashsimeq15{\$}. The inferred stellar mass, dust mass, and assembly history of JD1, or any other galaxy at these redshifts that exhibits a strong Balmer break, can provide a strong test of our best theoretical models from high-resolution cosmological simulations. In this work, we present the results from a cosmological radiation-hydrodynamics simulation of the region surrounding a massive Lyman-break galaxy. For two of our most massive systems, we show that dust preferentially resides in the vicinity of the young stars thereby increasing the strength of the measured Balmer break such that the simulated SEDs are consistent with the photometry of JD1 and two other {\$}z{\textgreater}9{\$} systems (GN-z10-3 and GN-z9-1) that have proposed Balmer breaks at high redshift. We find strong variations in the shape and luminosity of the SEDs of galaxies with nearly identical stellar and halo masses, indicating the importance of morphology, assembly history, and dust distribution in making inferences on the properties of individual galaxies at high redshifts. Our results stress the importance that dust may play in modulating the observable properties of galaxies, even at the extreme redshifts of {\$}z{\textgreater}9{\$}.},
archivePrefix = {arXiv},
arxivId = {1809.07210},
author = {Katz, Harley and Laporte, Nicolas and Ellis, Richard S. and Devriendt, Julien and Slyz, Adrianne},
eprint = {1809.07210},
file = {:Users/ben/Documents/Mendeley Desktop/1809.07210.pdf:pdf},
keywords = {dark ages,dust,extinction,first stars,formation,galaxies,high-redshift,radiative transfer,reionization,stellar content},
number = {September},
pages = {1--14},
title = {{Probing Cosmic Dawn: Modelling the Assembly History, SEDs, and Dust Content of Selected {\$}z\backslashsim9{\$} Galaxies}},
url = {http://arxiv.org/abs/1809.07210},
volume = {14},
year = {2018}
}
@article{Marsan2022,
abstract = {We present the census of massive (log(M{\$}{\_}{\{}*{\}}{\$}/M{\$}{\_}{\{}\backslashodot{\}}{\$}){\$}\backslashgeq 11{\$}) galaxies at {\$}3{\textless}z{\textless}6{\$} identified over the COSMOS/UltraVISTA Ultra-Deep field stripes: consisting of {\$}\backslashapprox100{\$} and {\$}\backslashapprox20{\$} high-confidence candidates at {\$}3{\textless}z{\textless}4{\$} and {\$}4{\textless}z{\textless}6{\$}, respectively. The {\$}3{\textless}z{\textless}4{\$} population is comprised of post-starburst, UV star-forming and dusty-star forming galaxies in roughly equal fractions, while UV-star-forming galaxies dominate at {\$}4{\textless}z{\textless}6{\$} . We account for various sources of biases in SED modelling, finding that the treatment of emission line contamination is essential for understanding the number densities and mass growth histories of massive galaxies at {\$}z{\textgreater}3{\$}. The significant increase in observed number densities at {\$}z\backslashsim4{\$} ({\$}{\textgreater}\backslashtimes{\$} 5 in {\$}\backslashlesssim600{\$} Myrs) implies that this is the epoch at which log(M{\$}{\_}{\{}*{\}}{\$}/M{\$}{\_}{\{}\backslashodot{\}}{\$}){\$}\backslashgeq 11{\$} galaxies emerge in significant numbers, with stellar ages ({\$}\backslashapprox500-900{\$} Myrs) indicating rapid formation epochs as early as {\$}z\backslashsim7{\$}. Leveraging ancillary multi-wavelength datasets, we perform panchromatic SED modelling to constrain the total star-formation activity of the sample. The star-formation activity of the sample is generally consistent with being on the star-formation main sequence at the considered redshifts, with {\$}\backslashapprox15-25\backslash{\%}{\$} of the population showing evidence of suppressed star-formation rates, indicating that quenching mechanisms are already at play by {\$}z\backslashsim4{\$}. We stack available HST imaging, confirming their compact nature ({\$}r{\_}{\{}e{\}}\backslashlesssim2.2{\$} kpc), consistent with expected sizes of high-{\$}z{\$} star-forming galaxies. Finally, we discuss how our results are in-line with the early formation epochs and short formation timescales inferred from the fossil records of the most massive galaxies in the Universe.},
archivePrefix = {arXiv},
arxivId = {2010.04725},
author = {Marsan, Z. Cemile and Muzzin, Adam and Marchesini, Danilo and Stefanon, Mauro and Martis, Nicholas and Annunziatella, Marianna and Chan, Jeffrey C. C. and Cooper, Michael C. and Forrest, Ben and Gomez, Percy and McConachie, Ian and Wilson, Gillian},
doi = {10.3847/1538-4357/ac312a},
eprint = {2010.04725},
file = {:Users/ben/Documents/Mendeley Desktop/Marsan et al. - 2022 - The Number Densities and Stellar Populations of Massive Galaxies at 3 z 6 A Diverse, Rapidly Forming Population.pdf:pdf},
issn = {15384357},
journal = {The Astrophysical Journal},
keywords = {Galaxy evolution},
number = {1},
pages = {25},
publisher = {IOP Publishing},
title = {{The Number Densities and Stellar Populations of Massive Galaxies at 3 {\textless} z {\textless} 6: A Diverse, Rapidly Forming Population in the Early Universe}},
url = {http://arxiv.org/abs/2010.04725},
volume = {924},
year = {2022}
}
@article{Khostovan2016,
abstract = {We investigate the properties of {\$}\backslashsim7000{\$} narrow-band selected galaxies with strong H{\$}\backslashbeta+{\$}[OIII] and [OII] nebular emission lines from the High-{\$}z{\$} Emission Line Survey (HiZELS) between {\$}z \backslashsim 0.8 - 5.0{\$}. Our sample covers a wide range in stellar mass ({\$}M\backslashsim10{\^{}}{\{}7.5 - 12.0{\}}{\$} M{\$}{\_}\backslashodot{\$}), rest-frame equivalent widths (EW{\$}{\_}\backslashmathrm{\{}rest{\}}\backslashsim 10 - 10{\^{}}5{\$} $\backslash$AA), and line luminosities ({\$}L\backslashsim10{\^{}}{\{}40.5 - 43.2{\}}{\$} erg s{\$}{\^{}}{\{}-1{\}}{\$}). We measure the H{\$}\backslashbeta+{\$}[OIII] and [OII]-selected stellar mass functions out to {\$}z \backslashsim 3.5{\$} and find that both {\$}M{\_}\backslashstar{\$} and {\$}\backslashphi{\_}\backslashstar{\$} increases with cosmic time, which may be due to the [OIII] selection including an increasing fraction of AGN at lower redshifts. The [OII]-selected stellar mass functions show a constant {\$}M{\_}\backslashstar\backslashsim10{\^{}}{\{}11.6{\}}{\$} M{\$}{\_}\backslashodot{\$} and a strong, increasing evolution with cosmic time in {\$}\backslashphi{\_}\backslashstar{\$} in line with H{\$}\backslashalpha{\$} studies. We also investigate the EW{\$}{\_}\backslashmathrm{\{}rest{\}}{\$} evolution as a function of redshift with a fixed mass range (10{\$}{\^{}}{\{}9.5 - 10.0{\}}{\$} M{\$}{\_}\backslashodot{\$}) and find an increasing trend best represented by {\$}(1+z){\^{}}{\{}3.81\backslashpm0.14{\}}{\$} and {\$}(1+z){\^{}}{\{}2.72\backslashpm0.19{\}}{\$} up to {\$}z\backslashsim 2{\$} and {\$}\backslashsim 3{\$} for H{\$}\backslashbeta{\$}+[OIII] and [OII] emitters, respectively. This is the first time that the EW evolution has been directly measured for H{\$}\backslashbeta+{\$}[OIII] and [OII] emitters up to these redshifts. There is evidence for a slower evolution for {\$}z {\textgreater} 2{\$} in the H{\$}\backslashbeta+{\$}[OIII] EW and a decreasing trend for {\$}z {\textgreater} 3{\$} in the [OII] EW evolution, which would imply low [OII] EW at the highest redshifts and higher [OIII]/[OII] line ratios. This suggests that the ionization parameter at higher redshift may be significantly higher than the local Universe. Our results set the stage for future near-IR space-based spectroscopic surveys to test our extrapolated predictions and also produce {\$}z {\textgreater} 5{\$} measurements to constrain the high-{\$}z{\$} end of the EW{\$}{\_}\backslashmathrm{\{}rest{\}}{\$} and [OIII]/[OII] evolution.},
archivePrefix = {arXiv},
arxivId = {1604.02456},
author = {Khostovan, Ali Ahmad and Sobral, David and Mobasher, Bahram and Smail, Ian and Darvish, Behnam and Nayyeri, Hooshang and Hemmati, Shoubaneh and Stott, John P.},
doi = {10.1093/mnras/stw2174},
eprint = {1604.02456},
file = {:Users/ben/Documents/Mendeley Desktop/Khostovan et al. - 2016 - The nature of H$\beta$O iii and O ii emitters to z ∼ 5 with HiZELS stellar mass functions and the evolution of EW.pdf:pdf},
issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {cosmology,evolution,fundamental parameters,galaxies,high-,mass function,observations,redshift,star formation},
month = {dec},
number = {3},
pages = {2363--2382},
title = {{The nature of H$\beta$+[O iii] and [O ii] emitters to z ∼ 5 with HiZELS: stellar mass functions and the evolution of EWs}},
url = {http://arxiv.org/abs/1604.02456 https://academic.oup.com/mnras/article-lookup/doi/10.1093/mnras/stw2174},
volume = {463},
year = {2016}
}
@article{Jackson2020,
abstract = {The stellar mass assembly of galaxies can be affected by both secular and environmental processes. In this study, for the first time, we investigate the stellar mass assembly of {\$}\backslashsim90,000{\$} low redshift, central galaxies selected from SDSS group catalogues (M{\$}{\_}{\{}\backslashrm Stellar{\}}\backslashgtrsim10{\^{}}{\{}9.5{\}}{\$}M{\$}{\_}{\{}\backslashodot{\}}{\$}, M{\$}{\_}{\{}\backslashrm Halo{\}}\backslashgtrsim10{\^{}}{\{}12{\}}{\$}M{\$}{\_}{\{}\backslashodot{\}}{\$}) as a function of both stellar and halo mass. We use estimates of the times at which 10, 50 and 90 per cent of the stellar mass was assembled from photometric spectral energy distribution fitting, allowing a more complete investigation than single stellar ages alone. We consider trends in both stellar and halo mass simultaneously, finding dependencies of all assembly times on both. We find that galaxies with higher stellar masses (at constant halo mass) have on average older lookback times, similar to previous studies of galaxy assembly. We also find that galaxies at higher halo mass (at constant stellar mass) have younger lookback times, possibly due to a larger reservoir of gas for star formation. An exception to this is a sub sample with high stellar-to-halo mass ratios, which are likely massive, field spirals. We compare these observed trends to those predicted by the TNG300 simulation, finding good agreement overall as a function of either stellar or halo mass. However, some differences in the assembly times (of up to {\$}\backslashsim 3{\$} Gyr) appear when considering both stellar and halo mass simultaneously, noticeably at intermediate stellar masses (M{\$}{\_}{\{}\backslashrm Stellar{\}} \backslashsim 10{\^{}}{\{}11{\}}{\$} M{\$}{\_}{\{}\backslashodot{\}}{\$}). These discrepancies are possibly linked to the quenched fraction of galaxies and the kinetic mode AGN feedback implemented in TNG300.},
archivePrefix = {arXiv},
arxivId = {2008.05114},
author = {Jackson, Thomas M. and Pasquali, A. and Pacifici, C. and Engler, C. and Pillepich, A. and Grebel, E. K.},
doi = {10.1093/mnras/staa2306},
eprint = {2008.05114},
file = {:Users/ben/Documents/Mendeley Desktop/2008.05114.pdf:pdf},
keywords = {evolution,formation,galaxies,general,groups},
number = {August},
pages = {1--16},
title = {{The Stellar Mass Assembly of Low Redshift, Massive, Central Galaxies in SDSS and the TNG300 simulation}},
url = {http://arxiv.org/abs/2008.05114{\%}0Ahttp://dx.doi.org/10.1093/mnras/staa2306},
volume = {16},
year = {2020}
}
@article{Marchesini2022,
abstract = {How passive galaxies form, and the physical mechanisms which prevent star formation over long timescales, are some of the most outstanding questions in understanding galaxy evolution. The properties of quiescent galaxies over cosmic time provide crucial information to identify the quenching mechanisms. Passive galaxies have been confirmed and studied out to {\$}z\backslashsim4{\$}, but all of these studies have been limited to massive systems (mostly with {\$}\backslashlog{\{}(M{\_}{\{}\backslashrm star{\}}/M{\_}{\{}\backslashodot{\}}){\}}{\textgreater}10.8{\$}). Using James Webb Space Telescope (JWST) NIRISS grism slitless spectroscopic data from the GLASS JWST ERS program, we present spectroscopic confirmation of two quiescent galaxies at {\$}z{\_}{\{}\backslashrm spec{\}}=2.650{\^{}}{\{}+0.004{\}}{\_}{\{}-0.006{\}}{\$} and {\$}z{\_}{\{}\backslashrm spec{\}}=2.433{\^{}}{\{}+0.032{\}}{\_}{\{}-0.016{\}}{\$} (3{\$}\backslashsigma{\$} errors) with stellar masses of {\$}\backslashlog{\{}(M{\_}{\{}\backslashrm star{\}}/M{\_}{\{}\backslashodot{\}}){\}}=10.53{\^{}}{\{}+0.18{\}}{\_}{\{}-0.06{\}}{\$} and {\$}\backslashlog{\{}(M{\_}{\{}\backslashrm star{\}}/M{\_}{\{}\backslashodot{\}}){\}}=9.93{\^{}}{\{}+0.06{\}}{\_}{\{}-0.07{\}}{\$} (corrected for magnification factors of {\$}\backslashmu=2.0{\$} and {\$}\backslashmu=2.1{\$}, respectively). The latter represents the first spectroscopic confirmation of the existence of low-mass quiescent galaxies at cosmic noon, showcasing the power of JWST to identify and characterize this enigmatic population.},
archivePrefix = {arXiv},
arxivId = {2207.13625},
author = {Marchesini, Danilo and Brammer, Gabriel and Morishita, Takahiro and Bergamini, Pietro and Wang, Xin and Bradac, Marusa and Roberts-Borsani, Guido and Strait, Victoria and Treu, Tommaso and Fontana, Adriano and Jones, Tucker and Santini, Paola and Vulcani, Benedetta and Acebron, Ana and Calabr{\`{o}}, Antonello and Castellano, Marco and Glazebrook, Karl and Grillo, Claudio and Mercurio, Amata and Nanayakkara, Themiya and Rosati, Piero and Tubthong, Chanita and Vanzella, Eros},
eprint = {2207.13625},
file = {:Users/ben/Documents/Mendeley Desktop/2207.13625.pdf:pdf},
keywords = {evolution,galaxies,high-redshift,quenched galaxies},
title = {{Early results from GLASS-JWST. IX: First spectroscopic confirmation of low-mass quiescent galaxies at {\$}z{\textgreater}2{\$} with NIRISS}},
url = {http://arxiv.org/abs/2207.13625},
year = {2022}
}
