issn = {1450-698X},
journal = {Serbian Astronomical Journal},
keywords = {evolution,galaxies,ultraviolet},
number = {189},
pages = {1--14},
title = {{Green valley galaxies}},
url = {http://arxiv.org/abs/1501.01963{\%}0Ahttp://dx.doi.org/10.2298/SAJ1489001S http://www.doiserbia.nb.rs/Article.aspx?ID=1450-698X1489001S},
volume = {189},
year = {2014}
}
