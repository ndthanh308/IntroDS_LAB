@article{Finnerty2020,
abstract = {We present rest-frame optical spectroscopic observations of 24 Hot Dust-Obscured Galaxies (Hot DOGs) at redshifts 1.7-4.6 with KECK/NIRES. Our targets are selected based on their extreme red colors to be the highest luminosity sources from the WISE infrared survey. In 20 sources with well-detected emission we fit the key [O III], H{\$}\backslashbeta{\$}, H{\$}\backslashalpha{\$}, [N II], and [S II] diagnostic lines to constrain physical conditions. Of the 17 targets with a clear detection of the [O III]{\$}\backslashrm \backslashlambda{\$}5007A emission line, 15 display broad blueshifted and asymmetric line profiles, with widths ranging from 1000 to 8000 {\$}\backslashrm km\backslash s{\^{}}{\{}-1{\}}{\$} and blueshifts up to 3000 {\$}\backslashrm km\backslash s{\^{}}{\{}-1{\}}{\$}. These kinematics provide strong evidence for the presence of massive ionized outflows of up to {\$}8000\backslash \backslashrm M{\_}\backslashodot\backslash yr{\^{}}{\{}-1{\}}{\$}, with a median of {\$}150\backslash \backslashrm M{\_}\backslashodot\backslash yr{\^{}}{\{}-1{\}}{\$}. As many as eight sources show optical emission line ratios consistent with vigorous star formation. Balmer line star-formation rates, uncorrected for reddening, range from 30--1300 {\$}\backslashrm M{\_}\backslashodot\backslash yr{\^{}}{\{}-1{\}}{\$}, with a median of {\$}50\backslash \backslashrm M{\_}\backslashodot\backslash yr{\^{}}{\{}-1{\}}{\$}. Estimates of the SFR from SED fitting of mid and far-infrared photometry suggest significantly higher values. We estimate the central black hole masses to be of order {\$}10{\^{}}{\{}8-10{\}}\backslashrm\backslash M{\_}\backslashodot{\$}, assuming the present-day {\$}\backslashrm M{\_}{\{}BH{\}}-\backslashsigma{\_}*{\$} relation. The bolometric luminosities and the estimated masses of the central black holes of these galaxies suggest that many of the AGN-dominated Hot DOGs are accreting at or above their Eddington limit. The combination of ongoing star formation, massive outflows, and high Eddington ratios suggest Hot DOGs are a transitional phase in galaxy evolution.},
author = {Finnerty, Luke and Larson, Kirsten and Soifer, B. T. and Armus, Lee and Matthews, Keith and Jun, Hyunsung D. and Moon, Dae-Sik and Melbourne, Jason and Gomez, Percy and Tsai, Chao-wei and D{\'{i}}az-Santos, Tanio and Eisenhardt, Peter and Cushing, Michael},
doi = {10.3847/1538-4357/abc3bf},
file = {:Users/ben/Documents/Mendeley Desktop/Finnerty{\_}2020{\_}ApJ{\_}905{\_}16.pdf:pdf},
issn = {1538-4357},
journal = {The Astrophysical Journal},
keywords = {Active ga,High-redshift galaxies,Infrared galaxies},
number = {1},
