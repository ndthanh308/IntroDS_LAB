@article{Shen2022,
abstract = {25 We present the radio properties of 66 spectroscopically-confirmed normal star-forming galaxies 26 (SFGs) at 4.4 {\textless} z {\textless} 5.9 in the COSMOS field that were [C ii] detected in the Atacama Large 27 Millimeter Array (ALMA) Large Program to INvestigate [C ii] at Early times (ALPINE). We separate 28 these galaxies (CII-detected-all) into lower redshift (CII-detected-lz, z = 4.5) and higher redshift 29 (CII-detected-hz, z = 5.6) sub-samples and stack multi-wavelength imaging for each sub-sample 30 from X-ray to radio bands. A radio signal is detected in the stacked 3 GHz image of CII-detected-all 31 and-lz samples at 3$\sigma$. We find that the infrared-radio correlation of our sample, quantified by 32 the infrared-to-1.4 GHz radio luminosity ratio (q TIR) is lower than the local q TIR relation for normal 33 SFGs at 2$\sigma$ significant level, and is instead broadly consistent with that of bright sub-mm galaxies 34 (SMGs) at 2 {\textless} z {\textless} 5. We exclude active galactic nuclei (AGN) component as the main contributor 35 to the observed deviation, since none of these sub-samples have an appreciable AGN component on 36 average measured by the cigale spectral energy distribution (SED) fitting on the stacked SED, as 37 expected by the construction of ALPINE sample. Although we cannot rule out the possible effect of 38 the assumed spectral index and the applied infrared (IR) SED templates, the lower obscured fraction 39 of star formation than at lower redshift can alleviate the tension between our stacked q TIR s and that 40 of local normal SFGs. It is possible that the dust buildup, which primarily governs the IR emission in 41 addition to older stellar populations, has not had enough time to occur fully in these galaxies, whereas 42 the radio emission can respond on a more rapid scale. Therefore, we might expect a lower q TIR value 43 to be a general property of high-redshift SFGs. 2 Shen et al.},
archivePrefix = {arXiv},
arxivId = {arXiv:2207.07137v1},
author = {Shen, Lu and Lemaux, Brian C and Lubin, Lori M and Liu, Guilin and B{\'{e}}thermin, Matthieu and Boquien, M{\'{e}}d{\'{e}}ric and Cucciati, Olga and Talia, Margherita and Zamorani, Gianni and Faisst, Andreas L and Romano, Michael and Ginolfi, Michele and Jones, Gareth C and Vergani, Daniela and Bardelli, Sandro and Zucca, Elena and Hathi, Nimish and Koekemoer, Anton M and {Le F`}, Olivier and Fang, Wenjuan and Forrest, Ben and Shah, Ektaben and Staab, Priti and Gal, Roy and Vanderhoof, Brittany},
eprint = {arXiv:2207.07137v1},
