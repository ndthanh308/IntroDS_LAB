eprint = {1809.03514},
file = {:Users/ben/Documents/Mendeley Desktop/Noble et al. - 2018 - Resolving CO (2-1) in z{\~{}}1.6 Gas-Rich Cluster Galaxies with ALMA Rotating Molecular Gas Disks with Possible Signatu.pdf:pdf},
keywords = {a,clusters,corresponding author,evolution,g,galaxies,general,high-redshift,ism,kinematics and dynamics,noble,radio lines},
