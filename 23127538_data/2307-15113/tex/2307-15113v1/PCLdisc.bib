arxivId = {1610.03877},
author = {Fensch, J. and Renaud, F. and Bournaud, F. and Duc, P. A. and Agertz, O. and Amram, P. and Combes, F. and {Di Matteo}, P. and Elmegreen, B. and Emsellem, E. and Jog, C. J. and Perret, V. and Struck, C. and Teyssier, R.},
doi = {10.1093/mnras/stw2920},
