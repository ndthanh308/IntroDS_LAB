eprint = {1909.13505},
file = {:Users/ben/Documents/Mendeley Desktop/1909.13505.pdf:pdf},
keywords = {dust,evolution,extinction,galaxies,ism,star formation},
pages = {1--9},
title = {{Understanding the Discrepancy between IRX and Balmer Decrement in Tracing Galaxy Dust Attenuation}},
