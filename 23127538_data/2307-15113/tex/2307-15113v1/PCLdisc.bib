@article{Whitaker2015a,
abstract = {It is well established that (1) star-forming galaxies follow a relation between their star formation rate (SFR) and stellar mass (M{\$}{\_}{\{}\backslashstar{\}}{\$}), the "star-formation sequence", and (2) the SFRs of galaxies correlate with their structure, where star-forming galaxies are less concentrated than quiescent galaxies at fixed mass. Here, we consider whether the scatter and slope of the star-formation sequence is correlated with systematic variations in the Sersic indices, {\$}n{\$}, of galaxies across the SFR-M{\$}{\_}{\{}\backslashstar{\}}{\$} plane. We use a mass-complete sample of 23,848 galaxies at {\$}0.5{\textless}z{\textless}2.5{\$} selected from the 3D-HST photometric catalogs. Galaxy light profiles parameterized by {\$}n{\$} are based on Hubble Space Telescope CANDELS near-infrared imaging. We use a single SFR indicator empirically-calibrated from stacks of Spitzer/MIPS 24{\$}\backslashmu{\$}m imaging, adding the unobscured and obscured star formation. We find that the scatter of the star-formation sequence is related in part to galaxy structure; the scatter due to variations in {\$}n{\$} at fixed mass for star-forming galaxies ranges from 0.14{\$}\backslashpm{\$}0.02 dex at {\$}z\backslashsim2{\$} to 0.30{\$}\backslashpm{\$}0.04 dex at {\$}z{\textless}1{\$}. While the slope of the log(SFR)-log(M{\$}{\_}{\{}\backslashstar{\}}{\$}) relation is of order unity for disk-like galaxies, galaxies with {\$}n{\textgreater}2{\$} (implying more dominant bulges) have significantly lower SFR/M{\$}{\_}{\{}\backslashstar{\}}{\$} than the main ridgeline of the star-formation sequence. These results suggest that bulges in massive {\$}z\backslashsim2{\$} galaxies are actively building up, where the stars in the central concentration are relatively young. At {\$}z{\textless}1{\$}, the presence of older bulges within star-forming galaxies lowers global SFR/M{\$}{\_}{\{}\backslashstar{\}}{\$}, decreasing the slope and contributing significantly to the scatter of the star-formation sequence.},
archivePrefix = {arXiv},
arxivId = {1508.04771},
author = {Whitaker, Katherine E. and Franx, Marijn and Bezanson, Rachel and Brammer, Gabriel B. and van Dokkum, Pieter G. and Kriek, Mariska T and Labbe, Ivo and Leja, Joel and Momcheva, Ivelina G and Nelson, Erica J and Rigby, Jane R. and Rix, Hans-Walter and Skelton, Rosalind E. and van der Wel, Arjen and Wuyts, Stijn},
doi = {10.1088/2041-8205/811/1/L12},
eprint = {1508.04771},
file = {:Users/ben/Documents/Mendeley Desktop/Whitaker et al. - 2015 - Galaxy Structure as a Driver of the Star Formation Sequence Slope and Scatter.pdf:pdf},
month = {aug},
