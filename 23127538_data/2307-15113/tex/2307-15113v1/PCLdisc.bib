keywords = {1 i n t,comparison between photo-,evolution,extragalactic studies usually require,fundamental parameters,galaxies,photometry,ro d u c,stellar content,t i o n},
month = {feb},
pages = {13},
title = {{Analytical approximations of K-corrections in optical and near-infrared bands}},
