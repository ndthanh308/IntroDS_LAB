title = {{The efficiency of ionizing photon production and the radiation energy balance in compact star-forming galaxies}},
url = {http://arxiv.org/abs/1702.02420{\%}0Ahttp://dx.doi.org/10.1093/mnras/stx347 http://academic.oup.com/mnras/article/467/4/4118/2980620/The-efficiency-of-ionizing-photon-production-and},
