@article{Barrufet2022,
abstract = {Over the last few years, both ALMA and Spitzer/IRAC observations have revealed a population of likely massive galaxies at {\$}z{\textgreater}3{\$} that was too faint to be detected in HST rest-frame ultraviolet imaging. However, due to the very limited photometry for individual galaxies, the true nature of these so-called HST-dark galaxies has remained elusive. Here, we present the first sample of such galaxies observed with very deep, high-resolution NIRCam imaging from the Early Release Science Program CEERS. 33 HST-dark sources are selected based on their red colours across 1.6 {\$}\backslashmu{\$}m to 4.4 {\$}\backslashmu{\$}m. Their physical properties are derived from 12-band multi-wavelength photometry, including ancillary HST imaging. We find that these galaxies are generally heavily dust-obscured ({\$}A{\_}{\{}V{\}}\backslashsim2{\$} mag), massive ({\$}\backslashlog (M/M{\_}{\{}\backslashodot{\}}) \backslashsim10{\$}), star-forming sources at {\$}z\backslashsim2-8{\$} with an observed surface density of {\$}\backslashsim0.8{\$} arcmin{\$}{\^{}}{\{}-2{\}}{\$}. This suggests that an important fraction of massive galaxies may have been missing from our cosmic census at {\$}z{\textgreater}3{\$} all the way into the reionization epoch. The HST-dark sources lie on the main-sequence of galaxies and add an obscured star formation rate density (SFRD) of {\$}\backslashmathrm{\{}1.3{\^{}}{\{}+1.6{\}}{\_}{\{}-1.0{\}} \backslashtimes 10{\^{}}{\{}-3{\}} M{\_}{\{}\backslashodot{\}}/yr/Mpc{\^{}}{\{}3{\}}{\}}{\$} at {\$}z\backslashsim6{\$}, similar to previous estimates. Our analysis shows the unique power of JWST to reveal this previously missing galaxy population and to provide a complete census of galaxies at {\$}z=2-8{\$} based on rest-frame optical imaging.},
archivePrefix = {arXiv},
arxivId = {2207.14733},
author = {Barrufet, L. and Oesch, P. A. and Weibel, A. and Brammer, G. and Bezanson, R. and Bouwens, R. and Fudamoto, Y. and Gonzalez, V. and Illingworth, G. and Heintz, K. E. and Holden, B. and Labbe, I. and Magee, D. and Naidu, R. P. and Nelson, E. and Stefanon, M. and Smit, R. and van Dokkum, P. and Weaver, J. and Williams, C.},
