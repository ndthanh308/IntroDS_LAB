file = {:Users/ben/Documents/Mendeley Desktop/2102.07775.pdf:pdf},
title = {{New Determinations of the UV Luminosity Functions from z{\~{}}9 to z{\~{}}2 show a remarkable consistency with halo growth and a constant star formation efficiency}},
url = {http://arxiv.org/abs/2102.07775},
year = {2021}
}
@article{Finlator2016,
abstract = {Recent analysis of strongly-lensed sources in the Hubble Frontier Fields indicates that the rest-frame UV luminosity function of galaxies at {\$}z={\$}6--8 rises as a power law down to {\$}M{\_}\backslashmathrm{\{}UV{\}}=-15{\$}, and possibly as faint as -12.5. We use predictions from a cosmological radiation hydrodynamic simulation to map these luminosities onto physical space, constraining the minimum dark matter halo mass and stellar mass that the Frontier Fields probe. While previously-published theoretical studies have suggested or assumed that early star formation was suppressed in halos less massive than {\$}10{\^{}}9{\$}--{\$}10{\^{}}{\{}11{\}} M{\_}\backslashodot{\$}, we find that recent observations demand vigorous star formation in halos at least as massive as (3.1, 5.6, 10.5){\$}\backslashtimes10{\^{}}9 M{\_}\backslashodot{\$} at {\$}z=(6,7,8){\$}. Likewise, we find that Frontier Fields observations probe down to stellar masses of (8.1, 18, 32){\$}\backslashtimes10{\^{}}6 M{\_}\backslashodot{\$}; that is, they are observing the likely progenitors of analogues to Local Group dwarfs such as Pegasus and M32. Our simulations yield somewhat different constraints than two complementary models that have been invoked in similar analyses, emphasizing the need for further observational constraints on the galaxy-halo connection.},
