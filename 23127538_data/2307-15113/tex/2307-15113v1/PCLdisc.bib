@article{Hill2017b,
abstract = {Using observed stellar mass functions out to {\$}z=5{\$}, we measure the main progenitor stellar mass growth of descendant galaxies with masses of {\$}\backslashlog{\{}M{\_}{\{}*{\}}/M{\_}{\{}\backslashodot{\}}{\}}=11.5,11.0,10.5,10.0{\$} at {\$}z\backslashsim0.1{\$} using an evolving cumulative number density selection. From these mass growth histories, we are able to measure the time at which half the total stellar mass of the descendant galaxy was assembled, {\$}t{\_}{\{}a{\}}{\$}, which, in order of decreasing mass corresponds to redshifts of {\$}z{\_}{\{}a{\}}=1.28, 0.92, 0.60{\$} and {\$}0.51{\$}. We compare this to the median light-weighted stellar age {\$}t{\_}{\{}*{\}}{\$} ({\$}z{\_}{\{}*{\}} = 2.08, 1.49, 0.82{\$} and {\$}0.37{\$}) of a sample of low redshift SDSS galaxies (from the literature) and find the timescales are consistent with more massive galaxies forming a higher fraction of their stars ex-situ compared to lower mass descendants. We find that both {\$}t{\_}{\{}*{\}}{\$} and {\$}t{\_}{\{}a{\}}{\$} strongly correlate with mass which is in contrast to what is found in the EAGLE hydrodynamical simulation which shows a flat relationship between {\$}t{\_}{\{}a{\}}{\$} and {\$}M{\_}{\{}*{\}}{\$}. However, the semi-analytic model of $\backslash$citet{\{}henriques2015{\}} is consistent with the observations in both {\$}t{\_}{\{}a{\}}{\$} and {\$}t{\_}{\{}*{\}}{\$} with {\$}M{\_}{\{}*{\}}{\$}, showing the most recent semi-analytic models are better able to decouple the evolution of the baryons from the dark matter in lower-mass galaxies.},
author = {Hill, Allison R. and Muzzin, Adam and Franx, Marijn and Marchesini, Danilo},
doi = {10.3847/2041-8213/aa951a},
file = {:Users/ben/Documents/Mendeley Desktop/Hill et al. - 2017 - The Mass Growth and Stellar Ages of Galaxies Observations versus Simulations.pdf:pdf},
issn = {2041-8213},
journal = {The Astrophysical Journal},
keywords = {evolution,formation,galaxies,galaxies: evolution,galaxies: formation},
number = {2},
pages = {L26},
publisher = {IOP Publishing},
title = {{The Mass Growth and Stellar Ages of Galaxies: Observations versus Simulations}},
url = {http://dx.doi.org/10.3847/2041-8213/aa951a},
volume = {849},
year = {2017}
}
@article{Nelson2015a,
abstract = {We present Ha maps at 1kpc spatial resolution for star-forming galaxies at z{\~{}}1, made possible by the WFC3 grism on HST. Employing this capability over all five 3D-HST/CANDELS fields provides a sample of 2676 galaxies. By creating deep stacked Halpha (Ha) images, we reach surface brightness limits of 1x10{\^{}}-18$\backslash$erg$\backslash$s$\backslash$cm{\^{}}2$\backslash$arcsec{\^{}}2, allowing us to map the distribution of ionized gas out to {\textgreater}10kpc for typical L* galaxies at this epoch. We find that the spatial extent of the Ha distribution increases with stellar mass as r(Ha)[kpc]=1.5(Mstars/10{\^{}}10Msun){\^{}}0.23. Furthermore, the Ha emission is more extended than the stellar continuum emission, consistent with inside-out assembly of galactic disks. This effect, however, is mass dependent with r(Ha)/r(stars)=1.1(M/10{\^{}}10Msun){\^{}}0.054, such that at low masses r(Ha){\~{}}r(stars). We map the Ha distribution as a function of SFR(IR+UV) and find evidence for `coherent star formation' across the SFR-M plane: above the main sequence, Ha is enhanced at all radii; below the main sequence, Ha is depressed at all radii. This suggests that at all masses the physical processes driving the enhancement or suppression of star formation act throughout the disks of galaxies. It also confirms that the scatter in the star forming main sequence is real and caused by variations in the star formation rate at fixed mass. At high masses (10{\^{}}10.5{\textless}M/Msun{\textless}10{\^{}}11), above the main sequence, Ha is particularly enhanced in the center, plausibly building bulges and/or supermassive black holes. Below the main sequence, the star forming disks are more compact and a strong central dip in the EW(Ha), and the inferred specific star formation rate, appears. Importantly though, across the entirety of the SFR-M plane, the absolute star formation rate as traced by Ha is always centrally peaked, even in galaxies below the main sequence.},
archivePrefix = {arXiv},
arxivId = {1507.03999},
author = {Nelson, Erica June and van Dokkum, Pieter G. and Schreiber, Natascha M. F{\"{o}}rster and Franx, Marijn and Brammer, Gabriel B. and Momcheva, Ivelina G. and Wuyts, Stijn and Whitaker, Katherine E. and Skelton, Rosalind E. and Fumagalli, Mattia and Kriek, Mariska and Labb{\'{e}}, Ivo and Leja, Joel and Rix, Hans-Walter and Tacconi, Linda J. and van der Wel, Arjen and van den Bosch, Frank C. and Oesch, Pascal A. and Dickey, Claire and Lange, Johannes Ulf},
doi = {10.3847/0004-637X/828/1/27},
eprint = {1507.03999},
file = {:Users/ben/Documents/Mendeley Desktop//Der - 2015 - . WHERE STARS FORM INSIDE-OUT GROWTH AND COHERENT STAR FORMATION FROM HST H $\alpha$ MAPS OF 2676 GALAXIES ACROSS THE MAIN SEQU.pdf:pdf},
month = {jul},
title = {{Where stars form: inside-out growth and coherent star formation from HST Halpha maps of 2676 galaxies across the main sequence at z{\~{}}1}},
url = {http://arxiv.org/abs/1507.03999 http://dx.doi.org/10.3847/0004-637X/828/1/27},
year = {2015}
}
@article{Wilkinson2017,
abstract = {There are multiple ways in which to select post-starburst galaxies in the literature. In this work, we present a study into how two well-used selection techniques have consequences on observable post-starburst galaxy parameters, such as colour, morphology and environment and how this affects interpretations of their role in the galaxy duty cycle. We identify a master sample of H{\$}\backslashdelta{\$} strong (EW{\$}{\_}{\{}H\backslashdelta{\}}{\$} {\textgreater} 3$\backslash$AA) post-starburst galaxies from the value-added catalogue in the 7th data release of the Sloan Digital Sky Survey (SDSS DR7) over a redshift range 0.01 {\textless} {\$}z{\$} {\textless} 0.1. From this sample we select two E+A subsets, both having a very little [OII] emission (EW{\$}{\_}{\{}[OII]{\}}{\$} {\$}{\textgreater} -2.5{\$}$\backslash$AA) but one having an additional cut on EW{\$}{\_}{\{}H\backslashalpha{\}}{\$} ({\$}{\textgreater} -3{\$}$\backslash$AA). We examine the differences in observables and AGN fractions to see what effect the H{\$}\backslashalpha{\$} cut has on the properties of post-starburst galaxies and what these differing samples can tell us about the duty cycle of post-starburst galaxies. We find that H{\$}\backslashdelta{\$} strong galaxies peak in the `blue cloud', E+As in the `green valley' and pure E+As in the `red sequence'. We also find that pure E+As have a more early-type morphology and a higher fraction in denser environments compared with the H{\$}\backslashdelta{\$} strong and E+A galaxies. These results suggest that there is an evolutionary sequence in the post-starburst phase from blue disky galaxies with residual star formation to passive red early-types.},
archivePrefix = {arXiv},
arxivId = {1708.03168},
author = {Wilkinson, C. L. and Pimbblet, K. A. and Stott, J. P.},
doi = {10.1093/mnras/stx2034},
eprint = {1708.03168},
file = {:Users/ben/Documents/Mendeley Desktop//Wilkinson, Pimbblet, Stott - 2017 - The evolutionary sequence of post-starburst galaxies(4).pdf:pdf},
issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {1 i n t,a galaxies,also known as,clusters,e,evolution,galaxies,general,interactions,post-starburst galaxies are a,rals and quiescent e,ro d u c,s0 galaxies,spi-,t i o n,vital link between star-forming},
month = {dec},
number = {2},
pages = {1447--1457},
title = {{The evolutionary sequence of post-starburst galaxies}},
url = {http://arxiv.org/abs/1708.03168{\%}0Ahttp://dx.doi.org/10.1093/mnras/stx2034 http://academic.oup.com/mnras/article/472/2/1447/4082091/The-evolutionary-sequence-of-poststarburst},
volume = {472},
year = {2017}
}
@article{Wright2018,
abstract = {We use the EAGLE simulations to study the connection between the quenching timescale, {\$}\backslashtau{\_}{\{}\backslashrm Q{\}}{\$}, and the physical mechanisms that transform star-forming galaxies into passive galaxies. By quantifying {\$}\backslashtau{\_}{\{}\backslashrm Q{\}}{\$} in two complementary ways - as the time over which (i) galaxies traverse the green valley on the colour-mass diagram, or (ii) leave the main sequence of star formation and subsequently arrive on the passive cloud in specific star formation rate (SSFR)-mass space - we find that the {\$}\backslashtau{\_}{\{}\backslashrm Q{\}}{\$} distribution of high-mass centrals, low-mass centrals and satellites are divergent. In the low stellar mass regime where {\$}M{\_}{\{}\backslashstar{\}}{\textless}10{\^{}}{\{}9.6{\}}M{\_}{\{}\backslashodot{\}}{\$}, centrals exhibit systematically longer quenching timescales than satellites ({\$}\backslashapprox 4{\$}Gyr compared to {\$}\backslashapprox 2{\$}Gyr). Satellites with low stellar mass relative to their halo mass cause this disparity, with ram pressure stripping quenching these galaxies rapidly. Low mass centrals are quenched as a result of stellar feedback, associated with long {\$}\backslashtau{\_}{\{}\backslashrm Q{\}}\backslashgtrsim 3{\$}Gyr. At intermediate stellar masses where {\$}10{\^{}}{\{}9.7{\}}\backslash,\backslashrm M{\_}{\{}\backslashodot{\}}{\textless}M{\_}{\{}\backslashstar{\}}{\textless}10{\^{}}{\{}10.3{\}}\backslash,\backslashrm M{\_}{\{}\backslashodot{\}}{\$}, {\$}\backslashtau{\_}{\{}\backslashrm Q{\}}{\$} are the longest for both centrals and satellites, particularly for galaxies living in groups with other gas-rich galaxies. At {\$}M{\_}{\{}\backslashstar{\}}\backslashgtrsim 10{\^{}}{\{}10.3{\}}\backslash,\backslashrm M{\_}{\{}\backslashodot{\}}{\$}, galaxy merger counts and black hole activity increase steeply for all galaxies. Quenching timescales for centrals and satellites decrease with stellar mass in this regime to {\$}\backslashtau{\_}{\{}\backslashrm Q{\}}\backslashlesssim2{\$}Gyr. In anticipation of new intermediate redshift observational galaxy surveys, we analyse the passive and star-forming fractions of galaxies across redshift, and find that the {\$}\backslashtau{\_}{\{}\backslashrm Q{\}}{\$} peak at intermediate stellar masses is responsible for a peak (inflection point) in the fraction of green valley central (satellite) galaxies at {\$}z\backslashapprox 0.5-0.7{\$}.},
annote = {Wright+2018

EAGLE simulations to analyze quenching timescale and causal mechanisms between high-mass centrals, low-mass centrals, and satellites.
At logMsol{\textless}9.6, centrals have longer quenching timescales than satellites (RPS quenches low mass satellites, while stellar feedback quenches centrals).
At 9.7 {\textless} logMsol {\textless} 10.3, quenching timescales are longest for centrals and satellites.
At logMsol {\textgreater} 10.3, mergers and black hole activity increase, resulting in a shorter quenching timescale.
These timescales result in a peak of f(green valley) central galaxies at z{\~{}}0.5.},
archivePrefix = {arXiv},
arxivId = {1810.07335},
author = {Wright, Ruby J. and Lagos, Claudia del P. and Davies, Luke J. M. and Power, Chris and Trayford, James W. and Wong, O. Ivy},
eprint = {1810.07335},
file = {:Users/ben/Documents/Mendeley Desktop/Wright et al. - 2018 - Quenching timescales of galaxies in the EAGLE simulations.pdf:pdf},
keywords = {evolution,formation - galaxies,galaxies},
number = {October},
pages = {1--16},
title = {{Quenching timescales of galaxies in the EAGLE simulations}},
url = {http://arxiv.org/abs/1810.07335},
volume = {16},
year = {2018}
}
