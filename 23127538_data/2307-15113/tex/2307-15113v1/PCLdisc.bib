title = {{Red and dead CANDELS: massive passive galaxies at the dawn of the Universe}},
url = {http://arxiv.org/abs/1909.07996 http://dx.doi.org/10.1093/mnras/stz2615 https://academic.oup.com/mnras/article/490/3/3309/5571825},
