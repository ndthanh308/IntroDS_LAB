file = {:Users/ben/Documents/Mendeley Desktop/Wilson et al. - 2009 - Spectroscopic confirmation of a massive red-sequence-selected galaxy cluster at Z = 1.34 in the SpARCS-south clus.pdf:pdf},
issn = {15384357},
