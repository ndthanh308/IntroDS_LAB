keywords = {1093,2016,2176,594,734,galaxies,galaxy evolution,high-redshift galaxies,near infrared astronomy,post-starburst,quenched galaxies},
title = {{The FENIKS Survey: Spectroscopic Confirmation of Massive Quiescent Galaxies at z {\~{}} 3-5}},
