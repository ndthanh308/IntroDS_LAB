@article{Wang2017,
abstract = {This paper uses radial colour profiles to infer the distributions of dust, gas and star formation in z=0.4-1.4 star-forming main sequence galaxies. We start with the standard UVJ-based method to estimate dust extinction and specific star formation rate (sSFR). By replacing J with I band, a new calibration method suitable for use with ACS+WFC3 data is created (i.e. UVI diagram). Using a multi-wavelength multi-aperture photometry catalogue based on CANDELS, UVI colour profiles of 1328 galaxies are stacked in stellar mass and redshift bins. The resulting colour gradients, covering a radial range of 0.2--2.0 effective radii, increase strongly with galaxy mass and with global {\$}A{\_}V{\$}. Colour gradient directions are nearly parallel to the Calzetti extinction vector, indicating that dust plays a more important role than stellar population variations. With our calibration, the resulting {\$}A{\_}V{\$} profiles fall much more slowly than stellar mass profiles over the measured radial range. sSFR gradients are nearly flat without central quenching signatures, except for {\$}M{\_}*{\textgreater}10{\^{}}{\{}10.5{\}} M{\_}{\{}\backslashodot{\}}{\$}, where central declines of 20--25 per cent are observed. Both sets of profiles agree well with previous radial sSFR and (continuum) {\$}A{\_}V{\$} measurements. They are also consistent with the sSFR profiles and, if assuming a radially constant gas-to-dust ratio, gas profiles in recent hydrodynamic models. We finally discuss the striking findings that SFR scales with stellar mass density in the inner parts of galaxies, and that dust content is high in the outer parts despite low stellar-mass surface densities there.},
archivePrefix = {arXiv},
arxivId = {1705.05404},
author = {Wang, Weichen and Faber, S M and Liu, F S and Guo, Yicheng and Pacifici, Camilla and Koo, David C and Kassin, Susan A and Mao, Shude and Fang, Jerome J and Chen, Zhu and Koekemoer, Anton M. and Kocevski, Dale D and Ashby, M L N},
doi = {10.1093/mnras/stx1148},
eprint = {1705.05404},
file = {:Users/ben/Documents/Mendeley Desktop//Wang et al. - 2017 - sequence galaxies in CANDELS dust extinction and star formation profiles.pdf:pdf},
issn = {0035-8711},
keywords = {dust,extinction,formation,galaxies,high-redshift,photometry,star formation},
number = {May},
pages = {1--22},
title = {{UVI colour gradients of 0.4{\textless}z{\textless}1.4 star-forming main sequence galaxies in CANDELS: dust extinction and star formation profiles}},
url = {http://arxiv.org/abs/1705.05404{\%}0Ahttp://dx.doi.org/10.1093/mnras/stx1148},
volume = {22},
year = {2017}
}
@article{Sanders2016,
abstract = {We present measurements of the electron-temperature based oxygen abundance for a highly star-forming galaxy at z=3.08, COSMOS-1908. This is the highest redshift at which [OIII]{\$}\backslashlambda{\$}4363 has been detected, and the first time that this line has been measured at z{\textgreater}2. We estimate an oxygen abundance of 12+log(O/H){\$}=8.00{\^{}}{\{}+0.13{\}}{\_}{\{}-0.14{\}}{\$}. This galaxy is a low-mass ({\$}10{\^{}}{\{}9.3{\}}{\$} M{\$}{\_}{\{}\backslashodot{\}}{\$}), highly star-forming ({\$}\backslashsim50{\$} M{\$}{\_}{\{}\backslashodot{\}}{\$} yr{\$}{\^{}}{\{}-1{\}}{\$}) system that hosts a young stellar population ({\$}\backslashsim160{\$} Myr). We investigate the physical conditions of the ionized gas in COSMOS-1908 and find that this galaxy has a high ionization parameter, little nebular reddening ({\$}E(B-V){\_}{\{}\backslashrm gas{\}}{\textless}0.14{\$}), and a high electron density ({\$}n{\_}e\backslashsim500{\$} cm{\$}{\^{}}{\{}-3{\}}{\$}). We compare the ratios of strong oxygen, neon, and hydrogen lines to the direct-method oxygen abundance for COSMOS-1908 and additional star-forming galaxies at z=0-1.8 with [OIII]{\$}\backslashlambda{\$}4363 measurements, and show that galaxies at z{\$}\backslashsim{\$}1-3 follow the same strong-line correlations as galaxies in the local universe. This agreement suggests that the relationship between ionization parameter and O/H is similar for z{\$}\backslashsim{\$}0 and high-redshift galaxies. These results imply that metallicity calibrations based on lines of oxygen, neon, and hydrogen do not strongly evolve with redshift and can reliably estimate abundances out to z{\$}\backslashsim{\$}3, paving the way for robust measurements of the evolution of the mass-metallicity relation to high redshift.},
archivePrefix = {arXiv},
arxivId = {1606.04107},
author = {Sanders, Ryan L. and Shapley, Alice E. and Kriek, Mariska and Reddy, Naveen A. and Freeman, William R. and Coil, Alison L. and Siana, Brian and Mobasher, Bahram and Shivaei, Irene and Price, Sedona H. and de Groot, Laura},
