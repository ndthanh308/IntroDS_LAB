keywords = {dwarf,fundamental parameters,galaxies,irreg-,ism,starburst,ular},
month = {nov},
number = {4},
pages = {4427--4434},
title = {{The bursting nature of star formation in compact star-forming galaxies from the Sloan Digital Sky Survey}},
url = {http://arxiv.org/abs/1608.01523 https://academic.oup.com/mnras/article-lookup/doi/10.1093/mnras/stw1973},
