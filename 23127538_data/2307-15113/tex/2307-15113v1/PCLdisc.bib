@article{Cohn2021,
abstract = {We present 0$\backslash$farcs{\{}14{\}}-resolution Atacama Large Millimeter/submillimeter Array (ALMA) CO(2{\$}-{\$}1) observations of the circumnuclear gas disk in UGC 2698, a local compact galaxy. The disk exhibits regular rotation with projected velocities rising to 450 km s{\$}{\^{}}{\{}-1{\}}{\$} near the galaxy center. We fit gas-dynamical models to the ALMA data cube, assuming the CO emission originates from a dynamically cold, thin disk, and measured the mass of the supermassive black hole (BH) in UGC 2698 to be {\$}M{\_}{\{}\backslashmathrm{\{}BH{\}}{\}} = (2.46 \backslashpm{\{}0.07{\}}{\$} [{\$}1\backslashsigma{\$} stat] {\$}{\^{}}{\{}+0.70{\}}{\_}{\{}-0.78{\}}{\$} [sys]){\$}\backslashtimes 10{\^{}}9{\$} {\$}M{\_}\backslashodot{\$}. UGC 2698 is part of a sample of nearby early-type galaxies that are plausible {\$}z\backslashsim2{\$} red nugget relics. Previous stellar-dynamical modeling for three galaxies in the sample found BH masses consistent with the BH mass{\$}-{\$}stellar velocity dispersion ({\$}M{\_}{\{}\backslashmathrm{\{}BH{\}}{\}}-\backslashsigma{\_}\backslashstar{\$}) relation but over-massive relative to the BH mass{\$}-{\$}bulge luminosity ({\$}M{\_}{\{}\backslashmathrm{\{}BH{\}}{\}}-L{\_}{\{}\backslashmathrm{\{}bul{\}}{\}}{\$}) correlation, suggesting that BHs may gain the majority of their mass before their host galaxies. However, UGC 2698 is consistent with both {\$}M{\_}{\{}\backslashmathrm{\{}BH{\}}{\}}-\backslashsigma{\_}\backslashstar{\$} and {\$}M{\_}{\{}\backslashmathrm{\{}BH{\}}{\}}-L{\_}{\{}\backslashmathrm{\{}bul{\}}{\}}{\$}. As UGC 2698 has the largest stellar mass and effective radius in the local compact galaxy sample, it may have undergone more recent mergers that brought it in line with the BH scaling relations. Alternatively, given that the three previously-measured compact galaxies are outliers from {\$}M{\_}{\{}\backslashmathrm{\{}BH{\}}{\}}-L{\_}{\{}\backslashmathrm{\{}bul{\}}{\}}{\$}, while UGC 2698 is not, there may be significant scatter at the poorly sampled high-mass end of the relation. Additional gas-dynamical {\$}M{\_}{\{}\backslashmathrm{\{}BH{\}}{\}}{\$} measurements for the compact galaxy sample will improve our understanding of BH{\$}-{\$}galaxy co-evolution.},
archivePrefix = {arXiv},
arxivId = {2104.07779},
author = {Cohn, Jonathan H. and Walsh, Jonelle L. and Boizelle, Benjamin D. and Barth, Aaron J. and Gebhardt, Karl and G{\"{u}}ltekin, Kayhan and Yıldırım, Akın and Buote, David A. and Darling, Jeremy and Baker, Andrew J. and Ho, Luis C. and Kabasares, Kyle M.},
eprint = {2104.07779},
file = {:Users/ben/Documents/Mendeley Desktop/2104.07779.pdf:pdf},
title = {{An ALMA Gas-dynamical Mass Measurement of the Supermassive Black Hole in the Local Compact Galaxy UGC 2698}},
url = {http://arxiv.org/abs/2104.07779},
year = {2021}
}
@article{Gupta2022,
abstract = {This paper presents deep K-band spectroscopic observations of galaxies at z=3-4 with composite photometric rest-frame Hb+[OIII] equivalent widths EW{\_}0{\textgreater}600A, comparable to the EW of galaxies observed during the epoch of reionisation (EoR, z{\textgreater}6). The typical spectroscopic [OIII] EW{\_}0 and stellar mass of our targets is {\~{}} 700A and log(M{\_}star/M{\_}sun)=8.98. By stacking the [OIII] emission profiles, we find evidence of a weak broad component with F{\_}broad/F{\_}narrow {\~{}} 0.2 and velocity width sigma{\_}{\{}broad{\}} {\~{}} 170 km/s. The strength and velocity width of the broad component does not change significantly with stellar mass and [OIII] EW{\_}0 of the stacked sample. Assuming similar broad component profiles for [OIII] and Halpha emission, we estimate a mass loading factor {\~{}}0.2, similar to low stellar mass galaxies at z{\textgreater}1 even if the star formation rates of our sample is 10 times higher. We hypothesize that either the multi-phase nature of supernovae driven outflows or the suppression of winds in the extreme star-forming regime is responsible for the weak signature of outflows in the EoR analogues.},
archivePrefix = {arXiv},
arxivId = {2211.13763},
author = {Gupta, Anshu and Tran, Kim-Vy and Mendel, Trevor and Harshan, Anishya and Forrest, Ben and Davies, R. L. and Wisnioski, Emily and Nanayakkara, Themiya and Kacprzak, Glenn G. and Kewley, Lisa J.},
eprint = {2211.13763},
file = {:Users/ben/Documents/Mendeley Desktop/2211.13763.pdf:pdf},
keywords = {evolution,galaxies,high-redshift,ism,kinematics and dynamics},
number = {November},
pages = {1--12},
title = {{MOSEL Survey: Extremely weak outflows in EoR analogues at z=3-4}},
url = {http://arxiv.org/abs/2211.13763},
volume = {12},
year = {2022}
}
