eprint = {2012.05935},
file = {:Users/ben/Documents/Mendeley Desktop/2012.05935.pdf:pdf},
keywords = {and dynamics,evolution,formation,galaxies,high-redshift,infrared,kinematics,stellar content,structure},
title = {{The Fundamental Plane of Massive Quiescent Galaxies at z{\~{}}2}},
url = {http://arxiv.org/abs/2012.05935},
