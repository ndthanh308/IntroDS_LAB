@article{Bian2020,
abstract = {We measure the LyC escape fraction in 54 faint Lyman Alpha Emitters (LAEs) at {\$}z\backslashsimeq3.1{\$} in the GOODS-South field. With the average magnitude of {\$}R=26.7{\$} AB ({\$}M{\_}{\{}UV{\}}=-18.8{\$}, {\$}L\backslashsimeq0.1L{\^{}}*{\$}), these galaxies represent a population of compact young dwarf galaxies. Their properties are likely to resemble those in the galaxies responsible for reionising the Universe at {\$}z{\textgreater}6{\$}. We do not detect LyC emission in any individual LAEs in the deep {\{}$\backslash$em HST{\}} F336W images, which covers the rest-frame 820$\backslash$r{\{}A{\}}. We do not detect the LyC emission of these LAEs in the stacked F336W images, either. The {\$}3\backslashsigma{\$} upper limit of LyC escape fractions is {\$}f{\_}{\{}\backslashrm esc{\}}{\textless}14-32\backslash{\%}{\$}. However, the high Ly{\$}\backslashalpha{\$} rest-frame equivalent width, low stellar mass and UV luminosity of these LAEs suggest that they should have {\$}f{\_}{\{}\backslashrm esc{\}}{\textgreater}50\backslash{\%}{\$}. The low LyC escape fraction from this work and other stacking analysis suggest that the LyC leaking galaxies with {\$}f{\_}{\{}\backslashrm esc{\}} {\textgreater} 50\backslash{\%}{\$} at {\$}z=2-3{\$} do not follow the relation between the {\$}f{\_}{\{}\backslashrm esc{\}}{\$} and UV luminosity and Ly{\$}\backslashalpha{\$} equivalent width (EW) derived from typical galaxies at similar redshift. Therefore, the UV luminosity and Ly{\$}\backslashalpha{\$} equivalent width (EW) are not the best indicators for the LyC escape fraction.},
