@article{Lonoce2015,
abstract = {We present the first estimate of age, stellar metallicity and chemical abundance ratios, for an individual early-type galaxy at high-redshift (z = 1.426) in the COSMOS (Cosmological Evolution Survey) field. Our analysis is based on observations obtained with the X-Shooter instrument at the Very Large Telescope (VLT), which cover the visual and near-infrared spectrum at high (R {\textgreater} 5000) spectral resolution. We measure the values of several spectral absorptions tracing chemical species, in particular magnesium and iron, besides determining the age-sensitive D4000 break.We compare the measured indices to stellar population models, finding good agreement. We find that our target is an old (t {\textgreater} 3 Gyr), high-metallicity ([Z/H] {\textgreater} 0.5) galaxy which formed its stars at zform {\textgreater} 5 within a short time-scale {\~{}} 0.1 Gyr, as testified by the strong [a/Fe] ratio ({\textgreater}0.4), and has passively evolved in the first {\textgreater} 3-4 Gyr of its life.We have verified that this result is robust against the choice and number of fitted spectral features, and stellar population model. The result of an old age and high-metallicity has important implications for galaxy formation and evolution confirming an early and rapid formation of the most massive galaxies in the Universe.},
author = {Lonoce, I. and Longhetti, M. and Maraston, C. and Thomas, D. and Mancini, C. and Cimatti, A. and Ciocca, F. and Citro, A. and Daddi, E. and {di Serego Alighieri}, S. and Gargiulo, A. and Maiolino, R. and Mannucci, F. and Moresco, M. and Pozzetti, L. and Quai, S. and Saracco, P.},
