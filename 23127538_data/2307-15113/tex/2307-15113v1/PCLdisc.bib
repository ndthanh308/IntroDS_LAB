file = {:Users/ben/Documents/Mendeley Desktop/2211.16527.pdf:pdf},
keywords = {classification of galaxies,formation,galaxies,galaxy environments,morphology,star formation},
title = {{Morphology {\&} Environment's Role on the Star Formation Rate -- Stellar Mass Relation in COSMOS from 0 {\textless} z {\textless} 3.5}},
url = {http://arxiv.org/abs/2211.16527},
year = {2022}
}
