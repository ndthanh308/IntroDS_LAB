year = {2020}
}
@article{Pavesi2018,
abstract = {We report the serendipitous discovery of a dusty, starbursting galaxy at {\$}z=5.667{\$} (hereafter called CRLE) in close physical association with the "normal" main-sequence galaxy HZ10 at {\$}z=5.654{\$}. CRLE was identified by detection of [CII], [NII] and CO(2-1) line emission, making it the highest redshift, most luminous starburst in the COSMOS field. This massive, dusty galaxy appears to be forming stars at a rate of at least 1500{\$}\backslash,M{\_}\backslashodot{\$} yr{\$}{\^{}}{\{}-1{\}}{\$} in a compact region only {\$}\backslashsim3{\$} kpc in diameter. The dynamical and dust emission properties of CRLE suggest an ongoing merger driving the starburst, in a potentially intermediate stage relative to other known dusty galaxies at the same epoch. The ratio of [CII] to [NII] may suggest that an important ({\$}\backslashsim15\backslash{\%}{\$}) contribution to the [CII] emission comes from a diffuse ionized gas component, which could be more extended than the dense, starbursting gas. CRLE appears to be located in a significant galaxy overdensity at the same redshift, potentially associated with a large-scale cosmic structure recently identified in a Lyman Alpha Emitter survey. This overdensity suggests that CRLE and HZ10 reside in a protocluster environment, offering the tantalizing opportunity to study the effect of a massive starburst on protocluster star formation. Our findings support the interpretation that a significant fraction of the earliest galaxy formation may occur from the inside out, within the central regions of the most massive halos, while rapidly evolving into the massive galaxy clusters observed in the local Universe.},
author = {Pavesi, Riccardo and Riechers, Dominik A. and Sharon, Chelsea E. and Smol{\v{c}}i{\'{c}}, Vernesa and Faisst, Andreas L. and Schinnerer, Eva and Carilli, Christopher L. and Capak, Peter L. and Scoville, Nick and Stacey, Gordon J.},
doi = {10.3847/1538-4357/aac6b6},
file = {:Users/ben/Documents/Mendeley Desktop/Pavesi{\_}2018{\_}ApJ{\_}861{\_}43.pdf:pdf},
issn = {1538-4357},
journal = {The Astrophysical Journal},
keywords = {cosmology,formation,galaxies,high-redshift,ism,observations,radio lines,starburst},
number = {1},
pages = {43},
title = {{ Hidden in Plain Sight: A Massive, Dusty Starburst in a Galaxy Protocluster at z = 5.7 in the COSMOS Field }},
volume = {861},
year = {2018}
}
@article{Jin2019a,
abstract = {We report Atacama Large Millimetre Array (ALMA) observations of four high-redshift dusty star-forming galaxy candidates selected from far-Infrared (FIR)/submm observations in the COSMOS field. We securely detect all galaxies in the continuum and spectroscopically confirm them at z=3.62--5.85 using ALMA 3mm line scans, detecting multiple CO and/or [CI] transitions. This includes the most distant dusty galaxy currently known in the COSMOS field, ID85001929 at z=5.847. These redshifts are lower than we had expected as these galaxies have substantially colder dust temperatures (i.e., their SEDs peak at longer rest frame wavelengths) than most literature sources at z{\textgreater}4. The observed cold dust temperatures are best understood as evidence for optically thick dust continuum in the FIR, rather than the result of low star formation efficiency with rapid metal enrichment. We provide direct evidence that, given their cold spectral energy distributions, CMB plays a significant role biasing their observed Rayleigh-Jeans (RJ) slopes to unlikely steep values and, possibly, reducing their CO fluxes by a factor of two. We recover standard RJ slopes when the CMB contribution is taken into account. High resolution ALMA imaging shows compact morphology and evidence for mergers. This work reveals a population of cold dusty star-forming galaxies that were under-represented in current surveys, and are even colder than typical Main Sequence galaxies at the same redshift. High FIR dust optical depth might be a widespread feature of compact starbursts at any redshift.},
archivePrefix = {arXiv},
arxivId = {arXiv:1906.00040v2},
author = {Jin, S. and Daddi, E. and Magdis, G. E. and Liu, D. and Schinnerer, E. and Papadopoulos, P. P. and Gu, Q. and Gao, Y. and Calabr{\`{o}}, A.},
doi = {10.3847/1538-4357/ab55d6},
eprint = {arXiv:1906.00040v2},
file = {:Users/ben/Documents/Mendeley Desktop/1906.00040.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
number = {2},
pages = {144},
title = {{ Discovery of Four Apparently Cold Dusty Galaxies at z = 3.62–5.85 in the COSMOS Field: Direct Evidence of Cosmic Microwave Background Impact on High-redshift Galaxy Observables }},
volume = {887},
year = {2019}
}
@article{Duncan2019,
abstract = {Galaxy mergers are expected to have a significant role in the mass assembly of galaxies in the early Universe, but there are very few observational constraints on the merger history of galaxies at {\$}z{\textgreater}2{\$}. We present the first study of galaxy major mergers (mass ratios {\$}{\textgreater}{\$} 1:4) in mass-selected samples out to {\$}z\backslashapprox6{\$}. Using all five fields of the HST/CANDELS survey and a probabilistic pair count methodology that incorporates the full photometric redshift posteriors and corrections for stellar mass completeness, we measure galaxy pair-counts for projected separations between 5 and 30 kpc in stellar mass selected samples at {\$}9.7 {\textless} \backslashlog{\_}{\{}10{\}}(\backslashrm{\{}M{\}}{\_}{\{}*{\}}/\backslashrm{\{}M{\}}{\_}{\{}\backslashodot{\}}) {\textless} 10.3{\$} and {\$}\backslashlog{\_}{\{}10{\}}(\backslashrm{\{}M{\}}{\_}{\{}*{\}}/\backslashrm{\{}M{\}}{\_}{\{}\backslashodot{\}}) {\textgreater} 10.3{\$}. We find that the major merger pair fraction rises with redshift to {\$}z\backslashapprox6{\$} proportional to {\$}(1+z){\^{}}{\{}m{\}}{\$}, with {\$}m = 0.8\backslashpm0.2{\$} ({\$}m = 1.8\backslashpm0.2{\$}) for {\$}\backslashlog{\_}{\{}10{\}}(\backslashrm{\{}M{\}}{\_}{\{}*{\}} / \backslashrm{\{}M{\}}{\_}{\{}\backslashodot{\}}) {\textgreater} 10.3{\$} ({\$}9.7 {\textless} \backslashlog{\_}{\{}10{\}}(\backslashrm{\{}M{\}}{\_}{\{}*{\}}/\backslashrm{\{}M{\}}{\_}{\{}\backslashodot{\}}) {\textless} 10.3{\$}). Investigating the pair fraction as a function of mass ratio between 1:20 and 1:1, we find no evidence for a strong evolution in the relative numbers of minor to major mergers out to {\$}z{\textless}3{\$}. Using evolving merger timescales we find that the merger rate per galaxy ({\$}\backslashmathcal{\{}R{\}}{\$}) rises rapidly from {\$}0.07\backslashpm 0.01{\$} Gyr{\$}{\^{}}{\{}-1{\}}{\$} at {\$}z {\textless} 1{\$} to {\$}7.6\backslashpm 2.7{\$} Gyr{\$}{\^{}}{\{}-1{\}}{\$} at {\$}z = 6{\$} for galaxies at {\$}\backslashlog{\_}{\{}10{\}}(\backslashrm{\{}M{\}}{\_}{\{}*{\}}/\backslashrm{\{}M{\}}{\_}{\{}\backslashodot{\}}) {\textgreater} 10.3{\$}. The corresponding co-moving major merger rate density remains roughly constant during this time, with rates of {\$}\backslashGamma \backslashapprox 10{\^{}}{\{}-4{\}}{\$} Gyr{\$}{\^{}}{\{}-1{\}}{\$} Mpc{\$}{\^{}}{\{}-3{\}}{\$}. Based on the observed merger rates per galaxy, we infer specific mass accretion rates from major mergers that are comparable to the specific star-formation rates for the same mass galaxies at {\$}z{\textgreater}3{\$} - observational evidence that mergers are as important a mechanism for building up mass at high redshift as in-situ star-formation.},
author = {Duncan, Kenneth and Conselice, Christopher J. and Mundy, Carl and Bell, Eric and Donley, Jennifer and Galametz, Audrey and Guo, Yicheng and Grogin, Norman A. and Hathi, Nimish and Kartaltepe, Jeyhan and Kocevski, Dale and Koekemoer, Anton M. and P{\'{e}}rez-Gonz{\'{a}}lez, Pablo G. and Mantha, Kameswara B. and Snyder, Gregory F. and Stefanon, Mauro},
doi = {10.3847/1538-4357/ab148a},
file = {:Users/ben/Documents/Mendeley Desktop/Duncan et al. - 2019 - Observational Constraints on the Merger History of Galaxies since z ≈ 6 Probabilistic Galaxy Pair Counts in the.pdf:pdf},
issn = {1538-4357},
