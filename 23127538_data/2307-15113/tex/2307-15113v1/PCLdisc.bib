journal = {The Astrophysical Journal},
keywords = {cD,cosmology: observations,galaxies: distances and redshifts,galaxies: elliptical and lenticular, cD,galaxies: evolution,galaxies: formation,galaxies: stellar content},
month = {jul},
number = {1},
