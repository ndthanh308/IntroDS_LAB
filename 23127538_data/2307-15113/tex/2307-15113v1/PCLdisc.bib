author = {McLure, R. J. and Dunlop, J. S. and Cullen, F. and Bourne, N. and Best, P. N. and Khochfar, S. and Bowler, R. A. A. and Biggs, A. D. and Geach, J. E. and Scott, D. and Michalowski, M. J. and Rujopakarn, W. and van Kampen, E. and Kirkpatrick, A. and Pope, A.},
