issn = {1538-4357},
journal = {The Astrophysical Journal},
keywords = {evolution,formation,galaxies,galaxies: evolution,galaxies: formation,galaxies: high-redshift,galaxies: interactions,galaxies: statistics,high-redshift,interactions},
