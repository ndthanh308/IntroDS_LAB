issn = {00358711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Galaxies: evolution,Galaxies: interactions,Galaxies: structure},
month = {dec},
number = {3},
pages = {1137--1162},
title = {{Galaxy merger morphologies and time-scales from simulations of equal-mass gas-rich disc mergers}},
