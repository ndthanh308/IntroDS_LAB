@article{Kroupa2003,
abstract = {Over the past years observations of young and populous star clusters have shown that the stellar initial mass function (IMF) appears to be an invariant featureless Salpeter power, law with an exponent a = 2.35 for stars more massive than a few M⊙. A consensus has also emerged that most, if not all, stars form in stellar groups and star clusters and that the mass function of young star clusters in the solar neighborhood and in interacting galaxies can be described, over the mass range of a few 10 to 107 M⊙ as a power law with an exponent $\beta$ ≈ 2. These two results imply that galactic-field IMFs for early-type stars cannot, under any circumstances, be a Salpeter power law, but that they must have a steeper exponent, $\alpha$field ≳ 2.8. This has important consequences for the distribution of stellar remnants and for the chemodynamical and photometric evolution of galaxies.},
author = {Kroupa, Pavel and Weidner, Carsten},
doi = {10.1086/379105},
file = {:Users/ben/Documents/Mendeley Desktop/Kroupa{\_}2003{\_}ApJ{\_}598{\_}1076.pdf:pdf},
