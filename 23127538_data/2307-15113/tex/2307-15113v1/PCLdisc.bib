doi = {10.1038/nature12279},
eprint = {1307.5842},
file = {:Users/ben/Documents/Mendeley Desktop/Meibom et al. - 2013 - The same frequency of planets inside and outside open clusters of stars.pdf:pdf},
issn = {1476-4687},
journal = {Nature},
month = {jul},
number = {7456},
pages = {18},
