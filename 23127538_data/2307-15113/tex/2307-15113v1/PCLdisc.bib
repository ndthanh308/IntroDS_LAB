year = {2014}
}
@article{Tan2014,
abstract = {We measure the dust and gas content of the three sub-millimeter galaxies (SMGs) in the GN20 proto-cluster at {\$}z=4.05{\$} using new IRAM Plateau de Bure interferometer (PdBI) CO(4-3) and 1.2-3.3 mm continuum observations. All these three SMGs are heavily dust obscured, with UV-based star formation rate (SFR) estimates significantly smaller than the ones derived from the bolometric infrared (IR), consistent with the spatial offsets revealed by HST and CO imaging. Based also on evaluations of the specific SFR, CO-to-H{\$}{\_}2{\$} conversion factor and gas depletion timescale, we classify all the three galaxies as starbursts (SBs), although with a lower confidence for GN20.2b that might be a later stage merging event. We place our measurements in the context of the evolutionary properties of main sequence (MS) and SB galaxies. ULIRGs have 3-5 times larger {\$}L'{\_}{\{}\backslashrm CO{\}}/M{\_}{\{}\backslashrm dust{\}}{\$} and {\$}M{\_}{\{}\backslashrm dust{\}}/M{\_}\backslashstar{\$} ratios than {\$}z=0{\$} MS galaxies, but by {\$}z\backslashsim2{\$} the difference appears to be blurred, probably due to differential metallicity evolution. SB galaxies appear to slowly evolve in their {\$}L'{\_}{\{}\backslashrm CO{\}}/M{\_}{\{}\backslashrm dust{\}}{\$} and {\$}M{\_}{\{}\backslashrm dust{\}}/M{\_}\backslashstar{\$} ratios all the way to {\$}z{\textgreater}6{\$} (consistent with rapid enrichment of SB events), while MS galaxies rapidly increase in {\$}M{\_}{\{}\backslashrm dust{\}}/M{\_}\backslashstar{\$} from {\$}z=0{\$} to 2 (due to gas fraction increase, compensated by a decrease of metallicities). While no IR/submm continuum detection is available for indisputably normal massive galaxies at {\$}z{\textgreater}2.5{\$}, we show that if metallicity indeed decrease rapidly for these systems at {\$}z{\textgreater}3{\$} as claimed in the literature, we should expect a strong decrease of their {\$}M{\_}{\{}\backslashrm dust{\}}/M{\_}\backslashstar{\$}, consistent with recent PdBI and ALMA upper limits. We conclude that the {\$}M{\_}{\{}\backslashrm dust{\}}/M{\_}\backslashstar{\$} ratio could be a powerful tool for distinguishing starbursts from normal galaxies at {\$}z{\textgreater}4{\$}.},
archivePrefix = {arXiv},
arxivId = {1403.7992},
author = {Tan, Qinghua and Daddi, Emanuele and Magdis, Georgios and Pannella, Maurilio and Sargent, Mark and Riechers, Dominik and B{\'{e}}thermin, Matthieu and Bournaud, Fr{\'{e}}d{\'{e}}ric and Carilli, Chris and da Cunha, Elisabete and Dannerbauer, Helmut and Dickinson, Mark and Elbaz, David and Gao, Yu and Hodge, Jacqueline and Owen, Frazer and Walter, Fabian},
doi = {10.1051/0004-6361/201423905},
