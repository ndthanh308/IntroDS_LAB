@article{Velazquez2020,
abstract = {Understanding the rate at which stars form is central to studies of galaxy formation. Observationally, the star formation rates (SFRs) of galaxies are measured using the luminosity in different frequency bands, often under the assumption of a time-steady SFR in the recent past. We use star formation histories (SFHs) extracted from cosmological simulations of star-forming galaxies from the FIRE project to analyze the time-scales to which the H{\$}{\{}\backslashalpha{\}}{\$} and far-ultraviolet (FUV) continuum SFR indicators are sensitive. In these simulations, the SFRs are highly time variable for all galaxies at high redshift, and continue to be bursty to z=0 in dwarf galaxies. When FIRE SFHs are partitioned into their bursty and time-steady phases, the best-fitting FUV time-scale fluctuates from its {\~{}}10 Myr value when the SFR is time-steady to {\textgreater}{\~{}}100 Myr immediately following particularly extreme bursts of star formation during the bursty phase. On the other hand, the best-fitting averaging time-scale for H{\$}{\{}\backslashalpha{\}}{\$} is generally insensitive to the SFR variability in the FIRE simulations and remains {\~{}}5 Myr at all times. These time-scales are shorter than the 100 Myr and 10 Myr time-scales sometimes assumed in the literature for FUV and H{\$}{\{}\backslashalpha{\}}{\$}, respectively, because while the FUV emission persists for stellar populations older than 100 Myr, the time-dependent luminosities are strongly dominated by younger stars. Our results confirm that the ratio of SFRs inferred using H{\$}{\{}\backslashalpha{\}}{\$} vs. FUV can be used to probe the burstiness of star formation in galaxies.},
archivePrefix = {arXiv},
arxivId = {2008.08582},
author = {Vel{\'{a}}zquez, Jos{\'{e}} A. Flores and Gurvich, Alexander B. and Faucher-Gigu{\`{e}}re, Claude-Andr{\'{e}} and Bullock, James S. and Starkenburg, Tjitske K. and Moreno, Jorge and Lazar, Alexandres and Mercado, Francisco J. and Stern, Jonathan and Sparre, Martin and Hayward, Christopher C. and Wetzel, Andrew and El-Badry, Kareem},
eprint = {2008.08582},
file = {:Users/ben/Documents/Mendeley Desktop/2008.08582.pdf:pdf},
keywords = {galaxies,high-redshift,star formation,ultraviolet},
number = {August},
pages = {1--13},
title = {{The time-scales probed by star formation rate indicators for realistic, bursty star formation histories from the FIRE simulations}},
url = {http://arxiv.org/abs/2008.08582},
volume = {13},
year = {2020}
}
@article{Hutchison2019,
abstract = {We present Keck/MOSFIRE {\$}H{\$}-band spectroscopy targeting C{\$}\backslash,{\$}III] {\$}\backslashlambda{\$}1907,1909 in a {\$}z=7.5056{\$} galaxy previously identified via Ly{\$}\backslashalpha{\$} emission. We detect strong line emission at {\$}1.621\backslashpm0.002\backslash,\backslashmu{\$}m with a line flux of ({\$}2.63\backslashpm0.52{\$}){\$}\backslashtimes10{\^{}}{\{}-18{\}}{\$} erg s{\$}{\^{}}{\{}-1{\}}{\$} cm{\$}{\^{}}{\{}-2{\}}{\$}. We tentatively identify this line as [C{\$}\backslash,{\$}III] {\$}\backslashlambda{\$}1907, but we are unable to detect C{\$}\backslash,{\$}III] {\$}\backslashlambda{\$}1909 owing to sky emission at the expected location. This gives a galaxy systemic redshift, {\$}z{\_}{\{}sys{\}}=7.5032\backslashpm0.0003{\$}, with a velocity offset to Ly{\$}\backslashalpha{\$} of {\$}\backslashDelta{\$}v{\$}{\_}{\{}Ly\backslashalpha{\}}{\$} = {\$}88\backslashpm27{\$} km s{\$}{\^{}}{\{}-1{\}}{\$}. The ratio of combined C{\$}\backslash,{\$}III]/Ly{\$}\backslashalpha{\$} is 0.30-0.45, one of the highest values measured for any {\$}z{\textgreater}2{\$} galaxy. We do not detect Si{\$}\backslash,{\$}III] {\$}\backslashlambda\backslashlambda{\$}1883, 1892, and place an upper limit on Si{\$}\backslash,{\$}III]/C{\$}\backslash,{\$}III] {\$}{\textless}{\$} 0.35 ({\$}2\backslashsigma{\$}). Comparing our results to photoionization models, the C{\$}\backslash,{\$}III] equivalent width (W{\$}{\_}{\{}\backslashtext{\{}CIII]{\}}{\}} = 16.23\backslashpm2.32\backslash,{\$}$\backslash$AA), low Si{\$}\backslash,{\$}III]/C{\$}\backslash,{\$}III] ratio, and high implied [O{\$}\backslash,{\$}III] equivalent width (from the {\$}Spitzer{\$}/IRAC [3.6]{\$}-{\$}[4.5]{\$}\backslashsimeq{\$}0.8 mag color) require sub-Solar metallicities ({\$}Z\backslashsimeq0.1-0.2 Z{\_}{\{}\backslashodot{\}}{\$}) and a high ionization parameter, log{\$}\backslash,{\$}U {\$}\backslashgtrsim -1.5{\$}. These results favor models that produce higher ionization, such as the BPASS models for the photospheres of high-mass stars, and that include both binary stellar populations and/or an IMF that extends to 300 M{\$}{\_}{\{}\backslashodot{\}}{\$}. The combined C{\$}\backslash,{\$}III] equivalent width and [3.6]{\$}-{\$}[4.5] color are more consistent with ionization from young stars than AGN, however we cannot rule out ionization from a combination of an AGN and young stars. We make predictions for {\$}James{\~{}}Webb{\~{}}Space{\~{}}Telescope{\$} spectroscopy using these different models, which will ultimately test the nature of the ionizing radiation in this source.},
