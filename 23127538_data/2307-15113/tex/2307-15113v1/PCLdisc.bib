eprint = {1307.2003},
file = {:Users/ben/Documents/Mendeley Desktop/Santerne et al. - 2013 - An unconsidered configuration of astrophysical false positives in exoplanet-transit surveys.pdf:pdf},
keywords = {binaries,eclipsing,photometric,planetary systems,techniques},
month = {jul},
pages = {9},
title = {{An unconsidered configuration of astrophysical false positives in exoplanet-transit surveys}},
