file = {:Users/ben/Documents/Mendeley Desktop/2203.04322.pdf:pdf},
issn = {0004-637X},
title = {{COSMOS2020: Ubiquitous AGN Activity of Massive Quiescent Galaxies at {\$}0{\textless}z{\textless}5{\$} Revealed by X-ray and Radio Stacking}},
