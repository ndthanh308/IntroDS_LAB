eprint = {1912.02238},
file = {:Users/ben/Documents/Mendeley Desktop/1912.02238.pdf:pdf},
month = {dec},
pages = {1--26},
title = {{The role of environment in galaxy evolution in the SERVS Survey I: density maps and cluster candidates}},
url = {http://arxiv.org/abs/1912.02238 http://dx.doi.org/10.3847/1538-4357/ab60a0},
