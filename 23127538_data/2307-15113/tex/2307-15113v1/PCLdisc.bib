issn = {0004-6361},
journal = {Astronomy {\&} Astrophysics},
keywords = {cosmology,dark ages,first stars,galaxies,high-redshift,reionization,starburst,ultraviolet},
pages = {L8},
title = {{The ionizing photon production efficiency of compact {\textless}i{\textgreater}z{\textless}/i{\textgreater} {\~{}} 0.3 Lyman continuum leakers and comparison with high-redshift galaxies}},
