arxivId = {1710.04225},
author = {Cowley, Michael J. and Spitler, Lee R. and Quadri, Ryan F. and Goulding, Andy D. and Papovich, Casey and Tran, K. V. H. and Labb{\'{e}}, I. and Alcorn, Leo and Allen, Rebecca J. and Forrest, Ben and Glazebrook, Karl and Kacprzak, Glenn G. and Morrison, Glenn and Nanayakkara, Themiya and Straatman, Caroline M. S. and Tomczak, Adam R.},
doi = {10.1093/mnras/stx2587},
eprint = {1710.04225},
file = {:Users/ben/Documents/Mendeley Desktop/Cowley et al. - 2018 - Decoupled Black Hole Accretion and Quenching The Relationship Between BHAR, SFR, and Quenching in Milky Way and A.pdf:pdf},
issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {galaxies},
month = {jan},
