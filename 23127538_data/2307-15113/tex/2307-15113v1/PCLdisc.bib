}
@article{Saracco2020,
abstract = {Thanks to very deep spectroscopic observations carried out at the Large Binocular Telescope, we measured simultaneously stellar age, metallicity and velocity dispersion for C1-23152, an ETG at redshift {\$}z{\$}=3.352, corresponding to an epoch when the Universe was {\$}\backslashsim{\$}1.8 Gyr old. The analysis of its spectrum shows that this galaxy, hosting an AGN, formed and assembled {\$}\backslashsim{\$}2{\$}\backslashtimes{\$}10{\$}{\^{}}{\{}11{\}}{\$} M{\$}{\_}\backslashodot{\$} shaping its morphology within the {\$}\backslashsim{\$}600 Myr preceding the observations, since {\$}z{\$}{\$}\backslashsim{\$}4.6. The stellar population has a mean mass-weighted age 400{\$}{\^{}}{\{}+30{\}}{\_}{\{}-70{\}}{\$} Myr and it is formed between {\$}\backslashsim{\$}600 Myr and {\$}\backslashsim{\$}150 Myr before the observed epoch, this latter being the time since quenching. Its high stellar velocity dispersion, {\$}\backslashsigma{\_}e{\$}=409{\$}\backslashpm{\$}60 km s{\$}{\^{}}{\{}-1{\}}{\$}, confirms the high mass (M{\$}{\_}{\{}dyn{\}}{\$}={\$}2.2(\backslashpm0.4){\$}{\$}\backslashtimes{\$}10{\$}{\^{}}{\{}11{\}}{\$} M{\$}{\_}\backslashodot{\$}) and the high mass density ({\$}\backslashSigma{\_}e{\^{}}{\{}M{\^{}}*{\}}{\$}={\$}\backslashSigma{\_}{\{}1kpc{\}}=3.2(\backslashpm0.7)\backslashtimes10{\^{}}{\{}10{\}}{\$} M{\$}{\_}\backslashodot{\$} kpc{\$}{\^{}}{\{}-2{\}}{\$}), suggesting a fast dissipative process at its origin. The analysis points toward a supersolar metallicity, [Z/H]=0.25{\$}{\^{}}{\{}+0.006{\}}{\_}{\{}-0.10{\}}{\$}, in agreement with the above picture, suggesting a star formation efficiency much higher than the replenishment time. However, sub-solar metallicity values cannot be firmly ruled out by our analysis. Quenching must have been extremely efficient to reduce the star formation to SFR{\$}{\textless}{\$}6.5 M{\$}{\_}\backslashodot{\$} yr{\$}{\^{}}{\{}-1{\}}{\$} in less than 150 Myr. This could be explained by the presence of the AGN, even if a causal relation cannot be established from the data. C1-23152 has the same stellar and physical properties of the densest ETGs in the local Universe of comparable mass, suggesting that they are C1-23152-like galaxies which evolved to {\$}z=0{\$} unperturbed.},
archivePrefix = {arXiv},
arxivId = {2011.04657},
author = {Saracco, Paolo and Marchesini, Danilo and Barbera, Francesco La and Gargiulo, Adriana and Annunziatella, Marianna and Forrest, Ben and {Lange Vagle}, Daniel J. and Marsan, Z. Cemile and Muzzin, Adam and Stefanon, Mauro and Wilson, Gillian},
