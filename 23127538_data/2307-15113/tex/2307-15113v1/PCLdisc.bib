journal = {The Astrophysical Journal},
keywords = {abun-,dances,evolution,fi gure set,formation,galaxies,galaxies: evolution,galaxies: formation,galaxies: high-redshift,galaxies: stellar content,high-redshift,machine-readable tables,stellar content,supporting material},
