journal = {Publications of the Astronomical Society of the Pacific},
number = {755},
pages = {63--75},
primaryClass = {astro-ph},
title = {{Correcting for the Effects of Interstellar Extinction}},
url = {http://arxiv.org/abs/astro-ph/9809387},
volume = {111},
year = {1999}
}
