file = {:Users/ben/Documents/Mendeley Desktop/1907.02970.pdf:pdf},
issn = {2041-8213},
journal = {The Astrophysical Journal},
keywords = {cfa,edu,fundamental parameters,galaxies,harvard,joel,leja,sandro,star formation,tacchella},
month = {jul},
number = {1},
