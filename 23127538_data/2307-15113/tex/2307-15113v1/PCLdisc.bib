issn = {0004-637X},
journal = {The Astrophysical Journal},
pages = {746},
primaryClass = {astro-ph},
title = {{A New Photometric Technique for the Joint Selection of Star-forming and Passive Galaxies at 1.4 {\textless}{\~{}} z {\textless}{\~{}} 2.5}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2004ApJ...617..746D{\&}link{\_}type=ABSTRACT{\%}5Cnpapers2://publication/doi/10.1086/425569},
volume = {617},
year = {2004}
}
