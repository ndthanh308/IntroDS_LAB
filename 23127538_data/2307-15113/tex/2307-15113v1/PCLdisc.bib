file = {:Users/ben/Documents/Mendeley Desktop/2104.01183.pdf:pdf},
keywords = {clusters,galaxies,kinematics and dynamics},
pages = {1--17},
title = {{The GOGREEN survey: the internal dynamics of clusters of galaxies at redshift 0.9-1.4}},
url = {http://arxiv.org/abs/2104.01183},
year = {2021}
}
