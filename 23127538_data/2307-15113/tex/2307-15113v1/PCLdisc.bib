issn = {0004-6361},
journal = {Astro-Ph},
keywords = {cosmic background radiation,cosmology,observations,reference systems,relativistic processes},
pages = {1--9},
title = {{Planck 2013 results. XXVII. Doppler boosting of the CMB: Eppur si muove}},
year = {2013}
}
