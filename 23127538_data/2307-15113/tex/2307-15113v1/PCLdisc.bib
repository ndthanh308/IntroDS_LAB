eprint = {1501.01311},
file = {:Users/ben/Documents/Mendeley Desktop/stv725.pdf:pdf},
issn = {13652966},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Cosmology: theory,Galaxies: evolution,Galaxies: formation,Galaxies: haloes},
number = {2},
