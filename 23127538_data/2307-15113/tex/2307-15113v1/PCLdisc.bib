title = {{The Variability of the Star Formation Rate in Galaxies. I. Star Formation Histories Traced by EW(H $\alpha$ ) and EW(H $\delta$ A )}},
url = {http://arxiv.org/abs/1912.06523 http://dx.doi.org/10.3847/1538-4357/ab7b7d https://iopscience.iop.org/article/10.3847/1538-4357/ab7b7d},
