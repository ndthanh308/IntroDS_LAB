title = {{HerMES: COSMIC INFRARED BACKGROUND ANISOTROPIES AND THE CLUSTERING OF DUSTY STAR-FORMING GALAXIES}},
url = {http://arxiv.org/abs/1208.5049{\%}5Cnhttp://iopscience.iop.org/0004-637X/772/1/77 http://stacks.iop.org/0004-637X/772/i=1/a=77?key=crossref.77dd68a5ee66a246e92b1a8147cac601},
