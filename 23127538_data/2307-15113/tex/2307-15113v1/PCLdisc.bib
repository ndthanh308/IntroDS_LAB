arxivId = {1907.04327},
author = {Kriek, Mariska and Price, Sedona H. and Conroy, Charlie and Suess, Katherine A. and Mowla, Lamiya and Pasha, Imad and Bezanson, Rachel and van Dokkum, Pieter and Barro, Guillermo},
doi = {10.3847/2041-8213/ab2e75},
eprint = {1907.04327},
