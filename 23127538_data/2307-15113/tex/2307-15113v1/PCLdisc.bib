arxivId = {1606.04090},
author = {Martis, Nicholas S. and Marchesini, Danilo and Brammer, Gabriel B. and Muzzin, Adam and Franx, Marijn and Labb{\'{e}}, Ivo and Momcheva, Ivelina G. and Skelton, Rosalind E. and Stefanon, Mauro and van Dokkum, Pieter G. and Whitaker, Katherine E.},
eprint = {1606.04090},
file = {:Users/ben/Documents/Mendeley Desktop/Martis et al. - 2016 - The Evolution of the Fractions of Quiescent and Star-forming Galaxies as a Function of Stellar Mass Since z=3 Inc.pdf:pdf},
keywords = {evolution,galaxies},
number = {2012},
pages = {1--7},
title = {{The Evolution of the Fractions of Quiescent and Star-forming Galaxies as a Function of Stellar Mass Since z=3: Increasing Importance of Massive, Dusty Star-forming Galaxies in the Early Universe}},
