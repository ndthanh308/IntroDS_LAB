eprint = {1612.00065},
file = {:Users/ben/Documents/Mendeley Desktop/Newman{\_}2017{\_}ApJ{\_}845{\_}157.pdf:pdf},
issn = {15384357},
journal = {The Astrophysical Journal},
keywords = {cd,elliptical and lenticular,galaxies,galaxies: elliptical and lenticular,cD,galaxies: s,gravitational lensing,machine-readable table,stellar content,strong,supporting material},
number = {2},
