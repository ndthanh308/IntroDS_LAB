file = {:Users/ben/Documents/Mendeley Desktop/2212.06683.pdf:pdf},
keywords = {dark ages,evolution,first stars,galaxies,high-redshift,reionization},
number = {December},
title = {{UV Luminosity Density Results at z{\textgreater}8 from the First JWST/NIRCam Fields: Limitations of Early Data Sets and the Need for Spectroscopy}},
