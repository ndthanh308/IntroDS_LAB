arxivId = {1207.3528},
author = {Buat, V. and Noll, S. and Burgarella, D. and Giovannoli, E. and Charmandaris, V. and Pannella, M. and Hwang, H. S. and Elbaz, D. and Dickinson, M. and Magdis, G. and Reddy, N. and Murphy, E. J.},
