@article{Forrest2017,
abstract = {Using deep multi-wavelength photometry of galaxies from ZFOURGE, we group galaxies at {\$}2.5{\textless}z{\textless}4.0{\$} by the shape of their spectral energy distributions (SEDs). We identify a population of galaxies with excess emission in the {\$}K{\_}s{\$}-band, which corresponds to [OIII]+H{\$}\backslashbeta{\$} emission at {\$}2.95{\textless}z{\textless}3.65{\$}. This population includes 78{\%} of the bluest galaxies with UV slopes steeper than {\$}\backslashbeta = -2{\$}. We de-redshift and scale this photometry to build two composite SEDs, enabling us to measure equivalent widths of these Extreme [OIII]+H{\$}\backslashbeta{\$} Emission Line Galaxies (EELGs) at {\$}z\backslashsim3.5{\$}. We identify 60 galaxies that comprise a composite SED with [OIII]+H{\$}\backslashbeta{\$} rest-frame equivalent width of {\$}803\backslashpm228{\$}$\backslash$AA$\backslash$ and another 218 galaxies in a composite SED with equivalent width of {\$}230\backslashpm90{\$}$\backslash$AA. These EELGs are analogous to the `green peas' found in the SDSS, and are thought to be undergoing their first burst of star formation due to their blue colors ({\$}\backslashbeta {\textless} -1.6{\$}), young ages ({\$}\backslashlog(\backslashrm{\{}age{\}}/yr)\backslashsim7.2{\$}), and low dust attenuation values. Their strong nebular emission lines and compact sizes (typically {\$}\backslashsim1.4{\$} kpc) are consistent with the properties of the star-forming galaxies possibly responsible for reionizing the universe at {\$}z{\textgreater}6{\$}. Many of the EELGs also exhibit Lyman-{\$}\backslashalpha{\$} emission. Additionally, we find that many of these sources are clustered in an overdensity in the Chandra Deep Field South, with five spectroscopically confirmed members at {\$}z=3.474 \backslashpm 0.004{\$}. The spatial distribution and photometric redshifts of the ZFOURGE population further confirm the overdensity highlighted by the EELGs.},
