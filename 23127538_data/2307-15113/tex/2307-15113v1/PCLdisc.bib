file = {:Users/ben/Documents/Mendeley Desktop/Greene et al. - 2012 - THE STELLAR HALOS OF MASSIVE ELLIPTICAL GALAXIES.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {cd,color figures,elliptical and lenticular,formation,galaxies,online-only material,stellar content},
month = {may},
number = {1},
