arxivId = {2201.09068},
author = {Lustig, Peter and Strazzullo, Veronica and Remus, Rhea-Silvia and D'Eugenio, Chiara and Daddi, Emanuele and Burkert, Andreas and {De Lucia}, Gabriella and Delvecchio, Ivan and Dolag, Klaus and Fontanot, Fabio and Gobat, Raphael and Mohr, Joseph J. and Onodera, Masato and Pannella, Maurilio and Pillepich, Annalisa and Renzini, Alvio},
eprint = {2201.09068},
file = {:Users/ben/Documents/Mendeley Desktop/Lustig et al. - 2022 - Massive quiescent galaxies at {\$}zsim3{\$} a comparison of selection, stellar population and structural properties wit.pdf:pdf},
keywords = {evolution,galaxies,high-redshift,star formation,structure},
