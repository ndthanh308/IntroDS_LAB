doi = {10.1088/0004-637X/732/2/126},
eprint = {arXiv:1102.3920v1},
file = {:Users/ben/Documents/Mendeley Desktop/Murphy et al. - 2011 - an Accounting of the Dust-Obscured Star Formation and Accretion Histories Over the Last ∼11 Billion Years.pdf:pdf},
