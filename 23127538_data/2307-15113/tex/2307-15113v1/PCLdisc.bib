arxivId = {1308.5744},
author = {Monroe, Talawanda R and Mel{\'{e}}ndez, Jorge and Ram{\'{i}}rez, Iv{\'{a}}n and Yong, David and Bergemann, Maria and Asplund, Martin and Bean, Jacob and Bedell, Megan and Maia, Marcelo Tucci and Lind, Karin and Alves-Brito, Alan and Casagrande, Luca and Castro, Matthieu and do Nascimento, Jos{\'{e}}-Dias and Bazot, Michael and Freitas, Fabr{\'{i}}cio C.},
