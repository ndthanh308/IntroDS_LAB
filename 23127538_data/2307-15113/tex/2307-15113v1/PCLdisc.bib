eprint = {1401.7878},
file = {:Users/ben/Documents/Mendeley Desktop//Wild et al. - 2014 - A new method for classifying galaxy SEDs from multiwavelength photometry 1 I N T RO D U C T I O N.pdf:pdf},
issn = {13652966},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Galaxies: fundamental parameters,Galaxies: luminosity function, mass function,Galaxies: photometry,Galaxies: statistics,Galaxies: stellar content,Methods: statistical},
