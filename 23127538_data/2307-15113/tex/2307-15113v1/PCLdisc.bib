@article{Pantoni2021,
abstract = {We present a panchromatic study of 11 (sub-)millimetre selected DSFGs with spectroscopically confirmed redshift ({\$}1.5{\textless} z{\_}{\{}\backslashrm spec{\}}{\textless}3{\$}) in the GOODS-S field, with the aim of constraining their astrophysical properties (e.g., age, stellar mass, dust and gas content) and characterizing their role in the context of galaxy evolution. The multi-wavelength coverage of GOODS-S, from X-rays to radio band, allow us to model galaxy SED by using CIGALE with a novel approach, based on a physical motivated modelling of stellar light attenuation by dust. Median stellar mass ({\$}\backslashsimeq6.5\backslashtimes10{\^{}}{\{}10{\}}{\$} M{\$}{\_}\backslashodot{\$}) and SFR ({\$}\backslashsimeq241{\$} M{\$}{\_}\backslashodot{\$} yr{\$}{\^{}}{\{}-1{\}}{\$}) are consistent with galaxy main-sequence at {\$}z\backslashsim2{\$}. The galaxies are experiencing an intense and dusty burst of star formation (median L{\$}{\_}{\{}\backslashrm IR{\}}\backslashsimeq2\backslashtimes10{\^{}}{\{}12{\}}{\$} L{\$}{\_}\backslashodot{\$}), with a median age of {\$}750{\$} Myr. The high median content of interstellar dust (M{\$}{\_}{\{}\backslashrm dust{\}}\backslashsimeq5\backslashtimes10{\^{}}8{\$} M{\$}{\_}\backslashodot{\$}) suggests a rapid enrichment of the ISM (on timescales {\$}\backslashsim10{\^{}}8{\$} yr). We derived galaxy total and molecular gas content from CO spectroscopy and/or Rayleigh-Jeans dust continuum ({\$}10{\^{}}{\{}10{\}}\backslashlesssim{\$} M{\$}{\_}{\{}\backslashrm gas{\}}/{\$}M{\$}{\_}\backslashodot\backslashlesssim10{\^{}}{\{}11{\}}{\$}), depleted over a typical timescale {\$}\backslashtau{\_}{\{}\backslashrm depl{\}}\backslashsim200{\$} Myr. X-ray and radio luminosities suggest that most of the galaxies hosts an accreting radio silent/quiet SMBH. This evidence, along with their compact multi-wavelength sizes (median r{\$}{\_}{\{}\backslashrm ALMA{\}}\backslashsim{\$} r{\$}{\_}{\{}\backslashrm VLA{\}}=1.8{\$} kpc, r{\$}{\_}{\{}\backslashrm HST{\}}=2.3{\$} kpc) measured from high-resolution imaging ({\$}\backslashtheta{\_}{\{}\backslashrm res{\}}\backslashlesssim{\$} 1 arcsec), indicates these objects as the high-z star-forming counterparts of massive quiescent galaxies, as predicted e.g., by the in-situ scenario. Four objects show some signatures of a forthcoming/ongoing AGN feedback, that is thought to trigger the morphological transition from star-forming disks to ETGs.},
archivePrefix = {arXiv},
arxivId = {2103.05011},
author = {Pantoni, L and Lapi, A and Massardi, M and Donevski, D and Bressan, A and Silva, L and Pozzi, F and Vignali, C and Talia, M and Cimatti, A and Ronconi, T and Danese, L},
