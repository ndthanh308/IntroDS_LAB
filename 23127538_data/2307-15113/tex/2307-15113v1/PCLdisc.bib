arxivId = {0704.2601},
author = {Haussler, Boris and McIntosh, Daniel H. and Barden, Marco and Bell, Eric F. and Rix, Hans-Walter and Borch, Andrea and Beckwith, Steven V. W. and Caldwell, John A. R. and Heymans, Catherine and Jahnke, Knud and Jogee, Shardha and Koposov, Sergey E. and Meisenheimer, Klaus and Sanchez, Sebastian F. and Somerville, Rachel S. and Wisotzki, Lutz and Wolf, Christian},
