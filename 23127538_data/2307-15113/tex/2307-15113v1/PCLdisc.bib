journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Dark ages,Galaxies: ISM,Galaxies: abundances,Galaxies: dwarf,Galaxies: fundamental parameters,Galaxies: starburst,first stars,reionization},
