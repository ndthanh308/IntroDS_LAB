@article{Stefanon2021a,
abstract = {We present new stellar mass functions at {\$}z\backslashsim6{\$}, {\$}z\backslashsim7{\$}, {\$}z\backslashsim8{\$}, {\$}z\backslashsim9{\$} and, for the first time, {\$}z\backslashsim10{\$}, constructed from {\$}\backslashsim800{\$} Lyman-Break galaxies previously identified over the XDF/UDF, parallels and the five CANDELS fields. Our study is distinctive due to (1) the much deeper ({\$}\backslashsim200{\$} hour) wide-area Spitzer/IRAC imaging at {\$}3.6\backslashmu{\$}m and {\$}4.5\backslashmu{\$}m from the GOODS Re-ionization Era wide Area Treasury from Spitzer (GREATS) program and (2) consideration of {\$}z\backslashsim6-10{\$} sources over a {\$}3\backslashtimes{\$} larger area than previous HST+Spitzer studies. The Spitzer/IRAC data enable {\$}\backslashge2\backslashsigma{\$} rest-frame optical detections for an unprecedented {\$}50\backslash{\%}{\$} of galaxies down to a stellar mass limit of {\$}\backslashsim10{\^{}}{\{}8{\}}\backslashmathcal{\{}M{\}}{\_}\backslashodot{\$} across all redshifts. Schechter fits to our volume densities suggest a combined evolution in characteristic mass {\$}\backslashmathcal{\{}M{\}}{\^{}}*{\$} and normalization factor {\$}\backslashphi{\^{}}*{\$} between {\$}z\backslashsim6{\$} and {\$}z\backslashsim8{\$}. The stellar mass density (SMD) increases by {\$}\backslashsim1000\backslashtimes{\$} in the {\$}\backslashsim500{\$} Myr between {\$}z\backslashsim10{\$} and {\$}z\backslashsim6{\$}, with indications of a steeper evolution between {\$}z\backslashsim10{\$} and {\$}z\backslashsim8{\$}, similar to the previously-reported trend of the star-formation rate density. Strikingly, abundance matching to the Bolshoi-Planck simulation indicates halo mass densities evolving at approximately the same rate as the SMD between {\$}z\backslashsim10{\$} and {\$}z\backslashsim4{\$}. Our results show that the stellar-to-halo mass ratios, a proxy for the star-formation efficiency, do not change significantly over the huge stellar mass build-up occurred from {\$}z\backslashsim10{\$} to {\$}z\backslashsim6{\$}, indicating that the assembly of stellar mass closely mirrors the build-up in halo mass in the first {\$}\backslashsim1{\$} Gyr of cosmic history. JWST is poised to extend these results into the "first galaxy" epoch at {\$}z\backslashgtrsim10{\$}.},
archivePrefix = {arXiv},
arxivId = {2103.16571},
author = {Stefanon, Mauro and Bouwens, Rychard J. and Labb{\'{e}}, Ivo and Illingworth, Garth D. and Gonzalez, Valentino and Oesch, Pascal A.},
