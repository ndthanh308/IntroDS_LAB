number = {2013},
pages = {1--12},
title = {{Predicting the redshift 2 Halpha luminosity function using [OIII] emission line galaxies}},
url = {http://arxiv.org/abs/1505.07843{\%}0Ahttp://dx.doi.org/10.1088/0004-637X/811/2/141},
year = {2015}
