arxivId = {1005.2497},
author = {Gordon, K. D. and Galliano, F. and Hony, S. and Bernard, J.-P. and Bolatto, A. and Bot, C. and Engelbracht, C. and Hughes, A. and Israel, F. P. and Kemper, F. and Kim, S. and Li, A. and Madden, S. C. and Matsuura, M. and Meixner, M. and Misselt, K. and Okumura, K. and Panuzzo, P. and Rubio, M. and Reach, W. T. and Roman-Duval, J. and Sauvage, M. and Skibba, R. and Tielens, a. G. G. M.},
