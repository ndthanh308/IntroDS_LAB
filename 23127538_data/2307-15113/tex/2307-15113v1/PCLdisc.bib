number = {875},
pages = {2--8},
title = {{The NEWFIRM Medium-Band Survey: Filter Definitions and First Results}},
url = {http://www.journals.uchicago.edu/doi/abs/10.1086/597138{\%}5Cnpapers2://publication/doi/10.1086/597138},
volume = {121},
year = {2009}
