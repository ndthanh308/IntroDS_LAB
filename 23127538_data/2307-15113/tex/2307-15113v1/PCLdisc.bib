@article{Griffith2018,
abstract = {While we observe a large amount of cold interstellar gas and dust in a subset of the early-type galaxy (ETG) population, the source of this material remains unclear. The two main, competing scenarios are external accretion of lower mass, gas-rich dwarfs and internal production from stellar mass loss and/or cooling from the hot interstellar medium (ISM). We test these hypotheses with measurements of the stellar and nebular metallicities of three ETGs (NGC 2768, NGC 3245, and NGC 4694) from new long-slit, high signal-to-noise ratio spectroscopy from the Multi-Object Double Spectographs (MODs) on the Large Binocular Telescope (LBT). These ETGs have modest star formation rates and minimal evidence of nuclear activity. We model the stellar continuum to derive chemical abundances and measure gas-phase abundances with standard nebular diagnostics. We find that the stellar and gas-phase abundances are very similar, which supports internal production and is very inconsistent with the accretion of smaller, lower metallicity dwarfs. All three of these galaxies are also consistent with an extrapolation of the mass-metallicity relation to higher mass galaxies with lower specific star formation rates. The emission line flux ratios along the long-slit, as well as global line ratios clearly indicate that photoionization dominates and ionization by alternate sources including AGN activity, shocks, cosmic rays, dissipative magnetohydrodynamic waves, and single degenerate Type Ia supernovae progenitors do not significantly affect the line ratios.},
annote = {Griffith+2018

3 ETGs with LBT/MODS spectroscopy to measure abundances - stellar and gas phase abundances are similar, consistent with internal production of large amounts of cold gas and dust; photoionization dominates other sources of emission features.

Use 'alf' to model spectra
Emission line flux ratios do not vary much along the slits.
2 of 3 have abundances similar to previous ETG trends, the other is significantly different.},
archivePrefix = {arXiv},
arxivId = {1809.05114},
author = {Griffith, Emily and Martini, Paul and Conroy, Charlie},
eprint = {1809.05114},
file = {:Users/ben/Documents/Mendeley Desktop/Griffith, Martini, Conroy - 2018 - A Comparison of Stellar and Gas-Phase Chemical Abundances in Dusty Early-Type Galaxies.pdf:pdf},
keywords = {abundances,cd,elliptical and lenticular,evolution,galaxies,general,ism,stellar content},
month = {sep},
number = {September},
pages = {1--12},
title = {{A Comparison of Stellar and Gas-Phase Chemical Abundances in Dusty Early-Type Galaxies}},
url = {https://arxiv.org/abs/1809.05114 http://arxiv.org/abs/1809.05114},
volume = {12},
year = {2018}
}
@article{Gao2017,
abstract = {The development of two-dimensional (2D) bulge-to-disk decomposition techniques has shown their advantages over traditional one-dimensional (1D) techniques, especially for galaxies with non-axisymmetric features. However, the full potential of 2D techniques has yet to be fully exploited. Secondary morphological features in nearby disk galaxies, such as bars, lenses, rings, disk breaks, and spiral arms, are seldom accounted for in 2D image decompositions, even though some image-fitting codes, such as GALFIT, are capable of handling them. We present detailed, 2D multi-model and multi-component decomposition of high-quality {\$}R{\$}-band images of a representative sample of nearby disk galaxies selected from the Carnegie-Irvine Galaxy Survey, using the latest version of GALFIT. The sample consists of five barred and five unbarred galaxies, spanning Hubble types from S0 to Sc. Traditional 1D decomposition is also presented for comparison. In detailed case studies of the 10 galaxies, we successfully model the secondary morphological features. Through a comparison of best-fit parameters obtained from different input surface brightness models, we identify morphological features that significantly impact bulge measurements. We show that nuclear and inner lenses/rings and disk breaks must be properly taken into account to obtain accurate bulge parameters, whereas outer lenses/rings and spiral arms have a negligible effect. We provide an optimal strategy to measure bulge parameters of typical disk galaxies, as well as prescriptions to estimate realistic uncertainties of them, which will benefit subsequent decomposition of a larger galaxy sample.},
archivePrefix = {arXiv},
arxivId = {1709.00746},
author = {Gao, Hua and Ho, Luis C.},
doi = {10.3847/1538-4357/aa7da4},
eprint = {1709.00746},
file = {:Users/ben/Documents/Mendeley Desktop/1709.00746.pdf:pdf},
issn = {0004-637X},
title = {{An Optimal Strategy for Accurate Bulge-to-disk Decomposition of Disk Galaxies}},
url = {http://arxiv.org/abs/1709.00746{\%}0Ahttp://dx.doi.org/10.3847/1538-4357/aa7da4},
year = {2017}
}
