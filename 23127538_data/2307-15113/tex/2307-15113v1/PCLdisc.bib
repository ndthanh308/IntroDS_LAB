@article{Whitaker2017,
abstract = {The total star formation budget of galaxies consists of the sum of the unobscured star formation, as observed in the rest-frame ultraviolet (UV), together with the obscured component that is absorbed and re-radiated by dust grains in the infrared. We explore how the fraction of obscured star formation depends on stellar mass for mass-complete samples of galaxies at {\$}0{\textless}z{\textless}2.5{\$}. We combine GALEX and WISE photometry for SDSS-selected galaxies with the 3D-HST treasury program and Spitzer/MIPS 24{\$}\backslashmu{\$}m photometry in the well-studied 5 extragalactic CANDELS fields. We find a strong dependence of the fraction of obscured star formation ({\$}f{\_}{\{}\backslashmathrm{\{}obscured{\}}{\}}{\$}=SFR{\$}{\_}{\{}\backslashmathrm{\{}IR{\}}{\}}{\$}/SFR{\$}{\_}{\{}\backslashmathrm{\{}UV+IR{\}}{\}}{\$}) on stellar mass, with remarkably little evolution in this fraction with redshift out to {\$}z{\$}=2.5. 50$\backslash${\%} of star formation is obscured for galaxies with log(M/M{\$}{\_}{\{}\backslashodot{\}}{\$})=9.4; although unobscured star formation dominates the budget at lower masses, there exists a tail of low mass extremely obscured star-forming galaxies at {\$}z{\textgreater}1{\$}. For log(M/M{\$}{\_}{\{}\backslashodot{\}}{\$}){\$}{\textgreater}{\$}10.5, {\$}{\textgreater}{\$}90$\backslash${\%} of star formation is obscured at all redshifts. We also show that at fixed total SFR, {\$}f{\_}{\{}\backslashmathrm{\{}obscured{\}}{\}}{\$} is lower at higher redshift. At fixed mass, high-redshift galaxies are observed to have more compact sizes and much higher star formation rates, gas fractions and hence surface densities (implying higher dust obscuration), yet we observe no redshift evolution in {\$}f{\_}{\{}\backslashmathrm{\{}obscured{\}}{\}}{\$} with stellar mass. This poses a challenge to theoretical models to reproduce, where the observed compact sizes at high redshift seem in tension with lower dust obscuration.},
archivePrefix = {arXiv},
arxivId = {1710.06872},
author = {Whitaker, Katherine E. and Pope, Alexandra and Cybulski, Ryan and Casey, Caitlin M. and Popping, Gerg{\"{o}} and Yun, Min S.},
