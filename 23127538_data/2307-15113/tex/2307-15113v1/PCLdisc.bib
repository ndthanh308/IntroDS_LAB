issn = {1432-0746},
journal = {Astronomy and Astrophysics},
keywords = {galaxies: ISM,galaxies: abundances,galaxies: starburst},
pages = {955},
primaryClass = {astro-ph},
title = {{The chemical composition of metal-poor emission-line galaxies in the Data Release 3 of the Sloan Digital Sky Survey}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2006A{\&}A...448..955I{\&}link{\_}type=ABSTRACT{\%}5Cnpapers://939e88c8-a723-4abe-bb43-ccd202439e84/Paper/p11025},
volume = {448},
