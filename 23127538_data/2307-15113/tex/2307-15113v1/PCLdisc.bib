keywords = {galaxy evolution,quenching,quiescent galaxies},
month = {sep},
pages = {A32},
title = {{HST grism spectroscopy of z ∼ 3 massive quiescent galaxies}},
url = {http://arxiv.org/abs/2012.02767 https://www.aanda.org/10.1051/0004-6361/202040067},
volume = {653},
year = {2021}
}
@article{Song2016,
abstract = {We report the results from some of the deepest Keck/MOSFIRE data yet obtained for candidate {\$}z \backslashgtrsim 7{\$} galaxies. Our data show one significant line detection with 6.5{\$}\backslashsigma{\$} significance in our combined 10 hours of integration which is independently detected on more than one night, ruling out the possibility that the detection is spurious. The asymmetric line profile and non-detection in the optical bands strongly imply that the detected line is Ly{\$}\backslashalpha{\$} emission from a galaxy at {\$}z{\$}(Ly{\$}\backslashalpha)=7.6637 \backslashpm 0.0011{\$}, making it the fourth spectroscopically confirmed galaxy at {\$}z{\textgreater}7.5{\$}. This galaxy is bright in the rest-frame ultraviolet (UV; {\$}M{\_}{\{}\backslashrm UV{\}} \backslashsim -21.2{\$}) with a moderately blue UV slope ({\$}\backslashbeta=-2.2{\^{}}{\{}+0.3{\}}{\_}{\{}-0.2{\}}{\$}), and exhibits a rest-frame Ly{\$}\backslashalpha{\$} equivalent width of EW(Ly{\$}\backslashalpha{\$}) {\$}\backslashsim 15.6{\^{}}{\{}+5.6{\}}{\_}{\{}-3.6{\}}{\$} $\backslash$AA. The non-detection of the 11 other {\$}z \backslashsim{\$} 7--8 galaxies in our long 10 hr integration, reaching a median 5{\$}\backslashsigma{\$} sensitivity of 28 $\backslash$AA$\backslash$ in the rest-frame EW(Ly{\$}\backslashalpha{\$}), implies a 1.3{\$}\backslashsigma{\$} deviation from the null hypothesis of a non-evolving distribution in the rest-frame EW(Ly{\$}\backslashalpha{\$}) between {\$}3{\textless}z{\textless}6{\$} and {\$}z={\$} 7--8. Our results are consistent with previous studies finding a decline in Ly{\$}\backslashalpha{\$} emission at {\$}z{\textgreater}6.5{\$}, which may signal the evolving neutral fraction in the intergalactic medium at the end of the reionization epoch, although our weak evidence suggests the need for a larger statistical sample to allow for a more robust conclusion.},
