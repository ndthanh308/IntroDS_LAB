author = {Trump, Jonathan R. and Konidaris, Nicholas P. and Barro, Guillermo and Koo, David C. and Kocevski, Dale D. and Juneau, St{\'{e}}phanie and Weiner, Benjamin J. and Faber, S. M. and McLean, Ian S. and Yan, Renbin and P{\'{e}}rez-Gonz{\'{a}}lez, Pablo G. and Villar, Victor},
doi = {10.1088/2041-8205/763/1/L6},
eprint = {1212.2218},
file = {:Users/ben/Documents/Mendeley Desktop/1212.2218.pdf:pdf},
issn = {20418205},
