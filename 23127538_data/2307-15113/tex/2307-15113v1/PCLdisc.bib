keywords = {ac,andrea,corresponding author,evolution,formation,galaxies,high-redshift,interac-,jp,nao,silva,silva et al,star formation,starburst,tions},
title = {{Galaxy mergers up to z{\textless}2.5 I : The star formation properties of merging galaxies at separations 3-15 kpc}},
