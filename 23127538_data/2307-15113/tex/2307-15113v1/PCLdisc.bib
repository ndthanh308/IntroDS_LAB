arxivId = {arXiv:1403.0743v1},
author = {Castellano, M and Sommariva, V and Fontana, A and Pentericci, L and Santini, P and Grazian, A and Amorin, R and Donley, J L and Dunlop, J S and Ferguson, H C and Fiore, F and Galametz, A and Giallongo, E and Guo, Yicheng and Huang, Kuang-Han and Maiolino, R and McLure, R J and Paris, D and Schaerer, D and Troncoso, P and Vanzella, E},
doi = {10.1051/0004-6361/201322704},
eprint = {arXiv:1403.0743v1},
file = {:Users/ben/Documents/Mendeley Desktop/Castellano et al. - 2014 - Constraints on the star-formation rate of z{\~{}}3 LBGs with measured metallicity in the CANDELS GOODS-South field.pdf:pdf},
