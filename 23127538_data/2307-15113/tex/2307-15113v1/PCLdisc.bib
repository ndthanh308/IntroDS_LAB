}
@article{Onodera2020,
abstract = {We present the selection, spectroscopic identification, and physical properties of extreme emission line galaxies (EELGs) at {\$}3{\textless}z{\textless}3.7{\$} aiming at studying physical properties of an analog population of star-forming galaxies (SFGs) at the epoch of reionization. The sample is selected based on the excess in the observed Ks broad band flux relative to the best-fit stellar continuum model flux. By applying a 0.3 mag excess as a primary criterion, we select 240 EELG candidates with intense emission lines and estimated observed-frame equivalent width (EW) of {\$}\backslashgtrsim 1000{\$} angstrom over the UltraVISTA-DR2 ultra-deep stripe in the COSMOS field. We then carried out a HK band follow-up spectroscopy for 23 of the candidates with Subaru/MOIRCS, and find that 19 and two of them are at {\$}z{\textgreater}3{\$} with intense [OIII] emission, and H{\$}\backslashalpha{\$} emitters at {\$}z\backslashsimeq 2{\$}, respectively. These spectroscopically identified EELGs at {\$}z\backslashsimeq 3.3{\$} show, on average, higher specific star formation rates (sSFR) than the star-forming main sequence, low dust attenuation of {\$}E(B-V) \backslashlesssim 0.1{\$} mag, and high [OIII]/[OII] ratios of {\$}\backslashgtrsim 3{\$}. We also find that our EELGs at {\$}z\backslashsimeq 3.3{\$} have higher hydrogen ionizing photon production efficiencies ({\$}\backslashxi{\_}\backslashmathrm{\{}ion{\}}{\$}) than the canonical value ({\$}\backslashsimeq 10{\^{}}{\{}25.2{\}}{\$} Hz/erg), indicating that they are efficient in ionizing their surrounding interstellar medium. These physical properties suggest that they are low metallicity galaxies with higher ionization parameters and harder UV spectra than normal SFGs, which is similar to galaxies with Lyman continuum (LyC) leakage. Among our EELGs, those with the largest [OIII]/[OII] and EW([OIII]) values would be the most promising candidates to search for LyC leakage.},
archivePrefix = {arXiv},
arxivId = {2010.07545},
author = {Onodera, Masato and Shimakawa, Rhythm and Suzuki, Tomoko L. and Tanaka, Ichi and Harikane, Yuichi and Hayashi, Masao and Kodama, Tadayuki and Koyama, Yusei and Nakajima, Kimihiko and Shibuya, Takatoshi},
eprint = {2010.07545},
file = {:Users/ben/Documents/Mendeley Desktop/2010.07545.pdf:pdf},
keywords = {1383,459,594,595,615,734,emission line galaxies,galaxy evolution,galaxy forma-,galaxy properties,high-redshift galaxies,reionization,tion},
title = {{Broad-band selection, spectroscopic identification, and physical properties of a population of extreme emission line galaxies at 3{\textless}z{\textless}3.7}},
