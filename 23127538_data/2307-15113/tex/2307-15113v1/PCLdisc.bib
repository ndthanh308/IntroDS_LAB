title = {{Quantifying the UV-continuum slopes of galaxies to z ∼ 10 using deep Hubble + Spitzer /IRAC observations}},
url = {http://arxiv.org/abs/1510.01514 http://dx.doi.org/10.1093/mnras/stv2263 http://mnras.oxfordjournals.org/lookup/doi/10.1093/mnras/stv2263},
volume = {455},
