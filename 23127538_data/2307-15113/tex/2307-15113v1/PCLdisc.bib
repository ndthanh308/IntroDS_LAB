journal = {The Astrophysical Journal},
number = {1},
pages = {57},
title = {{Luminosity Dependence and Redshift Evolution of Strong Emission-Line Diagnostics in Star-Forming Galaxies}},
url = {http://arxiv.org/abs/1512.0017{\%}5Cnhttp://stacks.iop.org/0004-637X/817/i=1/a=57?key=crossref.7daaa18a9451381c6d272142aa9d5854},
volume = {817},
