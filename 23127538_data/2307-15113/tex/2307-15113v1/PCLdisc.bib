title = {{Velocity Evolution and the Intrinsic Color of Type Ia Supernovae}},
url = {http://stacks.iop.org/0004-637X/742/i=2/a=89?key=crossref.ff921c83e2dd3f42983d39f23c9a5433 http://arxiv.org/abs/1107.3555},
