@article{Rezaee2022,
abstract = {It has long been uncertain how effective the dust-corrected H{\$}\backslashalpha{\$}-to-UV luminosity ratio ({\$}L(\backslashrm H\backslashalpha)/L(\backslashrm UV){\$}) is in probing bursty SFHs for high-redshift galaxies. To address this issue, we present a statistical analysis of the resolved distribution of star-formation-rate surface density ({\$}\backslashSigma{\_}{\{}\backslashrm SFR{\}}{\$}) as well as stellar age and their correlations with the globally measured {\$}L(\backslashrm H\backslashalpha)/L(\backslashrm UV){\$} for a sample of 310 star-forming galaxies in two redshift bins of {\$}1.37{\textless}z{\textless}1.70{\$} and {\$}2.09{\textless}z{\textless}2.61{\$} observed by the MOSDEF survey. We use the multi-waveband CANDELS/3D-HST imaging of MOSDEF galaxies to construct {\$}\backslashSigma{\_}{\{}\backslashrm SFR{\}}{\$} and stellar age maps. We also analyze the composite rest-frame far-UV spectra of a subsample of MOSDEF targets obtained by the Keck-LRIS, which includes 124 star-forming galaxies at redshifts {\$}1.4{\textless}z{\textless}2.6{\$}, to examine the average stellar population properties, and the strength of age-sensitive FUV spectral features in bins of {\$}L(\backslashrm H\backslashalpha)/L(\backslashrm UV){\$}. We find no evidence that galaxies with higher {\$}L(\backslashrm H\backslashalpha)/L(\backslashrm UV){\$} are undergoing a burst of star formation based on the distribution of {\$}\backslashSigma{\_}{\{}\backslashrm SFR{\}}{\$} and stellar age as well as the strengths of Siiv{\$}\backslashlambda\backslashlambda1393,1402{\$} and Civ{\$}\backslashlambda\backslashlambda1548,1550{\$} P-Cygni features from massive stars. Our results suggest that the {\$}L(\backslashrm H\backslashalpha)/L(\backslashrm UV){\$} ratio is not a reliable tracer of bursty SFHs for typical star-forming galaxies at high redshift. We also study the variations observed in the strength of the nebular Heii{\$}\backslashlambda1640{\$} emission between the {\$}L(\backslashrm H\backslashalpha)/L(\backslashrm UV){\$} subsamples. We find that variations in the upper-mass limit of the IMF cannot fully account for all the observed difference in the HeII emission between the {\$}L(\backslashrm H\backslashalpha)/L(\backslashrm UV){\$} subsamples, and that another source of He{\$}{\^{}}{\{}+{\}}{\$} ionizing photons, such as X-ray binaries, may be needed to explain such a difference.},
