@article{Williams2020,
abstract = {We present ALMA CO(2-1) spectroscopy of 6 massive (log{\$}{\_}{\{}10{\}}{\$}M{\$}{\_}{\{}\backslashrm{\{}*{\}}{\}}/\backslashrm{\{}M{\}}{\_}\backslashodot{\textgreater}{\$}11.3) quiescent galaxies at {\$}z\backslashsim1.5{\$}. These data represent the largest sample using CO emission to trace molecular gas in quiescent galaxies above {\$}z{\textgreater}1{\$}, achieving an average 3{\$}\backslashsigma{\$} sensitivity of M{\$}{\_}{\{}\backslashrm{\{}H{\_}{\{}2{\}}{\}}{\}}\backslashsim10{\^{}}{\{}10{\}}\backslashrm{\{}M{\}}{\_}\backslashodot{\$}. We detect one galaxy at 4{\$}\backslashsigma{\$} significance and place upper limits on the molecular gas reservoirs of the other 5, finding molecular gas mass fractions M{\$}{\_}{\{}\backslashrm{\{}H{\_}{\{}2{\}}{\}}{\}}{\$}/M{\$}{\_}{\{}\backslashrm{\{}*{\}}{\}}{\$}=f{\$}{\_}{\{}\backslashrm{\{}H{\_}{\{}2{\}}{\}}{\}}{\textless}2-6{\$}{\%} (3{\$}\backslashsigma{\$} upper limits). This is 1-2 orders of magnitude lower than coeval star-forming galaxies at similar stellar mass, and comparable to galaxies at {\$}z=0{\$} with similarly low sSFR. This indicates that their molecular gas reservoirs were rapidly and efficiently used up or destroyed, and that gas fractions are uniformly low ({\$}{\textless}{\$}6{\%}) despite the structural diversity of our sample. The implied rapid depletion time of molecular gas (t{\$}{\_}{\{}\backslashrm{\{}dep{\}}{\}}{\textless}0.6{\$} Gyr) disagrees with extrapolations of empirical scaling relations to low sSFR. We find that our low gas fractions are instead in agreement with predictions from both the recent SIMBA cosmological simulation, and from analytical "bathtub" models for gas accretion onto galaxies in massive dark matter halos (log{\$}{\_}{\{}10{\}}M{\_}{\{}\backslashrm{\{}halo{\}}{\}}/\backslashrm{\{}M{\}}{\_}\backslashodot\backslashsim14{\$} at {\$}z=0{\$}). Such high mass halos reach a critical mass of log{\$}{\_}{\{}10{\}}M{\_}{\{}\backslashrm{\{}halo{\}}{\}}/\backslashrm{\{}M{\}}{\_}\backslashodot{\textgreater}12{\$} by {\$}z\backslashsim4{\$} that halt the accretion of baryons early in the Universe. Our data is consistent with a simple picture where galaxies truncate accretion and then consume the existing gas at or faster than typical main sequence rates. Alternatively, we cannot rule out that these galaxies reside in lower mass halos, and low gas fractions may instead reflect either stronger feedback, or more efficient gas consumption.},
archivePrefix = {arXiv},
arxivId = {2012.01433},
author = {Williams, Christina C. and Spilker, Justin S. and Whitaker, Katherine E. and Dav{\'{e}}, Romeel and Woodrum, Charity and Brammer, Gabriel and Bezanson, Rachel and Narayanan, Desika and Weiner, Benjamin},
eprint = {2012.01433},
file = {:Users/ben/Documents/Mendeley Desktop/2012.01433.pdf:pdf},
pages = {1--21},
title = {{ALMA measures rapidly depleted molecular gas reservoirs in massive quiescent galaxies at z{\~{}}1.5}},
url = {http://arxiv.org/abs/2012.01433},
year = {2020}
}
@article{Puglisi2016,
abstract = {We combined the spectroscopic information from the 3D-HST survey with the PEP/Herschel data to characterize the H$\backslash$alpha dust attenuation properties of a sample of 79 normal star-forming galaxies at {\$}0.7\backslashleq z\backslashleq1.5{\$} in the GOODS-S field. The sample was selected in the far-IR, at $\backslash$lambda=100 and/or 160 $\backslash$mu m, and only includes galaxies with a secure H$\backslash$alpha detection (S/N{\textgreater}3). From the low resolution 3D-HST spectra we measured z and F(H$\backslash$alpha) for the whole sample, rescaling the observed flux by a constant factor of 1.2 to remove the contamination by [NII]. The stellar masses, infrared and UV luminosities were derived from the SEDs by fitting multi-band data from GALEX near-UV to SPIRE500 $\backslash$mu m. We derived the continuum extinction Estar(B-V) from both the IRX ratio and the UV-slope, and found an excellent agreement among them. Galaxies in the sample have 2.6x10{\^{}}9{\$}\backslashleq{\$}M*{\$}\backslashleq{\$}3.5x10{\^{}}11 Msun, intense infrared luminosity (L{\_}IR{\textgreater}1.2x10{\^{}}10 Lsun), high level of dust obscuration (0.1{\$}\backslashleq{\$}Estar(B-V){\$}\backslashleq{\$}1.1) and strong H$\backslash$alpha emission (typical observed fluxes Fobs(H$\backslash$alpha){\$}\backslashgeq{\$}4.1x10{\^{}}-17 erg/s/cm2). The nebular extinction was estimated by comparing the observed SFR{\_}H$\backslash$alpha and the SFR{\_}UV. We obtained f=Estar(B-V)/Eneb(B-V)=0.93{\$}\backslashpm{\$}0.06, i.e. higher than the value measured in the local Universe. This result could be partially due to the adopted selection criteria, picking up the most obscured but also the H$\backslash$alpha brightest sources. The derived dust correction produces a good agreement between H$\backslash$alpha and IR+UV SFRs for objects with SFR{\$}\backslashgtrsim{\$}20 Msun/yr and M*{\$}\backslashgtrsim{\$}5x10{\^{}}10 Msun, while sources with lower SFR and M* seem to require a smaller f-factor (i.e. higher H$\backslash$alpha extinction correction). Our results then imply that for our sample the nebular and optical-UV extinctions are comparable and suggest that the f-factor is a function of both M* and SFR, according with previous studies.},
