issn = {2041-8205},
journal = {The Astrophysical Journal},
keywords = {galaxies,galaxies: star formation,galaxies: stellar content,general,star clusters,star formation,stellar content},
number = {1},
pages = {L14},
publisher = {IOP Publishing},
title = {{Initial Mass Function Variability (or Not) among Low-velocity Dispersion, Compact Stellar Systems}},
url = {http://dx.doi.org/10.3847/2041-8213/aa970f},
