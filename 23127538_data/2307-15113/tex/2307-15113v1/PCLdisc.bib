issn = {2041-8213},
journal = {The Astrophysical Journal},
month = {jan},
number = {2},
pages = {L21},
title = {{THE RELATION BETWEEN GALAXY STRUCTURE AND SPECTRAL TYPE: IMPLICATIONS FOR THE BUILDUP OF THE QUIESCENT GALAXY POPULATION AT 0.5 {\textless} z {\textless} 2.0}},
url = {http://arxiv.org/abs/1601.02629 http://dx.doi.org/10.3847/2041-8205/817/2/L21 http://stacks.iop.org/2041-8205/817/i=2/a=L21?key=crossref.a075a8fd943412510cdafd5013001c28},
volume = {817},
