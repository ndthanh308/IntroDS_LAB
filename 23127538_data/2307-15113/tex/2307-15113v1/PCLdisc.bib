@article{Whitaker2014,
abstract = {We constrain the slope of the star formation rate ({\$}\backslashlog\backslashPsi{\$}) to stellar mass ({\$}\backslashlog\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}{\}}{\$}) relation down to {\$}\backslashlog(\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}/M{\_}{\{}\backslashodot{\}}{\}})=8.4{\$} ({\$}\backslashlog(\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}/M{\_}{\{}\backslashodot{\}}{\}})=9.2{\$}) at {\$}z=0.5{\$} ({\$}z=2.5{\$}) with a mass-complete sample of 39,106 star-forming galaxies selected from the 3D-HST photometric catalogs, using deep photometry in the CANDELS fields. For the first time, we find that the slope is dependent on stellar mass, such that it is steeper at low masses ({\$}\backslashlog\backslashmathrm{\{}\backslashPsi{\}}\backslashpropto\backslashlog\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}{\}}{\$}) than at high masses ({\$}\backslashlog\backslashmathrm{\{}\backslashPsi{\}}\backslashpropto(0.3-0.6)\backslashlog\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}{\}}{\$}). These steeper low mass slopes are found for three different star formation indicators: the combination of the ultraviolet (UV) and infrared (IR), calibrated from a stacking analysis of Spitzer/MIPS 24{\$}\backslashmu{\$}m imaging; {\$}\backslashbeta{\$}-corrected UV SFRs; and H{\$}\backslashalpha{\$} SFRs. The normalization of the sequence evolves differently in distinct mass regimes as well: for galaxies less massive than {\$}\backslashlog(\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}/M{\_}{\{}\backslashodot{\}}{\}}){\textless}10{\$} the specific SFR ({\$}\backslashPsi/\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}{\}}{\$}) is observed to be roughly self-similar with {\$}\backslashPsi/\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}{\}}\backslashpropto(1+z){\^{}}{\{}1.9{\}}{\$}, whereas more massive galaxies show a stronger evolution with {\$}\backslashPsi/\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}{\}}\backslashpropto(1+z){\^{}}{\{}2.2-3.5{\}}{\$} for {\$}\backslashlog(\backslashmathrm{\{}M{\_}{\{}\backslashstar{\}}/M{\_}{\{}\backslashodot{\}}{\}})=10.2-11.2{\$}. The fact that we find a steep slope of the star formation sequence for the lower mass galaxies will help reconcile theoretical galaxy formation models with the observations. The results of this study support the analytical conclusions of Leja et al. (2014).},
