@article{Farihi2013,
abstract = {The existence of water in extrasolar planetary systems is of great interest as it constrains the potential for habitable planets and life. Here, we report the identification of a circumstellar disk that resulted from the destruction of a water-rich and rocky, extrasolar minor planet. The parent body formed and evolved around a star somewhat more massive than the Sun, and the debris now closely orbits the white dwarf remnant of the star. The stellar atmosphere is polluted with metals accreted from the disk, including oxygen in excess of that expected for oxide minerals, indicating the parent body was originally composed of 26{\%} water by mass. This finding demonstrates that water-bearing planetesimals exist around A- and F-type stars that end their lives as white dwarfs.},
archivePrefix = {arXiv},
arxivId = {1310.3269},
author = {Farihi, J and G{\"{a}}nsicke, B. T. and Koester, D.},
doi = {10.1126/science.1239447},
eprint = {1310.3269},
file = {:Users/ben/Documents/Mendeley Desktop/Farihi, G{\"{a}}nsicke, Koester - 2013 - Evidence for Water in the Rocky Debris of a Disrupted Extrasolar Minor Planet.pdf:pdf},
month = {oct},
pages = {1--17},
title = {{Evidence for Water in the Rocky Debris of a Disrupted Extrasolar Minor Planet}},
url = {http://arxiv.org/abs/1310.3269},
year = {2013}
}
@article{Chartab2019,
abstract = {We present a robust method, weighted von Mises kernel density estimation, along with boundary correction to reconstruct the underlying number density field of galaxies. We apply this method to galaxies brighter than {\$}\backslashrm HST/F160w\backslashle 26{\$} AB mag at the redshift range of {\$}0.4\backslashleq z \backslashleq 5{\$} in the five CANDELS fields (GOODS-N, GOODS-S, EGS, UDS, and COSMOS). We then use these measurements to explore the environmental dependence of the star formation activity of galaxies. We find strong evidence of environmental quenching for massive galaxies ({\$}\backslashrm M \backslashgtrsim 10{\^{}}{\{}11{\}} \backslashrm {\{}M{\}}{\_}\backslashodot{\$}) out to {\$}z\backslashsim 3.5{\$} such that an over-dense environment hosts {\$}\backslashgtrsim 20\backslash{\%}{\$} more massive quiescent galaxies compared to an under-dense region. We also find that environmental quenching efficiency grows with stellar mass and reaches {\$}\backslashsim 60\backslash{\%}{\$} for massive galaxies at {\$}z\backslashsim 0.5{\$}. The environmental quenching is also more efficient in comparison to the stellar mass quenching for low mass galaxies ({\$}\backslashrm M \backslashlesssim 10{\^{}}{\{}10{\}} \backslashrm {\{}M{\}}{\_}\backslashodot{\$}) at low and intermediate redshifts ({\$}z\backslashlesssim 1.2{\$}). Our findings concur thoroughly with the "over-consumption" quenching model where the termination of cool gas accretion (cosmological starvation) happens in an over-dense environment and the galaxy starts to consume its remaining gas reservoir in depletion time. The depletion time depends on the stellar mass and could explain the evolution of environmental quenching efficiency with the stellar mass.},
archivePrefix = {arXiv},
arxivId = {1912.04890},
author = {Chartab, Nima and Mobasher, Bahram and Darvish, Behnam and Finkelstein, Steven L. and Guo, Yicheng and Kodra, Dritan and Lee, Kyoung-Soo and Newman, Jeffrey A. and Pacifici, Camilla and Papovich, Casey and Sattari, Zahra and Shahidi, Abtin and Dickinson, Mark E. and Faber, Sandra M. and Ferguson, Henry C. and Giavalisco, Mauro and Jafariyazani, Marziye},
eprint = {1912.04890},
file = {:Users/ben/Documents/Mendeley Desktop/1912.04890.pdf:pdf},
keywords = {galaxy environment,large-scale structures of the,star formation activity,universe},
title = {{Large Scale Structures in the CANDELS Fields: The Role of the Environment in Star Formation Activity}},
url = {http://arxiv.org/abs/1912.04890},
year = {2019}
}
@article{Bouwens2022,
abstract = {Here we present the first constraints on the prevalence of z{\textgreater}10 galaxies in the Hubble Ultra Deep Field (HUDF) leveraging new NIRCam medium-band observations taken with JWST. These NIRCam observations probe redward of 1.6microns, beyond the wavelength limit of HST, allowing us to search for galaxies to z{\textgreater}10. These observations indicate that the highest redshift candidate identified over the HUDF with HST, UDFj-39546284, has a redshift of z=12.0+/-0.1, as had been suggested in multiple analyses of the HUDF12/XDF data. This source thus appears to be the most distant galaxy discovered by HST in its more than 30 years of operation. Additionally, we identify nine other z{\~{}}8-13 candidate galaxies over the HUDF, two of which are entirely new discoveries and appear to lie at z{\~{}}11 and z{\~{}}12. We use these results to characterize the evolution of the UV luminosity function (LF) from z{\~{}}15 to z{\~{}}8.7. While our LF results at z{\~{}}8.7 and z{\~{}}10.5 are consistent with previous findings over the HUDF, our new LF estimates at z{\~{}}12.6 are substantially higher than other results in the literature, potentially pointing to a milder evolution in the UV luminosity density from z{\~{}}12.6. We emphasize that our LF results are uncertain given the small number of sources in our z{\~{}}12.6 selection and limited volume probed. The new NIRCam data also indicate that the faint z{\~{}}8-13 galaxies in the HUDF/XDF show very blue UV-continuum slopes beta{\~{}}-2.7, high specific star formation rates {\~{}}24.5 Gyr{\$}{\^{}}{\{}-1{\}}{\$}, and high EW ({\~{}}1300A) [OIII]+Hbeta emission, with two z{\~{}}8.5 sources showing [OIII]+Hbeta EWs of {\~{}}2300 Angstroms.},
archivePrefix = {arXiv},
arxivId = {2211.02607},
author = {Bouwens, Rychard J. and Stefanon, Mauro and Brammer, Gabriel and Oesch, Pascal A. and Herard-Demanche, Thomas and Illingworth, Garth D. and Matthee, Jorryt and Naidu, Rohan P. and van Dokkum, Pieter G. and van Leeuwen, Ivana F.},
eprint = {2211.02607},
file = {:Users/ben/Documents/Mendeley Desktop/2211.02607.pdf:pdf},
keywords = {dark ages,evolution,first stars,galaxies,high-redshift,reionization},
number = {November},
title = {{Evolution of the UV LF from z{\~{}}15 to z{\~{}}8 Using New JWST NIRCam Medium-Band Observations over the HUDF/XDF}},
url = {http://arxiv.org/abs/2211.02607},
year = {2022}
}
@article{Tran2017,
abstract = {We compare galaxy scaling relations as a function of environment at {\$}z\backslashsim2{\$} with our ZFIRE survey where we have measured H{\$}\backslashalpha{\$} fluxes for 90 star-forming galaxies selected from a mass-limited [{\$}\backslashlog(M{\_}{\{}\backslashstar{\}}/M{\_}{\{}\backslashodot{\}}){\textgreater}9{\$}] sample based on ZFOURGE. The cluster galaxies (37) are part of a confirmed system at z=2.095 and the field galaxies (53) are at {\$}1.9{\textless}z{\textless}2.4{\$}; all are in the COSMOS legacy field. There is no statistical difference between H{\$}\backslashalpha{\$}-emitting cluster and field populations when comparing their star formation rate (SFR), stellar mass ({\$}M{\_}{\{}\backslashstar{\}}{\$}), galaxy size ({\$}r{\_}{\{}eff{\}}{\$}), SFR surface density [{\$}\backslashSigma{\$}(H{\$}\backslashalpha{\_}{\{}star{\}}{\$})], and stellar age distributions. The only difference is that at fixed stellar mass, the H{\$}\backslashalpha{\$}-emitting cluster galaxies are {\$}\backslashlog(r{\_}{\{}eff{\}})\backslashsim0.1{\$} larger than in the field. Approximately 19{\%} of the H{\$}\backslashalpha{\$}-emitters in the cluster and 26{\%} in the field are IR-luminous ({\$}L{\_}{\{}IR{\}}{\textgreater}2\backslashtimes10{\^{}}{\{}11{\}} L{\_}{\{}\backslashodot{\}}{\$}). Because the LIRGs in our combined sample are {\$}\backslashsim5{\$} times more massive than the low-IR galaxies, their radii are {\$}\backslashsim70{\$}{\%} larger. To track stellar growth, we separate galaxies into those that lie above, on, and below the H{\$}\backslashalpha{\$} star-forming main sequence (SFMS) using {\$}\backslashDelta{\$}SFR{\$}(M{\_}{\{}\backslashstar{\}})=\backslashpm0.2{\$} dex. Galaxies above the SFMS (starbursts) tend to have higher H{\$}\backslashalpha{\$} SFR surface densities and younger light-weighted stellar ages compared to galaxies below the SFMS. Our results indicate that starbursts (+SFMS) in the cluster and field at {\$}z\backslashsim2{\$}