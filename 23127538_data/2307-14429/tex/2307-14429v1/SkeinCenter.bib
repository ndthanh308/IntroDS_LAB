@article{BECK199429,
title = {Representations of Quantum Groups at Even Roots of Unity},
journal = {Journal of Algebra},
volume = {167},
number = {1},
pages = {29-56},
year = {1994},
issn = {0021-8693},
doi = {https://doi.org/10.1006/jabr.1994.1174},
url = {https://www.sciencedirect.com/science/article/pii/S0021869384711744},
author = {J. Beck},
abstract = {Let g be a finite dimensional complex simple Lie algebra and U(g) its enveloping algebra. The quantum group of Drinfeld and Jimbo is a Hopf algebra denoted Uq(g) defined on Chevalley-like generators over C[q, q−1]. Through "specialization" of q at different ϵ ∈ C one obtains a parameterized family of Hopf algebras. When ϵ = 1 one recovers the classical universal enveloping algebra. Moreover, when ϵ is not a root of unity Lusztig (Adv. Math.70 (1988), 237-249) and Rosso, (Comm. Math. Phys. 117 (1988), 581-593) have shown independently that the representation theory of Uϵ(= Uϵ(g)) is analogous to that of U(g). When we fix ϵ a primitive root of unity the situation changes considerably. At odd roots of unity the representations of Uϵ are partitioned by conjugacy classes of the algebraic group G with Lie algebra g [DC-K]. This paper relates the representations of Uϵ at even roots of unity to conjugacy classes in the group G∨-the Langlands dual of G. The partition is somewhat finer than that at odd roots of unity and requires a more detailed analysis. This correspondence is then used to study the representations at even roots of unity. In particular, we obtain a "triangulability" result which allows us to calculate the degree of Uϵ using a deformation argument.}
}

