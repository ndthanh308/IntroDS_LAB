@inproceedings{oung_feasibility_2009,
	title = {Feasibility of a {Distributed} {Flight} {Array}},
	doi = {10.1109/CDC.2009.5400272},
	abstract = {This paper introduces the Distributed Flight Array (DFA) which is being developed at ETH Zurich. This multi-rotor platform consists of autonomous rotor modules that are able to drive, dock with their peers, and fly in a coordinated fashion. These modules are organized as distributed computational units with minimal sensory input. This is a complex system that is rich in dynamics with much room to explore various strategies of distributed estimation and control. In this paper, a simple distributed strategy for hover control is presented and feasibility of the DFA is demonstrated.},
