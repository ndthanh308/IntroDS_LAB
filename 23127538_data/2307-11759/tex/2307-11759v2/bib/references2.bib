	title = {Feedback design for {Harpy}: a test bed to inspect thruster-assisted legged locomotion},
	volume = {11425},
	shorttitle = {Feedback design for {Harpy}},
	url = {https://www.spiedigitallibrary.org/conference-proceedings-of-spie/11425/1142507/Feedback-design-for-Harpy--a-test-bed-to-inspect/10.1117/12.2558284.full},
