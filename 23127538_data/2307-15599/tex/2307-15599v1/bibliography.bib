	author = {Guilbaud, Fabien and Pham, Huyên},
	year = {2015},
	keywords = {market making, stochastic control, inventory risk, limit order book, marked point process, pro rata microstructure},
	pages = {545--575},
	file = {Full Text PDF:C\:\\Users\\Manolis\\Zotero\\storage\\N69BJBKX\\Guilbaud et Pham - 2015 - Optimal High-Frequency Trading in a Pro Rata Micro.pdf:application/pdf;Snapshot:C\:\\Users\\Manolis\\Zotero\\storage\\TUZVPKWQ\\mafi.html:text/html},
}

