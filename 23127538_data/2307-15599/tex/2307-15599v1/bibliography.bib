	note = {Publisher: INFORMS},
	keywords = {make–take fees, market making, principal–agent problem, stochastic control, actor–critic method, dark pools, deep reinforcement learning, Financial Engineering, regulation},
	pages = {727--749},
}

@article{barzykin_algorithmic_2023,
