   @incollection{interpretingQM,
    author = {Ruetsche, Laura},
    isbn = {9780199535408},
    title = "{192 Quantizing}",
    booktitle = "{Interpreting Quantum Theories}",
    publisher = {Oxford University Press},
    year = {2011},
    month = {06},
    abstract = "{This chapter explicates the Hamiltonian scheme for quantizing classical mechanical theories by finding a Hilbert space representation of the appropriate canonical commutation relations. The chapter also reviews the canonical anticommutation relations, which encapsulate the quantum mechanics of spin systems. Having catalogued reasons to regard unitary equivalence as a robust criterion of physical equivalence for quantum theories obtained by finding representations of the CCRs/CARs, the chapter presents a pair of theorems—the Stone-von Neumann and Jordan-Wigner theorems—that establish that, provided only finitely many degrees of freedom are involved, all representations of the CCRs/CARs for a given quantum theory are unitarily (and so presumptively physically) equivalent.}",
    doi = {10.1093/acprof:oso/9780199535408.003.0002}
}

