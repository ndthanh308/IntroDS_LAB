@article{HendershottMenkveld2014,
title = {Price pressures},
journal = {Journal of Financial Economics},
volume = {114},
number = {3},
pages = {405-423},
year = {2014},
issn = {0304-405X},
doi = {https://doi.org/10.1016/j.jfineco.2014.08.001},
url = {https://www.sciencedirect.com/science/article/pii/S0304405X14001639},
author = {Terrence Hendershott and Albert J. Menkveld},
keywords = {Liquidity, Inventory risk, Intermediary, Volatility},
abstract = {We study price pressures, i.e., deviations from the efficient price due to risk-averse intermediaries supplying liquidity to asynchronously arriving investors. Empirically, New York Stock Exchange intermediary data reveals economically large price pressures, 0.49\% on average with a half life of 0.92 days. Theoretically, a simple dynamic inventory model captures an intermediary׳s use of price pressure to mean-revert inventory. She trades off revenue loss due to price pressure against price risk associated with staying in a nonzero inventory state. The closed-form solution identifies the intermediary׳s risk aversion and the investors׳ private value distribution from the observed time series patterns of prices and inventories. These parameters imply a relative social cost due to price pressure, a deviation from constrained Pareto efficiency, of approximately 10\% of the cost of immediacy.}
}

@article{Seppi1997,
    author = {Seppi, Duane J.},
    title = "{Liquidity Provision with Limit Orders and a Strategic Specialist}",
    journal = {The Review of Financial Studies},
    volume = {10},
    number = {1},
    pages = {103-150},
    year = {1997},
    month = {06},
    abstract = "{This article presents a microstructure model of liquidity provision in which a specialist with market power competes against a competitive limit order book. General solutions, comparative statics and examples are provided first with uninformative orders and then when order flows are informative. The model is also used to address two optimal market design issues. The first is the effect of “tick” size — for example, eighths versus decimal pricing — on market liquidity. Institutions trading large blocks have a larger optimal tick size than small retail investors, but both prefer a tick size strictly greater than zero. Second, a hybrid specialist/limit order market (like the NYSE) provides better liquidity to small retail and institutional trades, but a pure limit order market (like the Paris Bourse) may offer better liquidity on mid-size orders.}",
    issn = {0893-9454},
    doi = {10.1093/rfs/10.1.103},
