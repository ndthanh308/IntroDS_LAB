@article{BrogaardHendershottRiordan2019JF,
author = {Brogaard, Jonathan and Hendershott, Terrence and Riordan, Ryan},
title = {Price Discovery without Trading: Evidence from Limit Orders},
journal = {The Journal of Finance},
volume = {74},
number = {4},
pages = {1621-1658},
doi = {https://doi.org/10.1111/jofi.12769},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/jofi.12769},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/jofi.12769},
abstract = {ABSTRACT We analyze the contribution to price discovery of market and limit orders by high-frequency traders (HFTs) and non-HFTs. While market orders have a larger individual price impact, limit orders are far more numerous. This results in price discovery occurring predominantly through limit orders. HFTs submit the bulk of limit orders and these limit orders provide most of the price discovery. Submissions of limit orders and their contribution to price discovery fall with volatility due to changes in HFTs’ behavior. Consistent with adverse selection arising from faster reactions to public information, HFTs’ informational advantage is partially explained by public information.},
year = {2019}
}


@article{FoucaultMenkveld2008,
author = {Foucault, Thierry and Menkveld, Albert J.},
title = {Competition for Order Flow and Smart Order Routing Systems},
