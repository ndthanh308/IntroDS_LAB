pages = {99-134},
year = {1999},
issn = {1386-4181},
doi = {https://doi.org/10.1016/S1386-4181(98)00012-3},
url = {https://www.sciencedirect.com/science/article/pii/S1386418198000123},
author = {Thierry Foucault},
keywords = {Market microstructure, Limit order markets, Limit and market orders, Trading costs, Order flow composition},
abstract = {This article provides a game theoretic model of price formation and order placement decisions in a dynamic limit order market. Investors can choose to either post limit orders or submit market orders. Limit orders result in better execution prices but face a risk of non-execution and a winner's curse problem. Solving for the equilibrium of this dynamic game, closed-form solutions for the order placement strategies are obtained. Thus, testable implications for the cross-sectional behavior of the mix between market and limit orders and trading costs in limit order markets are derived.}
}

@article{GoettlerParlourRajan2005,
author = {Goettler, Ronald L. and Parlour, Christine and Rajan, Uday},
title = {Equilibrium in a Dynamic Limit Order Market},
journal = {The Journal of Finance},
volume = {60},
number = {5},
pages = {2149-2192},
doi = {https://doi.org/10.1111/j.1540-6261.2005.00795.x},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1540-6261.2005.00795.x},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1540-6261.2005.00795.x},
abstract = {ABSTRACT We model a dynamic limit order market as a stochastic sequential game with rational traders. Since the model is analytically intractable, we provide an algorithm based on Pakes and McGuire (2001) to find a stationary Markov-perfect equilibrium. We then generate artificial time series and perform comparative dynamics. Conditional on a transaction, the midpoint of the quoted prices is not a good proxy for the true value. Further, transaction costs paid by market order submitters are negative on average, and negatively correlated with the effective spread. Reducing the tick size is not Pareto improving but increases total investor surplus.},
year = {2005}
}

@article{OHara2015,
title = {High frequency market microstructure},
journal = {Journal of Financial Economics},
volume = {116},
