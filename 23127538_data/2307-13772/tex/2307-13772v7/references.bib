volume = {63},
number = {1},
pages = {119-158},
doi = {https://doi.org/10.1111/j.1540-6261.2008.01312.x},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1540-6261.2008.01312.x},
