@article{PlambeckSiegel2008,
title = {Misère quotients for impartial games},
journal = {Journal of Combinatorial Theory, Series A},
volume = {115},
number = {4},
pages = {593-622},
year = {2008},
issn = {0097-3165},
doi = {https://doi.org/10.1016/j.jcta.2007.07.008},
url = {https://www.sciencedirect.com/science/article/pii/S0097316507001045},
author = {Thane E. Plambeck and Aaron N. Siegel},
keywords = {Combinatorial game, Impartial game, Misere game, Misere play, Misere quotient, Commutative monoid},
abstract = {We announce misère-play solutions to several previously-unsolved combinatorial games. The solutions are described in terms of misère quotients—commutative monoids that encode the additive structure of specific misère-play games. We also introduce several advances in the structure theory of misère quotients, including a connection between the combinatorial structure of normal and misère play.}
}

@article{DuRi2010,
title = {Invariant games},
journal = {Theoretical Computer Science},
volume = {411},
number = {34},
pages = {3169-3180},
year = {2010},
issn = {0304-3975},
doi = {https://doi.org/10.1016/j.tcs.2010.05.007},
url = {https://www.sciencedirect.com/science/article/pii/S0304397510002884},
author = {Eric Duchêne and Michel Rigo},
keywords = {Combinatorial games, Combinatorics on words, Beatty sequences},
abstract = {In the context of 2-player removal games, we define the notion of invariant game for which each allowed move is independent of the position it is played from. We present a family of invariant games which are variations of Wythoff’s game. The set of P-positions of these games is given by a pair of complementary Beatty sequences related to the irrational quadratic number αk=(1;1,k¯). We also provide a recursive characterization of this set.}
}

@article{LaHeFr2011,
title = {Invariant and dual subtraction games resolving the Duchêne–Rigo conjecture},
journal = {Theoretical Computer Science},
volume = {412},
number = {8},
pages = {729-735},
year = {2011},
issn = {0304-3975},
doi = {https://doi.org/10.1016/j.tcs.2010.11.015},
url = {https://www.sciencedirect.com/science/article/pii/S0304397510006262},
author = {Urban Larsson and Peter Hegarty and Aviezri S. Fraenkel},
keywords = {Beatty sequence, Complementary sequences, Impartial game, Invariant game, Superadditivity},
abstract = {We prove a recent conjecture of Duchêne and Rigo, stating that every complementary pair of homogeneous Beatty sequences represents the solution to an invariant impartial game. Here invariance means that each available move in a game can be played anywhere inside the game board. In fact, we establish such a result for a wider class of pairs of complementary sequences, and in the process generalize the notion of a subtraction game. Given a pair of complementary sequences (an) and (bn) of positive integers, we define a game G by setting {{an,bn}} as invariant moves. We then introduce the invariant game G⋆, whose moves are all non-zero P-positions of G. Provided the set of non-zero P-positions of G⋆ equals {{an,bn}}, this is the desired invariant game. We give sufficient conditions on the initial pair of sequences for this ‘duality’ to hold.}
}

