	copyright = {© 2013 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim},
	issn = {1522-2616},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mana.201200105},
	doi = {10.1002/mana.201200105},
	abstract = {We introduce and study a new family of pseudo-Riemannian metrics on the anti-de Sitter three-space . These metrics will be called “of Kaluza-Klein type” , as they are induced in a natural way by the corresponding metrics defined on the tangent sphere bundle . For any choice of three real parameters , the pseudo-Riemannian manifold is homogeneous. Moreover, we shall introduce and study some natural almost contact and paracontact structures , compatible with , such that is a homogeneous almost contact (respectively, paracontact) metric structure. These structures will be then used to show the existence of a three-parameter family of homogeneous metric mixed 3-structures on the anti-de Sitter three-space.},
	language = {en},
	number = {8-9},
	urldate = {2023-07-21},
	journal = {Mathematische Nachrichten},
	author = {Calvaruso, Giovanni and Perrone, Domenico},
	year = {2014},
	keywords = {53C25, 53C30, 53C43, 53D15, almost contact pseudo-metric structures, almost paracontact metric structures, homogeneous structures, Kaluza–Klein metrics, mixed 3-structures, unit tangent sphere bundles},
	pages = {885--902},
	file = {Snapshot:C\:\\Users\\u0140230\\Zotero\\storage\\7USV2BIK\\mana.html:text/html},
}

@misc{calvaruso_helix_2023,
	title = {Helix surfaces for {Berger}-like metrics on the anti-de {Sitter} space},
	url = {http://arxiv.org/abs/2301.07400},
	doi = {10.48550/arXiv.2301.07400},
	abstract = {We consider the Anti-de Sitter space \${\textbackslash}mathbb\{H\}{\textasciicircum}3\_1\$ equipped with Berger-like metrics, that deform the standard metric of \${\textbackslash}mathbb\{H\}{\textasciicircum}3\_1\$ in the direction of the hyperbolic Hopf vector field. Helix surfaces are the ones forming a constant angle with such vector field. After proving that these surfaces have (any) constant Gaussian curvature, we achieve their explicit local description in terms of a one-parameter family of isometries of the space and some suitable curves. These curves turn out to be general helices, which meet at a constant angle the fibers of the hyperbolic Hopf fibration.},
	urldate = {2023-07-21},
