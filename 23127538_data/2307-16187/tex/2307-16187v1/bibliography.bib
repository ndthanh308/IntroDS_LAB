@article{Jacobs2005,
abstract = {The late-time development of Richtmyer–Meshkov instability is studied in shock tube experiments. This investigation makes use of the experimental apparatus and visualization methods utilized in the earlier study of Collins and Jacobs ͓J. Fluid Mech. 464, 113 ͑2002͔͒ but employs stronger shocks and initial perturbations with shorter wavelengths to obtain much later-time ͑in the dimensionless sense͒ images of the single-mode instability. These modifications produce a very detailed look at the evolution of the late-time single-mode instability, revealing the transition and development of turbulence in the vortex cores that eventually results in the disintegration of the laminar vortex structures into small scale features. Amplitude measurements taken from these images are shown to be effectively collapsed when plotted in dimensionless variables defined using the wave number and the initial growth rate. The amplitude measurements are compared with several late-time nonlinear models and solutions. The best agreement is obtained with the model of Sadot et al. ͓Phys. Rev. Lett. 80, 1654 ͑1998͔͒ which can be slightly improved by modifying the expression for the late-time asymptotic growth rate.},
author = {Jacobs, J. W. and Krivets, V. V.},
doi = {10.1063/1.1852574},
file = {:C$\backslash$:/Users/micha/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jacobs, Krivets - 2005 - Experiments on the late-time development of single-mode Richtmyer-Meshkov instability.pdf:pdf},
issn = {10706631},
journal = {Physics of Fluids},
number = {3},
pages = {0--10},
title = {{Experiments on the late-time development of single-mode Richtmyer-Meshkov instability}},
volume = {17},
year = {2005}
}
