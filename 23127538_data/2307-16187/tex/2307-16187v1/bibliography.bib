@article{Chung2010,
abstract = {We report direct numerical simulation (DNS) and large-eddy simulation$\backslash$n(LES) of statistically stationary buoyancy-driven turbulent mixing$\backslash$nof an active scalar. We use an adaptation of the fringe-region technique,$\backslash$nwhich continually supplies the flow with unmixed fluids at two opposite$\backslash$nfaces of a triply periodic domain in the presence of gravity, effectively$\backslash$nmaintaining an unstably stratified, but statistically stationary$\backslash$nflow. We also develop a new method to solve the governing equations,$\backslash$nbased on the Helmholtz-Hodge decomposition, that guarantees discrete$\backslash$nmass conservation regardless of iteration errors. Whilst some statistics$\backslash$nwere found to be sensitive to the computational box size, we show,$\backslash$nfrom inner-scaled planar spectra, that the small scales exhibit similarity$\backslash$nindependent of Reynolds number, density ratio and aspect ratio. We$\backslash$nalso perform LES of the present flow using the stretched-vortex subgrid-scale$\backslash$n(SGS) model. The utility of an SGS scalar flux closure for passive$\backslash$nscalars is demonstrated in the present active-scalar, stably stratified$\backslash$nflow setting. The multi-scale character of the stretched-vortex SGS$\backslash$nmodel is shown to enable extension of some second-order statistics$\backslash$nto subgrid scales. Comparisons with DNS velocity spectra and velocity-density$\backslash$ncospectra show that both the resolved-scale and SGS-extended components$\backslash$nof the LES spectra accurately capture important features of the DNS$\backslash$nspectra, including small-scale anisotropy and the shape of the viscous$\backslash$nroll-off.},
author = {Chung, D. and Pullin, D. I.},
doi = {10.1017/S0022112009992801},
file = {:C$\backslash$:/Users/micha/Dropbox (Sydney Uni)/Reading/Papers/RT/Chung-JFM-2010.pdf:pdf},
issn = {00221120},
journal = {Journal of Fluid Mechanics},
keywords = {Modelling,Simulation,Turbulent mixing},
pages = {279--308},
publisher = {University of Sydney Library},
title = {{Direct numerical simulation and large-eddy simulation of stationary buoyancy-driven turbulence}},
volume = {643},
