@article{Cabayol2018,
    author = {Cabayol, L and Sevilla-Noarbe, I and Fernández, E and Carretero, J and Eriksen, M and Serrano, S and Alarcón, A and Amara, A and Casas, R and Castander, F J and de Vicente, J and Folger, M and García-Bellido, J and Gaztanaga, E and Hoekstra, H and Miquel, R and Padilla, C and Sánchez, E and Stothert, L and Tallada, P and Tortorelli, L},
