	doi = {10.1007/s10915-013-9800-2},
	id = {Castro2014},
	isbn = {1573-7691},
	journal = {Journal of Scientific Computing},
	number = {2},
	pages = {363--389},
	title = {A Class of Incomplete Riemann Solvers Based on Uniform Rational Approximations to the Absolute Value Function},
	url = {https://doi.org/10.1007/s10915-013-9800-2},
	volume = {60},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1007/s10915-013-9800-2}}



