@Article{sega2013slipsubstrate,
author ="Sega, Marcello and Sbragaglia, Mauro and Biferale, Luca and Succi, Sauro",
title  ="Regularization of the slip length divergence in water nanoflows by inhomogeneities at the Angstrom scale",
journal  ="Soft Matter",
