issn = {21603308},
journal = {Physical Review X},
keywords = {doi:10.1103/PhysRevX.10.031067 url:https://doi.org/10.1103/PhysRevX.10.031067,soft matter},
number = {3},
pages = {31067},
publisher = {American Physical Society},
title = {{Singular Nature of the Elastocapillary Ridge}},
url = {https://doi.org/10.1103/PhysRevX.10.031067},
volume = {10},
year = {2020}
}
