eprint = {2104.00900},
file = {:Users/takshingchan/Desktop/papers/wetting on elastic surface/RecentDownloaded2022Feb/soft-lubrication-interactions-between-a-rigid-sphere-and-an-elastic-wall.pdf:pdf},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {colloids,lubrication theory},
pages = {1--21},
title = {{Soft-lubrication interactions between a rigid sphere and an elastic wall}},
volume = {933},
