title = {{Model of spontaneous droplet transport on a soft viscoelastic substrate with nonuniform thickness}},
volume = {104},
year = {2021}
}
@article{Wang2015,
abstract = {We characterize the spatiotemporal deformation of an elastic film during the radial drainage of fluid from a narrowing gap. Elastic deformation of the film takes the form of a dimple and prevents full contact to be reached. With a thinner elastic film the stress becomes increasingly supported by the underlying rigid substrate and the dimple formation is suppressed, which allows the surfaces to reach full contact. We highlight the lag due to viscoelasticity on the surface profiles, and that for a given fluid film thickness deformation leads to stronger hydrodynamic forces than for rigid surfaces.},
