author = {Reiter, G{\"{u}}nter},
doi = {10.1007/978-3-319-55411-2_5},
file = {:Users/takshingchan/Desktop/papers/wetting on elastic surface/ridge/Nature379432a0.pdf:pdf},
isbn = {9783319554112},
