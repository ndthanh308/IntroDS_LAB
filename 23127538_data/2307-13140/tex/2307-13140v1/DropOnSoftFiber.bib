}
@article{Masurel2019,
abstract = {Understanding the interfacial properties of solids with their environment is a crucial problem in fundamental science and applications. Elastomers have challenged the scientific community in this respect, and a satisfying description is still missing. Here, we argue that the interfacial properties of elastomers, such as their wettability, can be understood with a nonlinear elastic model with the assumption of a strain-independent surface energy. We show that our model captures accurately available data on elastomer wettability and discuss its implications.},
