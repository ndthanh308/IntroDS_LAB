issn = {18790399},
journal = {Current Opinion in Colloid and Interface Science},
keywords = {Bacteria motility,Biofilm,Contact angle,Deformable substrates,Microbial contamination,Porous media,Soft solids,Space applications,Spreading,Wetting},
pages = {101426},
publisher = {Elsevier Ltd},
title = {{Wetting/spreading on porous media and on deformable, soluble structured substrates as a model system for studying the effect of morphology on biofilms wetting and for assessing anti-biofilm methods}},
url = {https://doi.org/10.1016/j.cocis.2021.101426},
volume = {53},
