file = {:Users/takshingchan/Desktop/papers/wetting on elastic surface/RecentDownloaded2022Feb/hxaa015 (1).pdf:pdf},
issn = {14643634},
journal = {IMA Journal of Applied Mathematics (Institute of Mathematics and Its Applications)},
keywords = {droplet motion,soft solids},
number = {3},
pages = {495--512},
title = {{Gradient-induced droplet motion over soft solids}},
volume = {85},
year = {2020}
}
@article{Pericet-Camara2008,
abstract = {Sessile liquid drops are predicted to deform an elastic surface onto which they are placed because of the combined action of the liquid surface tension at the periphery of the drop and the capillary pressure inside the drop. Here, we show for the first time the in situ experimental confirmation of the effect of capillary pressure on this deformation. We demonstrate micrometer-scale deformations made possible by using a low Young's modulus material as an elastic surface. The experimental profiles of the deformed surfaces fit well the theoretical predictions for surfaces with a Young's modulus between 25 and 340 kPa. {\textcopyright} 2008 American Chemical Society.},
