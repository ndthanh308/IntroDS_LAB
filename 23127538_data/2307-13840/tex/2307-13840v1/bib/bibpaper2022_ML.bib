@ARTICLE{1999AyA...348L..33G,
   author = {{Groenewegen}, M.~A.~T. and {Salaris}, M.},
    title = "{The absolute magnitudes of RR Lyrae stars from BT HIPPARCOS parallaxes parallaxes}",
  journal = {\aap},
