}


@article{Haus1999,
author = {Hermann A. Haus and Yijiang Chen},
journal = {J. Opt. Soc. Am. B},
keywords = {Fiber optics communications; Kerr effect; Phase shift; Pulse propagation; Scattering; Self phase modulation; Solitons},
number = {6},
pages = {889--894},
publisher = {Optica Publishing Group},
title = {Dispersion-managed solitons as nonlinear Bloch waves},
volume = {16},
month = {Jun},
year = {1999},
url = {https://opg.optica.org/josab/abstract.cfm?URI=josab-16-6-889},
doi = {10.1364/JOSAB.16.000889},
abstract = {It is shown that the dispersion-managed nonlinear pulse solutions can be viewed as nonlinear Bloch waves with a periodic scattering potential that is set up self-consistently by the wave itself. The pulses are shown to be chirp-free at the center of each dispersion segment. The essential physical mechanism is explained by the interaction of the m$=$0 and the m$=$2 Hermite--Gaussian components of the pulse.},
}

@article{Serkin2001,
title = {Bright and dark solitary nonlinear Bloch waves in dispersion managed fiber systems and soliton lasers},
journal = {Opt. Commun.},
volume = {196},
number = {1},
pages = {159-171},
year = {2001},
issn = {0030-4018},
doi = {https://doi.org/10.1016/S0030-4018(01)01365-7},
url = {https://www.sciencedirect.com/science/article/pii/S0030401801013657},
author = {V.N. Serkin and M. Matsumoto and T.L. Belyaeva},
keywords = {Physics of solitons, Nonlinear fiber optics, Nonlinear Bloch waves, Dispersion managed solitons, Soliton based communications, Soliton lasers},
abstract = {The nonlinear Bloch theorem for the fundamental and higher-order Schrödinger solitons in periodic dispersive and nonlinear structures has been proven. It is shown that bright and dark solitary nonlinear Bloch waves exist only under certain conditions and that the parameters describing dispersion and nonlinearity periodic inhomogeneities cannot be chosen independently. The formation, interaction and stability of the fundamental and higher-order solitary nonlinear Bloch waves have been investigated.}
}


