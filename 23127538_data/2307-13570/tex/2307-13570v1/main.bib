@article{Makovejs:10,
author = {Sergejs Makovejs and David S. Millar and Domanic Lavery and Carsten Behrens and Robert I. Killey and Seb J. Savory and Polina Bayvel},
journal = {Opt. Express},
keywords = {Fiber optics communications; Modulation; Optical systems; Phase estimation; Polarization division multiplexing; Polarization mode dispersion; Quadrature amplitude modulation; Signal processing},
number = {12},
pages = {12939--12947},
publisher = {Optica Publishing Group},
title = {{Characterization of long-haul 112Gbit/s PDM-QAM-16 transmission with and without digital nonlinearity compensation}},
volume = {18},
month = {Jun},
year = {2010},
url = {https://opg.optica.org/oe/abstract.cfm?URI=oe-18-12-12939},
doi = {10.1364/OE.18.012939},
abstract = {In this paper long-haul, single channel, polarization multiplexed 16-state quadrature amplitude modulation (PDM-QAM-16) transmission at 112Gbit/s is investigated. Novel digital signal processing techniques are used to perform carrier phase estimation and symbol estimation, in combination with nonlinear digital backpropagation. The results obtained demonstrate that the use of digital nonlinear backpropagation increases the optimum launch power from {\textminus}4dBm to {\textminus}1dBm with a consequent increase in maximum reach from 1440km to 2400km, which is a record transmission distance for QAM-16 reported to date for an SMF link with EDFAs only. Furthermore, experimental measurements are supported by simulations, based on the link used in the experiment.},
}% 

