@article{conjcoord,
author = "Grosse-Kunstleve, R. W. and Brunner, G. O. and Sloane, N. J. A.",
title = "{Algebraic Description of Coordination Sequences and Exact Topological Densities for Zeolites}",
journal = "Acta Crystallographica Section A",
year = "1996",
volume = "52",
number = "6",
pages = "879--889",
month = "Nov",
doi = {10.1107/S0108767396007519},
url = {https://doi.org/10.1107/S0108767396007519},
abstract = {Coordination sequences (CS) have been calculated for all approved zeolite topologies, all dense SiO${\sb 2}$ polymorphs and 16 selected non-tetrahedral structures and the algebraic structure of these CS's has been analyzed. Two algebraic descriptions of coordination sequences are presented. One description uses periodic sets of quadratic equations and is already established in the literature. The second description employs generating functions, which are well known in combinatorics but are used here for the first time in connection with coordination sequences. The algebraic analysis based on generating functions turns out to be more powerful than the other approach. Based on the algebraic analyses, exact topological densities are derived and tabulated for all the structures investigated. In addition, `{\it n}-dimensional sodalite' is observed to have an especially simple {\it n}-dimensional graph.},
}

