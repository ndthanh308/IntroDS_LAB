	urldate = {2016-09-12},
	journal = {Physical Review B},
	author = {Suzuki, H. and Yoshida, T. and Ideta, S. and Shibata, G. and Ishigami, K. and Kadono, T. and Fujimori, A. and Hashimoto, M. and Lu, D. H. and Shen, Z.-X. and Ono, K. and Sakai, E. and Kumigashira, H. and Matsuo, M. and Sasagawa, T.},
	month = sep,
	year = {2013},
	pages = {100501(R)},
	file = {APS Snapshot:C\:\\Users\\Fernando\\Zotero\\storage\\FRQ2LUJF\\PhysRevB.88.html:text/html},
}

@article{granado_pressure_2011,
	title = {Pressure and chemical substitution effects in the local atomic structure of {BaFe2As2}},
	volume = {83},
	issn = {1098-0121},
	doi = {10.1103/PhysRevB.83.184508},
	abstract = {The effects of K and Co substitutions and quasihydrostatic applied pressure (P {\textless} 9 GPa) in the local atomic structure of BaFe2As2, Ba(Fe0.937Co0.063)(2)As-2 and Ba0.85K0.15Fe2As2 superconductors were investigated by extended x-ray absorption fine structure (EXAFS) measurements in the As K absorption edge. The As-Fe bond length is found to be slightly reduced (less than or similar to 0.01 angstrom) by both Co and K substitutions, without any observable increment in the corresponding Debye-Waller factor. Also, this bond is shown to be compressible [kappa = 3.3(3) x 10(-3) GPa(-1)]. The observed contractions of As-Fe bond under pressure and chemical substitutions are likely related with a reduction of the local Fe magnetic moments, and should be an important tuning parameter in the phase diagrams of the Fe-based superconductors.},
