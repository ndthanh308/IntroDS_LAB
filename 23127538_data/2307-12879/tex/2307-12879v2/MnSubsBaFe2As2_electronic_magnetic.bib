@article{stadler_differentiating_2021,
	title = {Differentiating {Hund} from {Mott} physics in a three-band {Hubbard}-{Hund} model: {Temperature} dependence of spectral, transport, and thermodynamic properties},
	volume = {104},
	shorttitle = {Differentiating {Hund} from {Mott} physics in a three-band {Hubbard}-{Hund} model},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.104.115107},
	doi = {10.1103/PhysRevB.104.115107},
	abstract = {We study the interplay between Mott physics, driven by Coulomb repulsion U, and Hund physics, driven by Hund's coupling J, for a minimal model for Hund metals, the orbital-symmetric three-band Hubbard-Hund model (3HHM) for a lattice filling of 1/3. Hund-correlated metals are characterized by spin-orbital separation (SOS), a Hund's-rule-induced two-stage Kondo-type screening process, in which spin screening occurs at much lower energy scales than orbital screening. By contrast, in Mott-correlated metals, lying close to the phase boundary of a metal-insulator transition, the SOS window becomes negligibly small and the Hubbard bands are well separated. Using dynamical mean-field theory and the numerical renormalization group as real-frequency impurity solver, we identify numerous fingerprints distinguishing Hundness from Mottness in the temperature dependence of various physical quantities. These include ARPES-type spectra, the local self-energy, static local orbital and spin susceptibilities, resistivity, thermopower, and lattice and impurity entropies. Our detailed description of the behavior of these quantities within the context of a simple model Hamiltonian will be helpful for distinguishing Hundness from Mottness in experimental and theoretical studies of real materials.},
	number = {11},
	urldate = {2023-06-06},
	journal = {Physical Review B},
	author = {Stadler, K. M. and Kotliar, G. and Lee, S.-S. B. and Weichselbaum, A. and von Delft, J.},
	month = sep,
	year = {2021},
	pages = {115107},
	
}

@article{mcnally_hunds_2015,
	title = {From {Hund}'s insulator to {Fermi} liquid: {Optical} spectroscopy study of {K} doping in \$\{{\textbackslash}mathrm\{{BaMn}\}\}\_\{2\}\{{\textbackslash}mathrm\{{As}\}\}\_\{2\}\$},
	volume = {92},
	shorttitle = {From {Hund}'s insulator to {Fermi} liquid},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.92.115142},
	abstract = {We present optical transmission measurements that reveal a charge gap of 0.86 eV in the local-moment antiferromagnetic insulator BaMn2As2, an order of magnitude larger than previously reported. Density functional theory plus dynamical mean-field theory (DFT+DMFT) calculations correctly reproduce this charge gap only when a strong Hund's coupling is considered. Thus, BaMn2As2 is a member of a wider class of Mn pnictide compounds that are Mott{\textendash}Hund insulators. We also present optical reflectance for metallic 2\%-K-doped BaMn2As2 that we use to extract the optical conductivity at different temperatures. The optical conductivity $\sigma$1($\omega$) exhibits a metallic response that is well described by a simple Drude term. Both $\sigma$($\omega${\textrightarrow}0,T) and $\rho$(T) exhibit Fermi-liquid temperature dependencies. From these measurements, we argue that a more strongly correlated Hund-metal version of the parent compounds of the iron pnictide superconductors has not yet been realized by doping this class of Hund insulators.},
	number = {11},
	urldate = {2023-06-06},
	journal = {Physical Review B},
	author = {McNally, D. E. and Zellman, S. and Yin, Z. P. and Post, K. W. and He, Hua and Hao, K. and Kotliar, G. and Basov, D. and Homes, C. C. and Aronson, M. C.},
	month = sep,
	year = {2015},
	pages = {115142},
	
}

