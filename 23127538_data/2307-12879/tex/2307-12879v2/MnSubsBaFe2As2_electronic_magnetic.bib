@article{merz_substitution_2016,
	title = {Of {Substitution} and {Doping}: {Spatial} and {Electronic} {Structure} in {Fe} {Pnictides}},
	volume = {85},
	issn = {0031-9015},
	shorttitle = {Of {Substitution} and {Doping}},
	doi = {10.7566/JPSJ.85.044707},
	abstract = {A highly intriguing aspect in iron-pnictide superconductors is the composition-dependent electronic structure, in particular the question if and how charge carriers are introduced to the system upon substitution of Ba by alkali metals or of Fe by other transition metals, TM. We report on a systematic study of spatial structure and electronic states by x-ray diffraction and x-ray absorption on a large number of compositions in the (Ba,K)(Fe,TM)(2)As-2 family. The coherent combination of detailed structural information with an in-depth analysis of the electronic structure allows us to sensitively disentangle (charge-carrier) "doping" effects from "substitutional" effects. Results include a doping character that is site-decoupled, as well as TM 3d energy-level schemes that exhibit non-standard level sequences and even t(2)-e level crossings. Our study indicates that doping per se seems to play a lesser role than expected for pnictide superconductivity and magnetism.},
	language = {English},
	number = {4},
	journal = {Journal of the Physical Society of Japan},
	author = {Merz, Michael and Schweiss, Peter and Nagel, Peter and Huang, Meng-Jie and Eder, Robert and Wolf, Thomas and von Loehneysen, Hilbert and Schuppler, Stefan},
	month = apr,
	year = {2016},
	keywords = {state, superconductivity},
	pages = {044707},
}

@article{yi_role_2017,
