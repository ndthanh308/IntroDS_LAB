}

@article{kim_antiferromagnetic_2010,
	title = {Antiferromagnetic ordering in the absence of structural distortion in $\text{Ba}{({\text{Fe}}_{1\ensuremath{-}x}{\text{Mn}}_{x})}_{2}{\text{As}}_{2}$},
	author = {Kim, M. G. and Kreyssig, A. and Thaler, A. and Pratt, D. K. and Tian, W. and Zarestky, J. L. and Green, M. A. and Bud'ko, S. L. and Canfield, P. C. and McQueeney, R. J. and Goldman, A. I.},
	journal = {Phys. Rev. B},
