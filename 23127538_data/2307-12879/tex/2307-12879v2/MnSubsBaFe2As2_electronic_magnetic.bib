	doi = {10.1103/PhysRevB.95.184516},
	abstract = {We have performed an angle-resolved photoemission spectroscopy study of BaCr2As2, which has the same crystal structure as BaFe2As2, a parent compound of Fe-based superconductors. We determine the Fermi surface of this material and its band dispersion over 5 eV of binding energy. Very moderate band renormalization (1.35) is observed for only two bands. We attribute this small renormalization to enhanced direct exchange as compared to Fe in BaFe2As2, and to a larger contribution of the e(g) orbitals in the composition of the bands forming the Fermi surface, leading to an effective valence count that is reduced by Fe d-As p hybridization.},
	language = {English},
	number = {18},
	journal = {Physical Review B},
	author = {Richard, P. and van Roekeghem, A. and Lv, B. Q. and Qian, Tian and Kim, T. K. and Hoesch, M. and Hu, J.-P. and Sefat, Athena S. and Biermann, Silke and Ding, H.},
	month = may,
	year = {2017},
	keywords = {arpes, chalcogenides, fese films, high-temperature superconductivity, origin, pnictides, resolved photoemission-spectroscopy, waves},
	pages = {184516},
	file = {Accepted Version:C\:\\Users\\Fernando\\Zotero\\storage\\KCP4RAP2\\Richard et al. - 2017 - Is BaCr2As2 symmetrical to BaFe2As2 with respect t.pdf:application/pdf},
}


@article{gastiasoro_unconventional_2016,
	title = {Unconventional {Disorder} {Effects} in {Correlated} {Superconductors}},
	volume = {117},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.117.257002},
	doi = {10.1103/PhysRevLett.117.257002},
	abstract = {We study the effects of disorder on unconventional superconductors in the presence of correlations, and explore a novel correlated disorder paradigm dominated by strong deviations from standard Abrikosov-Gor’kov theory due to generation of local bound states and cooperative impurity behavior driven by Coulomb interactions. Specifically we explain under which circumstances magnetic disorder acts as a strong poison destroying high-Tc superconductivity at the sub-1\% level, and when nonmagnetic disorder, counterintuitively, hardly affects the unconventional superconducting state while concomitantly inducing an inhomogeneous full-volume magnetic phase. Recent experimental studies of Fe-based superconductors have discovered that such unusual disorder behavior seems to be indeed present in those systems.},
	number = {25},
	urldate = {2023-01-05},
	journal = {Physical Review Letters},
	author = {Gastiasoro, Maria N. and Bernardini, Fabio and Andersen, Brian M.},
	month = dec,
	year = {2016},
	pages = {257002},
}

@article{tanaskovic_effective_2004,
	title = {Effective model of the electronic Griffiths phase},
	author = {Tanaskovi\ifmmode \acute{c}\else \'{c}\fi{}, D. and Miranda, E. and Dobrosavljevi\ifmmode \acute{c}\else \'{c}\fi{}, V.},
	journal = {Phys. Rev. B},
	volume = {70},
	issue = {20},
	pages = {205108},
	numpages = {8},
	year = {2004},
	month = {Nov},
	publisher = {American Physical Society},
	doi = {10.1103/PhysRevB.70.205108},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.70.205108},
	abstract = {We present simple analytical arguments explaining the universal emergence of electronic Griffiths phases as precursors of disorder-driven metal-insulator transitions in correlated electronic systems. A simple effective model is constructed and solved within dynamical mean field theory. It is shown to capture all the qualitative and even quantitative aspects of such Griffiths phases.}
}

