@article{berlijn_transition-metal_2012-1,
	title = {Do {Transition}-{Metal} {Substitutions} {Dope} {Carriers} in {Iron}-{Based} {Superconductors}?},
	volume = {108},
	issn = {0031-9007},
	doi = {10.1103/PhysRevLett.108.207003},
	abstract = {We investigate the currently debated issue concerning whether transition-metal substitutions dope carriers in iron-based superconductors. From first-principles calculations of the configuration-averaged spectral function of BaFe2As2 with disordered Co or Zn substitutions of Fe, important doping effects are found beyond merely changing the carrier density. While the chemical potential shifts suggest doping of a large amount of carriers, a reduction of the coherent carrier density is found due to the loss of spectral weight. Therefore, none of the change in the Fermi surface, density of states, or charge distribution can be solely used for counting doped coherent carriers, let alone presenting the full effects of the disordered substitutions. Our study highlights the necessity of including disorder effects in the studies of doped materials in general.},
	language = {English},
	number = {20},
	journal = {Physical Review Letters},
	author = {Berlijn, Tom and Lin, Chia-Hui and Garber, William and Ku, Wei},
	month = may,
	year = {2012},
	keywords = {impurities},
	pages = {207003},
}

