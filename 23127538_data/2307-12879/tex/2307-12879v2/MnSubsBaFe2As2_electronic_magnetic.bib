	file = {IOP Full Text PDF:C\:\\Users\\Fernando\\Zotero\\storage\\NP5G7CCH\\Khan et al. - 2022 - Electronic structure, magnetic, and thermoelectric.pdf:application/pdf},
}

@article{chen_miscibility_2021,
	title = {Miscibility gap and possible intrinsic {Griffiths} phase in \${\textbackslash}mathrm\{{Sr}\}\{(\{{\textbackslash}mathrm\{{Fe}\}\}\_\{1{\textbackslash}ensuremath\{-\}x\}\{{\textbackslash}mathrm\{{Mn}\}\}\_\{x\})\}\_\{2\}\{{\textbackslash}mathrm\{{As}\}\}\_\{2\}\$ crystals grown by transition metal arsenide flux},
