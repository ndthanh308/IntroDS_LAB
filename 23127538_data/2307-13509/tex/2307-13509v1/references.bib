@article{golub1999,
author = {Golub, Gene H. and Hansen, Per Christian and O'Leary, Dianne P.},
title = {Tikhonov Regularization and Total Least Squares},
journal = {SIAM Journal on Matrix Analysis and Applications},
volume = {21},
number = {1},
pages = {185-194},
