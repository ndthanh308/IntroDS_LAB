@article{CROUX1999,
title = {Influence Function and Efficiency of the Minimum Covariance Determinant Scatter Matrix Estimator},
journal = {Journal of Multivariate Analysis},
volume = {71},
number = {2},
pages = {161-190},
year = {1999},
issn = {0047-259X},
