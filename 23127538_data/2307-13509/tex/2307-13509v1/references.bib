@article{MAS2007,
title = {Weak convergence in the functional autoregressive model},
journal = {Journal of Multivariate Analysis},
volume = {98},
number = {6},
pages = {1231-1261},
year = {2007},
issn = {0047-259X},
doi = {https://doi.org/10.1016/j.jmva.2006.05.010},
url = {https://www.sciencedirect.com/science/article/pii/S0047259X0600090X},
author = {André Mas},
keywords = {Functional data, Autoregressive model, Hilbert space, Weak convergence, Random operator, Perturbation theory, Linear inverse problem, Martingale difference arrays},
abstract = {The functional autoregressive model is a Markov model taylored for data of functional nature. It revealed fruitful when attempting to model samples of dependent random curves and has been widely studied along the past few years. This article aims at completing the theoretical study of the model by addressing the issue of weak convergence for estimates from the model. The main difficulties stem from an underlying inverse problem as well as from dependence between the data. Traditional facts about weak convergence in non-parametric models appear: the normalizing sequence is not an On, a bias term appears. Several original features of the functional framework are pointed out.}
}

@article{DAI2020,
title = {Functional outlier detection and taxonomy by sequential transformations},
journal = {Computational Statistics \& Data Analysis},
volume = {149},
pages = {106960},
year = {2020},
issn = {0167-9473},
doi = {https://doi.org/10.1016/j.csda.2020.106960},
url = {https://www.sciencedirect.com/science/article/pii/S0167947320300517},
author = {Wenlin Dai and Tomáš Mrkvička and Ying Sun and Marc G. Genton},
keywords = {Data transformation, Functional boxplot, Magnitude outliers, Multivariate functional data, Shape outliers},
abstract = {Functional data analysis can be seriously impaired by abnormal observations, which can be classified as either magnitude or shape outliers based on their way of deviating from the bulk of data. Identifying magnitude outliers is relatively easy, while detecting shape outliers is much more challenging. We propose turning the shape outliers into magnitude outliers through data transformation and detecting them using the functional boxplot. Besides easing the detection procedure, applying several transformations sequentially provides a reasonable taxonomy for the flagged outliers. A joint functional ranking, which consists of several transformations, is also defined here. Simulation studies are carried out to evaluate the performance of the proposed method using different functional depth notions. Interesting results are obtained in several practical applications.}
}

@InProceedings{ruiz2010,
