@Article{Brandt1991,
  Title                    = {{Free energy of the Falicov-Kimball model in large dimensions}},
  Author                   = {Brandt, U. and Mielsch, C.},
  Journal                  = {Zeitschrift f{\"{u}}r Phys. B Condens. Matter},
  Year                     = {1991},

  Month                    = {feb},
  Number                   = {1},
  Pages                    = {37--41},
  Volume                   = {82},

  Abstract                 = {We show how to calculate the free energy of the spinless Falicov Kimball model exactly in the limit of infinite dimensions, and do it explicitly for the homogeneous and the chessboard phase. By comparing the free energies for those two cases we study the transitions between the pure phases, and by looking for concavities in the course of the free energy as a function of the mean f -particle density we examine the possibility of phase segregation.},
  Doi                      = {10.1007/BF01313984},
  File                     = {::},
  Nourl                      = {http://link.springer.com/article/10.1007/BF01313984 http://link.springer.com/10.1007/BF01313984}
}

@Article{Brandt1990,
  Title                    = {{Thermodynamics of the Falicov-Kimball model in large dimensions II}},
  Author                   = {Brandt, U. and Mielsch, C.},
  Journal                  = {Zeitschrift f{\"{u}}r Phys. B Condens. Matter},
  Year                     = {1990},

  Month                    = {jun},
  Number                   = {2},
  Pages                    = {295--299},
  Volume                   = {79},

  Abstract                 = {In a former paper we presented the exact solution of the Falicov-Kimball-model in large dimensions and showed the existence of phase transitions for certain parameter combinations, especially for the symmetric case. In this contribution we investigate the conditions for transitions apart from the symmetric case and show that the phase following the homogeneous high temperature phase will have chessboard symmetry if the transition is a continuous charge density transition. The corresponding phase diagrams are shown as well as results on the order parameter of the chessboard phase.},
  Doi                      = {10.1007/BF01406598},
  File                     = {::},
  ISSN                     = {0722-3277},
  Nourl                      = {http://link.springer.com/10.1007/BF01406598}
}

