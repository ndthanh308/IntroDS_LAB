@Article{LeBlanc2016_2,
  Title                    = {Spin waves in the 2D Hubbard model},
  Author                   = {LeBlanc, J.P.F. and Chen, X. and Levy, R. and Antipov, Andrey E. and Millis, A.J. and Gull, E.},
  Journal                  = {(unpublished)},
  Year                     = {2017},

  Abstract                 = {We examine spin susceptibilities in the 2D Hubbard model for a variety of interaction strengths both at and away from half-filling. We first compute generalized two-particle correlated vertex functions within dynamical mean-field theory and then extend beyond DMFT using the technique of dual-fermions. By examining the dispersion of peaks in Im$\chi$($\omega$,q) which define spin modes $\omega$s(q) we find that both the exchange scale, Jeff, and magnon dispersion are in agreement with neutron experiments on the parent La2CuO4 cuprate for U/t ≈ 7.8. We examine as well the dependence of these modes on interaction strength, U/t, as well as doping, both electron and hole doped. We find that heavily doped strongly interacting systems have features in $\omega$s(q) similar to half-filled but weakly interacting cases. Finally, we relate the value of $\omega$s(q = ($\pi$,$\pi$)) to the antiferromagnetic correlation length, $\gamma$, to illustrate that (for t′ {\textless} 0) the electron doped system has smaller $\gamma$ than an equivalently hole doped system.}
}


