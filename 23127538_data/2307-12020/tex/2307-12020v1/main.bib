﻿@article{Lunkenheimer2006, 
  author = {Lunkenheimer, P. and Mayr, F. and Loidl, A.},
  year = {2006},
  title = {Dynamic conductivity from audio to optical frequencies of semiconducting manganites approaching the metal‐insulator transition },
  journal = {Annalen der Physik},
