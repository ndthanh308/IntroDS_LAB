@Article{Bulla1998,
  Title                    = {Numerical renormalization group calculations for the self-energy of the impurity Anderson model},
  Author                   = {R Bulla and A C Hewson and Th Pruschke},
  Journal                  = {Journal of Physics: Condensed Matter},
  Year                     = {1998},
  Number                   = {37},
  Pages                    = {8365},
  Volume                   = {10},

  Abstract                 = {We present a new method for calculating directly the one-particle self-energy of an impurity Anderson model with Wilson's numerical renormalization group method by writing this quantity as the ratio of two correlation functions. This way of calculating ##IMG## [http://ej.iop.org/images/0953-8984/10/37/021/img5.gif] turns out to be considerably more reliable and accurate than that via the impurity Green's function alone. We give results for the self-energy for the case of a constant coupling between the impurity and the conduction band ##IMG## [http://ej.iop.org/images/0953-8984/10/37/021/img6.gif] and the effective ##IMG## [http://ej.iop.org/images/0953-8984/10/37/021/img7.gif] arising in the dynamical mean-field theory of the Hubbard model. The implications of the problem of the metal-insulator transition in the Hubbard model are also discussed.},
  Nourl                      = {http://stacks.iop.org/0953-8984/10/i=37/a=021}
}

