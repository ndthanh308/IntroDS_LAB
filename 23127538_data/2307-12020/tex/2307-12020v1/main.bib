@article{Guterding2017,
author = {Guterding, Daniel and Backes, Steffen and Tomić, Milan and Jeschke, Harald O. and Valent\'{i}, Roser},
title = {Ab initio perspective on structural and electronic properties of iron-based superconductors},
journal = {physica status solidi (b)},

volume = {254},
number = {1},
pages = {1600164},
keywords = {density functional theory calculations, Dynamical mean-field theory, iron pnictides, iron selenide, superconductivity},
doi = {10.1002/pssb.201600164},
ignoreNO = {https://onlinelibrary.wiley.com/doi/abs/10.1002/pssb.201600164},
eprintNO = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/pssb.201600164},
abstract = {Abstract The discovery of iron pnictides and iron chalcogenides as a new class of unconventional superconductors in 2008 has generated an enormous amount of experimental and theoretical work that identifies these materials as correlated metals with multi-orbital physics, where magnetism, nematicity, and superconductivity are competing phases that appear as a function of pressure and doping. A microscopic understanding of the appearance of these phases is crucial in order to determine the nature of superconductivity in these systems. Here, we review our recent theoretical efforts to describe and understand from first principles the properties of iron pnictides and chalcogenides with special focus on (i) pressure dependence, (ii) effects of electronic correlation, and (iii) origin of magnetism and superconductivity.},
year = {2017}
}

