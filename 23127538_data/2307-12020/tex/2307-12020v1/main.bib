@Article{Dobrosavljevic2003,
  Title                    = {{Typical medium theory of Anderson localization: A local order parameter approach to strong-disorder effects}},
  Author                   = {Dobrosavljevi{\'{c}}, V. and Pastor, A. A. and Nikoli{\'{c}}, B. K.},
