@Article{Mancini2000,
  Title                    = {The Mott-Hubbard transition and the paramagnetic insulating state in the two-dimensional Hubbard model},
  Author                   = {F. Mancini},
  Journal                  = {EPL (Europhysics Letters)},
  Year                     = {2000},
  Number                   = {2},
  Pages                    = {229},
  Volume                   = {50},

  Abstract                 = {The Mott-Hubbard transition is studied in the context of the two-dimensional Hubbard model. Analytical calculations show the existence of a critical value U c of the potential strength which separates a paramagnetic metallic phase from a paramagnetic insulating phase. Calculations of the density of states and double occupancy show that the ground state in the insulating phase contains always a small fraction of empty and doubly occupied sites. The structure of the ground state is studied by considering the probability amplitude of intersite hopping. The results indicate that the ground state of the Mott insulator is characterized by a local antiferromagnetic order; the electrons keep some mobility, but this mobility must be compatible with the local ordering. The vanishing of some intersite probability amplitudes at U = U c puts a constraint on the electron mobility. It is suggested that such quantities might be taken as the quantities which control the order in the insulating phase.},
  Nourl                      = {http://stacks.iop.org/0295-5075/50/i=2/a=229}
}

