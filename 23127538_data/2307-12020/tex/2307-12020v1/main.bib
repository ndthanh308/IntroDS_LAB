@Article{Held2000,
  Title                    = {Mott transition of the f-electron system in the periodic Anderson model with nearest neighbor hybridization},
  Author                   = {Held, K.
and Bulla, R.},
  Journal                  = {The European Physical Journal B - Condensed Matter and Complex Systems},
  Year                     = {2000},

  Month                    = {Sep},
  Number                   = {1},
  Pages                    = {7--10},
  Volume                   = {17},

  Abstract                 = {We show analytically that, under certain assumptions, the periodic Anderson model and the Hubbard model become equivalent within the dynamical mean field theory for quasiparticle weight Z {\textrightarrow} 0. A scaling relation is derived which is validated numerically using the numerical renormalization group at zero temperature and quantum Monte Carlo simulations at finite temperatures. Our results show that the {\textflorin}-electrons of the half-filled periodic Anderson model with nearest neighbor hybridization get localized at a finite critical interaction strength U c, also at zero temperature. This transition is equivalent to the Mott-transition in the Hubbard model.},
  Day                      = {01},
  Doi                      = {10.1007/s100510070154},
