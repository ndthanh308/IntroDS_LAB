@Article{Maiti2001,
  Title                    = {Electronic structure of Ca$_{1-x}$Sr$_x$VO$_3$: A tale of two energy scales},
  Author                   = {K. Maiti and D. D. Sarma and M. J. Rozenberg and I. H. Inoue and H. Makino and O. Goto and M. Pedio and R. Cimino},
