	journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
	author = {Roussel, R. and Andonian, G. and Conde, M. and Deng, A. and Ha, G. and Hansel, J. and Lawler, G. and Lynn, W. and Power, J. and Robles, R. and Sanwalka, K. and Rosenzweig, J.},
