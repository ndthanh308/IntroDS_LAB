	pages = {2147--2154},
	annote = {ISSN: 1941-0026},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\773FV6Q4\\5949880.html:text/html;IEEE Xplore Abstract Record:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\NKKRI34R\\5949880.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\Y54AB63W\\Emmerich et al. - 2011 - Hypervolume-based expected improvement Monotonici.pdf:application/pdf},
