
@article{ha_perturbation-minimized_2016,
	title = {Perturbation-minimized triangular bunch for high-transformer ratio using a double dogleg emittance exchange beam line},
	volume = {19},
	url = {https://link.aps.org/doi/10.1103/PhysRevAccelBeams.19.121301},
	doi = {10.1103/PhysRevAccelBeams.19.121301},
	abstract = {The longitudinal shape, i.e., the current profile, of an electron bunch determines the transformer ratio in a collinear wakefield accelerator and thus methods are sought to control the longitudinal bunch shape. The emittance exchange (EEX) appears to be promising for creating a precisely controlled longitudinal bunch shapes. The longitudinal shape is perturbed by two sources: higher-order terms in the beam line optics and collective effects and these perturbations can lead to a significant drop of the transformer ratio. In this paper, we analytically and numerically investigate the perturbation to an ideal triangular longitudinal bunch shape and propose methods to minimize it.},
	number = {12},
	urldate = {2019-06-29},
	journal = {Physical Review Accelerators and Beams},
	author = {Ha, G. and Cho, M. H. and Gai, W. and Kim, K.-J. and Namkung, W. and Power, J. G.},
	month = dec,
	year = {2016},
