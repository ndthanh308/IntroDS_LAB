	keywords = {Computer Science - Computer Vision and Pattern Recognition, Physics - Instrumentation and Detectors},
	file = {arXiv Fulltext PDF:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\UMEYP9WG\\Lei et al. - 2022 - Implicit Neural Representation as a Differentiable.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\GX5I3BNR\\2211.html:text/html},
