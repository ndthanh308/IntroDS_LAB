	keywords = {Computer science, Pareto optimisation, Testing, computational complexity, constraint theory, Evolutionary computation, evolutionary computation, Design optimization, 3D solution spaces, aerodynamics, airfoil multipoint design, Automotive components, Electrostatic precipitators, evolutionary multiobjective optimisation algorithm, Extraterrestrial measurements, Genetic mutations, hypervolume measure, mathematical operators, nonlinear constraints, Pareto analysis, Pareto front, Pareto optimization, S-metric selection, selection operators, three-objective problem},
	pages = {1282--1289 Vol. 2},
	annote = {ISSN: 1941-0026},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\ZIUWWY22\\1554838.html:text/html;IEEE Xplore Abstract Record:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\9IS7ITNZ\\1554838.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\Z7ZTYPTS\\Naujoks et al. - 2005 - Multi-objective optimisation using S-metric select.pdf:application/pdf},
