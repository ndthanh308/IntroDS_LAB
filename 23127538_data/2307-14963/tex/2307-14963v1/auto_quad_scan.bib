	pages = {130--133},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\SFTJSXU6\\Roussel et al. - 2018 - Measurement of transformer ratio from ramped beams.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\Z5QTJP2F\\S0168900218301529.html:text/html;ScienceDirect Snapshot:C\:\\Users\\Ryan Roussel\\Zotero\\storage\\F8TERZRZ\\S0168900218301529.html:text/html},
