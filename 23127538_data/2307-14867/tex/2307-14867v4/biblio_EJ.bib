  language   = {en},
  shorttitle = {Deep {IV}},
  url        = {https://proceedings.mlr.press/v70/hartford17a.html},
  urldate    = {2023-04-05},
}

@InProceedings{Liao2020,
  author     = {Liao, Luofeng and Chen, You-Lin and Yang, Zhuoran and Dai, Bo and Kolar, Mladen and Wang, Zhaoran},
  booktitle  = {Advances in {Neural} {Information} {Processing} {Systems}},
  title      = {Provably Efficient Neural Estimation of Structural Equation Models: An Adversarial Approach},
  year       = {2020},
  pages      = {8947--58},
  volume     = {33},
  abstract   = {Structural equation models (SEMs) are widely 
used in sciences, ranging from economics to psychology,
to uncover causal relationships underlying a complex system
under consideration and estimate structural parameters of interest. 
We study estimation in a class of generalized SEMs where the object 
of interest is defined as the solution to a linear operator equation.
We formulate the linear operator equation as a min-max game, where both 
players are parameterized by neural networks (NNs), and learn the
parameters of these neural networks using the stochastic gradient descent.
We consider both 2-layer and multi-layer NNs with ReLU activation 
functions and prove global convergence in an overparametrized regime, where
the number of neurons is diverging. The results are established using 
techniques from online learning and local linearization of NNs,
and improve in several aspects the current state-of-the-art. For the first 
time we provide a tractable estimation procedure for SEMs
based on NNs with provable convergence and without the need for sample
splitting.},
  file       = {Liao et al_2020_Provably.pdf:C\:\\Users\\Pascal\\Dropbox\\Zotero\\Liao et al\\Liao et al_2020_Provably3.pdf:application/pdf},
  shorttitle = {Provably {Efficient} {Neural} {Estimation} of {Structural} {Equation} {Models}},
  url        = {https://proceedings.neurips.cc/paper/2020/hash/65a99bb7a3115fdede20da98b08a370f-Abstract.html},
  urldate    = {2023-04-05},
}

@Article{DHaultfoeuille2011,
  author   = {D’Haultfoeuille, Xavier},
  journal  = {Econometric Theory},
  title    = {On the completeness condition in nonparametric instrumental problems},
  year     = {2011},
  issn     = {1469-4360, 0266-4666},
  pages    = {460--71},
  volume   = {27},
  abstract = {The notion of completeness between two random elements has been considered recently to provide identification in nonparametric instrumental problems. This condition is quite abstract, however, and characterizations have been obtained only in special cases. This paper considers a nonparametric model between the two variables with an additive separability and a large support condition. In this framework, different versions of completeness are obtained, depending on which regularity conditions are imposed. This result allows one to establish identification in an instrumental nonparametric regression with limited endogenous regressor, a case where the control variate approach breaks down.},
  doi      = {10.1017/S0266466610000368},
  language = {en},
  url      = {https://www.cambridge.org/core/journals/econometric-theory/article/abs/on-the-completeness-condition-in-nonparametric-instrumental-problems/C560D83B9A313CADB46BA2CCF12219CE},
