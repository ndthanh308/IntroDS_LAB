@article{Tiemann12,
	abstract = {The fractional quantum Hall (FQH) effect at filling factor ? = 5/2 has recently come under close scrutiny, as its ground state may possess quasi-particle excitations obeying nonabelian statistics, a property sought for topologically protected quantum operations. However, its microscopic origin remains unknown, and candidate model wave functions include those with undesirable abelian statistics. We report direct measurements of the electron spin polarization of the ? = 5/2 FQH state using resistively detected nuclear magnetic resonance. We find the system to be fully polarized, which unambiguously rules out the most likely abelian contender and lends strong support for the ? = 5/2 state being nonabelian. Our measurements reveal an intrinsically different nature of interaction in the first excited Landau level underlying the physics at ? = 5/2.},
	author = {Tiemann, L. and Gamez, G. and Kumada, N. and Muraki, K.},
	doi = {10.1126/science.1216697},
	eprint = {http://www.sciencemag.org/content/335/6070/828.full.pdf},
	journal = {Science},
	number = {6070},
	pages = {828-831},
	title = {Unraveling the Spin Polarization of the $\nu= 5/2$ Fractional Quantum {Hall} State},
	url = {http://www.sciencemag.org/content/335/6070/828.abstract},
	volume = {335},
