@article{Shingla23,
	author={Shingla, Vidhi and Huang, Haoyun and Kumar, Ashwani and Pfeiffer, Loren N. and West, Kenneth W. and Baldwin, Kirk W. and Cs{\'a}thy, G{\'a}bor A.},
	title={A highly correlated topological bubble phase of composite fermions},
	journal={Nature Physics},
	year={2023},
	month={May},
	day={01},
	volume={19},
	number={5},
	pages={689-693},
abstract={Strong interactions and topology drive a wide variety of correlated ground states. Some of the most interesting of these ground states, such as fractional quantum Hall states and fractional Chern insulators, have fractionally charged quasiparticles. Correlations in these phases are captured by the binding of electrons and vortices into emergent particles called composite fermions. Composite fermion quasiparticles are randomly localized at high levels of disorder and may exhibit charge order when there is not too much disorder in the system. However, more complex correlations are predicted when composite fermion quasiparticles cluster into a bubble, and then these bubbles order on a lattice. Such a highly correlated ground state is termed the bubble phase of composite fermions. Here we report the observation of such a bubble phase of composite fermions, evidenced by the re-entrance of the fractional quantum Hall effect. We associate this re-entrance with a bubble phase with two composite fermion quasiparticles per bubble. Our results demonstrate the existence of a new class of strongly correlated topological phases driven by clustering and charge ordering of emergent quasiparticles.},
	issn={1745-2481},
	doi={10.1038/s41567-023-01939-2},
	url={https://doi.org/10.1038/s41567-023-01939-2}
}


