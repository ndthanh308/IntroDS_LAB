@article{Drichko19,
	abstract = {Using acoustic method we study dependences of transverse AC conductance, σ(ω), on magnetic field, temperature and the amplitude of AC electric field in a wide (75 nm) quantum well (QW) structure focusing on the vicinity of the filling factor ν = 1/2. Measurements are performed in the frequency domain 30--307 MHz and in the temperature domain 20--500 mK. Usually, in wide QW structures closely to ν = 1/2 the fractional quantum Hall effect (FQHE) regime is realized at some parameters of the sample. However, in our structure, at ν = 1/2 it is a compressible state corresponding to gas of composite fermions which is observed. This is confirmed by apparent frequency independence and weakly decreasing temperature dependence of Re σ(ω). Comparing the dependences of this quantity on temperature and power of the acoustic wave we conclude that the observed nonlinear behavior of the conductance is compatible with heating of the composite fermions by the acoustic wave. For comparison, we also study the vicinity of ν = 3/2 where the FQHE regime is clearly observed.},
	author = {I.L. Drichko and I. Yu Smirnov and A.V. Suslov and D. Kamburov and K.W. Baldwin and L.N. Pfeiffer and K.W. West and Y.M. Galperin},
	doi = {https://doi.org/10.1016/j.ssc.2019.113698},
	issn = {0038-1098},
	journal = {Solid State Communications},
	keywords = {A. Semiconductors, Two-dimensional electron gas, D. Quantum Hall effect},
	pages = {113698},
	title = {Composite fermions in a wide quantum well in the vicinity of the filling factor 1/2},
	url = {http://www.sciencedirect.com/science/article/pii/S0038109819304351},
	volume = {301},
	year = {2019},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/S0038109819304351},
	bdsk-url-2 = {https://doi.org/10.1016/j.ssc.2019.113698}}

