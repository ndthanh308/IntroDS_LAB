@article{Wojs11b,
	abstract = {We have employed large scale exact numerical diagonalization in Haldane spherical geometry in a comparative analysis of the correlated many-electron states in the half-filled low Landau levels of graphene and such conventional semiconductors as GaAs, including both spin and valley (i.e., pseudospin) degrees of freedom. We present evidence that the polarized Fermi sea of essentially non-interacting composite fermions remains stable against a pairing transition in both lowest Landau levels of graphene. However, it undergoes spontaneous depolarization, which in (ideal) graphene is unprotected for the lack of a single-particle pseudospin splitting. These results point to the absence of the non-Abelian Pfaffian phase in graphene.},
	author = {W\'ojs, Arkadiusz and M\"{o}ller, Gunnar and Cooper, Nigel R.},
