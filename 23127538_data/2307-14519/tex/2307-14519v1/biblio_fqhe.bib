@article{Li17,
	abstract = {The unique Landau level spectrum of bilayer graphene (BLG) is predicted to support a non-Abelian even-denominator fractional quantum Hall state (FQHE) similar to the state first identified in GaAs. However, the nature of this state has remained difficult to characterize. Here we report transport measurements of a robust sequence of even denominator FQHE in dual gated BLG devices. Parallel field measurement confirms the spin-polarized nature of the ground state, consistent with the Pfaffian/anti-Pfaffian description. The sensitivity of the even denominator states to both filling fraction and transverse displacement field, provides new opportunities for tunability. Our results suggest that BLG is a platform where topological ground states with possible non-Abelian excitations can be manipulated and controlled.},
	author = {Li, J. I. A. and Tan, C. and Chen, S. and Zeng, Y. and Taniguchi, T. and Watanabe, K. and Hone, J. and Dean, C. R.},
	doi = {10.1126/science.aao2521},
	eprint = {http://science.sciencemag.org/content/early/2017/10/04/science.aao2521.full.pdf},
	issn = {0036-8075},
	journal = {Science},
	publisher = {American Association for the Advancement of Science},
	title = {Even denominator fractional quantum {Hall} states in bilayer graphene},
	url = {http://science.sciencemag.org/content/early/2017/10/04/science.aao2521},
	year = {2017},
