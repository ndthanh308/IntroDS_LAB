	doi = {https://doi.org/10.1016/j.jcp.2011.01.020},
	journal = {Journal of Computational Physics},
	keywords = {AMR, Relativistic HD, Relativistic MHD, Shock-capturing schemes},
	number = {3},
	pages = {718-744},
	title = {Parallel, grid-adaptive approaches for relativistic hydro and magnetohydrodynamics},
	url = {https://www.sciencedirect.com/science/article/pii/S0021999111000386},
	volume = {231},
	year = {2012},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0021999111000386},
