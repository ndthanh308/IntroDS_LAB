	journal = {The Astrophysical Journal},
	month = {jul},
	number = {1},
	pages = {556--562},
	publisher = {American Astronomical Society},
	title = {Are {\textquotedblleft}{EIT} Waves{\textquotedblright} Fast-Mode {MHD} Waves?},
