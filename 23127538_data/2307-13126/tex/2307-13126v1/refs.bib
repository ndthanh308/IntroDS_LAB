	pages = {171--194},
}

@article{migliore_geometry_2008,
	title = {The {Geometry} of the {Weak} {Lefschetz} {Property} and {Level} {Sets} of {Points}},
	volume = {60},
	issn = {0008-414X, 1496-4279},
	url = {https://www.cambridge.org/core/journals/canadian-journal-of-mathematics/article/geometry-of-the-weak-lefschetz-property-and-level-sets-of-points/646D536EEC7FF1A66813B5BC38D7188C},
	doi = {10.4153/CJM-2008-019-2},
	abstract = {In a recent paper, F. Zanello showed that level Artinian algebras in 3 variables can fail to have the Weak Lefschetz Property (WLP)(WLP)({\textbackslash}text\{WLP\}), and can even fail to have unimodal Hilbert function. We show that the same is true for the Artinian reduction of reduced, level sets of points in projective 3-space. Our main goal is to begin an understanding of how the geometry of a set of points can prevent its Artinian reduction from having WLPWLP{\textbackslash}text\{WLP\}, which in itself is a very algebraic notion. More precisely, we produce level sets of points whose Artinian reductions have socle types 3 and 4 and arbitrary socle degree ≥ 12 (in the worst case), but fail to have WLPWLP{\textbackslash}text\{WLP\}. We also produce a level set of points whose Artinian reduction fails to have unimodal Hilbert function; our example is based on Zanello's example. Finally, we show that a level set of points can have Artinian reduction that has WLPWLP{\textbackslash}text\{WLP\} but fails to have the Strong Lefschetz Property. While our constructions are all based on basic double GGG-linkage, the implementations use very different methods.},
	number = {2},
	urldate = {2023-05-31},
	journal = {Canadian Journal of Mathematics},
	author = {Migliore, Juan C.},
	month = apr,
	year = {2008},
	keywords = {13C13, 13C40, 13D02, 13D40, 14C20, 14M05, arithmetically Cohen–Macaulay, arithmetically Gorenstein, Artinian reduction, basic double G-linkage, level, socle degree, socle type, Strong Lefschetz Property, Weak Lefschetz Property},
	pages = {391--411},
}

