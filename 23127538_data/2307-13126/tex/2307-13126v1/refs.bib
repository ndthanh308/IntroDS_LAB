@misc{abdallah_free_2022,
	title = {Free resolutions and {Lefschetz} properties of some {Artin} {Gorenstein} rings of codimension four},
	url = {http://arxiv.org/abs/2208.01536},
	doi = {10.48550/arXiv.2208.01536},
	abstract = {In 1978, Stanley constructed an example of an Artinian Gorenstein (AG) ring \$A\$ with non-unimodal \$H\$-vector \$(1,13,12,13,1)\$. Migliore-Zanello later showed that for regularity \$r=4\$, Stanley's example has the smallest possible codimension \$c\$ for an AG ring with non-unimodal \$H\$-vector. The weak Lefschetz property (WLP) has been much studied for AG rings; it is easy to show that an AG ring with non-unimodal \$H\$-vector fails to have WLP. In codimension \$c=3\$ it is conjectured that all AG rings have WLP. For \$c=4\$, Gondim showed that WLP always holds for \$r {\textbackslash}le 4\$ and gives a family where WLP fails for any \$r {\textbackslash}ge 7\$, building on an earlier example of Ikeda of failure of WLP for \$r=5\$. In this note we study the minimal free resolution of \$A\$ and relation to Lefschetz properties (both weak and strong) and Jordan type for \$c=4\$ and \$r {\textbackslash}le 6\$.},
	urldate = {2023-05-31},
	publisher = {arXiv},
	author = {Abdallah, Nancy and Schenck, Hal},
	month = aug,
	year = {2022},
	note = {arXiv:2208.01536 [math]},
	keywords = {13E10, 13F55, 14F05, 13D40, 13C13, 13D02, Mathematics - Algebraic Geometry, Mathematics - Commutative Algebra},
}

@misc{chiantini_configurations_2022,
	title = {Configurations of points in projective space and their projections},
	url = {http://arxiv.org/abs/2209.04820},
	doi = {10.48550/arXiv.2209.04820},
	abstract = {We call a set of points \$Z{\textbackslash}subset\{{\textbackslash}mathbb P\}{\textasciicircum}\{3\}\_\{{\textbackslash}mathbb C\}\$ an \$(a,b)\$-geproci set (for GEneral PROjection is a Complete Intersection) if its projection from a general point \$P\$ to a plane is a complete intersection of curves of degrees \$a\$ and \$b\$. Examples which we call grids have been known since 2011. The only nongrid nondegenerate examples previously known had \$ab=12, 16, 20, 24, 30, 36, 42, 48, 54\$ or \$60\$. Here, for any \$4 {\textbackslash}leq a {\textbackslash}leq b\$, we construct nongrid nondegenerate \$(a,b)\$-geproci sets in a systematic way. We also show that the only such example with \$a=3\$ is a \$(3,4)\$-geproci set coming from the \$D\_4\$ root system, and we describe the \$D\_4\$ configuration in detail. We also consider the question of the equivalence (in various senses) of geproci sets, as well as which sets occur over the reals, and which cannot. We identify several additional examples of geproci sets with interesting properties. We also explore the relation between unexpected cones and geproci sets and introduce the notion of \$d\$-Weddle schemes arising from special projections of finite sets of points. This work initiates the exploration of new perspectives on classical areas of geometry. We formulate and discuss a range of open problems in the final chapter.},
	urldate = {2023-05-31},
	publisher = {arXiv},
	author = {Chiantini, Luca and Farnik, Łucja and Favacchio, Giuseppe and Harbourne, Brian and Migliore, Juan and Szemberg, Tomasz and Szpond, Justyna},
	month = sep,
	year = {2022},
	note = {arXiv:2209.04820 [math]},
	keywords = {13C40, 14M05, 14M07, 14M10, 14M12, 14N05, 14N20, Mathematics - Algebraic Geometry, Mathematics - Combinatorics, Mathematics - Commutative Algebra},
}

@article{migliore_survey_2013,
	title = {Survey {Article}: {A} tour of the weak and strong {Lefschetz} properties},
	volume = {5},
	issn = {1939-2346},
	shorttitle = {Survey {Article}},
	url = {https://projecteuclid.org/journals/journal-of-commutative-algebra/volume-5/issue-3/Survey-Article--A-tour-of-the-weak-and-strong/10.1216/JCA-2013-5-3-329.full},
	doi = {10.1216/JCA-2013-5-3-329},
	abstract = {An artinian graded algebra, \$A\$, is said to have the weak Lefschetz property (WLP) if multiplication by a general linear form has maximal rank in every degree. A vast quantity of work has been done studying and applying this property, touching on numerous and diverse areas of algebraic geometry, commutative algebra and combinatorics. Amazingly, though, much of this work has a ``common ancestor" in a theorem originally due to Stanley, although subsequently reproved by others. In this paper we describe the different directions in which research has moved starting with this theorem, and we discuss some of the open questions that continue to motivate current research.},
	number = {3},
	urldate = {2023-05-31},
	journal = {Journal of Commutative Algebra},
	author = {Migliore, Juan and Nagel, Uwe},
	month = sep,
	year = {2013},
	pages = {329--358},
}

@misc{chiantini_classification_2023,
