@article{cook_line_2018,
	title = {Line arrangements and configurations of points with an unexpected geometric property},
	volume = {154},
	issn = {0010-437X, 1570-5846},
	url = {https://www.cambridge.org/core/journals/compositio-mathematica/article/line-arrangements-and-configurations-of-points-with-an-unexpected-geometric-property/E17C13A8AF9F072C4DBA7839CFE8A83E},
	doi = {10.1112/S0010437X18007376},
	abstract = {We propose here a generalization of the problem addressed by the SHGH conjecture. The SHGH conjecture posits a solution to the question of how many conditions a general union XX of fat points imposes on the complete linear system of curves in P2{\textbackslash}mathbb\{P\}{\textasciicircum}\{2\} of fixed degree dd, in terms of the occurrence of certain rational curves in the base locus of the linear subsystem defined by XX. As a first step towards a new theory, we show that rational curves play a similar role in a special case of a generalized problem, which asks how many conditions are imposed by a general union of fat points on linear subsystems defined by imposed base points. Moreover, motivated by work of Di Gennaro, Ilardi and Vallès and of Faenzi and Vallès, we relate our results to the failure of a strong Lefschetz property, and we give a Lefschetz-like criterion for Terao’s conjecture on the freeness of line arrangements.},
	number = {10},
	urldate = {2023-05-31},
	journal = {Compositio Mathematica},
	author = {Cook, David and Harbourne, Brian and Migliore, Juan and Nagel, Uwe},
	month = oct,
	year = {2018},
	keywords = {05E40, 13D02, 14C20, 14F05 (secondary), 14N05, 14N20 (primary), fat points, line arrangements, linear systems, splitting type, stable vector bundle, strong Lefschetz property, unexpected curves},
	pages = {2150--2194},
}

