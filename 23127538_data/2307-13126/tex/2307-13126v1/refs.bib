	doi = {10.1007/s00229-018-1091-3},
	abstract = {Our research is motivated by recent work of Cook II, Harbourne, Migliore, and Nagel on configurations of points in the projective plane with properties that are unexpected from the point of view of the postulation theory. In this note, we revisit the basic configuration of nine points appearing in work of Di Gennaro/Ilardi/Vallès and Harbourne, and we exhibit some additional new properties of this configuration. We then pass to projective three-space \$\${\textbackslash}mathbb \{P\}{\textasciicircum}3\$\$ and exhibit a surface with unexpected postulation properties there. Such higher dimensional phenomena have not been observed so far.},
	number = {3},
	urldate = {2023-05-30},
	journal = {manuscripta mathematica},
	author = {Bauer, Thomas and Malara, Grzegorz and Szemberg, Tomasz and Szpond, Justyna},
	month = mar,
	year = {2020},
	keywords = {14N20, 13A15, 13F20, Primary 4C20, Secondary 14J26},
	pages = {283--292},
}

