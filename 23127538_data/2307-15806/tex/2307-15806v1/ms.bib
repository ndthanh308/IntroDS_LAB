	archiveprefix = {arXiv},
	arxivurl = {http://arxiv.org/abs/1609.08945},
	author = {{De Ridder}, J. and {Molenberghs}, G. and {Eyer}, L. and {Aerts}, C.},
	date-added = {2018-04-26 20:33:31 +0000},
	date-modified = {2018-04-26 20:33:32 +0000},
	doi = {10.1051/0004-6361/201629799},
	eid = {L3},
	eprint = {1609.08945},
	journal = {\aap},
