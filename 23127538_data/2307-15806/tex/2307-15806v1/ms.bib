	archiveprefix = {arXiv},
	arxivurl = {http://arxiv.org/abs/1809.04073},
	author = {{Gieren}, W. and {Storm}, J. and {Konorski}, P. and {G{\'o}rski}, M. and {Pilecki}, B. and {Thompson}, I. and {Pietrzy{\'n}ski}, G. and {Graczyk}, D. and {Barnes}, T.~G. and {Fouqu{\'e}}, P. and {Nardetto}, N. and {Gallenne}, A. and {Karczmarek}, P. and {Suchomska}, K. and {Wielg{\'o}rski}, P. and {Taormina}, M. and {Zgirski}, B.},
