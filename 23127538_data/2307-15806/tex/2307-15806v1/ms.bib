	keywords = {Binary Stars, Cepheid Variables, Main Sequence Stars, Stellar Evolution, Stellar Oscillations, Stellar Spectra, Ultraviolet Spectra, Astronomical Observatories, Iue, Metallicity, Stellar Atmospheres, Stellar Luminosity},
	month = nov,
	pages = {273-285},
	title = {{Classical Cepheids with terminal age main-sequence companions: Constraints on evolution}},
