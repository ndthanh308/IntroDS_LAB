	date-added = {2018-10-08 13:22:59 +0000},
	date-modified = {2018-10-08 13:23:00 +0000},
	doi = {10.1093/mnras/stt2024},
	eprint = {1307.4405},
	journal = {\mnras},
	keywords = {methods: analytical, galaxies: general, Local Group, large-scale structure of Universe, infrared: galaxies},
	month = jan,
	pages = {2146-2162},
	title = {{The local hole revealed by galaxy counts and redshifts}},
	volume = 437,
	year = 2014,
