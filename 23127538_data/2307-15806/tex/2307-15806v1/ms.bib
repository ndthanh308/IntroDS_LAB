	author = {{Lindegren}, L. and {Lammers}, U. and {Hobbs}, D. and {O'Mullane}, W. and {Bastian}, U. and {Hern{\'a}ndez}, J.},
	date-added = {2018-04-26 20:30:42 +0000},
	date-modified = {2018-04-26 20:30:43 +0000},
	doi = {10.1051/0004-6361/201117905},
	eid = {A78},
