	doi = {10.3847/1538-4357/aac5e2},
	eid = {36},
	eprint = {1712.01065},
	journal = {\apj},
	keywords = {distance scale, stars: variables: Cepheids},
	month = jul,
	pages = {36},
	primaryclass = {astro-ph.SR},
	title = {{On Cepheid Distance Scale Bias Due to Stellar Companions and Cluster Populations}},
	volume = 861,
