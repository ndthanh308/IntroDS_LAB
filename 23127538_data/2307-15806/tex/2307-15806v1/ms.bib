	archiveprefix = {arXiv},
	arxivurl = {http://arxiv.org/abs/1309.1182},
	author = {{Rigault}, M. and {Copin}, Y. and {Aldering}, G. and {Antilogus}, P. and {Aragon}, C. and {Bailey}, S. and {Baltay}, C. and {Bongard}, S. and {Buton}, C. and {Canto}, A. and {Cellier-Holzem}, F. and {Childress}, M. and {Chotard}, N. and {Fakhouri}, H.~K. and {Feindt}, U. and {Fleury}, M. and {Gangler}, E. and {Greskovic}, P. and {Guy}, J. and {Kim}, A.~G. and {Kowalski}, M. and {Lombardo}, S. and {Nordin}, J. and {Nugent}, P. and {Pain}, R. and {P{\'e}contal}, E. and {Pereira}, R. and {Perlmutter}, S. and {Rabinowitz}, D. and {Runge}, K. and {Saunders}, C. and {Scalzo}, R. and {Smadja}, G. and {Tao}, C. and {Thomas}, R.~C. and {Weaver}, B.~A.},
