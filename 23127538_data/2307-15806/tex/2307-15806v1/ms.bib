	author = {{Gaia Collaboration} and {Brown}, A.~G.~A. and {Vallenari}, A. and {Prusti}, T. and {de Bruijne}, J.~H.~J. and {Babusiaux}, C. and {Bailer-Jones}, C.~A.~L.},
	date-added = {2018-04-26 19:51:36 +0000},
	date-modified = {2018-04-26 19:51:37 +0000},
