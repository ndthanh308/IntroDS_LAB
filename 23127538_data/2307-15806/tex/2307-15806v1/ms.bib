	journal = {\aap},
	keywords = {Galaxy: stellar content, Galaxy: general, Galaxy: evolution, Galaxy: kinematics and dynamics, Galaxy: structure},
	month = oct,
	pages = {523-540},
	title = {{A synthetic view on structure and evolution of the Milky Way}},
