	eprint = {1811.00537},
	journal = {arXiv e-prints},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	month = nov,
	title = {{Sounds Discordant: Classical Distance Ladder {\amp} $\Lambda$CDM -based Determinations of the Cosmological Sound Horizon}},
