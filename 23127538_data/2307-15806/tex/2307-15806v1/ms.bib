	eid = {L35},
	eprint = {1603.01559},
	journal = {\apjl},
	keywords = {dark energy, supernovae: general},
	month = may,
	pages = {L35},
	title = {{Measuring Type Ia Supernova Populations of Stretch and Color and Predicting Distance Biases}},
	volume = 822,
