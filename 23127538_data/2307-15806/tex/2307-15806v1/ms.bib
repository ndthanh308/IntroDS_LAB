	adsurl = {http://adsabs.harvard.edu/abs/2018ApJ...861..104H},
	archiveprefix = {arXiv},
	arxivurl = {http://arxiv.org/abs/1806.02900},
	author = {{Hatt}, D. and {Freedman}, W.~L. and {Madore}, B.~F. and {Beaton}, R.~L. and {Hoyt}, T.~J. and {Jang}, I.~S. and {Lee}, M.~G. and {Monson}, A.~J. and {Rich}, J.~A. and {Scowcroft}, V. and {Seibert}, M.},
	date-added = {2019-02-08 22:18:25 +0000},
