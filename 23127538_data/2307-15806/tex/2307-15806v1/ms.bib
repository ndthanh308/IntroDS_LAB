	eprint = {1706.09723},
	journal = {\mnras},
	keywords = {methods: numerical, galaxies: haloes, cosmological parameters, large-scale structure of Universe, cosmology: theory},
	month = nov,
	pages = {4946-4955},
	title = {{Sample variance in the local measurements of the Hubble constant}},
