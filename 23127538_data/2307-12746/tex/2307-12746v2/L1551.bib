@article{Feher2016,
	abstract = {{\textless}i{\textgreater}Aims. {\textless}i/{\textgreater}We examined the velocity, density, and temperature structure of Taurus molecular cloud-1 (TMC-1), a filamentary cloud in a nearby quiescent star forming area, to understand its morphology and evolution.{\textless}i{\textgreater}Methods. {\textless}i/{\textgreater}We observed high signal-to-noise (S/N), high velocity resolution NH{\textless}sub{\textgreater}3{\textless}sub/{\textgreater}(1,1), and (2, 2) emission on an extended map. By fitting multiple hyperfine-split line profiles to the NH{\textless}sub{\textgreater}3{\textless}sub/{\textgreater}(1, 1) spectra, we derived the velocity distribution of the line components and calculated gas parameters on several positions. {\textless}i{\textgreater}Herschel {\textless}i/{\textgreater}SPIRE far-infrared continuum observations were reduced and used to calculate the physical parameters of the {\textless}i{\textgreater}Planck {\textless}i/{\textgreater}Galactic Cold Clumps (PGCCs) in the region, including the two in TMC-1. The morphology of TMC-1 was investigated with several types of clustering methods in the parameter space consisting of position, velocity, and column density.{\textless}i{\textgreater}Results. {\textless}i/{\textgreater}Our {\textless}i{\textgreater}Herschel{\textless}i/{\textgreater}-based column density map shows a main ridge with two local maxima and a separated peak to the south-west. The H{\textless}sub{\textgreater}2{\textless}sub/{\textgreater} column densities and dust colour temperatures are in the range of 0.53.3  10{\textless}sup{\textgreater}22{\textless}sup/{\textgreater}cm{\textless}sup{\textgreater}-2{\textless}sup/{\textgreater} and 10.512K, respectively. The NH{\textless}sub{\textgreater}3{\textless}sub/{\textgreater} column densities and H{\textless}sub{\textgreater}2{\textless}sub/{\textgreater} volume densities are in the range of 2.814.210{\textless}sup{\textgreater}14{\textless}sup/{\textgreater}cm{\textless}sup{\textgreater}-2{\textless}sup/{\textgreater} and 0.42.810{\textless}sup{\textgreater}4{\textless}sup/{\textgreater}cm{\textless}sup{\textgreater}-3{\textless}sup/{\textgreater}. Kinetic temperatures are typically very low with a minimum of 9K at the maximum NH{\textless}sub{\textgreater}3{\textless}sub/{\textgreater} and H{\textless}sub{\textgreater}2{\textless}sub/{\textgreater} column density region. The kinetic temperature maximum was found at the protostar IRAS 04381+2540 with a value of 13.7K. The kinetic temperatures vary similarly to the colour temperatures in spite of the fact that densities are lower than the critical density for coupling between the gas and dust phase. The {\textless}i{\textgreater}k{\textless}i/{\textgreater}-means clustering method separated four sub-filaments in TMC-1 with masses of 32.5, 19.6, 28.9, and 45.9 {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}sub/{\textgreater} and low turbulent velocity dispersion in the range of 0.130.2kms{\textless}sup{\textgreater}-1{\textless}sup/{\textgreater}.{\textless}i{\textgreater}Conclusions. {\textless}i/{\textgreater}The main ridge of TMC-1 is composed of four sub-filaments that are close to gravitational equilibrium. We label these TMC-1F1 through F4. The sub-filaments TMC-1F1, TMC-1F2, and TMC-1F4 are very elongated, dense, and cold. TMC-1F3 is a little less elongated and somewhat warmer, and probably heated by the Class I protostar, IRAS 04381+2540, which is embedded in it. TMC-1F3 is approximately 0.1pc behind TMC1-F1. Because of its structure, TMC-1 is a good target to test filament evolution scenarios.},
	author = {Feh{\'{e}}r, O. and T{\'{o}}th, L. V. and Ward-Thompson, D. and Kirk, J. and Kraus, A. and Pelkonen, V.-M. and Pint{\'{e}}r, S. and Zahorecz, S.},
	doi = {10.1051/0004-6361/201424385},
	file = {::},
	journal = {Astronomy {\&} Astrophysics},
	month = {jun},
	pages = {A75},
	publisher = {EDP Sciences},
	title = {{Structure and stability in TMC-1: Analysis of NH3 molecular line and Herschel continuum data}},
	url = {http://www.aanda.org/10.1051/0004-6361/201424385},
	volume = {590},
	year = {2016}
}
