	file = {:home/anton/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lynch et al. - 2017 - A search for circularly polarized emission from young exoplanets.pdf:pdf},
	journal = {Monthly Notices of the Royal Astronomical Society},
	keywords = {radio continuum: planetary systems},
	month = {jun},
	number = {3},
	pages = {3447--3453},
