@article{Hodgson2017,
	abstract = {{\textless}i{\textgreater}Context. {\textless}i/{\textgreater}The {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}i/{\textgreater}-ray BL Lac object OJ287 is known to exhibit inner-parsec jet-wobbling, high degrees of variability at all wavelengths and quasi-stationary features, including an apparent (100) position-angle change in projection on the sky plane.{\textless}i{\textgreater}Aims. {\textless}i/{\textgreater}Sub-50 micro-arcsecond resolution 86GHz observations with the global mm-VLBI array (GMVA) supplement ongoing multi-frequency VLBI blazar monitoring at lower frequencies. Using these maps, together with cm/mm total intensity and {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}i/{\textgreater}-ray observations from {\textless}i{\textgreater}Fermi{\textless}i/{\textgreater}-LAT from 20082014, we aim to determine the location of {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}i/{\textgreater}-ray emission and to explain the inner-mas structural changes. {\textless}i{\textgreater}Methods. {\textless}i/{\textgreater}Observations with the GMVA offer approximately double the angular resolution compared with 43GHz VLBA observations and enable us to observe above the synchrotron self-absorption peak frequency. {\textless}i{\textgreater}Fermi{\textless}i/{\textgreater}-LAT {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}i/{\textgreater}-ray data were reduced and analysed. The jet was spectrally decomposed at multiple locations along the jet. From this, we could derive estimates of the magnetic field using equipartition and synchrotron self-absorption arguments. How the field decreases down the jet provided an estimate of the distance to the jet apex and an estimate of the magnetic field strength at the jet apex and in the broad line region. Combined with accurate kinematics, we attempt to locate the site of {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}i/{\textgreater}-ray activity, radio flares, and spectral changes.{\textless}i{\textgreater}Results. {\textless}i/{\textgreater}Strong {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}i/{\textgreater}-ray flares appeared to originate from either the so-called core region, a downstream stationary feature, or both, with {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}i/{\textgreater}-ray activity significantly correlated with radio flaring in the downstream quasi-stationary feature. Magnetic field estimates were determined at multiple locations along the jet, with the magnetic field found to be 1.6G in the core and 0.4G in the downstream quasi-stationary feature. We therefore found upper limits on the location of the VLBI core as 6.0pc from the jet apex and determined an upper limit on the magnetic field near the jet base of the order of thousands of Gauss.},
	author = {Hodgson, J A and Krichbaum, T P and Marscher, A P and Jorstad, S G and Rani, B and Marti-Vidal, I and Bach, U and Sanchez, S and Bremer, M and Lindqvist, M and Uunila, M and Kallunki, J and Vicente, P and Fuhrmann, L and Angelakis, E and Karamanavis, V and Myserlis, I and Nestoras, I and Chidiac, C and Sievers, A and Gurwell, M and Zensus, J A},
	doi = {10.1051/0004-6361/201526727},
	issn = {0004-6361},
	journal = {Astronomy {\&} Astrophysics},
	keywords = {BL Lacertae objects: individual: OJ 287,galaxies: active,galaxies: jets,magnetic fields,techniques: high angular resolution,techniques: interferometric},
	month = {jan},
	pages = {A80},
	publisher = {EDP Sciences},
	title = {{Location of {\textless}i{\textgreater}{\$}\gamma{\$}{\textless}/i{\textgreater}-ray emission and magnetic field strengths in OJ 287}},
	url = {http://www.aanda.org/10.1051/0004-6361/201526727},
	volume = {597},
	year = {2017}
}
