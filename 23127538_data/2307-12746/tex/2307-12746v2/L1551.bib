@article{Gould1979,
	abstract = {A framework is developed for the accurate calculation of Compton and synchrotron processes in a special class of spherically-symmetric sources. The models considered take a distribution in magnetic field values and high-energy electron density that varies smoothly from central values to values at the outer boundary of the source. Further, the assumption is made that the magnetic field has local disorder (isotropy) and the high-energy electron distribution has local isotropy. Then, in terms of parameters of the source, the synchrotron brightness distribution, total synchrotron flux, Compton-synchrotron flux, and synchrotron self-absorption turnover are all computed accurately. Results are given with a view toward application to the analysis of compact non-thermal sources.},
	author = {Gould, R. J.},
	journal = {Astronomy and Astrophysics},
	pages = {306--311},
	publisher = {EDP Sciences [etc.]},
	title = {{Compton and synchrotron processes in spherically-symmetric non-thermal sources}},
	url = {http://adsabs.harvard.edu/abs/1979A{\%}26A....76..306G},
	volume = {76},
	year = {1979}
}
