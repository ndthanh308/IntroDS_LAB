title = {{High-mass Outflows Identified from COHRS CO (32) Survey}},
url = {http://stacks.iop.org/0004-637X/867/i=2/a=167?key=crossref.51792e7bd882653414c7edbc74b44dbd},
volume = {867},
year = {2018}
