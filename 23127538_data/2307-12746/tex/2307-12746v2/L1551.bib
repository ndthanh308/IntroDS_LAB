	journal = {Journal of Geophysical Research: Space Physics},
	keywords = {Absolute flux density measurements,Cassini mission,Jupiter,Magnetosphere,Radio emissions,Radioastronomy},
	month = {sep},
	number = {A9},
	pages = {A09S15},
	publisher = {Blackwell Publishing Ltd},
	title = {{Jupiter's low-frequency radio spectrum from Cassini/Radio and Plasma Wave Science (RPWS) absolute flux density measurements}},
