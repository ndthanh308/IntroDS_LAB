@article{Liu2018,
	abstract = {Temporal photometric variations at near infrared to submillimeter wavelengths have been found in low-mass young stellar objects. These phenomena are generally interpreted as accretion events of star-disk systems with varying accretion rates. There is growing evidence suggesting that similar luminosity flaring also occurs in high-mass star/cluster-forming regions. We report in this Letter the rise and fall of the 900 {\$}{\{}\backslashmu{\}}{\$}m continuum emission and the newly found 349.1 GHz methanol maser emission in the massive star forming region S255IR{\~{}}SMA1 observed with the Submillimeter Array and the Atacama Large Millimeter/submillimeter Array. The level of flux variation at a factor of {\$}\backslashsim{\$} 2 at the submillimeter band and the relatively short 2-year duration of this burst suggest that the event is probably similar to those milder and more frequent minor bursts seen in 3D numerical simulations.},
	archivePrefix = {arXiv},
	arxivId = {1808.02192},
	author = {Liu, Sheng-Yuan and Su, Yu-Nung and Zinchenko, Igor and Wang, Kuo-Song and Wang, Yuan},
	doi = {10.3847/2041-8213/aad63a},
	eprint = {1808.02192},
	file = {::},
