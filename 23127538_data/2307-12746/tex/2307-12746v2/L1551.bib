	title = {{Jets from young stars}},
	url = {http://articles.adsabs.harvard.edu/cgi-bin/nph-iarticle{\_}query?1983ApJ...274L..83M{\&}data{\_}type=PDF{\_}HIGH{\&}whole{\_}paper=YES{\&}type=PRINTER{\&}filetype=.pdf},
	volume = {274},
