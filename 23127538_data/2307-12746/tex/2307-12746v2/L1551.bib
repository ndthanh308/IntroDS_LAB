}
@article{Louvet2018,
	abstract = {The disk-outflow connection is thought to play a key role in extracting excess angular momentum from a forming protostar. We present ALMA 0.25" angular resolution observations - in the dust continuum at 1.33 mm and of the molecular line transitions of {\$}{\^{}}{\{}12{\}}{\$}CO(2-1) and {\$}{\^{}}{\{}13{\}}{\$}CO(2-1) - of the circumstellar disk and outflow around the T Tauri star HH30, a rare and beautiful example of a pre-main sequence star exhibiting a flared edge-on disk, an optical jet, and a CO molecular outflow. The 1.3 mm continuum emission shows a remarkable elongated morphology along PA=31.2{\{}$\backslash$deg{\}}+/-0.1{\{}$\backslash$deg{\}} that has a constant brightness out to a radius of r=75 au. The emission is marginally resolved in the transverse direction, implying an intrinsic vertical width {\$}\backslashleq{\$}24 au and an inclination to the line-of-sight i{\$}\backslashgeq{\$}84.8{\{}$\backslash$deg{\}}. The monopolar outflow, detected in {\$}{\^{}}{\{}12{\}}{\$}CO, arises from the north-eastern face of the disk from a disk radius r{\$}\backslashleq{\$}22 au and extends up to 5" (or 700 au) above the disk plane. We derive a lower limit to the total mass of the CO cavity/outflow of 1.7E-5 M{\$}{\_}\backslashodot{\$}. The CO cavity morphology is that of a hollow cone with semi-opening angle {\~{}}35{\{}$\backslash$deg{\}}. The derived kinematics are consistent with gas flowing along the conical surface with a constant velocity of 9.3+/-0.7 km/s. We detect small rotation signatures (Vphi x sin(i) {\$}\backslashin{\$} [0.1;0.5] km/s) in the same sense as the underlying circumstellar disk. From these rotation signatures we infer an average specific angular momentum of the outflow of 38+/-15 au km/s at altitudes z{\textless}250 au. We also report the detection of small amplitude wiggling (1.2{\{}$\backslash$deg{\}}) of the CO axis around an average inclination to the line of sight of i=91{\{}$\backslash$deg{\}}. The derived morphology and kinematics of the CO cavity are compatible with expectations from a slow disk wind, originating either through photo-evaporation or magneto-centrifugal processes.},
	archivePrefix = {arXiv},
	arxivId = {1808.03285},
	author = {Louvet, Fabien and Dougados, Catherine and Cabrit, Sylvie and Mardones, Diego and M{\'{e}}nard, Fran{\c{c}}ois and Tabone, Beno{\^{i}}t and Pinte, Christophe and Dent, William},
	eprint = {1808.03285},
	file = {::},
	month = {aug},
	title = {{The HH30 edge-on T Tauri star A rotating and precessing monopolar outflow scrutinized by ALMA}},
	url = {http://arxiv.org/abs/1808.03285},
	year = {2018}
}
