@article{Rodriguez2017,
	abstract = {We present a comparison between the time-evolution over the past {\$}\backslashsim 20{\$} years of the radio continuum and H{\$}\backslashalpha{\$} emission of HH{\~{}}1 and 2. We find that the radio continuum and the H{\$}\backslashalpha{\$} emission of both objects show very similar trends, with HH{\~{}}1 becoming fainter and HH{\~{}}2 brightening quite considerably (about a factor of 2). We also find that the {\$}F{\_}{\{}\backslashrm H\backslashalpha{\}}/F{\_}{\{}ff{\}}{\$} (H{\$}\backslashalpha{\$} to free-free continuum) ratio of HH{\~{}}1 and 2 has higher values than the ones typically found in planetary nebulae (PNe) which we interpret as an indication that the H{\$}\backslashalpha{\$} and free-free emission of HH{\~{}}1/2 is produced in emitting regions with lower temperatures ({\$}\backslashsim 2000{\$}{\~{}}K) than the emission of PNe (with {\$}\backslashsim 10{\^{}}4{\$}{\~{}}K).},
