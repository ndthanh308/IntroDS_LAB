@article{winterSimulationsMachineLearning2023,
  title = {Simulations with Machine Learning Potentials Identify the Ion Conduction Mechanism Mediating Non-{{Arrhenius}} Behavior in {{LGPS}}},
  author = {Winter, Gavin and {G{\'o}mez-Bombarelli}, Rafael},
  year = {2023},
