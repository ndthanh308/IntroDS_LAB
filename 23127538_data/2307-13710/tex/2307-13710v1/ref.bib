@article{kulichenkoUncertaintydrivenDynamicsActive2023,
  title = {Uncertainty-Driven Dynamics for Active Learning of Interatomic Potentials},
  author = {Kulichenko, Maksim and Barros, Kipton and Lubbers, Nicholas and Li, Ying Wai and Messerly, Richard and Tretiak, Sergei and Smith, Justin S. and Nebgen, Benjamin},
