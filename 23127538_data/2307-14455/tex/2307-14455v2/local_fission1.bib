@article{Chen:2022,
	author = {Chen, M. and Li, T. and Schuetrumpf, B. and Reinhard, P.-G. and Nazarewicz, W.},
	date-added = {2022-09-03 19:09:05 -0700},
	date-modified = {2022-09-08 14:47:41 -0700},
	doi = {https://doi.org/10.1016/j.cpc.2022.108344},
	issn = {0010-4655},
	journal = {Comp. Phys. Comm.},
	keywords = {Nuclear density functional theory, Hartree-Fock-Bogoliubov method, Skyrme energy density functional, Finite volume methods, 3D coordinate-space representation},
	pages = {108344},
	title = {Three-dimensional Skyrme Hartree-Fock-Bogoliubov solver in coordinate-space representation},
	url = {https://www.sciencedirect.com/science/article/pii/S0010465522000625},
	volume = {276},
	year = {2022},
	abstract = {The coordinate-space representation of the Hartree-Fock-Bogoliubov theory is the method of choice to study weakly bound nuclei whose properties are affected by the quasiparticle continuum space. To describe such systems, we developed a three-dimensional Skyrme-Hartree-Fock-Bogoliubov solver HFBFFT based on the existing, highly optimized and parallelized Skyrme-Hartree-Fock code Sky3D. The code does not impose any self-consistent spatial symmetries such as mirror inversions or parity. The underlying equations are solved in HFBFFT directly in the canonical basis using the fast Fourier transform. To remedy the problems with pairing collapse, we implemented the soft energy cutoff and pairing annealing. The convergence of HFB solutions was improved by a sub-iteration method. The Hermiticity violation of differential operators brought by Fourier-transform-based differentiation has also been solved. The accuracy and performance of HFBFFT were tested by benchmarking it against other HFB codes, both spherical and deformed, for a set of nuclei, both well-bound and weakly-bound.},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0010465522000625},
	bdsk-url-2 = {https://doi.org/10.1016/j.cpc.2022.108344}}

