	doi = {10.1016/0375-9474(76)90004-X},
	issn = {0375-9474},
	journal = {Nucl. Phys. A},
	pages = {237},
	title = {{Derivation of an adiabatic time-dependent Hartree-Fock formalism from a variational principle}},
	url = {http://www.sciencedirect.com/science/article/pii/037594747690004X},
	volume = {258},
	year = {1976},
	abstract = {A derivation of the adiabatic time-dependent Hartree-Fock formalism is given, which is based on a variational principle analogous to Hamilton's principle in classical mechanics. The method leads to a Hamiltonian for collective motion which separates into a potential and a kinetic energy and gives mass and potential parameters in terms of the nucleon-nucleon interaction. The adiabatic approximation assumes slow motion but not small amplitudes and can therefore describe anharmonic effects. The RPA is a limiting case where both amplitudes and velocities are small. The variational approach provides a consistent way of extracting coordinates and momenta from the density matrix and of obtaining equations of motion when particular trial forms for this density matrix are chosen. One such choice leads to Thouless-Valatin formula. An other choice leads to irrotational hydrodynamics.},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/037594747690004X},
	bdsk-url-2 = {https://doi.org/10.1016/0375-9474(76)90004-X}}

