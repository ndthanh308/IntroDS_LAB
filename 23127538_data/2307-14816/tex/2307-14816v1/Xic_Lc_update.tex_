%
First, we discuss the matrix elements corresponding to the SD-contributions.
One has
%
\bea  
M_{\rm SD} (\Xi^0_c \to\Lambda_c^+\pi^-) &=&
\frac{G_F}{\sqrt{2}}\Big\{ V_{\rm CKM}^{(u)}\,
\bar u(p_2)\Big[ (C_2^{(u)} +\xi C_1^{(u)})\,D_{\rm Ia} + (C_2^{(u)}-C_1^{(u)})\,D_{\rm IIb}\Big] u(p_1)
\nn
&&
\phantom{\frac{G_F}{\sqrt{2}}}
+ V_{\rm CKM}^{(c)} (C_2^{(c)}-C_1^{(c)})\,
\bar u(p_2)\Big[ D_{\rm IIa} + D_{\rm III}\Big] u(p_1)
\Big\}\,.
\label{eq-SD:matrix_element}
\ena
Here we have introduced the short notations as follows
%
\be
V_{\rm CKM}^{(u)} =  |V_{us}^\ast V_{ud}|, \quad\text{and}\quad
V_{\rm CKM}^{(c)} =  |V_{cs}^\ast V_{cd}|.
\en

The contribution from the tree diagram factorizes into two pieces
according to
\bea
D_{\rm Ia}  &=&
N_c\,g_{M}\int\!\!\frac{d^4k}{(2\pi)^4i}\widetilde\Phi_{M}(-k^2)\,
\Tr\left[O^\mu_L S_u(k-w_u q) \gamma_5 S_d(k+w_d q)\right]
\nn
&\times&
6\,g_{B_1}g_{B_2}\int\!\!\frac{d^4k_1}{(2\pi)^4i}\int\!\!\frac{d^4k_2}{(2\pi)^4i}
\widetilde\Phi_{B_1}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\nn
&&
\times S_c(k_2)\Tr\left[ S_u(k_1+p_2) O_{\mu\, L} S_s(k_1+p_1)\gamma_5
  S_d(k_1+k_2)\gamma_5\right]
\nn[1.2ex] 
&=&
- 6 f_M q^\mu g_{B_1}g_{B_2}
\int\!\!\frac{d^4k_1}{(2\pi)^4i}\int\!\!\frac{d^4k_2}{(2\pi)^4i}
\widetilde\Phi_{B_1}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\nn
&&
\times S_c(k_2)\Tr\left[ S_u(k_1+p_2) O_{\mu\, L} S_s(k_1+p_1)\gamma_5
  S_d(k_1+k_2)\gamma_5\right]
\label{eq:tree}
\ena
%
where $q_1=k_2-w_1^{\rm in} p_1$, $q_2=-k_1-k_2-w_2^{\rm in} p_1$ and
$r_1=-k_2+w_1^{\rm out} p_2$, $r_2=-k_1-p_2+w_2^{\rm out} p_2$. The expression
for $\vec\Omega^{\,2}$ is given by Eq.~(\ref{eq:vertex}).
Hereafter we adopt the brief notations $B_1$ for the ingoing baryon with
the momentum $p_1$, $B_2$ for the outgoing baryon with
the momentum $p_2$ and $M$ for   the outgoing meson with the momentum $q$.
The minus sign  in front of $f_M$ appears because the momentum $q$ flows
in the opposite direction from the decay of $M$-meson.

The calculation of the three-loop $W$--exchange diagrams
is much more involved because the matrix element does not factorize.
One has
\bea
D_{\rm IIb} &=&
12 g_{B_1}g_{B_2}g_{M}
\Big[ \prod\limits_{i=1}^3\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
%\int\!\!\frac{d^4k_2}{(2\pi)^4i}
%\int\!\!\frac{d^4k_3}{(2\pi)^4i}
\widetilde\Phi_{B_1}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\widetilde\Phi_{M}(-P^2)\,
\nn
&\times&
S_c(k_3)\Tr\left[\gamma_5 S_d(k_2+p_2)(1+\gamma_5)S_u(k_2+k_3)\right]
\nn
&\times&
\Tr\left[S_u(k_1+p_2)\gamma_5S_d(k_1+p_1)\gamma_5 S_s(k_1+k_3)(1-\gamma_5)\right],
\nn[1.2ex]
&& q_1 = k_3-w_1^{\rm in} p_1, \quad  q_2 = k_1+p_1-w_2^{\rm in} p_1,
\nn
&& r_1 = -k_3+w_1^{\rm out} p_2, \quad  r_2 = k_2+k_3+w_2^{\rm out} p_2,\qquad
P=k_1+w_u p_1 + w_d p_2.
\label{eq:IIb}
\ena
%
\bea
D_{\rm IIa} &=&
6 g_{B_1}g_{B_2}g_{M}
\Big[ \prod\limits_{i=1}^3\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
\widetilde\Phi_{B_1}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\widetilde\Phi_{M}(-P^2)\,
\nn
&\times&
S_c(k_3-k_2)O^\mu_LS_c(k_1+p_1)
\Tr\left[S_d(k_3)\gamma_5 S_u(k_2+p_2)\gamma_5 S_d(k_2+p_1)O_{\mu\, L}
  S_s(k_3-k_1)\gamma_5\right]
\nn[1.2ex]
&& q_1 = k_1+p_1-w_1^{\rm in} p_1, \quad  q_2 = -k_3-w_2^{\rm in} p_1,
\nn
&& r_1 = k_2-k_3+w_1^{\rm out} p_2, \quad  r_2 = -k_2-p_2+w_2^{\rm out} p_2,\qquad
P=k_2+w_u p_1 + w_d p_2.
\label{eq:IIa}
\ena
%
\bea
D_{\rm III} &=&
6 g_{B_1}g_{B_2}g_{M}
\Big[\prod\limits_{i=1}^3\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
\widetilde\Phi_{B_1}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\widetilde\Phi_{M}(-P^2)\,
\nn
&\times&
S_c(k_3)O^\mu_LS_c(k_2)
\Tr\left[S_d(k_1+k_3)\gamma_5 S_u(k_1+p_2)\gamma_5 S_d(k_1+p_1)\gamma_5
  S_s(k_1+k_2)O_{\mu\, R}\right]
\nn[1.2ex]
&& q_1 = k_2-w_1^{\rm in} p_1, \quad  q_2 = k_1+p_1-w_2^{\rm in} p_1,
\nn
&& r_1 = -k_3+w_1^{\rm out} p_2, \quad  r_2 = -k_1-p_2+w_2^{\rm out} p_2,\qquad
P=k_1+w_u p_1 + w_d p_2.
\label{eq:III}
\ena
%

The calculation of the three-loop integrals proceeds in two steps, first,
to perform the loop integration by using  Fock-Schwinger representation
for the quark propagators, second, to calculate the integrals numerically
over Fock-Schwinger variables by adopting the quark confinement anzatz.
The details of such calculations may be found in our recent
papers~\cite{Gutsche:2017hux,Gutsche:2018utw}. 
The calculation is quite time-consuming both analytically and numerically.

Finally, the matrix element describing the SD-contributions are written
as
%
\be  
M_{\rm SD} (\Xi^0_c \to\Lambda_c^+\pi^-) =
\frac{G_F}{\sqrt{2}}
\bar u(p_2)\Big( A_{\rm\, SD} + \gamma_5 B_{\rm\, SD}\Big)u(p_1),
\label{eq-SD:full-matrix_element}
\en
where
\be
A_{\rm\, SD} =  V_{\rm CKM}^{(u)}\,
\Big[ (C_2^{(u)} +\xi C_1^{(u)})\,A_{\rm Ia} + (C_2^{(u)}-C_1^{(u)})\,A_{\rm IIb}\Big]
 + V_{\rm CKM}^{(c)} (C_2^{(c)}-C_1^{(c)})\,
\Big( A_{\rm IIa} + A_{\rm III}\Big)
\nonumber
\en
and similar expression for $B_{\rm\, SD}$.

%----------------------- LD-diagrams --------------------------------

%------------------- pole-1 ------------------------------

%
Now, we discuss the matrix elements corresponding to the LD-contributions
coming from the first diagram in Fig.~\ref{fig:pole} with
$B_{\rm res}=\Sigma^0_c$.
One has
%
\bea  
M_{\rm LD-I} \left(\Xi^0_c \to\Lambda_c^+\pi^-\right) &=&
\frac{G_F}{\sqrt{2}}\, V_{\rm CKM}^{(c)}\,\left(C_1^{(c)}-C_2^{(c)}\right)\,\nn
&\times&
\bar u(p_2)\,D_{B_{\rm res}B_2M}(p_1,p_2) S_{\rm res}(p_1)
D_{B_1B_{\rm res}}(p_1)\,u(p_1)
\label{eq-LD-I:matrix_element}
\ena
where $ S_{\rm res}(p_1) = 1/(m_{\rm res}-\not\!{p}_1) $.

The explicit form of $D$-functions are written down as
\bea
D_{B_{\rm res}B_2M} &=&
12\, g_{B_{\rm res}}\,g_{B_2}\,g_{M}
\Big[ \prod\limits_{i=1}^2\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
\widetilde\Phi_{B_{\rm res}}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\widetilde\Phi_{M}(-P^2)\,
\nn
&\times&
S_c(k_2)\gamma_\alpha\gamma_5\, \Tr\left[
  \gamma_5 S_u(k_1+p_2)\gamma_5 S_d(k_1+p_1)\gamma^\alpha S_d(k_1+k_2) \right]
\ena
where
\begin{align}
q_1 & = k_2-w_1^{\rm res} p_1, \qquad  q_2 = -k_1-k_2-w_2^{\rm res} p_1,
\nn
r_1 & =-k_2+w_1^{\rm out} p_2, \quad  r_2 = -k_1-(1-w_2^{\rm out}) p_2,\qquad
P=k_1+w_1^M p_1 + w_2^M p_2.
\nonumber
\end{align}
%
\bea
D_{B_1B_{\rm res}} &=&
12\, g_{B_1}\,g_{B_{\rm res}}
\Big[ \prod\limits_{i=1}^3\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
\widetilde\Phi_{B_1}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_{\rm res}}\Big(-\vec\Omega_r^{\,2}\Big)
\nn
&\times&
\gamma_\alpha\gamma_5 S_c(k_2+p_1)O_{\mu\,L} S_c(k_1+p_1)\,\Tr\left[
 S_d(k_3)\gamma^\alpha S_d(k_3-k_2) O^\mu_L S_s(k_3-k_1)\gamma_5 \right]
\ena
where
\begin{align}
q_1 & = k_1+(1-w_1^{\rm in}) p_1, \qquad  q_2 = -k_3-w_2^{\rm in} p_1,
\nn
r_1 & =-k_2-(1-w_1^{\rm res}) p_1, \quad  r_2 = k_3+w_2^{\rm res}) p_1.
\nonumber
\end{align}

By using the mass-shell conditions, one obtains
%
\bea
\bar u(p_2)D_{B_{\rm res}B_2M}(p_1,p_2) &=&
\bar u(p_2)\gamma_5(C_{\rm I}+\not\!{p}_1D_{\rm I}),
\nn
D_{B_1B_{\rm res}}(p_1) u(p_1) &=& (A_{\rm I}+\gamma_5B_{\rm I})u(p_1).
\label{eq:B1R1}
\ena
The final expression for the first pole diagram is written as
%
\be
M_{\rm LD-I} =  \frac{G_F}{\sqrt{2}}\,
\bar u(p_2) \Big(A_{LD-I} + \gamma_5 B_{LD-I} \Big) u(p_1),
\label{eq:LD-I}
\en
where
\bea
A_{\rm LD-I} &=&  V_{\rm CKM}^{(c)}\,\left(C_1^{(c)}-C_2^{(c)}\right)\,
\frac{B_{\rm I} (C_{\rm I} - m_1 D_{\rm I})}{m_{\rm res} + m_1},
\nn
B_{\rm LD-I} &=&  V_{\rm CKM}^{(c)}\,\left(C_1^{(c)}-C_2^{(c)}\right)\,
\frac{A_{\rm I} (C_{\rm I} + m_1 D_{\rm I})}{m_{\rm res} - m_1},
\nonumber
\ena

%------------------- pole-2 ------------------------------

The matrix element corresponding to the LD-contributions
coming from the second diagram in Fig.~\ref{fig:pole} with
$B_{\rm res}=\Xi^{'\,+}_c$ is calculated in a similar way.
We perform the necessary steps below.
%
\bea  
M_{\rm LD-II}  &=&
\frac{G_F}{\sqrt{2}}\,
\bar u(p_2)\Big\{
\left[
  V_{\rm CKM}^{(u)} \left(C_1^{(u)}-C_2^{(u)}\right) D^{(u)}_{B_1B_{\rm res}}(p_2)
+ V_{\rm CKM}^{(c)} \left(C_1^{(c)}-C_2^{(c)}\right) D^{(c)}_{B_1B_{\rm res}}(p_2)
\right]
\nn
&&\phantom{\frac{G_F}{\sqrt{2}}\,\bar u(p_2)}
\times S_{\rm res}(p_2)D_{B_1B_{\rm res}M}(p_1,p_2) 
\Big\}\,u(p_1)
\label{eq-LD-II:matrix_element}
\ena
where
%
\bea
D^{(u)}_{B_1B_{\rm res}}(p_2) &=&
-12\,g_{B_{\rm res}}\, g_{B_2}
\Big[ \prod\limits_{i=1}^3\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
\widetilde\Phi_{B_{\rm res}}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\nn
&\times&
S_c(k_3)\gamma_\alpha\gamma_5
\Tr\left[ S_u(k_2+p_2)(1+\gamma_5)S_d(k_2+k_3) \gamma_5\right]
\nn
&\times&
\Tr\left[ S_u(k_1+p_2)\gamma^\alpha S_s(k_1+k_3)(1-\gamma_5) \right]
\ena
\begin{align}
q_1 & = k_3-w_1^{\rm res} p_2, \qquad  q_2 = k_1+(1-w_2^{\rm res}) p_2,
\nn
r_1 & =-k_3+w_1^{\rm out} p_2, \quad  r_2 = -k_2-(1-w_2^{\rm out}) p_2.
\nonumber
\end{align}
%
\bea
D^{(c)}_{B_1B_{\rm res}}(p_2) &=&
-6\,g_{B_{\rm res}}\, g_{B_2}
\Big[ \prod\limits_{i=1}^3\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
\widetilde\Phi_{B_{\rm res}}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_2}\Big(-\vec\Omega_r^{\,2}\Big)
\nn
&\times&
S_c(k_2+p_2)O_{\mu\,L} S_c(k_1+p_2)\,\gamma_\alpha\gamma_5
\nn
&\times&
\Tr\left[S_u(k_3)\gamma_5 S_d(k_3-k_2) O_L^\mu S_s(k_3-k_1)\gamma^\alpha \right]
\ena
\begin{align}
q_1 & = k_1+(1-w_1^{\rm res}) p_2, \qquad  q_2 = -k_3-w_2^{\rm res} p_2,
\nn
r_1 & =-k_2-(1-w_1^{\rm out}) p_2, \quad  r_2 = k_3+w_2^{\rm out} p_2.
\nonumber
\end{align}
%
\bea
D_{ B_1 B_{\rm res} M} &=&
-6\,g_{B_1}\,g_{B_{\rm res}}\,g_{M}
\Big[ \prod\limits_{i=1}^2\int\!\!\frac{d^4k_i}{(2\pi)^4i} \Big]
\widetilde\Phi_{B_1}\Big(-\vec\Omega_q^{\,2}\Big)
\widetilde\Phi_{B_{\rm res}}\Big(-\vec\Omega_r^{\,2}\Big)
\widetilde\Phi_{M}(-P^2)\,
\nn
&\times&
\gamma_\alpha\gamma_5\,S_c(k_2) \Tr\left[
  S_u(k_1+p_2)\gamma_5 S_d(k_1+p_1)\gamma_5 S_s(k_1+k_2)\gamma^\alpha
  \right].
\ena
\begin{align}
q_1 & = k_2-w_1^{\rm in} p_1, \qquad  q_2 = k_1+(1-w_2^{\rm in}) p_1,
\nn
r_1 & =-k_2+w_1^{\rm res} p_2, \quad  r_2 = -k_1-(1-w_2^{\rm res}) p_2,\qquad
P=k_2+w_1^M p_1 + w_2^M p_2.
\nonumber
\end{align}
%
By using the mass-shell conditions, one obtains
%
\bea
\bar u(p_2) D^{(u,c)}_{B_{\rm res}B_2}(p_2) &=&
\bar u(p_2) (A^{(u,c)}_{\rm II}+\gamma_5 B^{(u,c)}_{\rm II})
\nn
D_{B_1 B_{\rm res} M}(p_1,p_2)u(p_1) &=&
\gamma_5( C_{\rm II} + \not\!{p}_2 D_{\rm II}) u(p_1).
\nonumber
\ena
The final expression for the second pole diagram is written as
%
\be
M_{\rm LD-II} =  \frac{G_F}{\sqrt{2}}\,
\bar u(p_2) \Big(A_{\rm LD-II} + \gamma_5 B_{\rm LD-II} \Big) u(p_1),
\label{eq:LD-II}
\en
where
%
\bea
A_{\rm LD-II} &=&
\Big\{
V_{\rm CKM}^{(u)}\,\left(C_1^{(u)}-C_2^{(u)}\right)\,B^{(u)}_{\rm II}
+
V_{\rm CKM}^{(c)}\,\left(C_1^{(c)}-C_2^{(c)}\right)\,B^{(c)}_{\rm II}
\Big\}
\frac{ (C_{\rm II} + m_2 D_{\rm II})}{m_{\rm res} + m_2},
\nn
B_{\rm LD-II} &=&
\Big\{
V_{\rm CKM}^{(u)}\,\left(C_1^{(u)}-C_2^{(u)}\right)\,A^{(u)}_{\rm II}
+
V_{\rm CKM}^{(c)}\,\left(C_1^{(c)}-C_2^{(c)}\right)\,A^{(c)}_{\rm II}
\Big\}
\frac{ (C_{\rm II} - m_2 D_{\rm II})}{m_{\rm res} - m_2},
\nonumber
\ena
%The full amplitudes describing the contributions of two resonances
%with quantum numbers $\frac12^+$ can be naturally written as
%
%\be
%A_{1/2^+} = A_{\rm LD-I}+A_{\rm LD-II}, \qquad\text{and}\qquad
%B_{1/2^+} = B_{\rm LD-I}+B_{\rm LD-II}.
%\en

It is widely accepted that S-wave amplitude is saturated by the $1/2^-$
resonances, see, e.g., Refs~\cite{Marshak,Bailin:1977gv} for the
original suggestions and
\cite{Ebert:1983yh,Ebert:1983ih,Cheng:1985dw,Cheng:2018hwl}
for the subsiquent applications.Ordinarily, their contributions are calculated
by using the well-known soft-pion theorem in the current-algebra approach.
It allows one to express the parity-violating S-wave amplitude in terms of
parity-conserving matrix elements. In our case, one has
%
\be
A_{1/2^-}\left(\Xi_c^0\to \Lambda_c^+ + \pi^-\right) =
\frac{1}{f_\pi} A_{\Xi_c\Sigma_c}, \qquad\text{where}\quad
A_{\Xi_c\Sigma_c} = V_{\rm CKM}^{(c)}\,\left(C_1^{(c)}-C_2^{(c)}\right)\,A_{\rm I}.
\label{eq:1/2minus}
\en
The amplitude $A_{\rm I}$ is defined in Eq.~(\ref{eq:B1R1}).

Finally, the transition $\Xi_c^0\to \Lambda_c^+ + \pi^-$ amplitude
is written in terms of invariant amplitudes as
%
\be
<\Lambda_c^+\,\pi^-| {\mathcal H}_{\rm eff} | \Xi_c^0 >
= \frac{G_F}{\sqrt{2}}\, \bar u(p_2) \left(A+\gamma_5 B\right)
\label{eq:ampl}
\en
where $A$ and $B$ are given by
%
\bea
A &=& A_{\rm SD} + A_{\rm LD}, \qquad
A_{\rm LD} = A_{\rm LD-I} + A_{\rm LD-II}  + A_{1/2^-} ,
\nn
B &=& B_{\rm SD} + B_{\rm LD}, \qquad
B_{\rm LD} = B_{\rm LD-I} + B_{\rm LD-II}.
\label{eq:fullAB}
\ena

It is more convenient to use helicity amplitudes $ H_{\lambda_1 \lambda_M}$
instead of invariant ones $A$ and $B$ as described in~\cite{Korner:1992wi}.
One has 
\be
H^V_{\tfrac12\,t} = \sqrt{Q_+}\,A\,,\qquad H^A_{\tfrac12\,t} = \sqrt{Q_-}\,B\,,
\en 
where $m_\pm=m_1\pm m_2$, $Q_{\pm}=m_\pm^2-q^2$.

Finally, the two-body decay width reads
\be
\Gamma(B_1\to B_2+M) = 
\frac{G_F^2}{32\pi}\frac{\mathbf{|p_2|}}{m_1^2}\,{\mathcal H}_S\,,
\quad
{\mathcal H}_S =  2\Big(\Big|H^V_{ \tfrac12\,t}\Big|^2 \,+\, 
\Big|H^A_{\tfrac12\,t}\Big|^2\Big) \, 
\label{eq:width}
\en
where $\mathbf{|p_2|}=\lambda^{1/2}(m_1^2,m_2^2,q^2)/(2m_1)$.   
 

