	journal = {Nat. Commun.},
	month = {Apr},
	number = {1},
	pages = {1854},
	title = {Quantitative prediction of grain boundary thermal conductivities from local atomic environments},
	url = {https://doi.org/10.1038/s41467-020-15619-9},
	volume = {11},
	year = {2020},
	bdsk-url-1 = {https://doi.org/10.1038/s41467-020-15619-9}}

