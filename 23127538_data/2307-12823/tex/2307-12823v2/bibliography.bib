	number = {3},
	pages = {034007},
	publisher = {{IOP} Publishing},
	title = {Quantum-assisted Helmholtz machines: A quantum{\textendash}classical deep learning framework for industrial datasets in near-term devices},
	url = {https://doi.org/10.1088/2058-9565/aabd98},
	volume = {3},
	year = 2018,
	bdsk-url-1 = {https://doi.org/10.1088/2058-9565/aabd98}}

@article{Benedetti2019,
