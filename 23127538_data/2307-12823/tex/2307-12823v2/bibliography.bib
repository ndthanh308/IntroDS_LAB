	doi = {10.1038/s41586-021-03928-y},
	id = {Egan2021},
	isbn = {1476-4687},
	journal = {Nature},
	number = {7880},
	pages = {281--286},
	title = {Fault-tolerant control of an error-corrected qubit},
	url = {https://doi.org/10.1038/s41586-021-03928-y},
	volume = {598},
	year = {2021},
	bdsk-url-1 = {https://doi.org/10.1038/s41586-021-03928-y}}

