@article{Fedorov2021-4,
	abstract = {Fascinating progress in understanding our world at the smallest scales moves us to the border of a new technological revolution governed by quantum physics. By taking advantage of quantum phenomena, quantum computing devices allow a speedup in solving diverse tasks. In this Perspective, we discuss the potential impact of quantum computing on computational biology. Bearing in mind the limitations of existing quantum computing devices, we attempt to indicate promising directions for further research in the emerging area of quantum computational biology.},
