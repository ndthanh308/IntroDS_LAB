	issn = {0027-8424},
	journal = {Proceedings of the National Academy of Sciences},
	number = {4},
	publisher = {National Academy of Sciences},
	title = {Quantum phases of Rydberg atoms on a kagome lattice},
	url = {https://www.pnas.org/content/118/4/e2015785118},
	volume = {118},
