	doi = {10.1038/s41565-020-00831-x},
	id = {Tomm2021},
	isbn = {1748-3395},
	journal = {Nature Nanotechnology},
	number = {4},
	pages = {399--403},
	title = {A bright and fast source of coherent single photons},
	url = {https://doi.org/10.1038/s41565-020-00831-x},
	volume = {16},
	year = {2021},
	bdsk-url-1 = {https://doi.org/10.1038/s41565-020-00831-x}}

