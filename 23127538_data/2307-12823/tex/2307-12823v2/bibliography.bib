	title = {Error-mitigated quantum gates exceeding physical fidelities in a trapped-ion system},
	url = {https://doi.org/10.1038/s41467-020-14376-z},
	volume = {11},
	year = {2020},
	bdsk-url-1 = {https://doi.org/10.1038/s41467-020-14376-z}}

