	doi = {10.1126/science.1252319},
	journal = {Science},
	number = {6195},
	pages = {420-424},
	title = {Defining and detecting quantum speedup},
	url = {https://www.science.org/doi/abs/10.1126/science.1252319},
	volume = {345},
	year = {2014},
	bdsk-url-1 = {https://www.science.org/doi/abs/10.1126/science.1252319},
	bdsk-url-2 = {https://doi.org/10.1126/science.1252319}}

