	doi = {10.1126/science.abi9917},
	journal = {Science},
	number = {6562},
	pages = {1511-1514},
	title = {Entanglement transport and a nanophotonic interface for atoms in optical tweezers},
	url = {https://www.science.org/doi/abs/10.1126/science.abi9917},
	volume = {373},
	year = {2021},
