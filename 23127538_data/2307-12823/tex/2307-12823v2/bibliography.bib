	journal = {Nature},
	number = {7893},
	pages = {343--347},
	title = {Quantum logic with spin qubits crossing the surface code threshold},
	url = {https://doi.org/10.1038/s41586-021-04273-w},
	volume = {601},
	year = {2022},
	bdsk-url-1 = {https://doi.org/10.1038/s41586-021-04273-w}}

