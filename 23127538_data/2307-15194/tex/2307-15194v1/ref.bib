	eprint = {2109.14617},
	journal = {\mnras},
	keywords = {gravitational waves, nuclear reactions, nucleosynthesis, abundances, radiative transfer, methods: numerical, stars: neutron, Astrophysics - High Energy Astrophysical Phenomena},
	month = feb,
	number = {2},
	pages = {2820-2840},
	primaryclass = {astro-ph.HE},
	title = {{Dynamical ejecta of neutron star mergers with nucleonic weak processes - II: kilonova emission}},
