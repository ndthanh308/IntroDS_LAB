  doi = {10.1007/JHEP01(2014)129},
  url = {http://arxiv.org/abs/1311.3241},
  urldate = {2023-08-03},
  abstract = {We study the interplay between a particular marginal deformation of \$\{\textbackslash cal N\}=4\$ super Yang-Mills theory, the \$\textbackslash beta\$ deformation, and integrability in the holographic setting. Using modern methods of analytic non-integrability of Hamiltonian systems, we find that, when the \$\textbackslash beta\$ parameter takes imaginary values, classical string trajectories on the dual background become non-integrable. We expect the same to be true for generic complex \$\textbackslash beta\$ parameter. By exhibiting the Poincar\textbackslash 'e sections and phase space trajectories for the generic complex \$\textbackslash beta\$ case, we provide numerical evidence of strong sensitivity to initial conditions. Our findings agree with expectations from weak coupling that the complex \$\textbackslash beta\$ deformation is non-integrable and provide a rigorous argument beyond the trial and error approach to non-integrability.},
  keywords = {High Energy Physics - Theory,Nonlinear Sciences - Chaotic Dynamics},
  file = {files/1925/Giataganas et al_2014_On Marginal Deformations and Non-Integrability.pdf}
}

@article{giataganasNonintegrabilityChaosUnquenched2017,
