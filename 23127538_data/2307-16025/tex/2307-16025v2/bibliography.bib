@article{ZUTIC201985,
title = {Proximitized materials},
journal = {Materials Today},
volume = {22},
pages = {85-107},
year = {2019},
issn = {1369-7021},
doi = {https://doi.org/10.1016/j.mattod.2018.05.003},
url = {https://www.sciencedirect.com/science/article/pii/S1369702118301111},
author = {Igor uti and Alex Matos-Abiague and Benedikt Scharf and Hanan Dery and Kirill Belashchenko},
abstract = {Advances in scaling down heterostructures and having an improved interface quality together with atomically thin two-dimensional materials suggest a novel approach to systematically design materials. A given material can be transformed through proximity effects whereby it acquires properties of its neighbors, for example, becoming superconducting, magnetic, topologically nontrivial, or with an enhanced spinorbit coupling. Such proximity effects not only complement the conventional methods of designing materials by doping or functionalization but also can overcome their various limitations. In proximitized materials, it is possible to realize properties that are not present in any constituent region of the considered heterostructure. While the focus is on magnetic and spinorbit proximity effects with their applications in spintronics, the outlined principles also provide a broader framework for employing other proximity effects to tailor materials and realize novel phenomena.}
}
