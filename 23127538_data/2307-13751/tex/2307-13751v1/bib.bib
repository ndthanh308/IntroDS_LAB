	Doi = {10.1093/mnras/stt582},
	Eprint = {1304.0986},
	Journal = {\mnras},
	Keywords = {galaxies: active, quasars: absorption lines, quasars: general},
	Month = jun,
	Pages = {1525-1543},
	Primaryclass = {astro-ph.CO},
	Title = {{The average absorption properties of broad absorption line quasars at 800 $<$ {$\lambda$}$_{rest}$ $<$ 3000 {\AA}, and the underlying physical parameters}},
	Volume = 432,
