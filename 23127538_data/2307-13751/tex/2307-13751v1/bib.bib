	Journal = {\mnras},
	Keywords = {Astronomical Models, Big Bang Cosmology, Galactic Clusters, Galactic Evolution, Lyman Alpha Radiation, Protostars, Clouds, Critical Mass, Fragmentation, Radiation Pressure, Stellar Evolution, Universe},
	Month = jun,
