	Keywords = {methods: numerical, galaxies: evolution, galaxies: formation},
	Month = feb,
	Pages = {289-311},
	Title = {{Cosmological smoothed particle hydrodynamics simulations: a hybrid multiphase model for star formation}},
	Volume = 339,
	Year = 2003,
