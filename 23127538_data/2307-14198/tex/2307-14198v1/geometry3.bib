	Issn = {1556-4681},
	Issue_Date = {May 2017},
	Journal = {ACM Trans. Knowl. Discov. Data},
	Keywords = {information theory, rich networks, community detection, Graph partitioning},
	Month = {nov},
	Number = {2},
