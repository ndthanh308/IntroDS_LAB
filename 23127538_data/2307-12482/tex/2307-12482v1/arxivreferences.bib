@article{kmsfairhouse,
title = {{On the Complexity of Fair House Allocation}},
journal = {Operations Research Letters},
volume = {49},
number = {4},
pages = {572-577},
year = {2021},
author = {Naoyuki Kamiyama and Pasin Manurangsi and Warut Suksompong},
keywords = {House allocation, Assignment, Fairness, Envy-freeness, Proportionality, Equitability},
abstract = {We study fairness in house allocation, where m houses are to be allocated among n agents so that every agent receives one house. We show that maximizing the number of envy-free agents is hard to approximate to within a factor of n1−γ for any constant γ>0, and that the exact version is NP-hard even for binary utilities. Moreover, we prove that deciding whether a proportional allocation exists is computationally hard, whereas the corresponding problem for equitability can be solved efficiently.}
}


@article{gsvfairhouse,
title = {{Envy-Freeness in House Allocation Problems}},
