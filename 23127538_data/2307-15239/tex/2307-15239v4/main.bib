@Article{Gardelle2010,
  author    = {Vincent de Gardelle and Sid Kouider},
  journal   = {Psychological Science},
  number    = {1},
  pages     = {58--66},
  title     = {How Spatial Frequencies and Visual Awareness Interact During Face Processing},
  volume    = {21},
  year      = {2010},
  url       = {http://www.jstor.org/stable/41062165},
  abstract  = {In vision, high and low spatial frequencies have been dissociated at the cognitive and neural levels. Usually, high spatial frequency (HSF) is associated with slow analysis along the ventral cortical stream, and low spatial frequency (LSF) is associated with fast and automatic processing. These findings suggest a specific relation between spatial-frequency processing and visual awareness. We investigated this issue using masked-face priming with hybrid prime images of variable visibility. We found subliminal priming for both LSF and HSF information, along with a strong interaction between spatial frequency and visibility: HSF-related priming increased with stimulus visibility, whereas LSF influences remained unchanged. We argue that the results limit the validity of the coarse-to-fine model of vision and of models equating ventral-stream activity with perceptual awareness. Interpreting our results in light of the diagnostic approach suggests a close relation between awareness and diagnosticity.},
  file      = {:Gardelle2010.pdf:PDF},
  issn      = {09567976, 14679280},
  priority  = {prio1},
  publisher = {[Association for Psychological Science, Sage Publications, Inc.]},
  urldate   = {2023-12-24},
}

 
