  file = {C\:\\Users\\hanikevi\\Zotero\\storage\\SGDDRLDY\\Babaeizadeh et al. - 2018 - Stochastic Variational Video Prediction.pdf;C\:\\Users\\hanikevi\\Zotero\\storage\\DK958FU7\\1710.html}
