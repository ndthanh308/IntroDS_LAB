@article{sawers2017,
  title = {Small Forces That Differ with Prior Motor Experience Can Communicate Movement Goals during Human-Human Physical Interaction},
  author = {Sawers, Andrew and Bhattacharjee, Tapomayukh and McKay, J. Lucas and Hackney, Madeleine E. and Kemp, Charles C. and Ting, Lena H.},
